{
	"$schema": "https://json-schema.org/draft/2019-09/schema",
	"description": "List of additional insurance options.",
	"type": "array",
	"minItems": 1,
	"items": {
		"type": "object",
		"properties": {
			"optionName": {
				"description": "Option name",
				"type": "string"
			},
			"optionValue": {
				"description": "Option value",
				"type": "string"
			}
		},
		"additionalProperties": false,
		"required": [
			"optionName",
			"optionValue"
		]
	}
}