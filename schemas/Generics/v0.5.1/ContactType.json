{
  "$schema": "https://json-schema.org/draft/2019-09/schema",
  "description": "Contact information details",
  "type": "object",
  "properties": {
    "contactSurname": {
      "description": "Surname of the contact",
      "type": "string"
    },
    "contactFirstName": {
      "description": "First name of the contact",
      "type": "string"
    },
    "contactMail": {
      "description": "E-Mail of the contact",
      "type": "string"
    },
    "contactPhone": {
      "description": "Phonenumber of the contact",
      "type": "string"
    }
  },
  "required": [
    "contactSurname",
    "contactFirstName",
    "contactMail",
    "contactPhone"
  ],
  "additionalProperties": false
}