{
	"$schema": "https://json-schema.org/draft/2019-09/schema",
	"description": "Data type for the register number.",
	"examples": ["CHE-FINMA-F12345678", "LIE-FMA-123456", "CHE-IGB2B-12345"],
	"type": "string",
	"maxLength": 19,
	"pattern": "^(CHE\\-(FINMA\\-F\\d{8}|IGB2B\\-(\\d{5}))|LIE\\-FMA\\-\\d{6})$"
}