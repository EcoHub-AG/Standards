<?xml version="1.0" encoding="UTF-8"?><!--XSD Standard for IG B2B Data exchangeCopyright © IG B2B for Insurers + Brokers, Switzerland--><!--Changes in Version V5.2: - Added translated strings in documentation- Changed to single reminderChanges in Version V5.2.1 from V5.2: - Change version values and names of referenced data schemas from 5.2 to 5.2.1--><xs:schema xmlns:xs="http://www.w3.org/2001/XMLSchema" xmlns="http://www.IGB2B.ch/XMLSchema" targetNamespace="http://www.IGB2B.ch/XMLSchema" elementFormDefault="qualified" version="5.2.1">	<xs:include schemaLocation="globals_V5.2.1.xsd"/>	<xs:include schemaLocation="billingTypeLib_V5.2.1.xsd"/>	<xs:element name="premiumReminder">		<xs:annotation>			<xs:documentation xml:lang="de">HAUPTSTRUKTUR für den Prozess Mahnung</xs:documentation>			<xs:documentation xml:lang="en">MAIN STRUCTURE for proccess premiumReminder</xs:documentation>			<xs:documentation xml:lang="fr">STRUCTURE PRINCIPALE pour le processus sommation</xs:documentation>			<xs:documentation xml:lang="it">STRUTTURA PRINCIPALE per il processo sollecito</xs:documentation>		</xs:annotation>		<xs:complexType>			<xs:sequence>				<xs:element name="header">					<xs:annotation>						<xs:documentation xml:lang="de">Header</xs:documentation>						<xs:documentation xml:lang="en">Header</xs:documentation>						<xs:documentation xml:lang="fr">Entête</xs:documentation>						<xs:documentation xml:lang="it">Header</xs:documentation>					</xs:annotation>					<xs:complexType>						<xs:complexContent>							<xs:extension base="headerType">								<xs:sequence>									<xs:element name="typeRec">										<xs:annotation>											<xs:documentation xml:lang="de">Recordart</xs:documentation>											<xs:documentation xml:lang="en">Record type</xs:documentation>											<xs:documentation xml:lang="fr">Genre d’enregistrement</xs:documentation>											<xs:documentation xml:lang="it">Tipo di record</xs:documentation>										</xs:annotation>										<xs:simpleType>											<xs:restriction base="typeRecType">												<xs:enumeration value="002">													<xs:annotation>														<xs:documentation xml:lang="de">Mahnungen</xs:documentation>														<xs:documentation xml:lang="en">Reminders</xs:documentation>														<xs:documentation xml:lang="fr">Sommations</xs:documentation>														<xs:documentation xml:lang="it">Solleciti</xs:documentation>													</xs:annotation>												</xs:enumeration>											</xs:restriction>										</xs:simpleType>									</xs:element>									<xs:element name="subTypeRec">										<xs:annotation>											<xs:documentation xml:lang="de">Subrecordart</xs:documentation>											<xs:documentation xml:lang="en">Sub-record type</xs:documentation>											<xs:documentation xml:lang="fr">Genre de sous-enregistrement</xs:documentation>											<xs:documentation xml:lang="it">Genere di subrecord</xs:documentation>										</xs:annotation>										<xs:simpleType>											<xs:restriction base="subTypeRecType">												<xs:enumeration value="000">													<xs:annotation>														<xs:documentation xml:lang="de">Kein Subrecord</xs:documentation>														<xs:documentation xml:lang="en">No sub record</xs:documentation>														<xs:documentation xml:lang="fr">Pas de sous-enregistrement</xs:documentation>														<xs:documentation xml:lang="it">Nessun subrecord</xs:documentation>													</xs:annotation>												</xs:enumeration>											</xs:restriction>										</xs:simpleType>									</xs:element>									<xs:element name="versionRec">										<xs:annotation>											<xs:documentation xml:lang="de">Recordversion</xs:documentation>											<xs:documentation xml:lang="en">Record version</xs:documentation>											<xs:documentation xml:lang="fr">Version de l’enregistrement</xs:documentation>											<xs:documentation xml:lang="it">Versione record</xs:documentation>										</xs:annotation>										<xs:simpleType>											<xs:restriction base="versionRecType">												<xs:enumeration value="5.2.1"/>											</xs:restriction>										</xs:simpleType>									</xs:element>									<xs:element name="contentRec">										<xs:annotation>											<xs:documentation xml:lang="de">Recordinhalt</xs:documentation>											<xs:documentation xml:lang="en">Record content</xs:documentation>											<xs:documentation xml:lang="fr">Contenu de l’enregistrement</xs:documentation>											<xs:documentation xml:lang="it">Contenuto record</xs:documentation>										</xs:annotation>										<xs:simpleType>											<xs:restriction base="contentRecType">												<xs:enumeration value="01">													<xs:annotation>														<xs:documentation xml:lang="de">Nichtleben</xs:documentation>														<xs:documentation xml:lang="en">Non-life</xs:documentation>														<xs:documentation xml:lang="fr">Non-vie</xs:documentation>														<xs:documentation xml:lang="it">Non vita</xs:documentation>													</xs:annotation>												</xs:enumeration>												<xs:enumeration value="02">													<xs:annotation>														<xs:documentation xml:lang="de">Einzelleben</xs:documentation>														<xs:documentation xml:lang="en">Single life</xs:documentation>														<xs:documentation xml:lang="fr">Vie individuelle</xs:documentation>														<xs:documentation xml:lang="it">Vita individuale</xs:documentation>													</xs:annotation>												</xs:enumeration>												<xs:enumeration value="07">													<xs:annotation>														<xs:documentation xml:lang="de">Nichtleben / Motorfahrzeug</xs:documentation>														<xs:documentation xml:lang="en">Non-life / motor vehicle</xs:documentation>														<xs:documentation xml:lang="fr">Non-vie / Assurances automobiles</xs:documentation>														<xs:documentation xml:lang="it">Non vita / Assicurazioni veicoli a motore</xs:documentation>													</xs:annotation>												</xs:enumeration>												<xs:enumeration value="08">													<xs:annotation>														<xs:documentation xml:lang="de">Nichtleben / ohne Motorfahrzeug</xs:documentation>														<xs:documentation xml:lang="en">Non-life / without motor vehicle</xs:documentation>														<xs:documentation xml:lang="fr">Non-vie / sans assurances automobiles</xs:documentation>														<xs:documentation xml:lang="it">Non vita / senza assicurazioni veicoli a motore</xs:documentation>													</xs:annotation>												</xs:enumeration>												<xs:enumeration value="05">													<xs:annotation>														<xs:documentation xml:lang="de">Nichtleben / Personenversicherungen (Unfall/Kranken)</xs:documentation>														<xs:documentation xml:lang="en">Non-life / Personal insurance (accident / sickness)</xs:documentation>														<xs:documentation xml:lang="fr">Non-vie / Assurances de personnes (accident/maladie)</xs:documentation>														<xs:documentation xml:lang="it">Non-vita / Assicurazioni di persone (infortuni/malattia)</xs:documentation>													</xs:annotation>												</xs:enumeration>												<xs:enumeration value="06">													<xs:annotation>														<xs:documentation xml:lang="de">Nichtleben / Vermögens- und Sachversicherung</xs:documentation>														<xs:documentation xml:lang="en">Non-life / asset and property insurance</xs:documentation>														<xs:documentation xml:lang="fr">Non-vie/ Assurances choses et patrimoine</xs:documentation>														<xs:documentation xml:lang="it">Non-vita / Assicurazioni patrimonio e cose</xs:documentation>													</xs:annotation>												</xs:enumeration>											</xs:restriction>										</xs:simpleType>									</xs:element>								</xs:sequence>							</xs:extension>						</xs:complexContent>					</xs:complexType>				</xs:element>				<xs:element name="contractNo" type="contractNoType">					<xs:annotation>						<xs:documentation xml:lang="de">Vertragsnummer des laufenden Vertrages</xs:documentation>						<xs:documentation xml:lang="de">Gültige Vertragsnummer des Versicherers. Diese Nummern sind Suchbegriffe und sind im gleichen Format darzustellen, in dem sie auf allen Kundendokumenten erscheinen. So können sie auch ohne Editierungsregel vom Versicherer gedruckt werden. Der Vertragsnummernaufbau des einzelnen Versicherers kann dem IG-Verzeichnis – Vertragsnummern-Aufbau Versicherer - entnommen werden. Das aktuelle Dokument steht unter www.igb2b.ch im Mitgliederbereich zur Verfügung. Option / Empfehlung für Broker: In Bezug auf das Mitversicherungsgeschäft sollte auch die Erfassung der internen Vertragsnummer der beteiligten Versicherer möglich sein, damit eine individuelle Abrechnung der einzelnen Anteile durch diese Versicherer erfasst werden kann.</xs:documentation>						<xs:documentation xml:lang="en">Policy number of current policy</xs:documentation>						<xs:documentation xml:lang="en">Valid insurer's policy number. These numbers are search terms and should be presented in the same format in which they appear in all client documents. This means they can be printed by the insurer without an editing rule. The individual insurer's policy number structure can be found in the IG directory – insurer policy number structure. The current document is available at www.igb2b.ch in the members area. Option/recommendation for brokers: With regard to co-insurance business, it should also be possible to record the internal contract numbers of the insurers involved so that specific invoicing of the individual shares can be recorded by these insurers.</xs:documentation>						<xs:documentation xml:lang="fr">Numéro de contrat du contrat en cours</xs:documentation>						<xs:documentation xml:lang="fr">Numéro de contrat valable de l’assureur. Ces numéros constituent des critères de recherche et doivent être représentés dans le même format que celui dans lequel ils apparaissent sur tous les documents client. Ils peuvent ainsi être imprimés par l’assureur sans règle d’édition. La structure des numéros de contrat de l’assureur figure dans le répertoire IG – structure des numéros de contrat de l’assureur. Le document actuel se trouve sur www.igb2b.ch, à la rubrique «Domaine membres». Option / Recommandation pour le broker: en ce qui concerne les affaires en coassurance, la saisie du numéro de contrat interne de l'assureur participant doit également être possible, afin qu'un décompte individuel des différentes parties puisse être saisi par ces assureurs.</xs:documentation>						<xs:documentation xml:lang="it">Numero del contratto in corso</xs:documentation>						<xs:documentation xml:lang="it">Numero di contratto valido del contraente. Questi numeri sono termini di ricerca e devono essere indicati nello stesso formato in cui compaiono su tutti i documenti del cliente. Così possono essere stampati dall’assicuratore anche senza regola di editing. La struttura del numero di contratto dei singoli assicuratori è riportata nell’elenco IG – struttura numero di contratto assicuratori. Il documento attuale è disponibile su www.igb2b.ch nella sezione Ambito membri.</xs:documentation>					</xs:annotation>				</xs:element>				<xs:element name="brokerCodeIns" type="brokerCodeInsLocType" minOccurs="0">					<xs:annotation>						<xs:documentation xml:lang="de">Brokercode Versicherer</xs:documentation>						<xs:documentation xml:lang="de">Broker-Nummer gemäss dem Codeverzeichnis des einzelnen Versicherers.</xs:documentation>						<xs:documentation xml:lang="en">Insurer's broker code</xs:documentation>						<xs:documentation xml:lang="en">Broker number according to the individual insurer's code directory</xs:documentation>						<xs:documentation xml:lang="fr">Code broker de l’assureur</xs:documentation>						<xs:documentation xml:lang="fr">Numéro du broker selon le répertoire des codes de chaque assureur.</xs:documentation>						<xs:documentation xml:lang="it">Codice broker assicuratore</xs:documentation>						<xs:documentation xml:lang="it">Numero di broker in base all’elenco dei codici del singolo assicuratore</xs:documentation>					</xs:annotation>				</xs:element>				<xs:element name="agency" type="agencyLocType" minOccurs="0">					<xs:annotation>						<xs:documentation xml:lang="de">Agentur/Verkaufsstelle/Provisionsbezüger</xs:documentation>						<xs:documentation xml:lang="de">Agentur-, Verkaufsstellen- oder Provisionsbezüger-Nummer gemäss Codeverzeichnis des einzelnen Versicherers.</xs:documentation>						<xs:documentation xml:lang="en">Agency/sales point/commission agent</xs:documentation>						<xs:documentation xml:lang="en">Agency, sales point or commission agent number according to the individual insurer's code directory.</xs:documentation>						<xs:documentation xml:lang="fr">Agence/Point de vente/Bénéficiaire de la commission </xs:documentation>						<xs:documentation xml:lang="fr">Numéro de l’agence, du point de vente ou du bénéficiaire de la commission selon le répertoire des codes de chaque assureur.</xs:documentation>						<xs:documentation xml:lang="it">Agenzia/punto vendita/beneficiario della provvigione </xs:documentation>						<xs:documentation xml:lang="it">Numero di agenzia, punto vendita o beneficiario della provvigione in base all’elenco dei codici del singolo assicuratore.</xs:documentation>					</xs:annotation>				</xs:element>				<xs:element name="invoiceNo" type="noInvoiceLocType" minOccurs="0">					<xs:annotation>						<xs:documentation xml:lang="de">Rechnungsnummer</xs:documentation>						<xs:documentation xml:lang="de">Die der Prämienrechnung vom Versicherer in seinem Inkasso-/Exkassosystem zugeteilte Rechnungs- oder Zahlscheinnummer.</xs:documentation>						<xs:documentation xml:lang="en">Number of invoice</xs:documentation>						<xs:documentation xml:lang="en">The invoice or payment slip number assigned to the premium invoice by the insurer in its collection/disbursement system.</xs:documentation>						<xs:documentation xml:lang="fr">Numéro de la facture</xs:documentation>						<xs:documentation xml:lang="fr">Le numéro de la facture ou du bordereau de paiement du décompte de prime de l'assureur fourni par le système d'encaissement/de décaissement de l'assureur.</xs:documentation>						<xs:documentation xml:lang="it">Numero di fattura</xs:documentation>						<xs:documentation xml:lang="it">Il numero della fattura o del modulo di versamento attribuito dall’assicuratore al calcolo premi nel proprio sistema di incassi/esborsi.</xs:documentation>					</xs:annotation>				</xs:element>				<xs:element name="invoiceDate" type="xs:date" minOccurs="0">					<xs:annotation>						<xs:documentation xml:lang="de">Rechnungserstellungsdatum</xs:documentation>						<xs:documentation xml:lang="de">JJJJ-MM-TT = Datum der Rechnungserstellung.</xs:documentation>						<xs:documentation xml:lang="en">Invoice creation date</xs:documentation>						<xs:documentation xml:lang="en">YYYY-MM-DD = Date of invoice creation.</xs:documentation>						<xs:documentation xml:lang="fr">Date d'établissement de la facture</xs:documentation>						<xs:documentation xml:lang="fr">AAAA-MM-JJ = Date de l'établissement de la facture</xs:documentation>						<xs:documentation xml:lang="it">Data di fatturazione</xs:documentation>						<xs:documentation xml:lang="it">YYYY-MM-DD = Data di fatturazione</xs:documentation>					</xs:annotation>				</xs:element>				<xs:element name="lobIG" type="LOBIGType">					<xs:annotation>						<xs:documentation xml:lang="de">Branche</xs:documentation>						<xs:documentation xml:lang="de">Branchencode gemäss IG-Verzeichnis. Das aktuelle Dokument – Verzeichnis der Branchencodes –  steht unter www.igb2b.ch im Mitgliederbereich zur Verfügung.</xs:documentation>						<xs:documentation xml:lang="en">Sector</xs:documentation>						<xs:documentation xml:lang="en">Sector code in accordance with IG directory. The current document – branch code directory – is available in the members area at www.igb2b.ch.</xs:documentation>						<xs:documentation xml:lang="fr">Branche</xs:documentation>						<xs:documentation xml:lang="fr">Codes des branches selon le répertoire IG. Le document actuel (répertoire des codes des branches) se trouve sur www.igb2b.ch, à la rubrique «Domaine membres».</xs:documentation>						<xs:documentation xml:lang="it">Settore</xs:documentation>						<xs:documentation xml:lang="it">Codice settoriale in base all’elenco IG. Il documento attuale – Elenco dei codici settoriali – è disponibile su www.igb2b.ch nella sezione Ambito membri.</xs:documentation>					</xs:annotation>				</xs:element>				<xs:element name="lobINS" type="LOBINSType" minOccurs="0">					<xs:annotation>						<xs:documentation xml:lang="de">Branche/Risikobezeichnung</xs:documentation>						<xs:documentation xml:lang="de">Branchen- oder Risikocode gemäss Codeverzeichnis des einzelnen Versicherers. Der Detaillierungsgrad sollte den Angaben auf der Prämienrechnung entsprechen, bei kombinierten Produkten mindestens jedoch dem Branchencode. Option Versicherer: Anstatt eines Branchen- oder Risikocodes kann die Bezeichnung auch als Text erfolgen.</xs:documentation>						<xs:documentation xml:lang="en">Sector / Risk description</xs:documentation>						<xs:documentation xml:lang="en">Sector or risk code according to the individual insurer's code directory. The degree of detail must correspond with the details on the premium invoice, combined products at least the sector code. Option Insurer: Instead of a sector or risk code the description can be also as text.</xs:documentation>						<xs:documentation xml:lang="fr">Branche / Description du risque</xs:documentation>						<xs:documentation xml:lang="fr">Codes des branches ou de risque selon le répertoire des codes de chaque assureur. Le degré de détail doit correspondre avec les données sur la facture des primes, mais au minimum aux codes des branches pour les produits combinés. Option assureur: au lieu d'un code de branches ou de risque la description peut également être faite en texte.</xs:documentation>						<xs:documentation xml:lang="it">Settore / Denominazione rischio</xs:documentation>						<xs:documentation xml:lang="it">Codice settoriale o di rischio in base all'elenco dei codici dell'assicuratore individuale. Il grado de dettaglio deve corrispondere ai dettagli della fattura premio, ma almeno al codice settoriale per i prodotti combinati. Opzione di assicurazione: Invece di un codice settoriale o di rischio, la denominazione potrebbe anche essere testuale.</xs:documentation>					</xs:annotation>				</xs:element>				<xs:element name="currency" type="currencyCodeType" minOccurs="0">					<xs:annotation>						<xs:documentation xml:lang="de">Währung</xs:documentation>						<xs:documentation xml:lang="de">Code gemäss ISO-Code. Ohne Angabe der Währung gilt immer die Referenzwährung, d.h. zur Zeit der Schweizer Franken (CHF).</xs:documentation>						<xs:documentation xml:lang="en">Currency</xs:documentation>						<xs:documentation xml:lang="en">Code according to ISO code. Without specifying the currency, the reference currency always applies, at the time Swiss francs (CHF).</xs:documentation>						<xs:documentation xml:lang="fr">Monnaie</xs:documentation>						<xs:documentation xml:lang="fr">Code selon le code ISO. Sans indication de la monnaie, c'est toujours la monnaie de référence qui s'applique, c'est-à-dire actuellement le franc suisse (CHF).</xs:documentation>						<xs:documentation xml:lang="it">Moneta</xs:documentation>						<xs:documentation xml:lang="it">Codice secondo il codice ISO. Senza indicazionde della moneta, si applica sempre la moneta riferimento, al momento il franco svizzero (CHF).</xs:documentation>					</xs:annotation>				</xs:element>				<xs:element name="amountInvoice" type="decimal10dot2Type">					<xs:annotation>						<xs:documentation xml:lang="de">Prämienbetrag</xs:documentation>						<xs:documentation xml:lang="de">Dem Versicherungsnehmer die in Rechnung gestellte Prämie inkl. Stempel. Der Betrag ist im Normalfall positiv. Prämie, die gesetzlich gemahnt wird.</xs:documentation>						<xs:documentation xml:lang="en">Premium amount</xs:documentation>						<xs:documentation xml:lang="en">The policyholder the billed premium including stamp. The amount is normally positive. Premium subject to legal reminders</xs:documentation>						<xs:documentation xml:lang="fr">Montant de la prime</xs:documentation>						<xs:documentation xml:lang="fr">La prime facturée au preneur d'assurance y compris le timbre. Le montant est généralement positif. La prime qui est l’objet de la sommation légale.</xs:documentation>						<xs:documentation xml:lang="it">Importo del premio</xs:documentation>						<xs:documentation xml:lang="it">Il contraente il premio fatturato incluso timbro. L'importo è generalmente positivo.</xs:documentation>					</xs:annotation>				</xs:element>				<xs:element name="amountCharges" type="decimal5dot2Type" minOccurs="0">					<xs:annotation>						<xs:documentation xml:lang="de">Mahnspesen</xs:documentation>						<xs:documentation xml:lang="de">Spesen, die vom Versicherer für den Versand der Mahnung belastet werden.</xs:documentation>						<xs:documentation xml:lang="en">Reminder expenses</xs:documentation>						<xs:documentation xml:lang="en">Expenses charged by the insurer for sending reminders.</xs:documentation>						<xs:documentation xml:lang="fr">Frais de sommation</xs:documentation>						<xs:documentation xml:lang="fr">Frais facturés par l'assureur pour l'envoi de la sommation.</xs:documentation>						<xs:documentation xml:lang="it">Spese di sollecito</xs:documentation>						<xs:documentation xml:lang="it">Spese addebitate dall’assicuratore per l’invio del sollecito.</xs:documentation>					</xs:annotation>				</xs:element>				<xs:element name="demandDate" type="xs:date">					<xs:annotation>						<xs:documentation xml:lang="de">Versanddatum der Mahnung</xs:documentation>						<xs:documentation xml:lang="de">JJJJ-MM-TT = Datum des Mahnungsversandes</xs:documentation>						<xs:documentation xml:lang="en">Dispatch date of the reminder</xs:documentation>						<xs:documentation xml:lang="en">YYYY-MM-DD = Date of dispatch of the reminder</xs:documentation>						<xs:documentation xml:lang="fr">Date de l'envoi de la sommation</xs:documentation>						<xs:documentation xml:lang="fr">AAAA-MM-JJ = Date de l'envoi de la sommation</xs:documentation>						<xs:documentation xml:lang="it">Data di spedizione di sollecito</xs:documentation>						<xs:documentation xml:lang="it">JJJJ-MM-TT = Data di spedizione di sollecito</xs:documentation>					</xs:annotation>				</xs:element>				<xs:element name="interruptionDate" type="xs:date" minOccurs="0">					<xs:annotation>						<xs:documentation xml:lang="de">Deckungsunterbruch ab</xs:documentation>						<xs:documentation xml:lang="de">JJJJ-MM-TT = Datum des allfälligen Deckungsunterbruches bzw. Deckungsänderung (Leben)</xs:documentation>						<xs:documentation xml:lang="en">Interruption of cover from</xs:documentation>						<xs:documentation xml:lang="en">YYYY-MM-DD = Date or any interruption of cover or change of cover (life)</xs:documentation>						<xs:documentation xml:lang="fr">Interruption de la couverture du</xs:documentation>						<xs:documentation xml:lang="fr">AAAA-MM-JJ = Date de l'éventuelle interruption de la couverture respectivement de la modification de couverture (Vie)</xs:documentation>						<xs:documentation xml:lang="it">Sospensione della garanzia da</xs:documentation>						<xs:documentation xml:lang="it">YYYY-MM-DD = Data dell'eventuale sospensione della garanzia o modifica della garanzia (Vita)</xs:documentation>					</xs:annotation>				</xs:element>				<xs:choice>					<xs:annotation>						<xs:documentation xml:lang="de">Wahl zwischen einem Geschäfts- und Privatkunde</xs:documentation>						<xs:documentation xml:lang="en">Choice between a business and a private client</xs:documentation>						<xs:documentation xml:lang="fr">Choix entre un client entreprise et un client privé</xs:documentation>						<xs:documentation xml:lang="it">Scelta tra un cliente aziendale e un cliente privato</xs:documentation>					</xs:annotation>					<xs:element name="ICBusiness" type="ICBusinessLocType">						<xs:annotation>							<xs:documentation xml:lang="de">Forderung für eine Firma</xs:documentation>							<xs:documentation xml:lang="en">Amount receivable for a company</xs:documentation>							<xs:documentation xml:lang="fr">Réclamation pour une entreprise</xs:documentation>							<xs:documentation xml:lang="it">Domanda per un'azienda</xs:documentation>						</xs:annotation>					</xs:element>					<xs:element name="ICPrivatePerson" type="ICPrivatePersonLocType">						<xs:annotation>							<xs:documentation xml:lang="de">Forderung für eine Privatperson</xs:documentation>							<xs:documentation xml:lang="en">Amount receivable for a private person</xs:documentation>							<xs:documentation xml:lang="fr">Réclamation pour une personne privée</xs:documentation>							<xs:documentation xml:lang="it">Domanda per una persona privata</xs:documentation>						</xs:annotation>					</xs:element>				</xs:choice>				<xs:element name="remarks" type="remarkType" minOccurs="0">					<xs:annotation>						<xs:documentation xml:lang="de">Bemerkungen</xs:documentation>						<xs:documentation xml:lang="de">Textfeld für maschinell ausgelöste und standardisierte Hinweise. Hinweis: Bemerkungen sind nur in Ausnahmefällen anzubringen und nach Möglichkeit zu unterlassen, da damit der vollständige Workflow unterbrochen wird.</xs:documentation>						<xs:documentation xml:lang="en">Remark</xs:documentation>						<xs:documentation xml:lang="en">Text field for machine triggered and standardised information. Note: Comments should be made only in exceptional cases and avoided where possible, as they interrupt the complete workflow.</xs:documentation>						<xs:documentation xml:lang="fr">Remarques</xs:documentation>						<xs:documentation xml:lang="fr">Zone de texte pour les notes déclanchées automatiquement et standardisées. Note: les commentaires doivent être faits que dans des cas exceptionnels et doivent être si possible évités, car cela interrompt le workflow.</xs:documentation>						<xs:documentation xml:lang="it">Osservazione</xs:documentation>						<xs:documentation xml:lang="it">Campo di testo per note generate meccanicamente e standardizzate.  Nota: riportare delle osservazioni solo in casi eccezionali e possibilmente tralasciarle perché interrompono il workflow completo.</xs:documentation>					</xs:annotation>				</xs:element>				<xs:element name="attachment" type="attachmentType" minOccurs="0" maxOccurs="unbounded">					<xs:annotation>						<xs:documentation xml:lang="de">Dateianhang</xs:documentation>						<xs:documentation xml:lang="en">Attachment</xs:documentation>						<xs:documentation xml:lang="fr">Pièce jointe</xs:documentation>						<xs:documentation xml:lang="it">Allegato</xs:documentation>					</xs:annotation>				</xs:element>			</xs:sequence>		</xs:complexType>	</xs:element></xs:schema>