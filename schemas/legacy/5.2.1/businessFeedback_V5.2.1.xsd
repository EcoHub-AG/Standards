<?xml version="1.0" encoding="UTF-8"?><!--XSD Standard for IG B2B Data exchangeCopyright © IG B2B for Insurers + Brokers, Switzerland--><!--Changes in Version V5.2: - Added translated strings in documentationChanges in Version V5.2.1 from V5.2: - Change version values and names of referenced data schemas from 5.2 to 5.2.1--><xs:schema xmlns:xs="http://www.w3.org/2001/XMLSchema" xmlns="http://www.IGB2B.ch/XMLSchema" targetNamespace="http://www.IGB2B.ch/XMLSchema" elementFormDefault="qualified" version="5.2.1">	<xs:include schemaLocation="globals_V5.2.1.xsd"/>	<xs:include schemaLocation="businessFeedbackTypeLib_V5.2.1.xsd"/>	<xs:element name="businessFeedback">		<xs:annotation>			<xs:documentation xml:lang="de">HAUPTSTRUKTUR für fachliches Feedback</xs:documentation>			<xs:documentation xml:lang="en">MAIN STRUCTURE for proccess business Feedback</xs:documentation>			<xs:documentation xml:lang="fr">STRUCTURE PRINCIPALE pour feedback spécialisé</xs:documentation>			<xs:documentation xml:lang="it">STRUTTURA PRINCIPALE per feedback specialistico</xs:documentation>		</xs:annotation>		<xs:complexType>			<xs:sequence>				<xs:element name="header">					<xs:annotation>						<xs:documentation xml:lang="de">Header</xs:documentation>						<xs:documentation xml:lang="en">Header</xs:documentation>						<xs:documentation xml:lang="fr">Entête</xs:documentation>						<xs:documentation xml:lang="it">Header</xs:documentation>					</xs:annotation>					<xs:complexType>						<xs:complexContent>							<xs:extension base="headerType">								<xs:sequence>									<xs:element name="typeRec">										<xs:annotation>											<xs:documentation xml:lang="de">Recordart</xs:documentation>											<xs:documentation xml:lang="en">Record type</xs:documentation>											<xs:documentation xml:lang="fr">Genre d’enregistrement</xs:documentation>											<xs:documentation xml:lang="it">Tipo di record</xs:documentation>										</xs:annotation>										<xs:simpleType>											<xs:restriction base="typeRecType">												<xs:enumeration value="900">													<xs:annotation>														<xs:documentation xml:lang="de">Fachliches Feedback</xs:documentation>														<xs:documentation xml:lang="en">Business Feedback</xs:documentation>														<xs:documentation xml:lang="fr">Feedback spécialisé</xs:documentation>														<xs:documentation xml:lang="it">Feedback specialistico</xs:documentation>													</xs:annotation>												</xs:enumeration>											</xs:restriction>										</xs:simpleType>									</xs:element>									<xs:element name="subTypeRec">										<xs:annotation>											<xs:documentation xml:lang="de">Subrecordart</xs:documentation>											<xs:documentation xml:lang="en">Sub-record type</xs:documentation>											<xs:documentation xml:lang="fr">Genre de sous-enregistrement</xs:documentation>											<xs:documentation xml:lang="it">Tipo di subrecord</xs:documentation>										</xs:annotation>										<xs:simpleType>											<xs:restriction base="subTypeRecType">												<xs:enumeration value="009">													<xs:annotation>														<xs:documentation xml:lang="de">Feedback</xs:documentation>														<xs:documentation xml:lang="en">Feedback</xs:documentation>														<xs:documentation xml:lang="fr">Feedback</xs:documentation>														<xs:documentation xml:lang="it">Feedback</xs:documentation>													</xs:annotation>												</xs:enumeration>											</xs:restriction>										</xs:simpleType>									</xs:element>									<xs:element name="versionRec">										<xs:annotation>											<xs:documentation xml:lang="de">Recordversion</xs:documentation>											<xs:documentation xml:lang="en">Record version</xs:documentation>											<xs:documentation xml:lang="fr">Version de l’enregistrement</xs:documentation>											<xs:documentation xml:lang="it">Versione record</xs:documentation>										</xs:annotation>										<xs:simpleType>											<xs:restriction base="versionRecType">												<xs:enumeration value="5.2.1"/>											</xs:restriction>										</xs:simpleType>									</xs:element>									<xs:element name="contentRec">										<xs:annotation>											<xs:documentation xml:lang="de">Recordinhalt</xs:documentation>											<xs:documentation xml:lang="en">Record content</xs:documentation>											<xs:documentation xml:lang="fr">Contenu de l’enregistrement</xs:documentation>											<xs:documentation xml:lang="it">Contenuto record</xs:documentation>										</xs:annotation>										<xs:simpleType>											<xs:restriction base="contentRecType">												<xs:enumeration value="90">													<xs:annotation>														<xs:documentation xml:lang="de">Business</xs:documentation>														<xs:documentation xml:lang="en">Business</xs:documentation>														<xs:documentation xml:lang="fr">Business</xs:documentation>														<xs:documentation xml:lang="it">Affari</xs:documentation>													</xs:annotation>												</xs:enumeration>											</xs:restriction>										</xs:simpleType>									</xs:element>								</xs:sequence>							</xs:extension>						</xs:complexContent>					</xs:complexType>				</xs:element>				<xs:choice>					<xs:annotation>						<xs:documentation xml:lang="de">Wählen Sie zwischen den verschiedenen Feedback-Typen</xs:documentation>						<xs:documentation xml:lang="en">Choose between the different feedback types</xs:documentation>						<xs:documentation xml:lang="fr">Choisissez parmi les différents types de feedback</xs:documentation>						<xs:documentation xml:lang="it">Scegli tra i diversi tipi di feedback</xs:documentation>					</xs:annotation>					<xs:element name="feedbackTypeReferenceNo">						<xs:annotation>							<xs:documentation xml:lang="de">Art der Rückmeldung für Referenznummer</xs:documentation>							<xs:documentation xml:lang="en">Type of feedback for reference number</xs:documentation>							<xs:documentation xml:lang="fr">Genre du retour pour le numéro de référence</xs:documentation>							<xs:documentation xml:lang="it">Tipo di feedback per numero di riferimento</xs:documentation>						</xs:annotation>						<xs:complexType>							<xs:sequence>								<xs:element name="feedbackSubType" type="feedbackSubTypeReferenceNoLocType">									<xs:annotation>										<xs:documentation xml:lang="de">Feedbacksubtyp, Werte für Feedbacktyp Referenznummer</xs:documentation>										<xs:documentation xml:lang="en">Feedback subtype, values ​​for feedback type reference number</xs:documentation>										<xs:documentation xml:lang="fr">Sous-type de feedback, valeurs pour le numéro de référence du type de feedback</xs:documentation>										<xs:documentation xml:lang="it">Sottotipo di feedback, valori per il numero di riferimento del tipo di feedback</xs:documentation>									</xs:annotation>								</xs:element>							</xs:sequence>						</xs:complexType>					</xs:element>					<xs:element name="feedbackTypeMandateNo">						<xs:annotation>							<xs:documentation xml:lang="de">Art der Rückmeldung für Mandat</xs:documentation>							<xs:documentation xml:lang="en">Type of feedback for Mandate</xs:documentation>							<xs:documentation xml:lang="fr">Genre du retour pour le mandat</xs:documentation>							<xs:documentation xml:lang="it">Tipo di feedback per mandati</xs:documentation>						</xs:annotation>						<xs:complexType>							<xs:sequence>								<xs:element name="feedbackSubType" type="feedbackSubTypeMandateNoLocType">									<xs:annotation>										<xs:documentation xml:lang="de">Feedbacksubtyp, Werte für Feedbacktyp Mandat</xs:documentation>										<xs:documentation xml:lang="en">Feedback subtype, values ​​​​for feedback type mandate</xs:documentation>										<xs:documentation xml:lang="fr">Sous-type de feedback, valeurs pour le type de feedback mandat</xs:documentation>										<xs:documentation xml:lang="it">Sottotipo di feedback, valori per il tipo di feedback mandato</xs:documentation>									</xs:annotation>								</xs:element>							</xs:sequence>						</xs:complexType>					</xs:element>					<xs:element name="feedbackTypeCustomerData">						<xs:annotation>							<xs:documentation xml:lang="de">Art der Rückmeldung für Kundendaten</xs:documentation>							<xs:documentation xml:lang="en">Type of feedback for customer data</xs:documentation>							<xs:documentation xml:lang="fr">Genre du retour pour les données client</xs:documentation>							<xs:documentation xml:lang="it">Tipo di feedback per dati del cliente</xs:documentation>						</xs:annotation>						<xs:complexType>							<xs:sequence>								<xs:element name="feedbackSubType" type="feedbackSubTypeCustomerDataLocType">									<xs:annotation>										<xs:documentation xml:lang="de">Feedbacksubtyp, Werte für Feedbacktyp Kundendaten</xs:documentation>										<xs:documentation xml:lang="en">Feedback subtype, values ​​​​for feedback type customer data</xs:documentation>										<xs:documentation xml:lang="fr">Sous-type de feedback, valeurs pour le type de feedback données client</xs:documentation>										<xs:documentation xml:lang="it">Sottotipo di feedback, valori per il tipo di feedback dati del cliente</xs:documentation>									</xs:annotation>								</xs:element>							</xs:sequence>						</xs:complexType>					</xs:element>					<xs:element name="feedbackTypeClaim">						<xs:annotation>							<xs:documentation xml:lang="de">Art der Rückmeldung für Schaden</xs:documentation>							<xs:documentation xml:lang="en">Type of feedback for claim</xs:documentation>							<xs:documentation xml:lang="fr">Genre du retour pour sinistres</xs:documentation>							<xs:documentation xml:lang="it">Tipo di feedback per sinistro</xs:documentation>						</xs:annotation>						<xs:complexType>							<xs:sequence>								<xs:element name="feedbackSubType" type="feedbackSubTypeClaimLocType">									<xs:annotation>										<xs:documentation xml:lang="de">Feedbacksubtyp, Werte für Feedbacktyp Schaden</xs:documentation>										<xs:documentation xml:lang="en">Feedback subtype, values ​​​​for feedback type claim</xs:documentation>										<xs:documentation xml:lang="fr">Sous-type de feedback, valeurs pour le type de feedback sinistres</xs:documentation>										<xs:documentation xml:lang="it">Sottotipo di feedback, valori per il tipo di feedback sinistro</xs:documentation>									</xs:annotation>								</xs:element>							</xs:sequence>						</xs:complexType>					</xs:element>				</xs:choice>				<xs:element name="feedbackMessage" type="remarkType">					<xs:annotation>						<xs:documentation xml:lang="de">Feedback Nachricht</xs:documentation>						<xs:documentation xml:lang="de">Ergänzende bzw. präzisierende Fehlermeldung, welche Versicherer- oder Broker spezifisch sein kann/darf</xs:documentation>						<xs:documentation xml:lang="en">Feedback message</xs:documentation>						<xs:documentation xml:lang="en">Complementary or more precise error message, which can be insurers or broker specific</xs:documentation>						<xs:documentation xml:lang="fr">Message de feedback</xs:documentation>						<xs:documentation xml:lang="fr">Message d'erreur complémentaire respectivement de précisions indiquant quel assureur ou broker spécifiquement peut être/ peut</xs:documentation>						<xs:documentation xml:lang="it">Messaggio di feedback</xs:documentation>						<xs:documentation xml:lang="it">Messaggio di errore aggiuntivo o preciso che indica quale assicuratore o broker può/può essere specifico</xs:documentation>					</xs:annotation>				</xs:element>			</xs:sequence>		</xs:complexType>	</xs:element></xs:schema>