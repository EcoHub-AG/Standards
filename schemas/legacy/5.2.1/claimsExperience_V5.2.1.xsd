<?xml version="1.0" encoding="UTF-8"?><!--XSD Standard for IG B2B Data exchangeCopyright © IG B2B for Insurers + Brokers, Switzerland--><!--Changes in Version V5.2: - Added translated strings in documentation- Bugfix: removed typo in codeCoInsurance (replaced "l" by "I")- Number of experiences changed from 11 to unboundedChanges in Version V5.2.1 from V5.2: - Change version values and names of referenced data schemas from 5.2 to 5.2.1--><xs:schema xmlns:xs="http://www.w3.org/2001/XMLSchema" xmlns="http://www.IGB2B.ch/XMLSchema" targetNamespace="http://www.IGB2B.ch/XMLSchema" elementFormDefault="qualified" version="5.2.1">	<xs:include schemaLocation="globals_V5.2.1.xsd"/>	<xs:include schemaLocation="claimsExperienceTypeLib_V5.2.1.xsd"/>	<xs:element name="claimsExperience">		<xs:annotation>			<xs:documentation xml:lang="de">HAUPTSTRUKTUR für die XML-Definition Schadenrendement</xs:documentation>			<xs:documentation xml:lang="en">MAIN STRUCTURE for the XML definition claimsExperience</xs:documentation>			<xs:documentation xml:lang="fr">STRUCTURE PRINCIPALE pour la définition XML rendement des sinistres</xs:documentation>			<xs:documentation xml:lang="it">STRUTTURA PRINCIPALE per la definizione XML rendimento sinistri</xs:documentation>		</xs:annotation>		<xs:complexType>			<xs:sequence>				<xs:element name="header">					<xs:annotation>						<xs:documentation xml:lang="de">Header</xs:documentation>						<xs:documentation xml:lang="en">Header</xs:documentation>						<xs:documentation xml:lang="fr">Entête</xs:documentation>						<xs:documentation xml:lang="it">Header</xs:documentation>					</xs:annotation>					<xs:complexType>						<xs:complexContent>							<xs:extension base="headerType">								<xs:sequence>									<xs:element name="typeRec">										<xs:annotation>											<xs:documentation xml:lang="de">Recordart</xs:documentation>											<xs:documentation xml:lang="en">Record type</xs:documentation>											<xs:documentation xml:lang="fr">Genre d’enregistrement</xs:documentation>											<xs:documentation xml:lang="it">Tipo di record</xs:documentation>										</xs:annotation>										<xs:simpleType>											<xs:restriction base="typeRecType">												<xs:enumeration value="004">													<xs:annotation>														<xs:documentation xml:lang="de">Schadenrendement</xs:documentation>														<xs:documentation xml:lang="en">Claims experience</xs:documentation>														<xs:documentation xml:lang="fr">Rendement des sinistres</xs:documentation>														<xs:documentation xml:lang="it">Rendimento sinistri</xs:documentation>													</xs:annotation>												</xs:enumeration>											</xs:restriction>										</xs:simpleType>									</xs:element>									<xs:element name="subTypeRec">										<xs:annotation>											<xs:documentation xml:lang="de">Subrecordart</xs:documentation>											<xs:documentation xml:lang="en">Sub record type</xs:documentation>											<xs:documentation xml:lang="fr">Genre de sous-enregistrement</xs:documentation>											<xs:documentation xml:lang="it">Tipo di subrecord</xs:documentation>										</xs:annotation>										<xs:simpleType>											<xs:restriction base="subTypeRecType">												<xs:enumeration value="000">													<xs:annotation>														<xs:documentation xml:lang="de">Kein Subrecord</xs:documentation>														<xs:documentation xml:lang="en">No subrecord</xs:documentation>														<xs:documentation xml:lang="fr">Pas de sous-enregistrement</xs:documentation>														<xs:documentation xml:lang="it">Nessun tipo di subrecord</xs:documentation>													</xs:annotation>												</xs:enumeration>											</xs:restriction>										</xs:simpleType>									</xs:element>									<xs:element name="versionRec">										<xs:annotation>											<xs:documentation xml:lang="de">Recordversion</xs:documentation>											<xs:documentation xml:lang="en">Record version</xs:documentation>											<xs:documentation xml:lang="fr">Version de l’enregistrement</xs:documentation>											<xs:documentation xml:lang="it">Versione record</xs:documentation>										</xs:annotation>										<xs:simpleType>											<xs:restriction base="versionRecType">												<xs:enumeration value="5.2.1"/>											</xs:restriction>										</xs:simpleType>									</xs:element>									<xs:element name="contentRec">										<xs:annotation>											<xs:documentation xml:lang="de">Recordinhalt</xs:documentation>											<xs:documentation xml:lang="en">Record content</xs:documentation>											<xs:documentation xml:lang="fr">Contenu de l’enregistrement</xs:documentation>											<xs:documentation xml:lang="it">Contenuto record</xs:documentation>										</xs:annotation>										<xs:simpleType>											<xs:restriction base="contentRecType">												<xs:enumeration value="01">													<xs:annotation>														<xs:documentation xml:lang="de">Nichtleben</xs:documentation>														<xs:documentation xml:lang="en">Non life</xs:documentation>														<xs:documentation xml:lang="fr">Non-vie</xs:documentation>														<xs:documentation xml:lang="it">Non vita</xs:documentation>													</xs:annotation>												</xs:enumeration>											</xs:restriction>										</xs:simpleType>									</xs:element>								</xs:sequence>							</xs:extension>						</xs:complexContent>					</xs:complexType>				</xs:element>				<xs:choice>					<xs:annotation>						<xs:documentation xml:lang="de">Wahl zwischen einem Geschäfts- und Privatkunde</xs:documentation>						<xs:documentation xml:lang="en">Choice between a business and a private client</xs:documentation>						<xs:documentation xml:lang="fr">Choix entre un client entreprise et un client privé</xs:documentation>						<xs:documentation xml:lang="it">Scelta tra un cliente aziendale e un cliente privato</xs:documentation>					</xs:annotation>					<xs:element name="ICBusiness" type="ICBusinessLocalityLocType">						<xs:annotation>							<xs:documentation xml:lang="de">Schadenrendement für eine Firma</xs:documentation>							<xs:documentation xml:lang="en">Claims experience for a company</xs:documentation>							<xs:documentation xml:lang="fr">Rendement des sinistres pour une entreprise</xs:documentation>							<xs:documentation xml:lang="it">Rendimento sinistri per un'azienda</xs:documentation>						</xs:annotation>					</xs:element>					<xs:element name="ICPrivatePerson" type="ICPrivatePersonLocalityLocType">						<xs:annotation>							<xs:documentation xml:lang="de">Schadenrendement für eine Privatperson</xs:documentation>							<xs:documentation xml:lang="en">Claims experience for a private person</xs:documentation>							<xs:documentation xml:lang="fr">Rendement des sinistres pour une personne privée</xs:documentation>							<xs:documentation xml:lang="it">Rendimento sinistri per una persona privata</xs:documentation>						</xs:annotation>					</xs:element>				</xs:choice>				<xs:element name="contractNo" type="contractNoType">					<xs:annotation>						<xs:documentation xml:lang="de">Vertragsnummer des laufenden Vertrages</xs:documentation>						<xs:documentation xml:lang="de">Gültige Vertragsnummer des Versicherers. Diese Nummern sind Suchbegriffe und sind im gleichen Format darzustellen, in dem sie auf allen Kundendokumenten erscheinen. So können sie auch ohne Editierungsregel vom Versicherer gedruckt werden. Der Vertragsnummernaufbau des einzelnen Versicherers kann dem IG-Verzeichnis – Vertragsnummern-Aufbau Versicherer - entnommen werden. Das aktuelle Dokument steht unter www.igb2b.ch im Mitgliederbereich zur Verfügung.</xs:documentation>						<xs:documentation xml:lang="en">Policy number of current policy</xs:documentation>						<xs:documentation xml:lang="en">Valid insurer's policy number. These numbers are search terms and should be presented in the same format in which they appear in all client documents. This means they can be printed by the insurer without an editing rule.  The individual insurer's policy number structure can be found in the IG directory – insurer policy number structure. The current document is available at www.igb2b.ch in the members area.</xs:documentation>						<xs:documentation xml:lang="fr">Numéro de contrat du contrat en cours</xs:documentation>						<xs:documentation xml:lang="fr">Numéro de contrat valable de l’assureur. Ces numéros constituent des critères de recherche et doivent être représentés dans le même format que celui dans lequel ils apparaissent sur tous les documents client. Ils peuvent ainsi être imprimés par l’assureur sans règle d’édition. La structure des numéros de contrat de l’assureur figure dans le répertoire IG – structure des numéros de contrat de l’assureur. Le document actuel se trouve sur www.igb2b.ch, à la rubrique «Domaine membres».</xs:documentation>						<xs:documentation xml:lang="it">Numero del contratto in corso</xs:documentation>						<xs:documentation xml:lang="it">Numero di contratto valido del contraente. Questi numeri sono termini di ricerca e devono essere indicati nello stesso formato in cui compaiono su tutti i documenti del cliente. Così possono essere stampati dall’assicuratore anche senza regola di editing.  La struttura del numero di contratto dell’ assicuratori individuali è riportata nell’elenco IG – struttura numero di contratto assicuratori. Il documento attuale è disponibile su www.igb2b.ch nella sezione “Ambito membri”.</xs:documentation>					</xs:annotation>				</xs:element>				<xs:element name="generalAgreement" type="generalAgreementLocType" minOccurs="0">					<xs:annotation>						<xs:documentation xml:lang="de">Rahmenvertrags-/Flottenvertragsnummer</xs:documentation>						<xs:documentation xml:lang="de">Bei Rahmenverträgen oder MF-Flottenverträgen ist die Angabe der Rahmenvertragsnummer erwünscht, sofern unter Policen-/Vertragsnummer die Nummer der einzelnen unter den Rahmen- oder Flottenvertrag fallenden Versicherungen angegeben wird (die Rahmenvertrags-Nr. erleichtert dem Broker die richtige Zuordnung).</xs:documentation>						<xs:documentation xml:lang="en">Framework agreement/fleet agreement number</xs:documentation>						<xs:documentation xml:lang="en">In the case of framework agreements or motor vehicle fleet agreements, the framework agreement number is required if, under the policy/agreement number, the number of the individual insurances under the framework or fleet agreement is provided (the framework agreement number makes correct assignment easier for the broker).</xs:documentation>						<xs:documentation xml:lang="fr">Numéro de contrat-cadre / de contrat de flotte</xs:documentation>						<xs:documentation xml:lang="fr">En cas de contrats-cadre ou de contrats de flotte VAM, l’indication du numéro du contrat-cadre est recommandée dans la mesure où le numéro des  différentes assurances relevant du contrat-cadre ou du contrat de flotte est indiqué sous le numéro de la police/du contrat (le numéro du contrat-cadre simplifie l’attribution pour le broker).</xs:documentation>						<xs:documentation xml:lang="it">Numero contratto quadro/contratto parchi veicoli</xs:documentation>						<xs:documentation xml:lang="it">In caso di contratti quadro o parchi veicoli VM è richiesto il numero del contratto quadro se alla voce numero di polizza/contratto viene riportato il numero delle singole assicurazioni che rientrano nel contratto quadro o parchi veicoli (il n. di contratto quadro aiuta il broker nella giusta attribuzione).</xs:documentation>					</xs:annotation>				</xs:element>				<xs:element name="custPartnerCodeIns" type="custPartnerCodeInsLocType" minOccurs="0">					<xs:annotation>						<xs:documentation xml:lang="de">Kunden- oder Partnernummer</xs:documentation>						<xs:documentation xml:lang="de">Interner Code des Schadensachbearbeiters</xs:documentation>						<xs:documentation xml:lang="en">Client or partner number</xs:documentation>						<xs:documentation xml:lang="en">Claims adjuster's internal code</xs:documentation>						<xs:documentation xml:lang="fr">Numéro du client ou du partenaire</xs:documentation>						<xs:documentation xml:lang="fr">Code interne du collaborateur des sinistres</xs:documentation>						<xs:documentation xml:lang="it">Numero del cliente o del partner</xs:documentation>						<xs:documentation xml:lang="it">Codice interno del responsabile sinistri</xs:documentation>					</xs:annotation>				</xs:element>				<xs:element name="brokerCodeIns" type="brokerCodeInsLocType" minOccurs="0">					<xs:annotation>						<xs:documentation xml:lang="de">Brokercode Versicherer</xs:documentation>						<xs:documentation xml:lang="de">Broker-Nummer gemäss dem Codeverzeichnis des einzelnen Versicherers.</xs:documentation>						<xs:documentation xml:lang="en">Insurer's broker code</xs:documentation>						<xs:documentation xml:lang="en">Broker number according to the individual insurer's code directory.</xs:documentation>						<xs:documentation xml:lang="fr">Code broker de l’assureur</xs:documentation>						<xs:documentation xml:lang="fr">Numéro du broker selon le répertoire des codes de chaque assureur.</xs:documentation>						<xs:documentation xml:lang="it">Codice broker assicuratore</xs:documentation>						<xs:documentation xml:lang="it">Numero di broker in base all’elenco dei codici del singolo assicuratore.</xs:documentation>					</xs:annotation>				</xs:element>				<xs:element name="agency" type="agencyLocType" minOccurs="0">					<xs:annotation>						<xs:documentation xml:lang="de">Agentur/Verkaufsstelle/Provisionsbezüger</xs:documentation>						<xs:documentation xml:lang="de">Agentur-, Verkaufsstellen- oder Provisionsbezüger-Nummer gemäss Codeverzeichnis des einzelnen Versicherers</xs:documentation>						<xs:documentation xml:lang="en">Agency/sales point/commission agent</xs:documentation>						<xs:documentation xml:lang="en">Agency, sales point or commission agent number according to the individual insurer's code directory.</xs:documentation>						<xs:documentation xml:lang="fr">Agence/Point de vente/Bénéficiaire de la commission</xs:documentation>						<xs:documentation xml:lang="fr">Numéro de l’agence, du point de vente ou du bénéficiaire de la commission selon le répertoire des codes de chaque assureur.</xs:documentation>						<xs:documentation xml:lang="it">Agenzia/punto vendita/beneficiario della provvigione</xs:documentation>						<xs:documentation xml:lang="it">Numero di agenzia, punto vendita o beneficiario della provvigione in base all’elenco dei codici dell’assicuratore individuale.</xs:documentation>					</xs:annotation>				</xs:element>				<xs:element name="periodicity" type="periodicityLocType">					<xs:annotation>						<xs:documentation xml:lang="de">Periodizität des Rendements</xs:documentation>						<xs:documentation xml:lang="en">Loss ratio periodicityxx</xs:documentation>						<xs:documentation xml:lang="fr">Périodicité du rendement</xs:documentation>						<xs:documentation xml:lang="it">Periodicità del rendimento</xs:documentation>					</xs:annotation>				</xs:element>				<xs:choice>					<xs:annotation>						<xs:documentation xml:lang="de">Wahl zwischen dem Fall, dass das Versicherungsjahr gleich dem Schadenjahr ist oder Angabe einer individuellen Definition.</xs:documentation>						<xs:documentation xml:lang="en">Choice between a case where the insurance year is the same as the year of loss or provision of an individual definition.</xs:documentation>						<xs:documentation xml:lang="fr">Choix entre les cas où l’année d’assurance correspond à la l’année du sinistre ou l’indication d’une définition individuelle.</xs:documentation>						<xs:documentation xml:lang="it">Scelta tra il caso in cui l’anno di copertura coincide con quello del sinistro e l’indicazione di una definizione particolare.</xs:documentation>					</xs:annotation>					<xs:element name="insuranceYearDefinition">						<xs:annotation>							<xs:documentation xml:lang="de">Element für die Definition eines individuellen Versicherungsjahres.</xs:documentation>							<xs:documentation xml:lang="en">Element for the definition of an individual insurance year.</xs:documentation>							<xs:documentation xml:lang="fr">Elément pour la définition d’une année d’assurance individuelle.</xs:documentation>							<xs:documentation xml:lang="it">Elemento per la definizione di un anno di copertura individuale</xs:documentation>						</xs:annotation>						<xs:complexType>							<xs:sequence>								<xs:element name="insuranceYearFrom" type="xs:date">									<xs:annotation>										<xs:documentation xml:lang="de">Beginn des Versicherungsjahres</xs:documentation>										<xs:documentation xml:lang="de">JJJJ-MM-TT =  Beginn des Versicherungsjahres</xs:documentation>										<xs:documentation xml:lang="en">Start of  insurance year</xs:documentation>										<xs:documentation xml:lang="en">YYYY-MM-DD = Start of insurance year</xs:documentation>										<xs:documentation xml:lang="fr">Début de l’année d‘assurance</xs:documentation>										<xs:documentation xml:lang="fr">AAAA-MM-JJ = Début de l’année d‘assurance</xs:documentation>										<xs:documentation xml:lang="it">Inizio dell’anno assicurativo</xs:documentation>										<xs:documentation xml:lang="it">AAAA-MM-GG = Inizio dell’anno assicurativo</xs:documentation>									</xs:annotation>								</xs:element>								<xs:element name="insuranceYearTo" type="xs:date">									<xs:annotation>										<xs:documentation xml:lang="de">Ende des Versicherungsjahres</xs:documentation>										<xs:documentation xml:lang="de">JJJJ-MM-TT =  Ende des Versicherungsjahres</xs:documentation>										<xs:documentation xml:lang="en">End of  insurance year</xs:documentation>										<xs:documentation xml:lang="en">YYYY-MM-DD = End of insurance year</xs:documentation>										<xs:documentation xml:lang="fr">Fin de l’année d‘assurance</xs:documentation>										<xs:documentation xml:lang="fr">AAAA-MM-JJ = Fin de l’année d‘assurance</xs:documentation>										<xs:documentation xml:lang="it">Fine dell’anno assicurativo</xs:documentation>										<xs:documentation xml:lang="it">AAAA-MM-GG = Fine dell’anno assicurativo</xs:documentation>									</xs:annotation>								</xs:element>							</xs:sequence>						</xs:complexType>					</xs:element>					<xs:element name="insuranceYearEqualsClaimsYear" type="setValueTrueType">						<xs:annotation>							<xs:documentation xml:lang="de">Explizite Angabe, dass das Versicherungsjahr und das Schadenjahr gleich sind.</xs:documentation>							<xs:documentation xml:lang="en">Explicit indication that the insurance year and the year of loss are the same.</xs:documentation>							<xs:documentation xml:lang="fr">Déclaration explicite que l’année d’assurance et l’année du sinistre sont les mêmes.</xs:documentation>							<xs:documentation xml:lang="it">Indicazione esplicita che l’anno di copertura coincide con quello del sinistro.</xs:documentation>						</xs:annotation>					</xs:element>				</xs:choice>				<xs:element name="periodFrom" type="xs:date">					<xs:annotation>						<xs:documentation xml:lang="de">Rendementperiode von</xs:documentation>						<xs:documentation xml:lang="de">JJJJ-MM-TT =  Beginn der Periode für das erstellte Rendement</xs:documentation>						<xs:documentation xml:lang="en">Loss ratio period from</xs:documentation>						<xs:documentation xml:lang="en">YYYY-MM-DD = Start of  period for the established loss ratio</xs:documentation>						<xs:documentation xml:lang="fr">Période de rendement du</xs:documentation>						<xs:documentation xml:lang="fr">AAAA-MM-JJ = Début de la période pour le rendement établi</xs:documentation>						<xs:documentation xml:lang="it">Periodo del rendimento dal</xs:documentation>						<xs:documentation xml:lang="it">AAAA-MM-GG = Inizio del periodo per il rendimento creato</xs:documentation>					</xs:annotation>				</xs:element>				<xs:element name="periodTo" type="xs:date">					<xs:annotation>						<xs:documentation xml:lang="de">Rendementperiode bis</xs:documentation>						<xs:documentation xml:lang="de">JJJJ-MM-TT =  Ende der Periode für das erstellte Rendement</xs:documentation>						<xs:documentation xml:lang="en">Loss ratio period to</xs:documentation>						<xs:documentation xml:lang="en">YYYY-MM-DD = End of the period for the established loss ratio</xs:documentation>						<xs:documentation xml:lang="fr">Période de rendement jusqu‘au</xs:documentation>						<xs:documentation xml:lang="fr">AAAA-MM-JJ = Fin de la période pour le rendement établi</xs:documentation>						<xs:documentation xml:lang="it">Periodo del rendimento fino al</xs:documentation>						<xs:documentation xml:lang="it">AAAA-MM-GG = Fine del periodo per il rendimento creato</xs:documentation>					</xs:annotation>				</xs:element>				<xs:element name="updates">					<xs:annotation>						<xs:documentation xml:lang="de">Aktualisierungsstand</xs:documentation>						<xs:documentation xml:lang="en">Date last updated</xs:documentation>						<xs:documentation xml:lang="fr">Etat de l'actualisation</xs:documentation>						<xs:documentation xml:lang="it">Stato di aggiornamento</xs:documentation>					</xs:annotation>					<xs:complexType>						<xs:sequence>							<xs:element name="experienceDataUpdate" type="xs:date">								<xs:annotation>									<xs:documentation xml:lang="de">Aktualisierungsstand der Rendementdaten</xs:documentation>									<xs:documentation xml:lang="de">JJJJ-MM-TT =  Datum der letzten Aktualisierung der Rendementdaten, jedoch ohne Schadenreserven.</xs:documentation>									<xs:documentation xml:lang="en">Date loss ratio details last updated</xs:documentation>									<xs:documentation xml:lang="en">YYYY-MM-DD = Date of the last update oft he loss ratio details bus without claims reserves</xs:documentation>									<xs:documentation xml:lang="fr">Etat de l'actualisation des données du rendement</xs:documentation>									<xs:documentation xml:lang="fr">AAAA-MM-JJ = date de la dernière actualisation des données du rendement, mais sans les réserves des sinistres</xs:documentation>									<xs:documentation xml:lang="it">Stato di aggiornamento dei dati sul rendimento</xs:documentation>									<xs:documentation xml:lang="it">AAAA-MM-GG = Data dell’ultimo aggiornamento dei dati sul rendimento, ma senza riserve sinistro</xs:documentation>								</xs:annotation>							</xs:element>							<xs:element name="claimsReservesUpdate" type="xs:date">								<xs:annotation>									<xs:documentation xml:lang="de">Aktualisierungsstand der Schadenreserven</xs:documentation>									<xs:documentation xml:lang="de">JJJJ-MM-TT =  Datum der letzten Aktualisierung der Schadenreservedaten, sofern diese im Rendement angegeben werden.</xs:documentation>									<xs:documentation xml:lang="en">Date claims reserves last updated</xs:documentation>									<xs:documentation xml:lang="en">YYYY-MM-DD = Date of the last update of the claims reserves data, if these are specified in the loss ratio.</xs:documentation>									<xs:documentation xml:lang="fr">Etat de l'actualisation des réserves des sinistres</xs:documentation>									<xs:documentation xml:lang="fr">AAAA-MM-JJ = date de la dernière actualisation des données des réserves des sinistres, si celles-ci sont données dans le rendement.</xs:documentation>									<xs:documentation xml:lang="it">Stato di aggiornamento delle riserve sinistro</xs:documentation>									<xs:documentation xml:lang="it">AAAA-MM-GG = Data dell’ultimo aggiornamento dei dati sulla riserva sinistri, se questi sono indicati nel rendimento.</xs:documentation>								</xs:annotation>							</xs:element>						</xs:sequence>					</xs:complexType>				</xs:element>				<xs:element name="contractStatus" minOccurs="0">					<xs:annotation>						<xs:documentation xml:lang="de">Vertragsstatus</xs:documentation>						<xs:documentation xml:lang="en">Agreement status</xs:documentation>						<xs:documentation xml:lang="fr">Statut du contrat</xs:documentation>						<xs:documentation xml:lang="it">Stato contratto</xs:documentation>					</xs:annotation>					<xs:simpleType>						<xs:restriction base="xs:NMTOKEN">							<xs:maxLength value="2"/>							<xs:enumeration value="01">								<xs:annotation>									<xs:documentation xml:lang="de">laufender Vertrag</xs:documentation>									<xs:documentation xml:lang="en">current agreement</xs:documentation>									<xs:documentation xml:lang="fr">Contrat en cours</xs:documentation>									<xs:documentation xml:lang="it">Contratto in corso</xs:documentation>								</xs:annotation>							</xs:enumeration>							<xs:enumeration value="02">								<xs:annotation>									<xs:documentation xml:lang="de">sistierter Vertrag</xs:documentation>									<xs:documentation xml:lang="en">suspended agreement</xs:documentation>									<xs:documentation xml:lang="fr">Contrat suspendu</xs:documentation>									<xs:documentation xml:lang="it">Contratto sospeso</xs:documentation>								</xs:annotation>							</xs:enumeration>							<xs:enumeration value="03">								<xs:annotation>									<xs:documentation xml:lang="de">ersetzter Vertrag</xs:documentation>									<xs:documentation xml:lang="en">superseded agreement</xs:documentation>									<xs:documentation xml:lang="fr">Contrat remplacé</xs:documentation>									<xs:documentation xml:lang="it">Contratto sostituito</xs:documentation>								</xs:annotation>							</xs:enumeration>							<xs:enumeration value="04">								<xs:annotation>									<xs:documentation xml:lang="de">aufgehobener Vertrag</xs:documentation>									<xs:documentation xml:lang="en">cancelled agreement</xs:documentation>									<xs:documentation xml:lang="fr">Contrat annulé</xs:documentation>									<xs:documentation xml:lang="it">Contratto annullato</xs:documentation>								</xs:annotation>							</xs:enumeration>						</xs:restriction>					</xs:simpleType>				</xs:element>				<xs:choice>					<xs:annotation>						<xs:documentation xml:lang="de">Mitbeteiligungscode</xs:documentation>						<xs:documentation xml:lang="en">Copayment code</xs:documentation>						<xs:documentation xml:lang="fr">Code de coparticipation</xs:documentation>						<xs:documentation xml:lang="it">Codice di partecipazione</xs:documentation>					</xs:annotation>					<xs:element name="codeCoInsurance100" type="setValueTrueType">						<xs:annotation>							<xs:documentation xml:lang="de">Mitbeteiligungscode 100% (Defaultwert)</xs:documentation>							<xs:documentation xml:lang="en">Copayment code 100% (default value)</xs:documentation>							<xs:documentation xml:lang="fr">Code de coparticipation 100% (valeur par défaut)</xs:documentation>							<xs:documentation xml:lang="it">Codice di partecipazione 100% (valore di default)</xs:documentation>						</xs:annotation>					</xs:element>					<xs:element name="codeCoInsurancePartOnly">						<xs:annotation>							<xs:documentation xml:lang="de">Mitbeteiligungscode "nur Anteil des Versicherers"</xs:documentation>							<xs:documentation xml:lang="en">Copayment code "insurer's share only"</xs:documentation>							<xs:documentation xml:lang="fr">Code de coparticipation "uniquement la part de l'assureur"</xs:documentation>							<xs:documentation xml:lang="it">Codice di partecipazione "solo quota dell'assicuratore"</xs:documentation>						</xs:annotation>						<xs:complexType>							<xs:sequence>								<xs:element name="ratioCoInsurance" type="decimal3dot2Type">									<xs:annotation>										<xs:documentation xml:lang="de">Höhe des aufgeführten Anteiles</xs:documentation>										<xs:documentation xml:lang="de">Anzeige des Anteiles in %.</xs:documentation>										<xs:documentation xml:lang="en">Amount of listed share</xs:documentation>										<xs:documentation xml:lang="en">Display of the share as %.</xs:documentation>										<xs:documentation xml:lang="fr">L’importance de la part représentée</xs:documentation>										<xs:documentation xml:lang="fr">Indication de la part en %</xs:documentation>										<xs:documentation xml:lang="it">Ammontare della quota riportata</xs:documentation>										<xs:documentation xml:lang="it">Indicazione della quota in %</xs:documentation>									</xs:annotation>								</xs:element>							</xs:sequence>						</xs:complexType>					</xs:element>					<xs:element name="codeCoInsuranceOtherRiskTakers" type="setValueTrueType">						<xs:annotation>							<xs:documentation xml:lang="de">Mitbeteiligungscode "weitere Risikoträger (z.B. für Rentenfälle)"</xs:documentation>							<xs:documentation xml:lang="en">Copayment code "other risk carriers (e.g. in annuity cases)"</xs:documentation>							<xs:documentation xml:lang="fr">Code de coparticipation "autres porteurs de risques (p. ex. pour les cas de rentes)"</xs:documentation>							<xs:documentation xml:lang="it">Codice di partecipazione "altri assuntori del rischio (ad es. per casi di rendita)"</xs:documentation>						</xs:annotation>					</xs:element>				</xs:choice>				<xs:element name="codePremium" type="codePremiumLocType">					<xs:annotation>						<xs:documentation xml:lang="de">Code betreffend Prämiendefinition</xs:documentation>						<xs:documentation xml:lang="de">Abweichungen von der Definition gemäss IG B2B sind unter "Bemerkungen" durch die Versicherer zu spezifizieren.</xs:documentation>						<xs:documentation xml:lang="en">Code regarding premium definition</xs:documentation>						<xs:documentation xml:lang="en">Deviations from the definition according to IG B2B should be specified by the insurer under "Comments".</xs:documentation>						<xs:documentation xml:lang="fr">Code correspondant à la définition de la prime</xs:documentation>						<xs:documentation xml:lang="fr">Les différences par rapport à la définition IG B2B doivent être précisées par les assureurs sous «Remarques».</xs:documentation>						<xs:documentation xml:lang="it">Codice relativo alla definizione del premio</xs:documentation>						<xs:documentation xml:lang="it">Gli assicuratori devono specificare le variazioni rispetto alla definizione in base a IG B2B alla voce “Osservazioni”.</xs:documentation>					</xs:annotation>				</xs:element>				<xs:element name="codeClaimsPayment" type="codeClaimsPaymentLocType">					<xs:annotation>						<xs:documentation xml:lang="de">Code betreffend Schadenzahlungsdefinition</xs:documentation>						<xs:documentation xml:lang="de">Abweichungen von der Definition gemäss IG B2B sind unter "Bemerkungen" durch die Versicherer zu spezifizieren.</xs:documentation>						<xs:documentation xml:lang="en">Claims payment definition code</xs:documentation>						<xs:documentation xml:lang="en">Deviations from the definition according to IG B2B should be specified by the insurer under "Comments".</xs:documentation>						<xs:documentation xml:lang="fr">Code correspondant à la définition du paiement des sinistres</xs:documentation>						<xs:documentation xml:lang="fr">Les différences par rapport à la définition IG B2B doivent être précisées par les assureurs sous «Remarques».</xs:documentation>						<xs:documentation xml:lang="it">Codice relativo alla definizione di pagamento del sinistro</xs:documentation>						<xs:documentation xml:lang="it">Gli assicuratori devono specificare le variazioni rispetto alla definizione in base a IG B2B alla voce “Osservazioni”.</xs:documentation>					</xs:annotation>				</xs:element>				<xs:element name="codeReserves" type="codeReservesLocType">					<xs:annotation>						<xs:documentation xml:lang="de">Code betreffend Schadenreserven</xs:documentation>						<xs:documentation xml:lang="en">Claims reserve code</xs:documentation>						<xs:documentation xml:lang="fr">Code correspondant aux réserves des sinistres</xs:documentation>						<xs:documentation xml:lang="it">Codice relativo alle riserve sinistro</xs:documentation>					</xs:annotation>				</xs:element>				<xs:element name="codeRatio" type="codeRatioLocType">					<xs:annotation>						<xs:documentation xml:lang="de">Code betreffend Schadenaufwand/-quote</xs:documentation>						<xs:documentation xml:lang="en">Claims expenditure/loss ratio code</xs:documentation>						<xs:documentation xml:lang="fr">Code correspondant au taux/à la charge de la sinistralité</xs:documentation>						<xs:documentation xml:lang="it">Codice relativo al costo / quota sinistri</xs:documentation>					</xs:annotation>				</xs:element>				<xs:element name="codeAssignment" type="codeAssignmentLocType">					<xs:annotation>						<xs:documentation xml:lang="de">Code betreffend Schadenzuordnung (Personenversicherungen)</xs:documentation>						<xs:documentation xml:lang="en">Claims allocation code (personal insurance)</xs:documentation>						<xs:documentation xml:lang="fr">Code correspondant à l'attribution des sinistres (assurances de personnes)</xs:documentation>						<xs:documentation xml:lang="it">Codice relativo all'attribuzione del sinistro (assicurazioni di persone)</xs:documentation>					</xs:annotation>				</xs:element>				<xs:sequence>					<xs:element name="experienceDetails">						<xs:complexType>							<xs:sequence>								<xs:element name="experience" maxOccurs="unbounded">									<xs:complexType>										<xs:sequence>											<xs:element name="lobIG" type="LOBIGType">												<xs:annotation>													<xs:documentation xml:lang="de">Branche</xs:documentation>													<xs:documentation xml:lang="de">Branchencode gemäss IG-Verzeichnis. Das aktuelle Dokument – Verzeichnis der Branchencodes – steht unter www.igb2b.ch im Mitgliederbereich zur Verfügung.</xs:documentation>													<xs:documentation xml:lang="en">Sector</xs:documentation>													<xs:documentation xml:lang="en">Sector code in accordance with IG directory. The current document – branch code directory – is available in the members area at www.igb2b.ch.</xs:documentation>													<xs:documentation xml:lang="fr">Branche</xs:documentation>													<xs:documentation xml:lang="fr">Codes des branches selon le répertoire IG. Le document actuel (répertoire des codes des branches) se trouve sur www.igb2b.ch, à la rubrique «Domaine membres».</xs:documentation>													<xs:documentation xml:lang="it">Settore</xs:documentation>													<xs:documentation xml:lang="it">Codice settoriale in base all’elenco IG. Il documento attuale – Elenco dei codici settoriali – è disponibile su www.igb2b.ch nella sezione "Ambito membri".</xs:documentation>																			</xs:annotation>											</xs:element>											<xs:element name="lobINS" type="LOBINSType" minOccurs="0">												<xs:annotation>													<xs:documentation xml:lang="de">Branche / Risikobezeichnung bei kombinierten Verträgen</xs:documentation>													<xs:documentation xml:lang="de">Branchen- oder Risikocode gemäss Codeverzeichnis des einzelnen Versicherers. Bei kombinierten Produkten, die  unterschiedlich provisioniert werden,  muss der Detaillierungsgrad mindestens der Differenzierung bei den Provisionssätzen entsprechen.</xs:documentation>													<xs:documentation xml:lang="en">Sector/risk description in the case of joint policies</xs:documentation>													<xs:documentation xml:lang="en">Sector or risk code according to the individual insurer's code directory. In the case of combined products that are differently commissioned, the degree of detail must correspond to at least the difference in the commission rates.</xs:documentation>													<xs:documentation xml:lang="fr">Branche / description du risque en cas de contrats combinés</xs:documentation>													<xs:documentation xml:lang="fr">Codes des branches ou de risque selon le répertoire des codes de chaque assureur. En cas de produits combinés soumis à des commissions différentes, le degré de détail doit correspondre au moins à la différenciation des taux de commission.</xs:documentation>													<xs:documentation xml:lang="it">Settore / denominazione rischio in caso di contratti combinati</xs:documentation>													<xs:documentation xml:lang="it">Codice settoriale o di rischio in base all’elenco dei codici dell'assicuratore individuale. In caso di prodotti combinati per i quali le provvigioni differiscono, il grado di dettaglio deve almeno corrispondere alla differenziazione dei tassi di provvigione.</xs:documentation>												</xs:annotation>											</xs:element>											<xs:element name="pastYear" minOccurs="0" maxOccurs="4">												<xs:annotation>													<xs:documentation xml:lang="de">Angabe der Versicherungsvorjahre / Rendementvorjahre (0-4)</xs:documentation>													<xs:documentation xml:lang="en">Details of previous insurance years / previous loss ratio years</xs:documentation>													<xs:documentation xml:lang="fr">Indication des années précédentes d’assurance / années précédentes de rendement (0-4)</xs:documentation>													<xs:documentation xml:lang="it">Indicazione degli anni assicurativi precedenti / degli anni di rendimento precedenti</xs:documentation>												</xs:annotation>												<xs:complexType>													<xs:sequence>														<xs:element name="premium" type="decimal10dot2Type">															<xs:annotation>																<xs:documentation xml:lang="de">Prämien der Vorjahre</xs:documentation>																<xs:documentation xml:lang="de">Der Betrag in CHF ist entweder 0.00 oder positiv</xs:documentation>																<xs:documentation xml:lang="en">Premiums of previous years</xs:documentation>																<xs:documentation xml:lang="en">The amount in CHF is either 0.00 or positive</xs:documentation>																<xs:documentation xml:lang="fr">Primes des années précédentes</xs:documentation>																<xs:documentation xml:lang="fr">Le montant en CHF est soit 0.00, soit positif</xs:documentation>																<xs:documentation xml:lang="it">Premi degli anni precedenti</xs:documentation>																<xs:documentation xml:lang="it">L’importo in CHF è o 0.00 o positivo</xs:documentation>															</xs:annotation>														</xs:element>														<xs:element name="payments" type="decimal10dot2Type">															<xs:annotation>																<xs:documentation xml:lang="de">Schadenzahlungen in den Vorjahren</xs:documentation>																<xs:documentation xml:lang="de">Der Betrag in CHF ist entweder 0.00 oder positiv</xs:documentation>																<xs:documentation xml:lang="en">Claims payment of previous years</xs:documentation>																<xs:documentation xml:lang="en">The amount in CHF is either 0.00 or positive</xs:documentation>																<xs:documentation xml:lang="fr">Paiements des sinistres des années précédentes</xs:documentation>																<xs:documentation xml:lang="fr">Le montant en CHF est soit 0.00, soit positif</xs:documentation>																<xs:documentation xml:lang="it">Pagamenti sinistri negli anni precedenti</xs:documentation>																<xs:documentation xml:lang="it">L’importo in CHF è o 0.00 o positivo</xs:documentation>															</xs:annotation>														</xs:element>														<xs:element name="claimsReserves" type="decimal10dot2Type">															<xs:annotation>																<xs:documentation xml:lang="de">Schadenreserve der Vorjahre</xs:documentation>																<xs:documentation xml:lang="de">Der Betrag in CHF ist entweder 0.00 oder positiv</xs:documentation>																<xs:documentation xml:lang="en">Claims reserves of previous years</xs:documentation>																<xs:documentation xml:lang="en">The amount in CHF is either 0.00 or positive</xs:documentation>																<xs:documentation xml:lang="fr">Réserves des sinistres des années précédentes</xs:documentation>																<xs:documentation xml:lang="fr">Le montant en CHF est soit 0.00, soit positif</xs:documentation>																<xs:documentation xml:lang="it">Riserve sinistro degli anni precedenti</xs:documentation>																<xs:documentation xml:lang="it">L’importo in CHF è o 0.00 o positivo</xs:documentation>															</xs:annotation>														</xs:element>														<xs:element name="claimsExpenditure" type="decimal10dot2Type">															<xs:annotation>																<xs:documentation xml:lang="de">Schadenaufwand der Vorjahre</xs:documentation>																<xs:documentation xml:lang="de">Der Betrag in CHF ist entweder 0.00 oder positiv</xs:documentation>																<xs:documentation xml:lang="en">Claims exepnses of previous years></xs:documentation>																<xs:documentation xml:lang="en">The amount in CHF is either 0.00 or positive</xs:documentation>																<xs:documentation xml:lang="fr">Charge des sinistres des années précédentes</xs:documentation>																<xs:documentation xml:lang="fr">Le montant en CHF est soit 0.00, soit positif</xs:documentation>																<xs:documentation xml:lang="it">Costo sinistri degli anni precedenti</xs:documentation>																<xs:documentation xml:lang="it">L’importo in CHF è o 0.00 o positivo</xs:documentation>															</xs:annotation>														</xs:element>														<xs:element name="claimsRatio" type="decimal3dot2Type">															<xs:annotation>																<xs:documentation xml:lang="de">Schadenbelastungsquote der Vorjahre in Prozenten</xs:documentation>																<xs:documentation xml:lang="en">Claims ratio of previous years as %</xs:documentation>																<xs:documentation xml:lang="fr">Taux de charge de la sinistralité des années précédentes en pourcent %</xs:documentation>																<xs:documentation xml:lang="it">Quota onere sinistri degli anni precedenti in %</xs:documentation>															</xs:annotation>														</xs:element>														<xs:element name="claimsReported" type="decimal3dot0Type">															<xs:annotation>																<xs:documentation xml:lang="de">Anzahl angemeldeter Schadenfälle der Vorjahre</xs:documentation>																<xs:documentation xml:lang="en">Number of claims reported of previous years</xs:documentation>																<xs:documentation xml:lang="fr">Nombre de sinistres déclarés des années précédentes</xs:documentation>																<xs:documentation xml:lang="it">Numero sinistri notificati degli anni precedenti</xs:documentation>															</xs:annotation>														</xs:element>														<xs:element name="claimsPending" type="decimal3dot0Type">															<xs:annotation>																<xs:documentation xml:lang="de">Anzahl offener Schadenfälle der Vorjahre</xs:documentation>																<xs:documentation xml:lang="en">Number of open claims of previous years</xs:documentation>																<xs:documentation xml:lang="fr">Nombre de sinistres ouverts des années précédentes</xs:documentation>																<xs:documentation xml:lang="it">Numero sinistri in sospeso degli anni precedenti</xs:documentation>															</xs:annotation>														</xs:element>														<xs:element name="claimsSettled" type="decimal3dot0Type" minOccurs="0">															<xs:annotation>																<xs:documentation xml:lang="de">Anzahl erledigter Schadenfälle der Vorjahre</xs:documentation>																<xs:documentation xml:lang="en">Number of claims resolved of previous years</xs:documentation>																<xs:documentation xml:lang="fr">Nombre de sinistres réglés des années précédentes</xs:documentation>																<xs:documentation xml:lang="it">Numero sinistri sbrigati degli anni precedenti</xs:documentation>															</xs:annotation>														</xs:element>								