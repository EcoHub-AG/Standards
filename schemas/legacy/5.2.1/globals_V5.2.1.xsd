	<!--Communication Types-->
	<xs:simpleType name="telephoneNoType">
		<xs:annotation>
			<xs:documentation xml:lang="de">Datentyp für internationale Telefonnummern</xs:documentation>
			<xs:documentation xml:lang="de">Telefonnummer Format: + LAND (Leerzeichen) PREFIX ohne führendes Null (Leerzeichen) mindestens 6 Ziffern mit oder ohne Abstände wobei das letzte Zeichen eine Ziffer sein muss</xs:documentation>
			<xs:documentation xml:lang="de">Beispiele: +41 76 517 66 66 | +41 76 5176666 | +3906 3651 22 22 22 22 22 22 | +49 2232 2</xs:documentation>
			<xs:documentation xml:lang="en">Data type for international telephone numbers</xs:documentation>
			<xs:documentation xml:lang="en">Telephone number format: + COUNTRY (space) PREFIX without leading zero (space) 6 digit blocks with or without spacing and the last character of the sting must be a number</xs:documentation>
			<xs:documentation xml:lang="en">Examples: +41 76 517 66 66 | +41 76 5176666 | +3906 3651 22 22 22 22 22 22 | +49 2232 2</xs:documentation>
			<xs:documentation xml:lang="fr">Type de données pour les numéros de téléphone internationaux</xs:documentation>
			<xs:documentation xml:lang="fr">Format numéro de téléphone : + Pays (espace) PREFIXE sans le premier zéro (espace) au minimum 6 chiffres avec ou sans espace cependant le dernier caractère doit être un chiffre</xs:documentation>
			<xs:documentation xml:lang="fr">Exemples: +41 76 517 66 66 | +41 76 5176666 | +3906 3651 22 22 22 22 22 22 | +49 2232 2</xs:documentation>
			<xs:documentation xml:lang="it">Tipo di dati per numero telefono internazionale</xs:documentation>
			<xs:documentation xml:lang="it">Formato numero di telefono: + PAESE (Spazio) PREFIX senza il primo zero (Spazio) almeno 6 cifre con o senza spazi in cui l'ultimo carattere deve essere una cifra</xs:documentation>
			<xs:documentation xml:lang="it">Esempi: +41 76 517 66 66 | +41 76 5176666 | +3906 3651 22 22 22 22 22 22 | +49 2232 2</xs:documentation>
		</xs:annotation>
		<xs:restriction base="xs:string">
			<xs:maxLength value="30"/>
			<xs:pattern value="\+(1|20|211|212|213|216|218|220|221|222|223|224|225|226|227|228|229|230|231|232|233|234|235|236|237|238|239|240|241|242|243|244|245|246|247|248|249|250|251|252|253|254|255|256|257|258|260|261|262|263|264|265|266|267|268|269|27|290|291|297|298|299|30|31|32|33|34|350|351|352|353|354|355|356|357|358|359|36|370|371|372|373|374|375|376|377|378|379|380|381|382|383|385|386|387|388|389|39|3906|40|41|420|421|423|43|44|45|46|47|48|49|500|501|502|503|504|505|506|507|508|509|51|52|53|54|55|56|57|58|590|591|592|593|594|595|596|597|598|599|60|61|62|63|64|65|66|670|672|673|674|675|676|677|678|679|680|681|682|683|685|686|687|688|689|690|691|692|7|7|800|808|81|82|84|850|852|853|855|856|86|870|878|880|881|882|883|886|888|90|91|92|93|94|95|960|961|962|963|964|965|966|967|968|970|971|972|973|974|975|976|977|979|98|991|992|993|994|995|996|998) [1-9]{1}[0-9]{0,3} ([0-9]* ?){0,5}([0-9]+)"/>
		</xs:restriction>
	</xs:simpleType>
	<xs:simpleType name="emailAddressType">
		<xs:annotation>
			<xs:documentation xml:lang="de">Datentyp für E-Mail Adressen</xs:documentation>
			<xs:documentation xml:lang="en">Data type for email addresses</xs:documentation>
