<!--
Changes in Version V5.2: 
- Added translated strings in documentation
- Restriction of emailAddressType changed (see incident 1798)
Changes in Version V5.2.1 from V5.2: 
- Change version value from 5.2 to 5.2.1
- Change maxLength and pattern of simpleType name="regFINMANoType"
-->
<xs:schema xmlns:xs="http://www.w3.org/2001/XMLSchema" xmlns="http://www.IGB2B.ch/XMLSchema" targetNamespace="http://www.IGB2B.ch/XMLSchema" elementFormDefault="qualified" version="5.2.1">
	<xs:complexType name="headerType">
		<xs:sequence>
			<xs:element name="identificationNo" type="UUIDType">
				<xs:annotation>
					<xs:documentation xml:lang="de">Identifikations-Nummer</xs:documentation>
					<xs:documentation xml:lang="de">bei der Prozessinitiierung erstellte UUID (Version 4)
						Format: XXXXXXXX-XXXX-XXXX-XXXX-XXXXXXXXXXXX
						Zahl im Hexadezimalformat in fünf Gruppen unterteilt, Angabe des Wertes inkl. Gruppen-Separator</xs:documentation>
					<xs:documentation xml:lang="en">Identification Number</xs:documentation>
					<xs:documentation xml:lang="en">In the process initiation created UUID (version 4) 
						Format: XXXXXXXX-XXXX-XXXX-XXXX-XXXXXXXXXXXX
						Number in hexadecimal format divided into five groups, details of the value including group separator</xs:documentation>
					<xs:documentation xml:lang="fr">Numéro d’identification</xs:documentation>
					<xs:documentation xml:lang="fr">UUID créée lors du lancement du processus (version 4)
						Format: XXXXXXXX-XXXX-XXXX-XXXX-XXXXXXXXXXXX
						Nombre réparti en cinq groupes au format hexadécimal, indication de la valeur y c. séparateur de groupes</xs:documentation>
					<xs:documentation xml:lang="it">Numero d’identificazione</xs:documentation>
					<xs:documentation xml:lang="it">Nell’avvio del processo creare IDI (versione 4)
						Formato: XXXXXXXX-XXXX-XXXX-XXXX-XXXXXXXXXXXX
						Cifra in formato esadecimale suddiviso in cinque gruppi, indicazione del valore incluso separatore del gruppo</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="dateTimeCreation" type="xs:dateTime">
				<xs:annotation>
					<xs:documentation xml:lang="de">Erstellt am</xs:documentation>
					<xs:documentation xml:lang="de">yyyy-mm-ddThh:mm:ss = Erstellungsdatum des Records</xs:documentation>
					<xs:documentation xml:lang="en">Created on</xs:documentation>
					<xs:documentation xml:lang="en">yyyy-mm-ddThh:mm:ss = Record creation date</xs:documentation>
					<xs:documentation xml:lang="fr">Créé le</xs:documentation>
					<xs:documentation xml:lang="fr">aaaa-mm-jjThh:mm:ss = Date de création de l’enregistrement</xs:documentation>
					<xs:documentation xml:lang="it">Creato il</xs:documentation>
					<xs:documentation xml:lang="it">yyyy-mm-ddThh:mm:ss = Data di creazione del record</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="insurer">
				<xs:annotation>
					<xs:documentation xml:lang="de">Angaben zum Versicherer</xs:documentation>
					<xs:documentation xml:lang="en">Information about the insurer</xs:documentation>
					<xs:documentation xml:lang="fr">Informations relatives à l'assureur</xs:documentation>
					<xs:documentation xml:lang="it">Informazioni sull'assicuratore</xs:documentation>
				</xs:annotation>
				<xs:complexType>
					<xs:annotation>
						<xs:documentation xml:lang="de">Strukturdefinition für grundlegende Informationen über den Versicherer</xs:documentation>
						<xs:documentation xml:lang="en">Data type definition for structure of basic insurer information</xs:documentation>
						<xs:documentation xml:lang="fr">Définition de la structure pour les informations de base relatives à l'assureur</xs:documentation>
						<xs:documentation xml:lang="it">Definizione della struttura per l'informazione di base sull'assicuratore</xs:documentation>
					</xs:annotation>
					<xs:sequence>
						<xs:element name="insurerCode" type="insurerCodeType">
							<xs:annotation>
								<xs:documentation xml:lang="de">Versicherernummer FINMA</xs:documentation>
								<xs:documentation xml:lang="en">FINMA insurer number</xs:documentation>
								<xs:documentation xml:lang="fr">Numéro FINMA de l'assureur</xs:documentation>
								<xs:documentation xml:lang="it">Numero assicuratore FINMA</xs:documentation>
							</xs:annotation>
						</xs:element>
						<xs:element name="insurerName" minOccurs="0">
							<xs:annotation>
								<xs:documentation xml:lang="de">Name des Versicherers</xs:documentation>
								<xs:documentation xml:lang="en">Insurer's name</xs:documentation>
								<xs:documentation xml:lang="fr">Nom de l’assureur</xs:documentation>
								<xs:documentation xml:lang="it">Nome dell’assicuratore</xs:documentation>
							</xs:annotation>
							<xs:simpleType>
								<xs:restriction base="textLine">
									<xs:maxLength value="255"/>
								</xs:restriction>
							</xs:simpleType>
						</xs:element>
						<xs:element name="insurerAddress" type="addressType" minOccurs="0">
							<xs:annotation>
								<xs:documentation xml:lang="de">Adresse des Versicherers</xs:documentation>
								<xs:documentation xml:lang="en">Insurer's address</xs:documentation>
								<xs:documentation xml:lang="fr">Adresse de l'assureur</xs:documentation>
								<xs:documentation xml:lang="it">Indirizzo dell'assicuratore</xs:documentation>
							</xs:annotation>
						</xs:element>
						<xs:element name="insurerEmail" type="emailAddressType" minOccurs="0">
							<xs:annotation>
								<xs:documentation xml:lang="de">E-Mail Adresse des Versicherers</xs:documentation>
								<xs:documentation xml:lang="en">Email address of the insurer</xs:documentation>
								<xs:documentation xml:lang="fr">E-mail de l’assureur</xs:documentation>
								<xs:documentation xml:lang="it">E-mail assicuratore</xs:documentation>
							</xs:annotation>
						</xs:element>
						<xs:element name="insurerURL" type="xs:anyURI" minOccurs="0">
							<xs:annotation>
								<xs:documentation xml:lang="de">Webseite des Versicherers</xs:documentation>
								<xs:documentation xml:lang="en">Website of the insurer</xs:documentation>
								<xs:documentation xml:lang="fr">Site web de l'assureur</xs:documentation>
								<xs:documentation xml:lang="it">Sito web dell'assicuratore</xs:documentation>
							</xs:annotation>
						</xs:element>
					</xs:sequence>
				</xs:complexType>
			</xs:element>
			<xs:element name="broker">
				<xs:annotation>
					<xs:documentation xml:lang="de">Angaben zum Broker</xs:documentation>
					<xs:documentation xml:lang="en">Information about the broker</xs:documentation>
					<xs:documentation xml:lang="fr">Informations relatives au broker</xs:documentation>
					<xs:documentation xml:lang="it">Informazioni al broker</xs:documentation>
				</xs:annotation>
				<xs:complexType>
					<xs:annotation>
						<xs:documentation xml:lang="de">Strukturdefinition für grundlegende Informationen über den Broker</xs:documentation>
						<xs:documentation xml:lang="en">Data type definition for structure of basic broker information</xs:documentation>
						<xs:documentation xml:lang="fr">Définition de la structure pour les informations de base relatives au broker</xs:documentation>
						<xs:documentation xml:lang="it">Definizione della struttura per l'informazione di base sul broker</xs:documentation>
					</xs:annotation>
					<xs:sequence>
						<xs:element name="brokerRegisterNo" type="regFINMANoType">
							<xs:annotation>
								<xs:documentation xml:lang="de">Register Nummer des Brokers</xs:documentation>
								<xs:documentation xml:lang="de">Registernummer für die unabhängigen Vermittler gemäss Definition der IG B2B, d.h. Prefix mit Ländercode und Registerstelle plus Registernummer der FINMA bzw. FMA (Beispiel: CHE-FINMA-10001). Hinweis für Versicherer: Für diejenigen Vermittler, die sich nicht bei der FINMA bzw. FMA registrieren müssen, jedoch am elektronischen Datenaustausch teilnehmen wollen, z.B. Inhouse-Broker, muss der von der IG B2B zugeteilte Code (Beispiel: CHE-IGB2B-50001) erfasst werden.</xs:documentation>
								<xs:documentation xml:lang="en">Register number of the broker</xs:documentation>
								<xs:documentation xml:lang="en">Registers number for the independent intermediaries as defined by the IG B2B ; prefix with country code and register plus register number FINMA or FMA (example: CHE-FINMA-10001). Note for insurers: For those intermediaries who are not at FINMA or FMA but want to participate on electronic data exchange, e.g., in-house brokers, the code assigned by IG B2B (example: CHE-IGB2B-50001) must be recorded.</xs:documentation>
								<xs:documentation xml:lang="fr">Numéro de registre du broker</xs:documentation>
								<xs:documentation xml:lang="fr">Numéro de registre pour les intermédiaires indépendants selon la définition d'IG B2B, c'est-à-dire avec le préfixe du pays et du registre, plus le numéro de registre FINMA resp. FMA (par exemple : CHE-FINMA-10001). Note pour l'assureur: pour les intermédiaires qui ne doivent pas s'enregister auprès de la FINMA resp. FMA mais qui désirent participer à l'échange électronique des données, par exemple : broker cabinet d'auto-courtage, le code attribué par IG B2B doit être saisi (par exemple : CHE-IGB2B-50001).</xs:documentation>
								<xs:documentation xml:lang="it">Numero di registro broker</xs:documentation>
								<xs:documentation xml:lang="it">Numero di registro per gli intermediari indipendenti in base alla definizione di IG B2B, ovvero prefisso con codice del Paese e registro più numero di registro della FINMA o FMA (esempio: CHE-FINMA-10001). Nota per gli assicuratori: Per gli intermediari che non devono registrarsi presso la FINMA o FMA ma vogliono partecipare allo scambio elettronico di dati e utilizzare il Viewer XML come ad es. gli inhouse broker, è necessario inserire il codice assegnato da IG B2B (esempio: CHE-IGB2B-50001).</xs:documentation>
							</xs:annotation>
						</xs:element>
						<xs:element name="brokerName" minOccurs="0">
							<xs:annotation>
								<xs:documentation xml:lang="de">Name des Brokers</xs:documentation>
								<xs:documentation xml:lang="en">Name of the broker</xs:documentation>
								<xs:documentation xml:lang="fr">Nom du broker</xs:documentation>
								<xs:documentation xml:lang="it">Nome del broker</xs:documentation>
							</xs:annotation>
							<xs:simpleType>
								<xs:restriction base="textLine">
									<xs:maxLength value="255"/>
								</xs:restriction>
							</xs:simpleType>
						</xs:element>
						<xs:element name="brokerAddress" type="addressType" minOccurs="0">
							<xs:annotation>
								<xs:documentation xml:lang="de">Adresse des Brokers</xs:documentation>
								<xs:documentation xml:lang="en">Address of the broker</xs:documentation>
								<xs:documentation xml:lang="fr">Adresse du broker</xs:documentation>
								<xs:documentation xml:lang="it">Indirizzo del broker</xs:documentation>
							</xs:annotation>
						</xs:element>
						<xs:element name="brokerEmail" type="emailAddressType" minOccurs="0">
							<xs:annotation>
								<xs:documentation xml:lang="de">E-Mail Adresse des Brokers</xs:documentation>
								<xs:documentation xml:lang="en">Email address of the broker</xs:documentation>
								<xs:documentation xml:lang="fr">Adresse électronique (e-mail) du broker</xs:documentation>
								<xs:documentation xml:lang="it">E-Mail broker</xs:documentation>
							</xs:annotation>
						</xs:element>
						<xs:element name="brokerURL" type="xs:anyURI" minOccurs="0">
							<xs:annotation>
								<xs:documentation xml:lang="de">Webseite des Brokers</xs:documentation>
								<xs:documentation xml:lang="en">Website of the broker</xs:documentation>
								<xs:documentation xml:lang="fr">Site Internet du broker</xs:documentation>
								<xs:documentation xml:lang="it">Sito web del broker</xs:documentation>
							</xs:annotation>
						</xs:element>
					</xs:sequence>
				</xs:complexType>
			</xs:element>
		</xs:sequence>
	</xs:complexType>
	<xs:simpleType name="typeRecType">
		<xs:annotation>
			<xs:documentation xml:lang="de">Datentyp für den Record Type gemäss IG B2B Spezifikation</xs:documentation>
			<xs:documentation xml:lang="en">Data type for main records according to IG B2B specification</xs:documentation>
			<xs:documentation xml:lang="fr">Type de données pour le type d'enregistrement selon spécification IG B2B</xs:documentation>
			<xs:documentation xml:lang="it">Tipo di dati per tipo di record in base alle specifiche IG B2B</xs:documentation>
		</xs:annotation>
		<xs:restriction base="xs:NMTOKEN">
			<xs:maxLength value="3"/>
		</xs:restriction>
	</xs:simpleType>
	<xs:simpleType name="subTypeRecType">
		<xs:annotation>
			<xs:documentation xml:lang="de">Datentyp für den Sub Record Type gemäss IG B2B Spezifikation</xs:documentation>
			<xs:documentation xml:lang="en">Data type for sub records according to IG B2B specification</xs:documentation>
			<xs:documentation xml:lang="fr">Type de données pour le type de sous-enregistrement selon spécification IG B2B </xs:documentation>
			<xs:documentation xml:lang="it">Tipo di dati per tipo di subrecord in base alle specifiche IG B2B</xs:documentation>
		</xs:annotation>
		<xs:restriction base="xs:NMTOKEN">
			<xs:maxLength value="3"/>
		</xs:restriction>
	</xs:simpleType>
	<xs:simpleType name="versionRecType">
		<xs:annotation>
			<xs:documentation xml:lang="de">Datentyp für die Version gemäss IG B2B Spezifikation</xs:documentation>
			<xs:documentation xml:lang="en">Data type for record versions according IG B2B specification</xs:documentation>
			<xs:documentation xml:lang="fr">Type de données pour l'enregistrement de la version selon spécification IG B2B</xs:documentation>
			<xs:documentation xml:lang="it">Tipo di dati per la versione in base alle specifiche IG B2B</xs:documentation>
		</xs:annotation>
		<xs:restriction base="xs:string">
			<xs:maxLength value="12"/>
		</xs:restriction>
	</xs:simpleType>
	<xs:simpleType name="contentRecType">
		<xs:annotation>
			<xs:documentation xml:lang="de">Datentyp für den Content Record gemäss IG B2B Spezifikation</xs:documentation>
			<xs:documentation xml:lang="en">Data type for record content according IG B2B specification</xs:documentation>
			<xs:documentation xml:lang="fr">Type de données pour l'enregistrement du contenu selon spécification IG B2B</xs:documentation>
			<xs:documentation xml:lang="it">Tipo di dati per il tipo di contenuto in base alle specifiche IG B2B</xs:documentation>
		</xs:annotation>
		<xs:restriction base="xs:NMTOKEN">
			<xs:maxLength value="2"/>
		</xs:restriction>
	</xs:simpleType>
	<!--General Type Definitions-->
	<xs:simpleType name="textLine">
		<xs:annotation>
			<xs:documentation xml:lang="de">Datentyp für einzeilige Textfelder</xs:documentation>
			<xs:documentation xml:lang="en">Data type for single line text fields</xs:documentation>
			<xs:documentation xml:lang="fr">Type de données pour les champs de texte sur une seule ligne</xs:documentation>
			<xs:documentation xml:lang="it">Tipo di dati per campi di testo a riga singola</xs:documentation>
		</xs:annotation>
		<xs:restriction base="xs:string">
			<xs:pattern value="[\S ]*[\S][\S ]*"/>
		</xs:restriction>
	</xs:simpleType>
	<xs:simpleType name="textMultiLine">
		<xs:annotation>
			<xs:documentation xml:lang="de">Datentyp für mehrzeilige Textfelder</xs:documentation>
			<xs:documentation xml:lang="en">Data type for multi line text fields</xs:documentation>
			<xs:documentation xml:lang="fr">Type de données pour les champs de texte sur plusieurs lignes</xs:documentation>
			<xs:documentation xml:lang="it">Tipo di dati per campi di testo multilinea</xs:documentation>
		</xs:annotation>
		<xs:restriction base="xs:string">
			<xs:pattern value="(.|\r|\n)*[\S](.|\r|\n)*"/>
		</xs:restriction>
	</xs:simpleType>
	<xs:simpleType name="yesNoType">
		<xs:annotation>
			<xs:documentation xml:lang="de">Datentyp für ja / nein Felder</xs:documentation>
			<xs:documentation xml:lang="en">Data type for yes/no fields</xs:documentation>
			<xs:documentation xml:lang="fr">Type de données pour champs oui / non</xs:documentation>
			<xs:documentation xml:lang="it">Tipo di dati per i campi si / no</xs:documentation>
		</xs:annotation>
		<xs:restriction base="xs:NMTOKEN">
			<xs:maxLength value="2"/>
			<xs:enumeration value="01">
				<xs:annotation>
					<xs:documentation xml:lang="de">ja</xs:documentation>
					<xs:documentation xml:lang="en">yes</xs:documentation>
					<xs:documentation xml:lang="fr">oui</xs:documentation>
					<xs:documentation xml:lang="it">si</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="02">
				<xs:annotation>
					<xs:documentation xml:lang="de">nein</xs:documentation>
					<xs:documentation xml:lang="en">no</xs:documentation>
					<xs:documentation xml:lang="fr">non</xs:documentation>
					<xs:documentation xml:lang="it">no</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
		</xs:restriction>
	</xs:simpleType>
	<xs:simpleType name="languageType">
		<xs:annotation>
			<xs:documentation xml:lang="de">Datentyp für Sprachen</xs:documentation>
			<xs:documentation xml:lang="en">Data type for languages</xs:documentation>
			<xs:documentation xml:lang="fr">Type de données pour la langue</xs:documentation>
			<xs:documentation xml:lang="it">Tipo di dati per le lingue</xs:documentation>
		</xs:annotation>
		<xs:restriction base="xs:NMTOKEN">
			<xs:maxLength value="2"/>
			<xs:enumeration value="01">
				<xs:annotation>
					<xs:documentation xml:lang="de">Deutsch</xs:documentation>
					<xs:documentation xml:lang="en">German</xs:documentation>
					<xs:documentation xml:lang="fr">Allemand</xs:documentation>
					<xs:documentation xml:lang="it">Tedesco</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="02">
				<xs:annotation>
					<xs:documentation xml:lang="de">Französisch</xs:documentation>
					<xs:documentation xml:lang="en">French</xs:documentation>
					<xs:documentation xml:lang="fr">Français</xs:documentation>
					<xs:documentation xml:lang="it">Francese</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="03">
				<xs:annotation>
					<xs:documentation xml:lang="de">Italienisch</xs:documentation>
					<xs:documentation xml:lang="en">Italian</xs:documentation>
					<xs:documentation xml:lang="fr">Italien</xs:documentation>
					<xs:documentation xml:lang="it">Italiano</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="04">
				<xs:annotation>
					<xs:documentation xml:lang="de">Englisch</xs:documentation>
					<xs:documentation xml:lang="en">English</xs:documentation>
					<xs:documentation xml:lang="fr">Anglais</xs:documentation>
					<xs:documentation xml:lang="it">Inglese</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
		</xs:restriction>
	</xs:simpleType>
	<xs:simpleType name="remarkType">
		<xs:annotation>
			<xs:documentation xml:lang="de">Datentyp für generelle Kommentare</xs:documentation>
			<xs:documentation xml:lang="en">Data type for general comments</xs:documentation>
			<xs:documentation xml:lang="fr">Type de données pour commentaires généraux</xs:documentation>
			<xs:documentation xml:lang="it">Tipo di dati per commenti generali</xs:documentation>
		</xs:annotation>
		<xs:restriction base="textMultiLine">
			<xs:maxLength value="2000"/>
		</xs:restriction>
	</xs:simpleType>
	<xs:simpleType name="setValueTrueType">
		<xs:annotation>
			<xs:documentation xml:lang="de">Datentyp für die Spezifikation eines expliziten JA</xs:documentation>
			<xs:documentation xml:lang="en">Data type for specifying an explicit true value</xs:documentation>
			<xs:documentation xml:lang="fr">Type de données pour spécifier une valeur vraie explicite OUI</xs:documentation>
			<xs:documentation xml:lang="it">Tipo di dati per specificare un esplicito SI</xs:documentation>
		</xs:annotation>
		<xs:restriction base="xs:NMTOKEN">
			<xs:maxLength value="5"/>
			<xs:enumeration value="TRUE">
				<xs:annotation>
					<xs:documentation xml:lang="de">Wahr</xs:documentation>
					<xs:documentation xml:lang="en">True</xs:documentation>
					<xs:documentation xml:lang="fr">Vrai</xs:documentation>
					<xs:documentation xml:lang="it">Vero</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
		</xs:restriction>
	</xs:simpleType>
	<xs:simpleType name="periodicityType">
		<xs:annotation>
			<xs:documentation xml:lang="de">Datentyp für Periodizität</xs:documentation>
			<xs:documentation xml:lang="en">Data type for specifying the periodicity</xs:documentation>
			<xs:documentation xml:lang="fr">Type de données pour spécifier la périodicité</xs:documentation>
			<xs:documentation xml:lang="it">Tipo di dati per specificare la periodicità</xs:documentation>
		</xs:annotation>
		<xs:restriction base="xs:NMTOKEN">
			<xs:length value="2"/>
			<xs:enumeration value="01">
				<xs:annotation>
					<xs:documentation xml:lang="de">jährlich</xs:documentation>
					<xs:documentation xml:lang="en">annually</xs:documentation>
					<xs:documentation xml:lang="fr">annuel</xs:documentation>
					<xs:documentation xml:lang="it">annuale</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="02">
				<xs:annotation>
					<xs:documentation xml:lang="de">halbjährlich</xs:documentation>
					<xs:documentation xml:lang="en">1/2 year</xs:documentation>
					<xs:documentation xml:lang="fr">semestriel</xs:documentation>
					<xs:documentation xml:lang="it">semestrale</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="03">
				<xs:annotation>
					<xs:documentation xml:lang="de">dritteljährlich (ausser UVG)</xs:documentation>
					<xs:documentation xml:lang="en">1/3 year (except UVG)</xs:documentation>
					<xs:documentation xml:lang="fr">trois fois par an (sauf LAA)</xs:documentation>
					<xs:documentation xml:lang="it">terzo anno (eccetto LAINF)</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="04">
				<xs:annotation>
					<xs:documentation xml:lang="de">vierteljährlich</xs:documentation>
					<xs:documentation xml:lang="en">1/4 year</xs:documentation>
					<xs:documentation xml:lang="fr">trimestriel</xs:documentation>
					<xs:documentation xml:lang="it">trimestrale</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="05">
				<xs:annotation>
					<xs:documentation xml:lang="de">monatlich (ausser UVG)</xs:documentation>
					<xs:documentation xml:lang="en">monthly (except UVG)</xs:documentation>
					<xs:documentation xml:lang="fr">mensuel (sauf LAA)</xs:documentation>
					<xs:documentation xml:lang="it">mensile (eccetto LAINF)</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="06">
				<xs:annotation>
					<xs:documentation xml:lang="de">einmalig</xs:documentation>
					<xs:documentation xml:lang="en">onetime</xs:documentation>
					<xs:documentation xml:lang="fr">une fois</xs:documentation>
					<xs:documentation xml:lang="it">una volta</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
		</xs:restriction>
	</xs:simpleType>
	<!--IC Field Definitions-->
	<xs:simpleType name="genderType">
		<xs:annotation>
