<?xml version="1.0" encoding="UTF-8"?><!--XSD Standard for IG B2B Data exchangeCopyright © IG B2B for Insurers + Brokers, Switzerland--><!--Changes in Version V5.2: - Added translated strings in documentation- Contract-Request: attachment is optionalChanges in Version V5.2.1 from V5.2: - Change version values and names of referenced data schemas from 5.2 to 5.2.1--><xs:schema xmlns:xs="http://www.w3.org/2001/XMLSchema" xmlns="http://www.IGB2B.ch/XMLSchema" targetNamespace="http://www.IGB2B.ch/XMLSchema" elementFormDefault="qualified" version="5.2.1">	<xs:include schemaLocation="globals_V5.2.1.xsd"/>	<xs:include schemaLocation="contractGenericTypeLib_V5.2.1.xsd"/>	<xs:element name="contractRequest">		<xs:annotation>			<xs:documentation xml:lang="de">HAUPTSTRUKTUR für den Prozess Vertragserstellung Anfrage</xs:documentation>			<xs:documentation xml:lang="en">MAIN STRUCTURE for the process contract request</xs:documentation>			<xs:documentation xml:lang="fr">STRUCTURE PRINCIPALE pour le processus demande d’établissement du contrat </xs:documentation>			<xs:documentation xml:lang="it">STRUTTURA PRINCIPALE per il processo richesta d'allestimento contratti</xs:documentation>		</xs:annotation>		<xs:complexType>			<xs:sequence>				<xs:element name="header">					<xs:annotation>						<xs:documentation xml:lang="de">Header</xs:documentation>						<xs:documentation xml:lang="en">Header</xs:documentation>						<xs:documentation xml:lang="fr">Entête</xs:documentation>						<xs:documentation xml:lang="it">Header</xs:documentation>					</xs:annotation>					<xs:complexType>						<xs:complexContent>							<xs:extension base="headerType">								<xs:sequence>									<xs:element name="typeRec">										<xs:annotation>											<xs:documentation xml:lang="de">Recordart</xs:documentation>											<xs:documentation xml:lang="en">Record type</xs:documentation>											<xs:documentation xml:lang="fr">Genre d’enregistrement</xs:documentation>											<xs:documentation xml:lang="it">Tipo di record</xs:documentation>										</xs:annotation>										<xs:simpleType>											<xs:restriction base="typeRecType">												<xs:enumeration value="010">													<xs:annotation>														<xs:documentation xml:lang="de">Vertragserstellung</xs:documentation>														<xs:documentation xml:lang="en">Contract</xs:documentation>														<xs:documentation xml:lang="fr">Etablissement du contrat</xs:documentation>														<xs:documentation xml:lang="fr">Allestimento contratti</xs:documentation>													</xs:annotation>												</xs:enumeration>											</xs:restriction>										</xs:simpleType>									</xs:element>									<xs:element name="subTypeRec">										<xs:annotation>											<xs:documentation xml:lang="de">Subrecordart</xs:documentation>											<xs:documentation xml:lang="en">Sub record type</xs:documentation>											<xs:documentation xml:lang="fr">Genre de sous-enregistrement</xs:documentation>											<xs:documentation xml:lang="it">Tipo di subrecord</xs:documentation>										</xs:annotation>										<xs:simpleType>											<xs:restriction base="subTypeRecType">												<xs:enumeration value="003">													<xs:annotation>														<xs:documentation xml:lang="de">Vertragserstellung Anfrage</xs:documentation>														<xs:documentation xml:lang="en">Contracting request</xs:documentation>														<xs:documentation xml:lang="fr">Demande d‘établissement du contrat</xs:documentation>														<xs:documentation xml:lang="it">Richiesta d'allestimento contratti</xs:documentation>													</xs:annotation>												</xs:enumeration>											</xs:restriction>										</xs:simpleType>									</xs:element>									<xs:element name="versionRec">										<xs:annotation>											<xs:documentation xml:lang="de">Recordversion</xs:documentation>											<xs:documentation xml:lang="en">Record version</xs:documentation>											<xs:documentation xml:lang="fr">Version de l’enregistrement</xs:documentation>											<xs:documentation xml:lang="it">Versione record</xs:documentation>										</xs:annotation>										<xs:simpleType>											<xs:restriction base="versionRecType">												<xs:enumeration value="5.2.1"/>											</xs:restriction>										</xs:simpleType>									</xs:element>									<xs:element name="contentRec">										<xs:annotation>											<xs:documentation xml:lang="de">Recordinhalt</xs:documentation>											<xs:documentation xml:lang="en">Record content</xs:documentation>											<xs:documentation xml:lang="fr">Contenu de l’enregistrement</xs:documentation>											<xs:documentation xml:lang="it">Contenuto record</xs:documentation>										</xs:annotation>										<xs:simpleType>											<xs:restriction base="contentRecType"/>										</xs:simpleType>									</xs:element>								</xs:sequence>							</xs:extension>						</xs:complexContent>					</xs:complexType>				</xs:element>				<xs:choice>					<xs:annotation>						<xs:documentation xml:lang="de">Wahl zwischen einem Geschäfts- und Privatkunde</xs:documentation>						<xs:documentation xml:lang="en">Choice between a business and a private customer</xs:documentation>						<xs:documentation xml:lang="fr">Choix entre un client entreprise et un client privé</xs:documentation>						<xs:documentation xml:lang="it">Scelta tra un cliente aziendale e un cliente privato</xs:documentation>					</xs:annotation>					<xs:element name="ICBusiness" type="ICBusinessLocType">						<xs:annotation>							<xs:documentation xml:lang="de">Vertragsstellung für eine Firma</xs:documentation>							<xs:documentation xml:lang="en">Contract for a company</xs:documentation>							<xs:documentation xml:lang="fr">Etablissement du contrat pour une entreprise</xs:documentation>							<xs:documentation xml:lang="it">Allestimento contratti per un'azienda</xs:documentation>						</xs:annotation>					</xs:element>					<xs:element name="ICPrivatePerson" type="ICPrivatePersonLocType">						<xs:annotation>							<xs:documentation xml:lang="de">Vertragsstellung für eine Privatperson</xs:documentation>							<xs:documentation xml:lang="en">Contract for a private person</xs:documentation>							<xs:documentation xml:lang="fr">Etablissement du contrat pour une personne privée</xs:documentation>							<xs:documentation xml:lang="it">Allestimento contratti per una persona privata</xs:documentation>						</xs:annotation>					</xs:element>				</xs:choice>				<xs:element name="contractNo" type="contractNoType">					<xs:annotation>						<xs:documentation xml:lang="de">Vertragsnummer bzw. Policennummer</xs:documentation>						<xs:documentation xml:lang="de">Gültige Vertragsnummer des Versicherers. Diese Nummern sind Suchbegriffe und sind im gleichen Format darzustellen, in dem sie auf allen Kundendokumenten erscheinen. So können sie auch ohne Editierungsregel vom Versicherer gedruckt werden. Der Vertragsnummernaufbau des einzelnen Versicherers kann dem IG-Verzeichnis – Vertragsnummern-Aufbau Versicherer – entnommen werden. Das aktuelle Dokument steht unter www.igb2b.ch im Mitgliederbereich zur Verfügung.</xs:documentation>						<xs:documentation xml:lang="en">Policy number</xs:documentation>						<xs:documentation xml:lang="en">Valid insurer's policy number. These numbers are search terms and should be presented in the same format in which they appear in all client documents. This means they can be printed by the insurer without an editing rule. The individual insurer's policy number structure can be found in the IG directory – insurer policy number structure. The current document is available at www.igb2b.ch in the members area.</xs:documentation>						<xs:documentation xml:lang="fr">Numéro du contrat resp. du numéro de police</xs:documentation>						<xs:documentation xml:lang="fr">Numéro de contrat valable de l’assureur. Ces numéros constituent des critères de recherche et doivent figurer dans le même format que celui dans lequel ils apparaissent sur tous les documents client. Ils peuvent ainsi être imprimés par l’assureur sans règle d’édition. La structure des numéros de contrat de l’assureur figure dans le répertoire IG – structure des numéros de contrat de l’assureur. Le document actuel se trouve sur www.igb2b.ch, à la rubrique "Domaine membres".</xs:documentation>						<xs:documentation xml:lang="it">Numero del contratto</xs:documentation>						<xs:documentation xml:lang="it">Numero di contratto valido del contraente. Questi numeri sono termini di ricerca e devono essere indicati nello stesso formato in cui compaiono su tutti i documenti del cliente. Così possono essere stampati dall’assicuratore anche senza regola di editing. La struttura del numero di contratto dei singoli assicuratori è riportata nell’elenco IG – struttura numero di contratto assicuratori. Il documento attuale è disponibile su www.igb2b.ch nella sezione "Ambito membri".</xs:documentation>					</xs:annotation>				</xs:element>				<xs:element name="adviser">					<xs:annotation>						<xs:documentation xml:lang="de">Angaben zum Kundenberater</xs:documentation>						<xs:documentation xml:lang="en">Information about the customer adviser</xs:documentation>						<xs:documentation xml:lang="fr">Informations sur le conseiller à la clientèle</xs:documentation>						<xs:documentation xml:lang="it">Informazioni sul consulente alla clientela</xs:documentation>					</xs:annotation>					<xs:complexType>						<xs:sequence>							<xs:element name="adviserFirstname" type="firstnameType">								<xs:annotation>									<xs:documentation xml:lang="de">Vorname des Kundenberaters</xs:documentation>									<xs:documentation xml:lang="en">First name of client adviser</xs:documentation>									<xs:documentation xml:lang="fr">Prénom du conseiller à la clientèle</xs:documentation>									<xs:documentation xml:lang="it">Nome del consulente alla clientela</xs:documentation>								</xs:annotation>							</xs:element>							<xs:element name="adviserSurname" type="surnameType">								<xs:annotation>									<xs:documentation xml:lang="de">Nachname des Kundenberaters</xs:documentation>									<xs:documentation xml:lang="en">Name of client adviser</xs:documentation>									<xs:documentation xml:lang="fr">Nom du conseiller à la clientèle</xs:documentation>									<xs:documentation xml:lang="it">Cognome del consulente alla clientela</xs:documentation>								</xs:annotation>							</xs:element>							<xs:element name="adviserPhone" type="telephoneNoType" minOccurs="0">								<xs:annotation>									<xs:documentation xml:lang="de">Telefonnummer des Kundenberaters</xs:documentation>									<xs:documentation xml:lang="en">Phone number of adviser</xs:documentation>									<xs:documentation xml:lang="fr">Numéro de téléphone du conseiller à la clientèle</xs:documentation>									<xs:documentation xml:lang="it">Numero di telefono del consulente alla clientela</xs:documentation>								</xs:annotation>							</xs:element>						</xs:sequence>					</xs:complexType>				</xs:element>				<xs:element name="remarks" type="remarkType" minOccurs="0">					<xs:annotation>						<xs:documentation xml:lang="de">Bemerkungen</xs:documentation>						<xs:documentation xml:lang="de">Textfeld für maschinell ausgelöste und standardisierte Hinweise. Hinweis: Bemerkungen sind nur in Ausnahmefällen anzubringen und nach Möglichkeit zu unterlassen, da damit der vollständige Workflow unterbrochen wird.</xs:documentation>						<xs:documentation xml:lang="en">Remarks</xs:documentation>						<xs:documentation xml:lang="en">Text field for machine-triggered and standardized notes. Note: Comments are only to be made in exceptional cases and if possible omitted, as this will interrupt the entire workflow.</xs:documentation>						<xs:documentation xml:lang="fr">Remarques</xs:documentation>						<xs:documentation xml:lang="fr">Zone de texte pour les notes déclanchées automatiquement et standardisées. Note: les commentaires doivent être faits que dans des cas exceptionnels et doivent être si possible évités, car cela interrompt le workflow.</xs:documentation>						<xs:documentation xml:lang="it">Osservazione</xs:documentation>						<xs:documentation xml:lang="it">Campo di testo per note generate meccanicamente e standardizzate.  Nota: riportare delle osservazioni solo in casi eccezionali e possibilmente tralasciarle perché interrompono il workflow completo.</xs:documentation>					</xs:annotation>				</xs:element>				<xs:element name="attachment" type="attachmentType" minOccurs="0" maxOccurs="unbounded">					<xs:annotation>						<xs:documentation xml:lang="de">Dateianhang</xs:documentation>						<xs:documentation xml:lang="en">Attachment</xs:documentation>						<xs:documentation xml:lang="fr">Pièce jointe</xs:documentation>						<xs:documentation xml:lang="it">Allegato</xs:documentation>					</xs:annotation>				</xs:element>			</xs:sequence>		</xs:complexType>	</xs:element></xs:schema>