<?xml version="1.0" encoding="UTF-8"?><!--XSD Standard for IG B2B Data exchangeCopyright © IG B2B for Insurers + Brokers, Switzerland--><!--Changes in Version V5.2: - Added translated strings in documentationChanges in Version V5.2.1 from V5.2: - Change version values and names of referenced data schemas from 5.2 to 5.2.1--><xs:schema xmlns:xs="http://www.w3.org/2001/XMLSchema" xmlns="http://www.IGB2B.ch/XMLSchema" targetNamespace="http://www.IGB2B.ch/XMLSchema" elementFormDefault="qualified" version="5.2.1">	<xs:include schemaLocation="globals_V5.2.1.xsd"/>	<xs:include schemaLocation="technicalErrorTypeLib_V5.2.1.xsd"/>	<xs:element name="technicalError">		<xs:annotation>			<xs:documentation xml:lang="de">HAUPTSTRUKTUR für den Prozess technischer Fehler</xs:documentation>			<xs:documentation xml:lang="en">MAIN STRUCTURE for proccess technicalError</xs:documentation>			<xs:documentation xml:lang="fr">STRUCTURE PRINCIPALE pour le processus d'erreurs techniques</xs:documentation>			<xs:documentation xml:lang="it">STRUTTURA PRINCIPALE per il processo di errori tecnici</xs:documentation>		</xs:annotation>		<xs:complexType>			<xs:sequence>				<xs:element name="header">					<xs:annotation>						<xs:documentation xml:lang="de">Header</xs:documentation>						<xs:documentation xml:lang="en">Header</xs:documentation>						<xs:documentation xml:lang="fr">Entête</xs:documentation>						<xs:documentation xml:lang="it">Header</xs:documentation>					</xs:annotation>					<xs:complexType>						<xs:complexContent>							<xs:extension base="headerType">								<xs:sequence>									<xs:element name="typeRec">										<xs:annotation>											<xs:documentation xml:lang="de">Recordart</xs:documentation>											<xs:documentation xml:lang="en">Record type</xs:documentation>											<xs:documentation xml:lang="fr">Genre d’enregistrement</xs:documentation>											<xs:documentation xml:lang="it">Tipo di record</xs:documentation>										</xs:annotation>										<xs:simpleType>											<xs:restriction base="typeRecType">												<xs:enumeration value="999">													<xs:annotation>														<xs:documentation xml:lang="de">Technischer Fehler</xs:documentation>														<xs:documentation xml:lang="en">Technical Error</xs:documentation>														<xs:documentation xml:lang="fr">Erreur technique</xs:documentation>														<xs:documentation xml:lang="it">Errore tecnico</xs:documentation>													</xs:annotation>												</xs:enumeration>											</xs:restriction>										</xs:simpleType>									</xs:element>									<xs:element name="subTypeRec">										<xs:annotation>											<xs:documentation xml:lang="de">Subrecordart</xs:documentation>											<xs:documentation xml:lang="en">Sub record type</xs:documentation>											<xs:documentation xml:lang="fr">Genre de sous-enregistrement</xs:documentation>											<xs:documentation xml:lang="it">Tipo di subrecord</xs:documentation>										</xs:annotation>										<xs:simpleType>											<xs:restriction base="subTypeRecType">												<xs:enumeration value="009">													<xs:annotation>														<xs:documentation xml:lang="de">Fehler</xs:documentation>														<xs:documentation xml:lang="en">Error</xs:documentation>														<xs:documentation xml:lang="fr">Erreur</xs:documentation>														<xs:documentation xml:lang="it">Errore</xs:documentation>													</xs:annotation>												</xs:enumeration>											</xs:restriction>										</xs:simpleType>									</xs:element>									<xs:element name="versionRec">										<xs:annotation>											<xs:documentation xml:lang="de">Recordversion</xs:documentation>											<xs:documentation xml:lang="en">Record version</xs:documentation>											<xs:documentation xml:lang="fr">Version de l’enregistrement</xs:documentation>											<xs:documentation xml:lang="it">Versione record</xs:documentation>										</xs:annotation>										<xs:simpleType>											<xs:restriction base="versionRecType">												<xs:enumeration value="5.2.1"/>											</xs:restriction>										</xs:simpleType>									</xs:element>									<xs:element name="contentRec">										<xs:annotation>											<xs:documentation xml:lang="de">Recordinhalt</xs:documentation>											<xs:documentation xml:lang="en">Record content</xs:documentation>											<xs:documentation xml:lang="fr">Contenu de l’enregistrement</xs:documentation>											<xs:documentation xml:lang="it">Contenuto record</xs:documentation>										</xs:annotation>										<xs:simpleType>											<xs:restriction base="contentRecType">												<xs:enumeration value="99">													<xs:annotation>														<xs:documentation xml:lang="de">Technisch</xs:documentation>														<xs:documentation xml:lang="en">Technical</xs:documentation>														<xs:documentation xml:lang="fr">Techniquement</xs:documentation>														<xs:documentation xml:lang="it">Tecnicamente</xs:documentation>													</xs:annotation>												</xs:enumeration>											</xs:restriction>										</xs:simpleType>									</xs:element>								</xs:sequence>							</xs:extension>						</xs:complexContent>					</xs:complexType>				</xs:element>				<xs:choice>					<xs:annotation>						<xs:documentation xml:lang="de">Wahl zwischen den verschiedenen Typen der technischen Fehlerrückmeldung</xs:documentation>						<xs:documentation xml:lang="en">Choose between on of the error type feedbacks</xs:documentation>						<xs:documentation xml:lang="fr">Choix entre les différents types de retour d'erreur technique</xs:documentation>						<xs:documentation xml:lang="it">Scelta tra i diversi tipi di feedback degli errori tecnici</xs:documentation>					</xs:annotation>					<xs:element name="errorSchema">						<xs:annotation>							<xs:documentation xml:lang="de">Fehlertyp entspricht Schema</xs:documentation>							<xs:documentation xml:lang="en">Type of error schema</xs:documentation>							<xs:documentation xml:lang="fr">Le type d'erreur correspond au schéma</xs:documentation>							<xs:documentation xml:lang="it">Tipo di errore corrisponde allo schema</xs:documentation>						</xs:annotation>						<xs:complexType>							<xs:sequence>								<xs:element name="errorSubType" type="errorSchemaSubTypeLocType">									<xs:annotation>										<xs:documentation xml:lang="de">Feedbacksubtyp, Werte für Feedbacktyp Schema</xs:documentation>										<xs:documentation xml:lang="en">Feedback subtype, values for feedback type schema</xs:documentation>										<xs:documentation xml:lang="fr">Sous-type de feedback, valeurs pour le type de feedback schéma</xs:documentation>										<xs:documentation xml:lang="it">Sottotipo di feedback, valori per lo schema del tipo di feedback</xs:documentation>									</xs:annotation>								</xs:element>							</xs:sequence>						</xs:complexType>					</xs:element>					<xs:element name="errorAuthorization">						<xs:annotation>							<xs:documentation xml:lang="de">Fehlertyp entspricht Berechtigung</xs:documentation>							<xs:documentation xml:lang="en">Type of error authorization</xs:documentation>							<xs:documentation xml:lang="fr">Le type d'erreur correspond à l'autorisation</xs:documentation>							<xs:documentation xml:lang="it">Tipo di errore corrisponde all'autorizzazione</xs:documentation>						</xs:annotation>						<xs:complexType>							<xs:sequence>								<xs:element name="errorSubType" type="errorAuthorizationSubTypeLocType">									<xs:annotation>										<xs:documentation xml:lang="de">Feedbacksubtyp, Werte für Feedbacktyp Berechtigung</xs:documentation>										<xs:documentation xml:lang="en">Feedback subtype, values ​​for feedback type authorization</xs:documentation>										<xs:documentation xml:lang="fr">Sous-type de feedback, valeurs pour le type de feedback autorisation</xs:documentation>										<xs:documentation xml:lang="it">Sottotipo di feedback, valori per l'autorizzazione del tipo all'autorizzazione</xs:documentation>									</xs:annotation>								</xs:element>							</xs:sequence>						</xs:complexType>					</xs:element>					<xs:element name="errorProcessHandling">						<xs:annotation>							<xs:documentation xml:lang="de">Fehlertyp entspricht Prozessabwicklung</xs:documentation>							<xs:documentation xml:lang="en">Type of error for process handling</xs:documentation>							<xs:documentation xml:lang="fr">Le type d'erreur correspond au déroulement du processus</xs:documentation>							<xs:documentation xml:lang="it">Tipo di errore corrisponde all'esecuzione del processo</xs:documentation>						</xs:annotation>						<xs:complexType>							<xs:sequence>								<xs:element name="errorSubType" type="errorProcessHandlingSubTypeLocType">									<xs:annotation>										<xs:documentation xml:lang="de">Feedbacksubtyp, Werte für Feedbacktyp Prozessabwicklung</xs:documentation>										<xs:documentation xml:lang="en">Feedback subtype, values ​​for feedback type process handling</xs:documentation>										<xs:documentation xml:lang="fr">Sous-type de feedback, valeurs pour le type de feedback déroulement du processus</xs:documentation>										<xs:documentation xml:lang="it">Sottotipo di feedback, valori per l'esecuzione del processo di tipo feedback</xs:documentation>									</xs:annotation>								</xs:element>							</xs:sequence>						</xs:complexType>					</xs:element>					<xs:element name="errorEmbeddedFileContent">						<xs:annotation>							<xs:documentation xml:lang="de">Fehlertyp entspricht eingebetteter Dateiinhalt</xs:documentation>							<xs:documentation xml:lang="en">Type of error embedded file content</xs:documentation>							<xs:documentation xml:lang="fr">Le type d'erreur correspond au contenu du fichier inséré</xs:documentation>							<xs:documentation xml:lang="it">Tipo di errore corrisponde al contenuto del file incorporato</xs:documentation>						</xs:annotation>						<xs:complexType>							<xs:sequence>								<xs:element name="errorSubType" type="errorEmbeddedFileContentSubTypeLocType">									<xs:annotation>										<xs:documentation xml:lang="de">Feedbacksubtyp, Werte für Feedbacktyp eingebetteter Dateiinhalt</xs:documentation>										<xs:documentation xml:lang="en">Feedback subtype, values ​​for feedback type embedded file content</xs:documentation>										<xs:documentation xml:lang="fr">Sous-type de feedback, valeurs pour le type de feedback fichier inséré</xs:documentation>										<xs:documentation xml:lang="it">Sottotipo di feedback, valori di errore per contenuto del file incorporato</xs:documentation>									</xs:annotation>								</xs:element>							</xs:sequence>						</xs:complexType>					</xs:element>				</xs:choice>				<xs:element name="errorMessage" type="remarkType">					<xs:annotation>						<xs:documentation xml:lang="de">Fehlermeldung</xs:documentation>						<xs:documentation xml:lang="de">Ergänzende bzw. präzisierende Fehlermeldung, welche Versicherer- oder Brokerspezifisch sein kann/darf</xs:documentation>						<xs:documentation xml:lang="en">Error message</xs:documentation>						<xs:documentation xml:lang="en">Complementary or more precise error message, which can be insurers or broker specific</xs:documentation>						<xs:documentation xml:lang="fr">Message d'erreur</xs:documentation>						<xs:documentation xml:lang="fr">Message d'erreur complémentaire respectivement de précisions indiquant quel assureur ou broker spécifiquement peut être/ peut</xs:documentation>						<xs:documentation xml:lang="it">Messaggio di errore</xs:documentation>						<xs:documentation xml:lang="it">Messaggio di errore supplementare o preciso su quale assicuratore o broker può / può essere specifico</xs:documentation>					</xs:annotation>				</xs:element>			</xs:sequence>		</xs:complexType>	</xs:element></xs:schema>