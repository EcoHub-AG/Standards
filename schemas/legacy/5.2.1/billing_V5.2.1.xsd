<?xml version="1.0" encoding="UTF-8"?><!--XSD Standard for IG B2B Data exchangeCopyright © IG B2B for Insurers + Brokers, Switzerland--><!--Changes in Version V5.2: - Added translated strings in documentation- Changed from multiple invoices to single invoiceChanges in Version V5.2.1 from V5.2: - Change version values and names of referenced data schemas from 5.2 to 5.2.1--><xs:schema xmlns:xs="http://www.w3.org/2001/XMLSchema" xmlns="http://www.IGB2B.ch/XMLSchema" targetNamespace="http://www.IGB2B.ch/XMLSchema" elementFormDefault="qualified" version="5.2.1">	<xs:include schemaLocation="globals_V5.2.1.xsd"/>	<xs:include schemaLocation="billingTypeLib_V5.2.1.xsd"/>	<xs:element name="billing">		<xs:annotation>			<xs:documentation xml:lang="de">HAUPTSTRUKTUR für den Prozess Rechnungserstellung</xs:documentation>			<xs:documentation xml:lang="en">MAIN STRUCTURE for proccess billing</xs:documentation>			<xs:documentation xml:lang="fr">STRUCTURE PRINCIPALE pour le processus établissement de la facture</xs:documentation>			<xs:documentation xml:lang="it">STRUTTURA PRINCIPALE per il processo fatturazione</xs:documentation>		</xs:annotation>		<xs:complexType>			<xs:sequence>				<xs:element name="header">					<xs:annotation>						<xs:documentation xml:lang="de">Header</xs:documentation>						<xs:documentation xml:lang="en">Header</xs:documentation>						<xs:documentation xml:lang="fr">Entête</xs:documentation>						<xs:documentation xml:lang="it">Header</xs:documentation>					</xs:annotation>					<xs:complexType>						<xs:complexContent>							<xs:extension base="headerType">								<xs:sequence>									<xs:element name="typeRec">										<xs:annotation>											<xs:documentation xml:lang="de">Recordart</xs:documentation>											<xs:documentation xml:lang="en">Record type</xs:documentation>											<xs:documentation xml:lang="fr">Genre d’enregistrement</xs:documentation>											<xs:documentation xml:lang="it">Tipo di record</xs:documentation>										</xs:annotation>										<xs:simpleType>											<xs:restriction base="typeRecType">												<xs:enumeration value="001">													<xs:annotation>														<xs:documentation xml:lang="de">Prämien</xs:documentation>														<xs:documentation xml:lang="en">Premiums</xs:documentation>														<xs:documentation xml:lang="fr">Primes</xs:documentation>														<xs:documentation xml:lang="it">Premi</xs:documentation>													</xs:annotation>												</xs:enumeration>											</xs:restriction>										</xs:simpleType>									</xs:element>									<xs:element name="subTypeRec">										<xs:annotation>											<xs:documentation xml:lang="de">Subrecordart</xs:documentation>											<xs:documentation xml:lang="en">Sub-record type</xs:documentation>											<xs:documentation xml:lang="fr">Genre de sous-enregistrement</xs:documentation>											<xs:documentation xml:lang="it">Tipo di subrecord</xs:documentation>										</xs:annotation>										<xs:simpleType>											<xs:restriction base="subTypeRecType">												<xs:enumeration value="001">													<xs:annotation>														<xs:documentation xml:lang="de">Erst- und Teilprämien</xs:documentation>														<xs:documentation xml:lang="en">First and partial premiums</xs:documentation>														<xs:documentation xml:lang="fr">Prime initiale et partielle</xs:documentation>														<xs:documentation xml:lang="it">Premi primi e premi parziali</xs:documentation>													</xs:annotation>												</xs:enumeration>												<xs:enumeration value="002">													<xs:annotation>														<xs:documentation xml:lang="de">Folgeprämien</xs:documentation>														<xs:documentation xml:lang="en">Renewal premiums</xs:documentation>														<xs:documentation xml:lang="fr">Prime subséquente</xs:documentation>														<xs:documentation xml:lang="it">Premi di rinnovo</xs:documentation>													</xs:annotation>												</xs:enumeration>											</xs:restriction>										</xs:simpleType>									</xs:element>									<xs:element name="versionRec">										<xs:annotation>											<xs:documentation xml:lang="de">Recordversion</xs:documentation>											<xs:documentation xml:lang="en">Record version</xs:documentation>											<xs:documentation xml:lang="fr">Version de l’enregistrement</xs:documentation>											<xs:documentation xml:lang="it">Versione record</xs:documentation>										</xs:annotation>										<xs:simpleType>											<xs:restriction base="versionRecType">												<xs:enumeration value="5.2.1"/>											</xs:restriction>										</xs:simpleType>									</xs:element>									<xs:element name="contentRec">										<xs:annotation>											<xs:documentation xml:lang="de">Recordinhalt</xs:documentation>											<xs:documentation xml:lang="en">Record content</xs:documentation>											<xs:documentation xml:lang="fr">Contenu de l’enregistrement</xs:documentation>											<xs:documentation xml:lang="it">Contenuto record</xs:documentation>										</xs:annotation>										<xs:simpleType>											<xs:restriction base="contentRecType">												<xs:enumeration value="01">													<xs:annotation>														<xs:documentation xml:lang="de">Nichtleben</xs:documentation>														<xs:documentation xml:lang="en">Non-life</xs:documentation>														<xs:documentation xml:lang="fr">Non-vie</xs:documentation>														<xs:documentation xml:lang="it">Non-vita</xs:documentation>													</xs:annotation>												</xs:enumeration>												<xs:enumeration value="02">													<xs:annotation>														<xs:documentation xml:lang="de">Einzelleben</xs:documentation>														<xs:documentation xml:lang="en">Individual life</xs:documentation>														<xs:documentation xml:lang="fr">Vie individuelle</xs:documentation>														<xs:documentation xml:lang="it">Vita individuale</xs:documentation>													</xs:annotation>												</xs:enumeration>												<xs:enumeration value="07">													<xs:annotation>														<xs:documentation xml:lang="de">Nichtleben / Motorfahrzeug</xs:documentation>														<xs:documentation xml:lang="en">Non-Life / Vehicle Insurance</xs:documentation>														<xs:documentation xml:lang="fr">Non-vie / Assurances automobiles</xs:documentation>														<xs:documentation xml:lang="it">Non vita / Assicurazioni veicoli a motore</xs:documentation>													</xs:annotation>												</xs:enumeration>												<xs:enumeration value="08">													<xs:annotation>														<xs:documentation xml:lang="de">Nichtleben / ohne Motorfahrzeug</xs:documentation>														<xs:documentation xml:lang="en">Non-life / without vehicle Insurance</xs:documentation>														<xs:documentation xml:lang="fr">Non-vie / sans assurances automobiles</xs:documentation>														<xs:documentation xml:lang="it">Non vita / senza assicurazioni veicoli a motore</xs:documentation>													</xs:annotation>												</xs:enumeration>												<xs:enumeration value="05">													<xs:annotation>														<xs:documentation xml:lang="de">Nichtleben / Personenversicherungen (Unfall/Kranken)</xs:documentation>														<xs:documentation xml:lang="en">Non-Life Personal Insurance (accident/health)</xs:documentation>														<xs:documentation xml:lang="fr">Non-vie / Assurances de personnes (accident/maladie)</xs:documentation>														<xs:documentation xml:lang="it">Non-vita / Assicurazioni di persone (infortuni/malattia)</xs:documentation>													</xs:annotation>												</xs:enumeration>												<xs:enumeration value="06">													<xs:annotation>														<xs:documentation xml:lang="de">Nichtleben / Vermögens- und Sachversicherung</xs:documentation>														<xs:documentation xml:lang="en">Non-life / Financial loss and property insurance</xs:documentation>														<xs:documentation xml:lang="fr">Non-vie/ Assurances choses et patrimoine</xs:documentation>														<xs:documentation xml:lang="it">Non-vita / Assicurazioni patrimonio e cose</xs:documentation>													</xs:annotation>												</xs:enumeration>											</xs:restriction>										</xs:simpleType>									</xs:element>								</xs:sequence>							</xs:extension>						</xs:complexContent>					</xs:complexType>				</xs:element>				<xs:element name="contractNo" type="contractNoType">					<xs:annotation>						<xs:documentation xml:lang="de">Vertragsnummer des laufenden Vertrages</xs:documentation>						<xs:documentation xml:lang="de">Gültige Vertragsnummer des Versicherers. Diese Nummern sind Suchbegriffe und sind im gleichen Format darzustellen, in dem sie auf allen Kundendokumenten erscheinen. So können sie auch ohne Editierungsregel vom Versicherer gedruckt werden. Der Vertragsnummernaufbau des einzelnen Versicherers kann dem IG-Verzeichnis – Vertragsnummern-Aufbau Versicherer - entnommen werden. Das aktuelle Dokument steht unter www.igb2b.ch im Mitgliederbereich zur Verfügung. Option / Empfehlung für Broker: In Bezug auf das Mitversicherungsgeschäft sollte auch die Erfassung der internen Vertragsnummer der beteiligten Versicherer möglich sein, damit eine individuelle Abrechnung der einzelnen Anteile durch diese Versicherer erfasst werden kann.</xs:documentation>						<xs:documentation xml:lang="en">Policy number of current policy</xs:documentation>						<xs:documentation xml:lang="en">Valid insurer's policy number. These numbers are search terms and should be presented in the same format in which they appear in all client documents. This means they can be printed by the insurer without an editing rule. The individual insurer's policy number structure can be found in the IG directory – insurer policy number structure. The current document is available at www.igb2b.ch in the members area. Option / Recommendation for broker: With regard to co-insurance business, it should also be possible to record the internal contract numbers of the insurers involved so that specific invoicing of the individual shares can be recorded by these insurers.</xs:documentation>						<xs:documentation xml:lang="fr">Numéro de contrat du contrat en cours</xs:documentation>						<xs:documentation xml:lang="fr">Numéro de contrat valable de l’assureur. Ces numéros constituent des critères de recherche et doivent être représentés dans le même format que celui dans lequel ils apparaissent sur tous les documents client. Ils peuvent ainsi être imprimés par l’assureur sans règle d’édition. La structure des numéros de contrat de l’assureur figure dans le répertoire IG – structure des numéros de contrat de l’assureur. Le document actuel se trouve sur www.igb2b.ch, à la rubrique «Domaine membres». Option / Recommandation pour le broker: en ce qui concerne les affaires en coassurance, la saisie du numéro de contrat interne de l'assureur participant doit également être possible, afin qu'un décompte individuel des différentes parties puisse être saisi par ces assureurs.</xs:documentation>						<xs:documentation xml:lang="it">Numero del contratto in corso</xs:documentation>						<xs:documentation xml:lang="it">Numero di contratto valido dell'assicuratore. Questi numeri sono termini di ricerca e devono essere indicati nello stesso formato in cui compaiono su tutti i documenti del cliente. Così possono essere stampati dall’assicuratore anche senza regola di editing. La struttura del numero di contratto dei singoli assicuratori è riportata nell’elenco IG – struttura numero di contratto assicuratori. Il documento attuale è disponibile su www.igb2b.ch nella sezione Ambito membri.</xs:documentation>					</xs:annotation>				</xs:element>				<xs:element name="contractNoReplaced" type="contractNoType" minOccurs="0">					<xs:annotation>						<xs:documentation xml:lang="de">Vertragsnummer des ersetzen Vertrages</xs:documentation>						<xs:documentation xml:lang="de">Sofern der Versicherer aus technischen Gründen dem Vertrag eine neue Vertragsnummer zuteilt, so sollte dem Broker für die maschinelle Verarbeitung des Dokumentes einmalig zusätzlich die Vertragsnummer des ersetzten Vertrages mitgeliefert werden. Im XML-Viewer wird diese Vertragsnummer nicht angezeigt! Hinweis für Versicherer und Broker: Sofern der Versicherer ein neues Dokument erstellt, das dem Broker die Erfassung der neuen Vertragsnummer ermöglicht, ist die Information nicht notwendig.</xs:documentation>						<xs:documentation xml:lang="en">Number of replaced contract</xs:documentation>						<xs:documentation xml:lang="en">If the insurer assigns a new number of contract for technical reasons, the broker should be provided with the contract number of the replaced contract on a one-off basis for machine processing of the document. This contract number is not displayed in the XML viewer! Note for insurer and broker: If the insurer creates a new document that allows the broker to enter the new contract number, the information is not necessary.</xs:documentation>						<xs:documentation xml:lang="fr">Numéro de contrat du contrat remplacé</xs:documentation>						<xs:documentation xml:lang="fr">Dès lors que l'assureur pour des raisons techniques attribue un nouveau numéro de contrat au contrat, il faudrait livrer au broker, pour le traitement automatique du document, une seule fois additionnellement le numéro de contrat du contrat remplacé. Dans le viewer XML ce numéro de contrat ne sera pas indiqué! Note pour les assureurs et les brokers: dès lors que l'assureur établit un nouveau document, qui permet au broker la saisie du nouveau numéro de contrat, l'information n'est pas nécessaire.</xs:documentation>						<xs:documentation xml:lang="it">Numero del contratto sostituito</xs:documentation>						<xs:documentation xml:lang="it">Se per motivi tecnici l’assicuratore assegna al contratto un nuovo numero, al broker deve essere inviato una tantum il numero del contratto sostituito ai fini dell’elaborazione meccanica del documento. Nel Viewer XML il numero non viene visualizzato! Nota per gli assicuratori e i broker: se l’assicuratore emette un nuovo documento che consente al broker il rilevamento del nuovo numero di contratto, tale informazione non è necessaria.</xs:documentation>					</xs:annotation>				</xs:element>				<xs:element name="brokerCodeIns" type="brokerCodeInsLocType" minOccurs="0">					<xs:annotation>						<xs:documentation xml:lang="de">Brokercode Versicherer</xs:documentation>						<xs:documentation xml:lang="de">Broker-Nummer gemäss dem Codeverzeichnis des einzelnen Versicherers.</xs:documentation>						<xs:documentation xml:lang="en">Brokercode Insurer</xs:documentation>						<xs:documentation xml:lang="en">Broker number according to the individual insurer's code directory.</xs:documentation>						<xs:documentation xml:lang="fr">Code broker de l’assureur</xs:documentation>						<xs:documentation xml:lang="fr">Numéro du broker selon le répertoire des codes de chaque assureur.</xs:documentation>						<xs:documentation xml:lang="it">Codice broker assicuratore</xs:documentation>						<xs:documentation xml:lang="it">Numero broker in base all’elenco dei codici del singolo assicuratore.</xs:documentation>					</xs:annotation>				</xs:element>				<xs:element name="agency" type="agencyLocType" minOccurs="0">					<xs:annotation>						<xs:documentation xml:lang="de">Agentur/Verkaufsstelle/Provisionsbezüger</xs:documentation>						<xs:documentation xml:lang="de">Agentur-, Verkaufsstellen- oder Provisionsbezüger-Nummer gemäss Codeverzeichnis des einzelnen Versicherers.</xs:documentation>						<xs:documentation xml:lang="en">Agency/sales point/commission agent</xs:documentation>						<xs:documentation xml:lang="en">Agency, sales point or commission agent number according to the individual insurer's code directory.</xs:documentation>						<xs:documentation xml:lang="fr">Agence/point de vente/bénéficiaire de la commission</xs:documentation>						<xs:documentation xml:lang="fr">Numéro de l’agence, du point de vente ou du bénéficiaire de la commission selon le répertoire des codes de chaque assureur.</xs:documentation>						<xs:documentation xml:lang="it">Agenzia/punto vendita/beneficiario della provvigione</xs:documentation>						<xs:documentation xml:lang="it">Numero di agenzia, punto vendita o beneficiario della provvigione in base all’elenco dei codici dell'assicuratore individuale.</xs:documentation>					</xs:annotation>				</xs:element>				<xs:element name="invoiceNo" type="noInvoiceLocType" minOccurs="0">					<xs:annotation>						<xs:documentation xml:lang="de">Rechnungsnummer</xs:documentation>						<xs:documentation xml:lang="de">Die der Prämienrechnung vom Versicherer in seinem Inkasso-/Exkassosystem zugeteilte Rechnungs- oder Zahlscheinnummer. Empfehlung an Versicherer: Um die Verbindung mit dem Prämienrecord sicherzustellen, sollte die Rechnungs- oder Zahlscheinnummer in den Provisionsrecord übernommen werden, da dadurch die Zuordnung zur Prämienrechnungsperiode eindeutig festgelegt wird.</xs:documentation>						<xs:documentation xml:lang="en">Invoice number</xs:documentation>						<xs:documentation xml:lang="en">The invoice or payment slip number assigned to the premium invoice by the insurer in its collection/disbursement system. Recommendation for insurers: To create a link with the premium record, the invoice or payment slip number should be transferred to the provisioning record as this ensures unequivocal allocation to the premium invoice period.</xs:documentation>						<xs:documentation xml:lang="fr">Numéro de la facture</xs:documentation>						<xs:documentation xml:lang="fr">Le numéro de la facture ou du bordereau de paiement du décompte de prime de l'assureur fourni par le système d'encaissement/de décaissement de l'assureur. Recommandation à l'assureur: afin d'assurer la connexion avec l'enregistrement des primes, le numéro de la facture ou du bordereau de paiement doit être repris dans l'enregistrement de la commission, puisque cela détermine clairement l'attribution à la période de calcul des primes.</xs:documentation>						<xs:documentation xml:lang="it">Numero della fattura</xs:documentation>						<xs:documentation xml:lang="it">Il numero della fattura o del modulo di versamento attribuito dall’assicuratore al calcolo premi nel proprio sistema di incassi/esborsi. Raccomandazione agli assicuratori: per garantire il collegamento con il record dei premi, il numero della fattura o del modulo di versamento deve essere acquisito nel record delle provvigioni in modo da attribuire chiaramente il rispettivo periodo.</xs:documentation>					</xs:annotation>				</xs:element>				<xs:element name="invoiceDate" type="xs:date" minOccurs="0">					<xs:annotation>						<xs:documentation xml:lang="de">Rechnungserstellungsdatum</xs:documentation>						<xs:documentation xml:lang="de">JJJJ-MM-TT = Datum der Rechnungserstellung.</xs:documentation>						<xs:documentation xml:lang="en">Date invoice generated</xs:documentation>						<xs:documentation xml:lang="en">YYYY-MM-DD = Date of invoice generated.</xs:documentation>						<xs:documentation xml:lang="fr">Date d'établissement de la facture</xs:documentation>						<xs:documentation xml:lang="fr">AAAA-MM-JJ = Date d'établissement de la facture.</xs:documentation>						<xs:documentation xml:lang="it">Data di fatturazione</xs:documentation>						<xs:documentation xml:lang="it">AAAA-MM-GG = Data di fatturazione.</xs:documentation>					</xs:annotation>				</xs:element>				<xs:element name="mailing" type="mailingLocType" minOccurs="0">					<xs:annotation>						<xs:documentation xml:lang="de">Rechnungsversand</xs:documentation>						<xs:documentation xml:lang="en">Despatch of invoices'</xs:documentation>						<xs:documentation xml:lang="fr">Envoi de la facture</xs:documentation>						<xs:documentation xml:lang="it">Invio della fattura</xs:documentation>					</xs:annotation>				</xs:element>				<xs:element name="invoice">					<xs:annotation>						<xs:documentation xml:lang="de">Rechnungsdaten</xs:documentation>						<xs:documentation xml:lang="en">Data of invoice</xs:documentation>						<xs:documentation xml:lang="fr">Données de la facture</xs:documentation>						<xs:documentation xml:lang="it">Dati di fatturazione</xs:documentation>					</xs:annotation>					<xs:complexType>						<xs:sequence>							<xs:element name="invoiceTotal" type="invoiceTotalType">								<xs:annotation>									<xs:documentation xml:lang="de">Rechnungstotal</xs:documentation>									<xs:documentation xml:lang="en">Total of invoice</xs:documentation>									<xs:documentation xml:lang="fr">Total de la facture</xs:documentation>									<xs:documentation xml:lang="it">Totale della fattura</xs:documentation>								</xs:annotation>							</xs:element>							<xs:element name="invoiceDetails" minOccurs="0" maxOccurs="unbounded">								<xs:annotation>									<xs:documentation xml:lang="de">Rechnungsdetails</xs:documentation>									<xs:documentation xml:lang="en">Detail of invoice</xs:documentation>									<xs:documentation xml:lang="fr">Détails de la facture</xs:documentation>									<xs:documentation xml:lang="it">Dettagli della fattura</xs:documentation>								</xs:annotation>								<xs:complexType>									<xs:sequence>										<xs:element name="datePeriodPremFrom" type="xs:date">											<xs:annotation>												<xs:documentation xml:lang="de">Prämie von (Rechnungsperiode)</xs:documentation>												<xs:documentation xml:lang="de">JJJJ-MM-TT = Beginn der Periode, für welche die Prämie belastet oder gutgeschrieben wird.</xs:documentation>												<xs:documentation xml:lang="en">Premium from (billing period)</xs:documentation>												<xs:documentation xml:lang="en">YYYY-MM-DD = Beginning of the period for which the premium is debited or credited.</xs:documentation>												<xs:documentation xml:lang="fr">Prime du (période de facturation)</xs:documentation>												<xs:documentation xml:lang="fr">AAAA-MM-JJ = Début de la période, pour laquelle la prime est créditée ou débitée.</xs:documentation>												<xs:documentation xml:lang="it">Premio dal (periodo di fatturazione)</xs:documentation>												<xs:documentation xml:lang="it">AAAA-MM-GG = Inizio del periodo per quale il premio viene addebitato o accreditato.</xs:documentation>											</xs:annotation>										</xs:element>										<xs:element name="datePeriodPremTo" type="xs:date">											<xs:annotation>												<xs:documentation xml:lang="de">Prämie bis (Rechnungsperiode)</xs:documentation>												<xs:documentation xml:lang="de">JJJJ-MM-TT = Ende der Periode, für welche die Prämie belastet oder gutgeschrieben wird.</xs:documentation>												<xs:documentation xml:lang="en">Premium to (billing period)</xs:documentation>												<xs:documentation xml:lang="en">YYYY-MM-DD = End o period for which the premium is debited or credited.</xs:documentation>												<xs:documentation xml:lang="fr">Prime jusqu'au (période de facturation)</xs:documentation>												<xs:documentation xml:lang="fr">AAAA-MM-JJ = Fin de la période, pour laquelle la prime est créditée ou débitée.</xs:documentation>												<xs:documentation xml:lang="it">Premio fino al (periodo di fatturazione)</xs:documentation>												<xs:documentation xml:lang="it">AAA-MM-GG = Fine del periodo per quale il premio viene addebitato o accreditato.</xs:documentation>											</xs:annotation>										</xs:element>										<xs:element name="kindPremium" type="kindPremiumLocType">											<xs:annotation>												<xs:documentation xml:lang="de">Prämienart</xs:documentation>												<xs:documentation xml:lang="en">Premium form</xs:documentation>												<xs:documentation xml:lang="fr">Genre de prime</xs:documentation>												<xs:documentation xml:lang="it">Tipo di premio</xs:documentation>											</xs:annotation>										</xs:element>										<xs:element name="typePremium" type="typePremiumLocType">											<xs:annotation>												<xs:documentation xml:lang="de">Prämientyp</xs:documentation>												<xs:documentation xml:lang="de">Option für Versicherer: Die vorerwähnten Prämientypen können aufgrund der eigenen Codierung auch nach anderen Kriterien mit den folgenden Codes ausgewiesen bzw. zusammengefasst werden, sofern die Aufteilung nach Prämientyp 1 – 7 nicht möglich ist: 08 = Prämien gemäss Definition des einzelnen Versicherers / 09 = Zusammenfassung von zwei oder mehreren der übrigen Prämientypen gemäss Definition des einzelnen Versicherers</xs:documentation>												<xs:documentation xml:lang="en">Premium type</xs:documentation>												<xs:documentation xml:lang="en">Option for insurer: The previously mentioned premium types can be entered or summarised based on their own coding or other criteria with the following codes if allocation according to premium types 1 – 7 is not possible: 08 = Premiums according to the definition of the individual insurer / 09 = Summary of two or more of the other premium types according to the definition of the individual insurer</xs:documentation>												<xs:documentation xml:lang="fr">Type de prime</xs:documentation>												<xs:documentation xml:lang="fr">Option pour l'assureur: en raison de leur propre codage, les types de primes susmentionnés peuvent également être mentionnés resp. réunis selon d'autres critères avec les codes suivants, dès l'instant où la répartition selon le type de prime 1 – 7 n'est pas possible: 08 = Primes selon la définition de chaque assureur / 09 = Groupement de deux ou plusieurs des autres types de primes selon la définition de chaque assureur</xs:documentation>												<xs:documentation xml:lang="it">Tipo premio</xs:documentation>												<xs:documentation xml:lang="it">Opzione per l'assicuratore: In base al tipo di codifica, i suddetti tipi di premio possono essere indicati o riuniti anche secondo altri criteri con i seguenti codici, a condizione che la suddivisione secondo la tipologia 1 – 7 non sia possibile: 08 = premi secondo la definizione del singolo assicuratore / 09 = unione di due o più degli altri tipi di premio secondo la definizione del singolo assicuratore</xs:documentation>											</xs:annotation>										</xs:element>										<xs:element name="lobIG" type="LOBIGType">											<xs:annotation>												<xs:documentation xml:lang="de">Branche</xs:documentation>												<xs:documentation xml:lang="de">Branchencode gemäss IG-Verzeichnis. Das aktuelle Dokument – Verzeichnis der Branchencodes –  steht unter www.igb2b.ch im Mitgliederbereich zur Verfügung.</xs:documentation>												<xs:documentation xml:lang="en">Sector</xs:documentation>												<xs:documentation xml:lang="en">Sector code in accordance with IG directory. The current document – branch code directory – is available in the members area at www.igb2b.ch.</xs:documentation>												<xs:documentation xml:lang="fr">Branche</xs:documentation>												<xs:documentation xml:lang="fr">Codes des branches selon le répertoire IG. Le document actuel (répertoire des codes des branches) se trouve sur www.igb2b.ch, à la rubrique «Domaine membres».</xs:documentation>												<xs:documentation xml:lang="it">Settore</xs:documentation>												<xs:documentation xml:lang="it">Codice settoriale in base all’elenco IG. Il documento attuale – Elenco dei codici settoriali – è disponibile su www.igb2b.ch nella sezione Ambito membri.</xs:documentation>											</xs:annotation>										</xs:element>										<xs:element name="lobINS" type="LOBINSType" minOccurs="0">											<xs:annotation>												<xs:documentation xml:lang="de">Branche / Risikobezeichnung</xs:documentation>												<xs:documentation xml:lang="de">Branchen- oder Risikocode gemäss Codeverzeichnis des einzelnen Versicherers. Der Detaillierungsgrad sollte den Angaben auf der Prämienrechnung entsprechen, bei kombinierten Produkten mindestens jedoch dem Branchencode. Option Versicherer: Anstatt eines Branchen- oder Risikocodes kann die Bezeichnung auch als Text erfolgen.</xs:documentation>												<xs:documentation xml:lang="en">Sector / Risk description</xs:documentation>												<xs:documentation xml:lang="en">Sector or risk code according to the individual insurer's code directory. The degree of detail must correspond with the details on the premium invoice, combined products at least the sector code. Option Insurer: Instead of a sector or risk code the description can be also as text.</xs:documentation>												<xs:documentation xml:lang="fr">Branche / Description du risque</xs:documentation>												<xs:documentation xml:lang="fr">Codes des branches ou de risque selon le répertoire des codes de chaque assureur. Le degré de détail doit correspondre avec les données sur la facture des primes, mais au minimum aux codes des branches pour les produits combinés. Option assureur: Au lieu d'un code de branches ou de risque la description peut également être faite en texte.</xs:documentation>												<xs:documentation xml:lang="it">Settore / Denominazione rischio</xs:documentation>												<xs:documentation xml:lang="it">Codice settoriale o di rischio in base all'elenco dei codici dell'assicuratore individuale. Il grado de dettaglio deve corrispondere ai dettagli della fattura premio, ma almeno al codice settoriale per i prodotti combinati. Opzione di assicuratore: Invece di un codice settoriale o di rischio, la denominazione potrebbe anche essere testuale.</xs:documentation>											</xs:annotation>										</xs:element>										<xs:element name="currency" type="currencyCodeType" minOccurs="0">											<xs:annotation>												<xs:documentation xml:lang="de">Währung</xs:documentation>												<xs:documentation xml:lang="de">Code gemäss ISO-Code. Ohne Angabe der Währung gilt immer die Referenzwährung, d.h. zur Zeit der Schweizer Franken (CHF).</xs:documentation>												<xs:documentation xml:lang="en">Currency</xs:documentation>												<xs:documentation xml:lang="en">Code according to ISO code. Without specifying the currency, the reference currency always applies, at the time Swiss francs (CHF).</xs:documentation>												<xs:documentation xml:lang="fr">Monnaie</xs:documentation>												<xs:documentation xml:lang="fr">Code selon le code ISO. Sans indication de la monnaie, c'est toujours la monnaie de référence qui s'applique, c'est-à-dire actuellement le franc suisse (CHF).</xs:documentation>												<xs:documentation xml:lang="it">Moneta</xs:documentation>												<xs:documentation xml:lang="it">Codice secondo il codice ISO. Senza indicazione della moneta, si applica sempre la moneta riferimento, al momento il franco svizzero (CHF).</xs:documentation>											</xs:annotation>										</xs:element>										<xs:element name="annPremTariff" type="decimal10dot2Type" minOccurs="0">											<xs:annotation>												<xs:documentation xml:lang="de">Tarifprämie / Grundprämie / Bruttoprämie</xs:documentation>												<xs:documentation xml:lang="de">Jahresprämie ohne Stempelsteuer gemäss Tarif des Versicherers.</xs:documentation>												<xs:documentation xml:lang="en">Tariff premium / Basic premium / Gross premium</xs:documentation>												<xs:documentation xml:lang="en">Annual premium without stamp duty according to insurer's tariff</xs:documentation>												<xs:documentation xml:lang="fr">Prime tarifaire / prime de base / prime brute</xs:documentation>												<xs:documentation xml:lang="fr">Prime annuelle sans droit de timbre selon tarif de l'assureur.</xs:documentation>												<xs:documentation xml:lang="it">Premio tariffario / premio di base / premio lordo</xs:documentation>												<xs:documentation xml:lang="it">Premio annuale senza imposta di bollo secondo la tariffa dell'assicuratore.</xs:documentation>											</xs:annotation>										</xs:element>										<xs:element name="amountInvoice" type="decimal10dot2Type">											<xs:annotation>												<xs:documentation xml:lang="de">Fakturierte Prämie</xs:documentation>												<xs:documentation xml:lang="de">Dem Versicherungsnehmer die in Rechnung gestellte Prämie inkl. Stempel. Der Betrag ist im Normalfall positiv.</xs:documentation>												<xs:documentation xml:lang="en">Invoiced premium</xs:documentation>												<xs:documentation xml:lang="en">The policyholder the billed premium including stamp. The amount is normaly positive.</xs:documentation>												<xs:documentation xml:lang="fr">Prime facturée</xs:documentation>												<xs:documentation xml:lang="fr">La prime facturée au preneur d'assurance y compris le timbre. Le montant est généralement positif.</xs:documentation>												<xs:documentation xml:lang="it">Premio fatturato</xs:documentation>												<xs:documentation xml:lang="it">Il contraente è il premio fatturato incluso il timbro. L'importo è generalmente positivo.</xs:documentation>											</xs:annotation>										</xs:element>										<xs:element name="scaleBonus" type="scaleBonusLocType" minOccurs="0">											<xs:annotation>												<xs:documentation xml:lang="de">Bonussystem</xs:documentation>												<xs:documentation xml:lang="de">Bonussystem gemäss Tarif des einzelnen Versicherers.</xs:documentation>												<xs:documentation xml:lang="en">Bonus system</xs:documentation>												<xs:documentation xml:lang="en">Bonus system according to the individual insurer's tariff.</xs:documentation>												<xs:documentation xml:lang="fr">Système de bonus</xs:documentation>												<xs:documentation xml:lang="fr">Système de bonus selon le tarif de chaque assureur.</xs:documentation>												<xs:documentation xml:lang="it">Sistema di bonus</xs:documentation>												<xs:documentation xml:lang="it">Sistema di bonus in base alla tariffa dell'assicuratore individuale.</xs:documentation>											</xs:annotation>										</xs:element>										<xs:element name="degreePremium" type="degreePremiumLocType" minOccurs="0">											<xs:annotation>												<xs:documentation xml:lang="de">Prämienstufe</xs:documentation>												<xs:documentation xml:lang="de">Prämienstufe, die gemäss Schadenverlauf zur Anwendung kommt.</xs:documentation>												<xs:documentation xml:lang="en">Premium level</xs:documentation>												<xs:documentation xml:lang="en">Premium level applied according to claims history.</xs:documentation>												<xs:documentation xml:lang="fr">Degré de prime</xs:documentation>												<xs:documentation xml:lang="fr">Degré de prime qui est appliqué en fonction de l'évolution des sinistres.</xs:documentation>												<xs:documentation xml:lang="it">Grado di premio</xs:documentation>												<xs:documentation xml:lang="it">Grado di premio, che viene utilizzato in base all'andamento dei sinistri.</xs:documentation>											</xs:annotation>										</xs:element>										<xs:element name="premiumComm" type="decimal10dot2Type" minOccurs="0">											<xs:annotation>												<xs:documentation xml:lang="de">Provisionsberechnungsbasis</xs:documentation>												<xs:documentation xml:lang="de">Provisionsberechtigte Prämie oder Produktion im Einzellebensgeschäft gemäss Definition des Versicherers. Der Betrag ist im Normalfall positiv.</xs:documentation>												<xs:documentation xml:lang="en">Basis on which premium calculated</xs:documentation>												<xs:documentation xml:lang="en">Entitled premium or production in individual life according to insurer's definition. The amount is normally positive.</xs:documentation>												<xs:documentation xml:lang="fr">Base de calcul de la commission</xs:documentation>												<xs:documentation xml:lang="fr">Prime commissionnée ou production en affaires vie individuelle suivant la définition de l'assureur. Le montant est généralement positif.</xs:documentation>												<xs:documentation xml:lang="it">Base per il calcolo delle provvigioni</xs:documentation>												<xs:documentation xml:lang="it">Provvigione intitolata o produzione in vita individuale in base alla definizione dell'assicuratore. L'importo è generalmente positivo.</xs:documentation>											</xs:annotation>										</xs:element>										<xs:element name="generalAgreement" type="generalAgreementLocType" minOccurs="0">											<xs:annotation>												<xs:documentation xml:lang="de">Rahmenvertrags-/Flottenvertragsnummer</xs:documentation>												<xs:documentation xml:lang="de">Bei Rahmenverträgen oder Flottenverträgen ist hier die Angabe der Rahmenvertrags-Nummer erwünscht, sofern unter Vertragsnummer die Nummer der einzelnen unter den Rahmen- oder Flottenvertrag fallenden Versicherungen angegeben wird (die Rahmenvertrags-Nr. erleichtert dem Broker die richtige Zuordnung der Prämien).</xs:documentation>												<xs:documentation xml:lang="en">Framework agreement/fleet agreement number</xs:documentation>												<xs:documentation xml:lang="en">In the case of framework agreements or fleet agreements, the framework agreement number is required if, under the agreement number, the number of the individual insurances under the framework or fleet agreement is provided (the framework agreement number makes correct assignment of the premiums easier for the broker).</xs:documentation>												<xs:documentation xml:lang="fr">Numéro de contrat-cadre / de contrat de flotte</xs:documentation>												<xs:documentation xml:lang="fr">En cas de contrats-cadre ou de contrats de flotte, l’indication du numéro du contrat-cadre est recommandée dans la mesure où le numéro des différentes assurances relevant du contrat-cadre ou du contrat de flotte est indiqué sous le numéro de contrat (le numéro du contrat-cadre simplifie l’attribution des primes pour le broker).</xs:documentation>												<xs:documentation xml:lang="it">Numero contratto quadro/contratto parchi veicoli</xs:documentation>												<xs:documentation xml:lang="it">In caso di contratti quadro o parchi veicoli è richiesto il numero del contratto quadro se alla voce numero di contratto viene riportato il numero delle singole assicurazioni che rientrano nel contratto quadro o parchi veicoli (il n. di contratto quadro aiuta il broker nella giusta attribuzione i premi).</xs:documentation>											</xs:annotation>										</xs:element>									</xs:sequence>								</xs:complexType>							</xs:element>						</xs:sequence>					</xs:complexType>				</xs:element>				<xs:choice>					<xs:annotation>						<xs:documentation xml:lang="de">Wahl zwischen einem Geschäfts- und Privatkunde</xs:documentation>						<xs:documentation xml:lang="en">Choice between a business and private client</xs:documentation>						<xs:documentation xml:lang="fr">Choix entre un client entreprise et un client privé</xs:documentation>						<xs:documentation xml:lang="it">Scelta tra un cliente aziendale e un cliente privato</xs:documentation>					</xs:annotation>					<xs:element name="ICBusiness" type="ICBusinessLocType">						<xs:annotation>							<xs:documentation xml:lang="de">Mandatseinreichung für eine Firma</xs:documentation>							<xs:documentation xml:lang="en">Mandate submission for a company</xs:documentation>							<xs:documentation xml:lang="fr">Remise du mandat pour une entreprise</xs:documentation>							<xs:documentation xml:lang="it">Invio dei mandati per una ditta</xs:documentation>						</xs:annotation>					</xs:element>					<xs:element name="ICPrivatePerson" type="ICPrivatePersonLocType">						<xs:annotation>							<xs:documentation xml:lang="de">Mandatseinreichung für eine Privatperson</xs:documentation>							<xs:documentation xml:lang="en">Mandate submission for a private person</xs:documentation>							<xs:documentation xml:lang="fr">Remise du mandat pour une personne privée</xs:documentation>							<xs:documentation xml:lang="it">Invio dei mandati per una persona privata</xs:documentation>						</xs:annotation>					</xs:element>				</xs:choice>				<xs:element name="remarks" type="remarkType" minOccurs="0">					<xs:annotation>						<xs:documentation xml:lang="de">Bemerkungen</xs:documentation>						<xs:documentation xml:lang="de">Textfeld für maschinell ausgelöste und standardisierte Hinweise. Hinweis: Bemerkungen sind nur in Ausnahmefällen anzubringen und nach Möglichkeit zu unterlassen, da damit der vollständige Workflow unterbrochen wird.</xs:documentation>						<xs:documentation xml:lang="en">Remark</xs:documentation>						<xs:documentation xml:lang="en">Text field for machine triggered and standardised information. Note: Comments should be made only in exceptional cases and avoided where possible, as they interrupt the complete workflow.</xs:documentation>						<xs:documentation xml:lang="fr">Remarques</xs:documentation>						<xs:documentation xml:lang="fr">Zone de texte pour les notes déclanchées automatiquement et standardisées. Note: les commentaires doivent être faits que dans des cas exceptionnels et doivent être si possible évités, car cela interrompt le workflow.</xs:documentation>						<xs:documentation xml:lang="it">Osservazione</xs:documentation>						<xs:documentation xml:lang="it">Campo di testo per note generate meccanicamente e standardizzate. Nota: riportare delle osservazioni solo in casi eccezionali e possibilmente tralasciarle perché interrompono il workflow completo.</xs:documentation>					</xs:annotation>				</xs:element>				<xs:element name="attachment" type="attachmentType" minOccurs="0" maxOccurs="unbounded">					<xs:annotation>						<xs:documentation xml:lang="de">Dateianhang</xs:documentation>						<xs:documentation xml:lang="en">Attachment</xs:documentation>						<xs:documentation xml:lang="fr">Pièce jointe</xs:documentation>						<xs:documentation xml:lang="it">Allegato</xs:documentation>					</xs:annotation>				</xs:element>			</xs:sequence>		</xs:complexType>	</xs:element></xs:schema>