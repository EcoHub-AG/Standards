<?xml version="1.0" encoding="UTF-8"?><!--XSD Standard for IG B2B Data exchangeCopyright © IG B2B for Insurers + Brokers, Switzerland--><!--Changes in Version V5.2: - Added translated strings in documentationChanges in Version V5.2.1 from V5.2: - Change version values and name of referenced data schema from 5.2 to 5.2.1--><xs:schema xmlns:xs="http://www.w3.org/2001/XMLSchema" xmlns="http://www.IGB2B.ch/XMLSchema" targetNamespace="http://www.IGB2B.ch/XMLSchema" elementFormDefault="qualified" version="5.2.1">	<xs:include schemaLocation="globals_V5.2.1.xsd"/>	<xs:complexType name="ICBusinessLocType">		<xs:annotation>			<xs:documentation xml:lang="de">Datentyp für einen Firmenkunden</xs:documentation>			<xs:documentation xml:lang="en">Data type for a company customer</xs:documentation>			<xs:documentation xml:lang="fr">Type de données pour un client entreprise</xs:documentation>			<xs:documentation xml:lang="fr">Tipo di dati per un cliente commerciale</xs:documentation>		</xs:annotation>		<xs:sequence>			<xs:element name="ICBusinessName" type="companyNameType">				<xs:annotation>					<xs:documentation xml:lang="de">Firmenname</xs:documentation>					<xs:documentation xml:lang="de">Firmenname des Versicherungsnehmers. Bei einer einfachen Gesellschaft ist Vor- und Nachname einzutragen.</xs:documentation>					<xs:documentation xml:lang="en">Company name</xs:documentation>					<xs:documentation xml:lang="en">Company name of policy holder. For a simple company, name and surname must be entered.</xs:documentation>					<xs:documentation xml:lang="fr">Raison sociale</xs:documentation>					<xs:documentation xml:lang="fr">Raison sociale du preneur d'assurance. Pour une société simple, le nom et le prénom doivent être mentionnés.</xs:documentation>					<xs:documentation xml:lang="it">Nome della ditta</xs:documentation>					<xs:documentation xml:lang="it">Nome della ditta del contraente. Per una società semplice, è necessario inserire nome e cognome.</xs:documentation>				</xs:annotation>			</xs:element>		</xs:sequence>	</xs:complexType>	<xs:complexType name="ICPrivatePersonLocType">		<xs:annotation>			<xs:documentation xml:lang="de">Datentyp für einen Privatkunden</xs:documentation>			<xs:documentation xml:lang="en">Data type for a private client</xs:documentation>			<xs:documentation xml:lang="fr">Type de données pour un client privé</xs:documentation>			<xs:documentation xml:lang="it">Tipo di dati per un cliente privato</xs:documentation>		</xs:annotation>		<xs:sequence>			<xs:element name="ICPrivatePersonFirstname" type="firstnameType">				<xs:annotation>					<xs:documentation xml:lang="de">Vorname des Versicherungsnehmers</xs:documentation>					<xs:documentation xml:lang="en">First name of policy holder</xs:documentation>					<xs:documentation xml:lang="fr">Prénom du preneur d'assurance</xs:documentation>					<xs:documentation xml:lang="it">Nome del contraente</xs:documentation>				</xs:annotation>			</xs:element>			<xs:element name="ICPrivatePersonSurname" type="surnameType">				<xs:annotation>					<xs:documentation xml:lang="de">Nachname des Versicherungsnehmers</xs:documentation>					<xs:documentation xml:lang="en">Surname of policy holder</xs:documentation>					<xs:documentation xml:lang="fr">Nom du preneur d'assurance</xs:documentation>					<xs:documentation xml:lang="it">Cognome del contraente</xs:documentation>				</xs:annotation>			</xs:element>		</xs:sequence>	</xs:complexType>	<xs:simpleType name="mutationReasonLocType">			<xs:annotation>				<xs:documentation xml:lang="de">Datentyp für den Mutationsgrund</xs:documentation>				<xs:documentation xml:lang="en">Data type for amendment reason</xs:documentation>				<xs:documentation xml:lang="fr">Type de données pour la raison de la mutation</xs:documentation>				<xs:documentation xml:lang="it">Tipo di dati per motivo della modifica</xs:documentation>			</xs:annotation>			<xs:restriction base="xs:NMTOKEN">				<xs:maxLength value="2"/>				<xs:enumeration value="01">					<xs:annotation>						<xs:documentation xml:lang="de">Neuabschluss</xs:documentation>						<xs:documentation xml:lang="en">New business</xs:documentation>						<xs:documentation xml:lang="fr">Nouvelle affaire</xs:documentation>						<xs:documentation xml:lang="it">Nuovi affari</xs:documentation>					</xs:annotation>				</xs:enumeration>				<xs:enumeration value="02">					<xs:annotation>						<xs:documentation xml:lang="de">Vertragsänderung</xs:documentation>						<xs:documentation xml:lang="en">Contract modification</xs:documentation>						<xs:documentation xml:lang="fr">Modification du contrat</xs:documentation>						<xs:documentation xml:lang="it">Modifica del contratto</xs:documentation>					</xs:annotation>				</xs:enumeration>				<xs:enumeration value="03">					<xs:annotation>						<xs:documentation xml:lang="de">Ersatzgeschäft mit anderer Vertragsnummer</xs:documentation>						<xs:documentation xml:lang="en">Substitute transaction with another contract number</xs:documentation>						<xs:documentation xml:lang="fr">Affaire de remplacement avec un autre numéro de contrat</xs:documentation>						<xs:documentation xml:lang="it">Operazione sostitutiva con un altro numero di contratto</xs:documentation>					</xs:annotation>				</xs:enumeration>				<xs:enumeration value="04">					<xs:annotation>						<xs:documentation xml:lang="de">Vertragsaufhebung</xs:documentation>						<xs:documentation xml:lang="en">Contract cancellation</xs:documentation>						<xs:documentation xml:lang="fr">Résiliation du contrat</xs:documentation>						<xs:documentation xml:lang="it">Disdetta del contratto</xs:documentation>					</xs:annotation>				</xs:enumeration>			</xs:restriction>		</xs:simpleType>	</xs:schema>