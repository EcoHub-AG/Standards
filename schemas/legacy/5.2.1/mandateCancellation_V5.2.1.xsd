<?xml version="1.0" encoding="UTF-8"?><!--XSD Standard for IG B2B Data exchangeCopyright © IG B2B for Insurers + Brokers, Switzerland--><!--Changes in Version V5.2rc1: - Added translated strings in documentationChanges in Version V5.2.1 from V5.2rc1: - Change version values and names of referenced data schemas from V5.2rc1 to 5.2.1--><xs:schema xmlns:xs="http://www.w3.org/2001/XMLSchema" xmlns="http://www.IGB2B.ch/XMLSchema" targetNamespace="http://www.IGB2B.ch/XMLSchema" elementFormDefault="qualified" version="5.2.1">	<xs:include schemaLocation="globals_V5.2.1.xsd"/>	<xs:include schemaLocation="mandateTypeLib_V5.2.1.xsd"/>	<xs:element name="mandateCancellation">		<xs:annotation>			<xs:documentation xml:lang="de">HAUPTSTRUKTUR für den Prozess Mandatskündigung</xs:documentation>			<xs:documentation xml:lang="en">MAIN STRUCTURE for proccess mandate cancellation</xs:documentation>			<xs:documentation xml:lang="fr">STRUCTURE PRINCIPALE pour le processus résiliation du mandat</xs:documentation>			<xs:documentation xml:lang="it">STRUTTURA PRINCIPALE per il processo mandati disdetta</xs:documentation>		</xs:annotation>		<xs:complexType>			<xs:sequence>				<xs:element name="header">					<xs:annotation>						<xs:documentation xml:lang="de">Header</xs:documentation>						<xs:documentation xml:lang="en">Header</xs:documentation>						<xs:documentation xml:lang="fr">Entête</xs:documentation>						<xs:documentation xml:lang="it">Header</xs:documentation>					</xs:annotation>					<xs:complexType>						<xs:complexContent>							<xs:extension base="headerType">								<xs:sequence>									<xs:element name="typeRec">										<xs:annotation>											<xs:documentation xml:lang="de">Recordart</xs:documentation>											<xs:documentation xml:lang="en">Record type</xs:documentation>											<xs:documentation xml:lang="fr">Genre d’enregistrement</xs:documentation>											<xs:documentation xml:lang="it">Tipo di record</xs:documentation>										</xs:annotation>										<xs:simpleType>											<xs:restriction base="typeRecType">												<xs:enumeration value="016">													<xs:annotation>														<xs:documentation xml:lang="de">Mandatswesen</xs:documentation>														<xs:documentation xml:lang="en">Mandate</xs:documentation>														<xs:documentation xml:lang="fr">Gestion des mandats</xs:documentation>														<xs:documentation xml:lang="it">Allestimento mandati</xs:documentation>													</xs:annotation>												</xs:enumeration>											</xs:restriction>										</xs:simpleType>									</xs:element>									<xs:element name="subTypeRec">										<xs:annotation>											<xs:documentation xml:lang="de">Subrecordart</xs:documentation>											<xs:documentation xml:lang="en">Sub record type</xs:documentation>											<xs:documentation xml:lang="fr">Genre de sous-enregistrement</xs:documentation>											<xs:documentation xml:lang="it">Tipo di subrecord</xs:documentation>										</xs:annotation>										<xs:simpleType>											<xs:restriction base="subTypeRecType">												<xs:enumeration value="003">													<xs:annotation>														<xs:documentation xml:lang="de">Mandatskündigung</xs:documentation>														<xs:documentation xml:lang="en">Mandate cancellation</xs:documentation>														<xs:documentation xml:lang="fr">Résiliation du mandat</xs:documentation>														<xs:documentation xml:lang="it">Disdetta del mandato</xs:documentation>													</xs:annotation>												</xs:enumeration>											</xs:restriction>										</xs:simpleType>									</xs:element>									<xs:element name="versionRec">										<xs:annotation>											<xs:documentation xml:lang="de">Recordversion</xs:documentation>											<xs:documentation xml:lang="en">Record version</xs:documentation>											<xs:documentation xml:lang="fr">Version de l’enregistrement</xs:documentation>											<xs:documentation xml:lang="it">Versione record</xs:documentation>										</xs:annotation>										<xs:simpleType>											<xs:restriction base="versionRecType">												<xs:enumeration value="5.2.1"/>											</xs:restriction>										</xs:simpleType>									</xs:element>									<xs:element name="contentRec">										<xs:annotation>											<xs:documentation xml:lang="de">Recordinhalt</xs:documentation>											<xs:documentation xml:lang="en">Record content</xs:documentation>											<xs:documentation xml:lang="fr">Contenu de l’enregistrement</xs:documentation>											<xs:documentation xml:lang="it">Contenuto record</xs:documentation>										</xs:annotation>										<xs:simpleType>											<xs:restriction base="contentRecType">												<xs:enumeration value="00">													<xs:annotation>														<xs:documentation xml:lang="de">Kein Recordinhalt</xs:documentation>														<xs:documentation xml:lang="en">No content</xs:documentation>														<xs:documentation xml:lang="fr">Pas de contenu d'enregistrement</xs:documentation>														<xs:documentation xml:lang="it">Nessun contenuto del record</xs:documentation>													</xs:annotation>												</xs:enumeration>											</xs:restriction>										</xs:simpleType>									</xs:element>								</xs:sequence>							</xs:extension>						</xs:complexContent>					</xs:complexType>				</xs:element>				<xs:element name="mandateKind" type="mandateKindLocType">					<xs:annotation>						<xs:documentation xml:lang="de">Art von Brokermandat</xs:documentation>						<xs:documentation xml:lang="de">Hinweis: Code 02 berechtigt Broker zur Prüfung und Beratung der Deckung, jedoch ohne Umteilung des Bestandes beim Versicherer</xs:documentation>						<xs:documentation xml:lang="en">Type of broker mandate</xs:documentation>						<xs:documentation xml:lang="en">Note: Code 02 entitles brokers to review and advise coverage, but without redistributing the portfolio to the insurer</xs:documentation>						<xs:documentation xml:lang="fr">Genre du mandat de courtage</xs:documentation>						<xs:documentation xml:lang="fr">Note: le code 02 autorise le broker à examiner la couverture et à conseiller, toutefois sans transfert du portefeuille auprès de l’assureur</xs:documentation>						<xs:documentation xml:lang="it">Tipo di mandato di brokeraggio</xs:documentation>						<xs:documentation xml:lang="it">Nota: il codice 02 autorizza i broker a rivedere e consigliare la copertura, ma senza ridistribuire il portafoglio all'assicuratore</xs:documentation>					</xs:annotation>				</xs:element>				<xs:element name="mandateValid" type="xs:date">					<xs:annotation>						<xs:documentation xml:lang="de">Gültigkeitsdatum der Mandatskündigung</xs:documentation>						<xs:documentation xml:lang="de">JJJJ-MM-TT = Datum ab wann die Mandatskündigung gilt.</xs:documentation>						<xs:documentation xml:lang="en">Validity date of mandate cancellation</xs:documentation>						<xs:documentation xml:lang="en">YYYY-MM-DD = Date from when the mandate cancellation applies.</xs:documentation>						<xs:documentation xml:lang="fr">Date de validité de la résiliation du mandat</xs:documentation>						<xs:documentation xml:lang="fr">AAAA-MM-JJ = Date à partir de laquelle l’annulation du mandat s’applique.</xs:documentation>						<xs:documentation xml:lang="it">Data di validità di disdetta del mandato</xs:documentation>						<xs:documentation xml:lang="it">AAAA-MM-GG = Data a partire dalla quale si applica la disdetta del mandato.</xs:documentation>					</xs:annotation>				</xs:element>				<xs:choice>					<xs:annotation>						<xs:documentation xml:lang="de">Wahl zwischen einem Geschäfts- und Privatkunde</xs:documentation>						<xs:documentation xml:lang="en">Choice between a business and private client</xs:documentation>						<xs:documentation xml:lang="fr">Choix entre un client entreprise et un client privé</xs:documentation>						<xs:documentation xml:lang="it">Scelta tra un cliente aziendale e un cliente privato</xs:documentation>					</xs:annotation>					<xs:element name="ICBusiness" type="ICBusinessLocType">						<xs:annotation>							<xs:documentation xml:lang="de">Mandatseinreichung für eine Firma</xs:documentation>							<xs:documentation xml:lang="en">Mandate submission for a company</xs:documentation>							<xs:documentation xml:lang="fr">Remise du mandat pour une entreprise</xs:documentation>							<xs:documentation xml:lang="it">Invio dei mandati per un'azienda</xs:documentation>						</xs:annotation>					</xs:element>					<xs:element name="ICPrivatePerson" type="ICPrivatePersonLocType">						<xs:annotation>							<xs:documentation xml:lang="de">Mandatseinreichung für eine Privatperson</xs:documentation>							<xs:documentation xml:lang="en">Mandate submission for a private person</xs:documentation>							<xs:documentation xml:lang="fr">Remise du mandat pour une personne privée</xs:documentation>							<xs:documentation xml:lang="it">Invio dei mandati per una persona privata</xs:documentation>						</xs:annotation>					</xs:element>				</xs:choice>				<xs:element name="contractLOB" maxOccurs="unbounded">					<xs:annotation>						<xs:documentation xml:lang="de">Verträge (1-n)</xs:documentation>						<xs:documentation xml:lang="en">Contracts (1-n)</xs:documentation>						<xs:documentation xml:lang="fr">Contrats (1-n)</xs:documentation>						<xs:documentation xml:lang="it">Contratti (1-n)</xs:documentation>					</xs:annotation>					<xs:complexType>						<xs:sequence>							<xs:element name="contractLOBIG" type="LOBIGType">								<xs:annotation>									<xs:documentation xml:lang="de">Branche</xs:documentation>									<xs:documentation xml:lang="de">Branchencode gemäss IG B2B-Verzeichnis. Das aktuelle Dokument – Verzeichnis der Branchencodes - steht unter www.igb2b.ch im Mitgliederbereich zur Verfügung.</xs:documentation>									<xs:documentation xml:lang="en">Sector</xs:documentation>									<xs:documentation xml:lang="en">Sector code in accordance with IG directory. The current document – branch code directory – is available in the members area at www.igb2b.ch.</xs:documentation>									<xs:documentation xml:lang="fr">Branche</xs:documentation>									<xs:documentation xml:lang="fr">Codes des branches selon le répertoire IG. Le document actuel (répertoire des codes des branches) se trouve sur www.igb2b.ch, à la rubrique «Domaine membres».</xs:documentation>									<xs:documentation xml:lang="it">Settore</xs:documentation>									<xs:documentation xml:lang="it">Codice settoriale in base all’elenco IG. Il documento attuale – Elenco dei codici settoriali – è disponibile su www.igb2b.ch nella sezione "Ambito membri".</xs:documentation>								</xs:annotation>							</xs:element>							<xs:element name="contractLOBINS" type="LOBINSType" minOccurs="0">								<xs:annotation>									<xs:documentation xml:lang="de">Branche / Risikobezeichnung / Tarifposition</xs:documentation>									<xs:documentation xml:lang="de">Branchen- oder Risikocode gemäss Codeverzeichnis des einzelnen Versicherers. Der Detaillierungsgrad sollte den Angaben auf der Prämienrechnung entsprechen, bei kombinierten Produkten mindestens jedoch dem Branchencode. Option Versicherer: Anstatt eines Branchen- oder Risikocodes kann die Bezeichnung auch als Text erfolgen.</xs:documentation>									<xs:documentation xml:lang="en">Line / Risk description / Tariff position</xs:documentation>									<xs:documentation xml:lang="en">Sector or risk code according to the individual insurer's code directory. The degree of detail must correspond with the details on the premium invoice, combined products at least the sector code. Option Insurer: Instead of a sector or risk code the description can be also as text.</xs:documentation>									<xs:documentation xml:lang="fr">Branche / description du risque / position tarifaire</xs:documentation>									<xs:documentation xml:lang="fr">Codes des branches ou de risque selon le répertoire des codes de chaque assureur. Le degré de détail doit correspondre avec les données sur la facture des primes, mais au minimum aux codes des branches pour les produits combinés. Option assureur: au lieu d'un code de branches ou de risque la description peut également être faite en texte</xs:documentation>									<xs:documentation xml:lang="it">Settore / denominazione rischio / posizione tariffale</xs:documentation>									<xs:documentation xml:lang="it">Codice settoriale o di rischio in base all’elenco dei codici dell'assicuratore individuale. Il grado de dettaglio deve corrispondere ai dettagli della fattura premio, ma almeno al codice settoriale per i prodotti combinati. Opzione di assicurazione: Invece di un codice settoriale o di rischio, la denominazione potrebbe anche essere testuale.</xs:documentation>								</xs:annotation>							</xs:element>						</xs:sequence>						<xs:attribute name="contractLOBNo" type="contractNoType" use="required">							<xs:annotation>								<xs:documentation xml:lang="de">Vertragsnummer</xs:documentation>								<xs:documentation xml:lang="en">Policy number</xs:documentation>								<xs:documentation xml:lang="fr">Numéro de contrat</xs:documentation>								<xs:documentation xml:lang="it">Numero del contratto</xs:documentation>							</xs:annotation>						</xs:attribute>					</xs:complexType>				</xs:element>				<xs:element name="orgUnitID" type="orgUnitIDType">					<xs:annotation>						<xs:documentation xml:lang="de">Filialen-Nummer (ID-Nummer)</xs:documentation>						<xs:documentation xml:lang="de">Numerische Angabe der Filialen-Nummer gemäss Identifikation-Nummer in BrokerGate.</xs:documentation>						<xs:documentation xml:lang="en">Branch number (ID number)</xs:documentation>						<xs:documentation xml:lang="en">Numerical detail of branch number according to identification number of BrokerGate.</xs:documentation>						<xs:documentation xml:lang="fr">Numéro de l’unité d’organisation (numéro ID)</xs:documentation>						<xs:documentation xml:lang="fr">Indication numérique du numéro de l’unité d’organisation selon le numéro d'identification du BrokerGate.</xs:documentation>						<xs:documentation xml:lang="it">Numero di filiale (numero ID)</xs:documentation>						<xs:documentation xml:lang="it">Indicazione numerica del numero di filiale in base al numero d'identificazione in BrokerGate.</xs:documentation>					</xs:annotation>				</xs:element>				<xs:element name="adviser">					<xs:annotation>						<xs:documentation xml:lang="de">Organisationseinheit Broker / Kundenberater</xs:documentation>						<xs:documentation xml:lang="en">Organisation unit broker / Client adviser</xs:documentation>						<xs:documentation xml:lang="fr">Unité d’organisation du broker / Conseiller à la clientèle</xs:documentation>						<xs:documentation xml:lang="it">Unità organizzativa broker / Consulente alla clientela</xs:documentation>					</xs:annotation>					<xs:complexType>						<xs:sequence>							<xs:element name="adviserFirstname" type="firstnameType">								<xs:annotation>									<xs:documentation xml:lang="de">Vorname des Kundenberaters</xs:documentation>									<xs:documentation xml:lang="en">First name of client adviser</xs:documentation>									<xs:documentation xml:lang="fr">Prénom du conseiller à la clientèle</xs:documentation>									<xs:documentation xml:lang="it">Nome del consulente alla clientela</xs:documentation>								</xs:annotation>							</xs:element>							<xs:element name="adviserSurname" type="surnameType">								<xs:annotation>									<xs:documentation xml:lang="de">Nachname des Kundenberaters</xs:documentation>									<xs:documentation xml:lang="en">Name of client adviser</xs:documentation>									<xs:documentation xml:lang="fr">Nom du conseiller à la clientèle</xs:documentation>									<xs:documentation xml:lang="it">Cognome del consulente alla clientela</xs:documentation>								</xs:annotation>							</xs:element>						</xs:sequence>					</xs:complexType>				</xs:element>				<xs:element name="remarks" type="remarkType" minOccurs="0">					<xs:annotation>						<xs:documentation xml:lang="de">Bemerkungen</xs:documentation>						<xs:documentation xml:lang="de">Textfeld für maschinell ausgelöste und standardisierte Hinweise. Hinweis: Bemerkungen sind nur in Ausnahmefällen anzubringen und nach Möglichkeit zu unterlassen, da damit der vollständige Workflow unterbrochen wird.</xs:documentation>						<xs:documentation xml:lang="en">Remarks</xs:documentation>						<xs:documentation xml:lang="en">Text field for machine triggered and standardised information. Note: Comments should be made only in exceptional cases and avoided where possible, as they interrupt the complete workflow.</xs:documentation>						<xs:documentation xml:lang="fr">Remarques</xs:documentation>						<xs:documentation xml:lang="fr">Zone de texte pour les notes déclanchées automatiquement et standardisées. Note: les commentaires doivent être faits que dans des cas exceptionnels et doivent être si possible évités, car cela interrompt le workflow.</xs:documentation>						<xs:documentation xml:lang="it">Osservazione</xs:documentation>						<xs:documentation xml:lang="it">Campo di testo per note generate meccanicamente e standardizzate. Nota: riportare delle osservazioni solo in casi eccezionali e possibilmente tralasciarle perché interrompono il workflow completo.</xs:documentation>					</xs:annotation>				</xs:element>				<xs:element name="attachment" type="attachmentType" minOccurs="0" maxOccurs="unbounded">					<xs:annotation>						<xs:documentation xml:lang="de">Dateianhang</xs:documentation>						<xs:documentation xml:lang="en">Attachment</xs:documentation>						<xs:documentation xml:lang="fr">Pièce jointe</xs:documentation>						<xs:documentation xml:lang="it">Allegato</xs:documentation>					</xs:annotation>				</xs:element>			</xs:sequence>		</xs:complexType>	</xs:element></xs:schema>