<?xml version="1.0" encoding="UTF-8"?><!-- Mit XMLSpy v2017 sp2 (x64) (http://www.altova.com) von Michael Roth (TEMET AG) bearbeitet --><!--XSD Standard for IG B2B Data exchangeCopyright © IG B2B for Insurers + Brokers, Switzerland--><!--Changes in Version V5.2: - Added translated strings in documentationChanges in Version V5.2.1 from V5.2: - Change version values and names of referenced data schemas from 5.2 to 5.2.1--><xs:schema xmlns:xs="http://www.w3.org/2001/XMLSchema" xmlns="http://www.IGB2B.ch/XMLSchema" targetNamespace="http://www.IGB2B.ch/XMLSchema" elementFormDefault="qualified" version="5.2.1">	<xs:include schemaLocation="globals_V5.2.1.xsd"/>	<xs:include schemaLocation="commissionTypeLib_V5.2.1.xsd"/>	<xs:element name="commission">		<xs:annotation>			<xs:documentation xml:lang="de">HAUPTSTRUKTUR für den Prozess Provisionierung</xs:documentation>			<xs:documentation xml:lang="en">MAIN STRUCTURE for process commission</xs:documentation>			<xs:documentation xml:lang="fr">STRUCTURE PRINCIPALE pour le processus commissionnement</xs:documentation>			<xs:documentation xml:lang="it">STRUTTURA PRINCIPALE per il processo provvigione</xs:documentation>		</xs:annotation>		<xs:complexType>			<xs:sequence>				<xs:element name="header">					<xs:annotation>						<xs:documentation xml:lang="de">Header</xs:documentation>						<xs:documentation xml:lang="en">Header</xs:documentation>						<xs:documentation xml:lang="fr">Entête</xs:documentation>						<xs:documentation xml:lang="it">Header</xs:documentation>					</xs:annotation>					<xs:complexType>						<xs:complexContent>							<xs:extension base="headerType">								<xs:sequence>									<xs:element name="typeRec">										<xs:annotation>											<xs:documentation xml:lang="de">Recordart</xs:documentation>											<xs:documentation xml:lang="en">Record type</xs:documentation>											<xs:documentation xml:lang="fr">Genre d’enregistrement</xs:documentation>											<xs:documentation xml:lang="it">Tipo di record</xs:documentation>										</xs:annotation>										<xs:simpleType>											<xs:restriction base="typeRecType">												<xs:enumeration value="003">													<xs:annotation>														<xs:documentation xml:lang="de">Provisionen</xs:documentation>														<xs:documentation xml:lang="en">Commission</xs:documentation>														<xs:documentation xml:lang="fr">Commissions</xs:documentation>														<xs:documentation xml:lang="it">Provvigione</xs:documentation>													</xs:annotation>												</xs:enumeration>											</xs:restriction>										</xs:simpleType>									</xs:element>									<xs:element name="subTypeRec">										<xs:annotation>											<xs:documentation xml:lang="de">Subrecordart</xs:documentation>											<xs:documentation xml:lang="en">Sub-record type</xs:documentation>											<xs:documentation xml:lang="fr">Genre de sous-enregistrement</xs:documentation>											<xs:documentation xml:lang="it">Tipo di subrecord</xs:documentation>										</xs:annotation>										<xs:simpleType>											<xs:restriction base="subTypeRecType">												<xs:enumeration value="000">													<xs:annotation>														<xs:documentation xml:lang="de">Kein Subrecord</xs:documentation>														<xs:documentation xml:lang="en">No sub record</xs:documentation>														<xs:documentation xml:lang="fr">Pas de sous-enregistrement</xs:documentation>														<xs:documentation xml:lang="it">Nessun subrecord</xs:documentation>													</xs:annotation>												</xs:enumeration>											</xs:restriction>										</xs:simpleType>									</xs:element>									<xs:element name="versionRec">										<xs:annotation>											<xs:documentation xml:lang="de">Recordversion</xs:documentation>											<xs:documentation xml:lang="en">Record version</xs:documentation>											<xs:documentation xml:lang="fr">Version de l’enregistrement</xs:documentation>											<xs:documentation xml:lang="it">Versione record</xs:documentation>										</xs:annotation>										<xs:simpleType>											<xs:restriction base="versionRecType">												<xs:enumeration value="5.2.1"/>											</xs:restriction>										</xs:simpleType>									</xs:element>									<xs:element name="contentRec">										<xs:annotation>											<xs:documentation xml:lang="de">Recordinhalt</xs:documentation>											<xs:documentation xml:lang="en">Record content</xs:documentation>											<xs:documentation xml:lang="fr">Contenu de l’enregistrement</xs:documentation>											<xs:documentation xml:lang="it">Contenuto record</xs:documentation>										</xs:annotation>										<xs:simpleType>											<xs:restriction base="contentRecType">												<xs:enumeration value="01">													<xs:annotation>														<xs:documentation xml:lang="de">Nichtleben</xs:documentation>														<xs:documentation xml:lang="en">Non-life</xs:documentation>														<xs:documentation xml:lang="fr">Non-vie</xs:documentation>														<xs:documentation xml:lang="it">Non vita</xs:documentation>													</xs:annotation>												</xs:enumeration>												<xs:enumeration value="02">													<xs:annotation>														<xs:documentation xml:lang="de">Einzelleben</xs:documentation>														<xs:documentation xml:lang="en">Single life</xs:documentation>														<xs:documentation xml:lang="fr">Vie individuelle</xs:documentation>														<xs:documentation xml:lang="it">Vita individuale</xs:documentation>													</xs:annotation>												</xs:enumeration>												<xs:enumeration value="03">													<xs:annotation>														<xs:documentation xml:lang="de">Kollektivleben</xs:documentation>														<xs:documentation xml:lang="en">Group life</xs:documentation>														<xs:documentation xml:lang="fr">Vie collective</xs:documentation>														<xs:documentation xml:lang="it">Vita collettiva</xs:documentation>													</xs:annotation>												</xs:enumeration>												<xs:enumeration value="04">													<xs:annotation>														<xs:documentation xml:lang="de">Kombination einzelner oder aller Geschäftsbereiche</xs:documentation>														<xs:documentation xml:lang="en">Combination of individual or all business units</xs:documentation>														<xs:documentation xml:lang="fr">Combinaison d'un ou de plusieurs domaines d'activités</xs:documentation>														<xs:documentation xml:lang="it">Combinazione di singole o di tutti settori operativi</xs:documentation>													</xs:annotation>												</xs:enumeration>											</xs:restriction>										</xs:simpleType>									</xs:element>								</xs:sequence>							</xs:extension>						</xs:complexContent>					</xs:complexType>				</xs:element>				<xs:element name="exchangeDateFrom" type="xs:date">					<xs:annotation>						<xs:documentation xml:lang="de">Abrechnungsperiode von</xs:documentation>						<xs:documentation xml:lang="de">JJJJ-MM-TT = Beginn des Monates, für den die Daten ausgetauscht werden. Das Datum bezieht sich nicht auf die einzelne Police, sondern auf den gesamten Datenaustausch dieses Datenfiles.</xs:documentation>						<xs:documentation xml:lang="en">Statement period from</xs:documentation>						<xs:documentation xml:lang="en">YYYY-MM-DD = Beginning of month, for which the data is exchanged. The date does not refer to the individual police but to the whole date exchange of this data file.</xs:documentation>						<xs:documentation xml:lang="fr">Période de décompte du</xs:documentation>						<xs:documentation xml:lang="fr">AAAA-MM-JJ = début du mois pour lequel les données sont échangées. La date ne se rapporte pas à chaque police, mais à l'ensemble de l'échange des données de ce fichier de données.</xs:documentation>						<xs:documentation xml:lang="it">Periodo di conteggio di</xs:documentation>						<xs:documentation xml:lang="it">AAAA-MM-GG = Inizio del mese per cui vengono scambiati i dati. La data non si riferisce alla singola polizza, ma all’intero scambio di questo file dati.</xs:documentation>					</xs:annotation>				</xs:element>				<xs:element name="exchangeDateTo" type="xs:date">					<xs:annotation>						<xs:documentation xml:lang="de">Abrechnungsperiode bis</xs:documentation>						<xs:documentation xml:lang="de">JJJJ-MM-TT = Ende des Monates, für den die Daten ausgetauscht werden. Das Datum bezieht sich nicht auf die einzelne Police, sondern auf den gesamten Datenaustausch dieses Datenfiles.</xs:documentation>						<xs:documentation xml:lang="en">Statement period till</xs:documentation>						<xs:documentation xml:lang="en">YYYY-MM-DD= End of months, for which the data is exchanged. The date does not refer to the individual police but to the whole date exchange of this data file.</xs:documentation>						<xs:documentation xml:lang="fr">Période de décompte jusqu'au</xs:documentation>						<xs:documentation xml:lang="fr">AAAA-MM-JJ = Fin du mois pour lequel les données sont échangées. La date ne se rapporte pas à chaque police, mais à l'ensemble de l'échange des données de ce fichier de données.</xs:documentation>						<xs:documentation xml:lang="it">Periodo di conteggio fino al</xs:documentation>						<xs:documentation xml:lang="it">AAAA-MM-GG =  Fine del mese per cui vengono scambiati i dati. La data non si riferisce alla singola polizza, ma all’intero scambio di questo file dati.</xs:documentation>					</xs:annotation>				</xs:element>				<xs:element name="contractC" maxOccurs="unbounded">					<xs:annotation>						<xs:documentation xml:lang="de">Abzurechnender Vertrag des Kunden (1-n)</xs:documentation>						<xs:documentation xml:lang="en">Customer contract to be billed (1-n)</xs:documentation>						<xs:documentation xml:lang="fr">Contrat du client à prendre en compte (1-n)</xs:documentation>						<xs:documentation xml:lang="it">Contratto del cliente da rendicontare (1-n)</xs:documentation>					</xs:annotation>					<xs:complexType>						<xs:sequence>							<xs:element name="contractNoReplaced" type="contractNoType" minOccurs="0">								<xs:annotation>									<xs:documentation xml:lang="de">Vertragsnummer des ersetzten Vertrages</xs:documentation>									<xs:documentation xml:lang="de">Sofern der Versicherer aus technischen Gründen dem Vertrag eine neue Vertragsnummer zuteilt, so sollte dem Broker für die maschinelle Verarbeitung des Dokumentes einmalig zusätzlich die Vertragsnummer des ersetzten Vertrages mitgeliefert werden. Im XML-Viewer wird diese Vertragsnummer nicht angezeigt! Hinweis für Versicherer und Broker: Sofern der Versicherer ein neues Dokument erstellt, das dem Broker die Erfassung der neuen Vertragsnummer ermöglicht, ist die Information nicht notwendig.</xs:documentation>									<xs:documentation xml:lang="en">Policy number of superseded agreement</xs:documentation>									<xs:documentation xml:lang="en">If the insurer assigns a new number of contract for technical reasons, the broker should be provided with the contract number of the replaced contract on a one-off basis for machine processing of the document. This contract number is not displayed in the XML-Viewer! Note for insurer and broker: If the insurer creates a new document that allows the broker to enter the new contract number, the information is not necessary.</xs:documentation>									<xs:documentation xml:lang="fr">Numéro de contrat du contrat remplacé</xs:documentation>									<xs:documentation xml:lang="fr">Dès lors que l'assureur pour des raisons techniques attribue un nouveau numéro de contrat au contrat, il faudrait livrer au broker, pour le traitement automatique du document, une seule fois additionnellement le numéro de contrat du contrat remplacé. Dans le viewer XML ce numéro de contrat ne sera pas indiqué! Note pour les assureurs et les brokers: dès lors que l'assureur établit un nouveau document, qui permet au broker la saisie du nouveau numéro de contrat, l'information n'est pas nécessaire.</xs:documentation>									<xs:documentation xml:lang="it">Numero del contratto sostituito</xs:documentation>									<xs:documentation xml:lang="it">Se per motivi tecnici l’assicuratore assegna al contratto un nuovo numero, al broker deve essere inviato una tantum il numero del contratto sostituito ai fini dell’elaborazione meccanica del documento. Nel Viewer XML il numero non viene visualizzato! Nota per gli assicuratori e i broker: se l’assicuratore emette un nuovo documento che consente al broker il rilevamento del nuovo numero di contratto, tale informazione non è necessaria.</xs:documentation>								</xs:annotation>							</xs:element>							<xs:element name="brokerCodeIns" type="brokerCodeInsLocType" minOccurs="0">								<xs:annotation>									<xs:documentation xml:lang="de">Brokercode Versicherer</xs:documentation>									<xs:documentation xml:lang="de">Broker-Nummer gemäss dem Codeverzeichnis des einzelnen Versicherers.</xs:documentation>									<xs:documentation xml:lang="en">Insurer's broker code</xs:documentation>									<xs:documentation xml:lang="en">Broker number according to the individual insurer's code directory</xs:documentation>									<xs:documentation xml:lang="fr">Code broker de l’assureur</xs:documentation>									<xs:documentation xml:lang="fr">Numéro du broker selon le répertoire des codes de chaque assureur.</xs:documentation>									<xs:documentation xml:lang="it">Codice broker assicuratore</xs:documentation>									<xs:documentation xml:lang="it">Numero di broker in base all’elenco dei codici del singolo assicuratore</xs:documentation>								</xs:annotation>							</xs:element>							<xs:element name="agency" type="agencyLocType" minOccurs="0">								<xs:annotation>									<xs:documentation xml:lang="de">Agentur/Verkaufsstelle/Provisionsbezüger</xs:documentation>									<xs:documentation xml:lang="de">Agentur-, Verkaufsstellen- oder Provisionsbezüger-Nummer gemäss Codeverzeichnis des einzelnen Versicherers.</xs:documentation>									<xs:documentation xml:lang="en">Agency/sales point/commission agent</xs:documentation>									<xs:documentation xml:lang="en">Agency, sales point or commission agent number according to the individual insurer's code directory.</xs:documentation>									<xs:documentation xml:lang="fr">Agence/Point de vente/Bénéficiaire de la commission </xs:documentation>									<xs:documentation xml:lang="fr">Numéro de l’agence, du point de vente ou du bénéficiaire de la commission selon le répertoire des codes de chaque assureur.</xs:documentation>									<xs:documentation xml:lang="it">Agenzia/punto vendita/beneficiario della provvigione </xs:documentation>									<xs:documentation xml:lang="it">Numero di agenzia, punto vendita o beneficiario della provvigione in base all’elenco dei codici dell'assicuratore individuale.</xs:documentation>								</xs:annotation>							</xs:element>							<xs:element name="invoiceNo" type="noInvoiceLocType" minOccurs="0">								<xs:annotation>									<xs:documentation xml:lang="de">Rechnungsnummer</xs:documentation>									<xs:documentation xml:lang="de">Die der Prämienrechnung vom Versicherer in seinem Inkasso-/Exkassosystem zugeteilte Rechnungs- oder Zahlscheinnummer. Empfehlung an Versicherer: Um die Verbindung mit dem Prämienrecord sicherzustellen, sollte die Rechnungs- oder Zahlscheinnummer in den Provisionsrecord übernommen werden, da dadurch die Zuordnung zur Prämienrechnungsperiode eindeutig festgelegt wird.</xs:documentation>									<xs:documentation xml:lang="en">Number of invoice</xs:documentation>									<xs:documentation xml:lang="en">The invoice or payment slip number assigned to the premium invoice by the insurer in its collection/disbursement system. Recommendation for insurers: To create a link with the premium record, the invoice or payment slip number should be transferred to the provisioning record as this ensures unequivocal allocation to the premium invoice period.</xs:documentation>									<xs:documentation xml:lang="fr">Numéro de la facture</xs:documentation>									<xs:documentation xml:lang="fr">Le numéro de la facture ou du bordereau de paiement du décompte de prime de l'assureur fourni par le système d'encaissement/de décaissement de l'assureur. Recommandation à l'assureur: afin d'assurer la connexion avec l'enregistrement des primes, le numéro de la facture ou du bordereau de paiement doit être repris dans l'enregistrement de la commission, puisque cela détermine clairement l'attribution à la période de calcul des primes.</xs:documentation>									<xs:documentation xml:lang="it">Numero della fattura</xs:documentation>									<xs:documentation xml:lang="it">Il numero della fattura o del modulo di versamento attribuito dall’assicuratore al calcolo premi nel proprio sistema di incassi/esborsi. Raccomandazione agli assicuratori: per garantire il collegamento con il record dei premi, il numero della fattura o del modulo di versamento deve essere acquisito nel record delle provvigioni in modo da attribuire chiaramente il rispettivo periodo.</xs:documentation>								</xs:annotation>							</xs:element>							<xs:element name="invoiceDate" type="xs:date" minOccurs="0">								<xs:annotation>									<xs:documentation xml:lang="de">Rechnungserstellungsdatum</xs:documentation>									<xs:documentation xml:lang="de">JJJJ-MM-TT = Datum der Rechnungserstellung.</xs:documentation>									<xs:documentation xml:lang="en">Invoice creation date</xs:documentation>									<xs:documentation xml:lang="en">YYYY-MM-DD = Date of invoice creation.</xs:documentation>									<xs:documentation xml:lang="fr">Date d'établissement de la facture</xs:documentation>									<xs:documentation xml:lang="fr">AAAA-MM-JJ = Date de l'établissement de la facture</xs:documentation>									<xs:documentation xml:lang="it">Data di fatturazione</xs:documentation>									<xs:documentation xml:lang="it">AAAA-MM-GG = Data di fatturazione</xs:documentation>								</xs:annotation>							</xs:element>							<xs:element name="commStatement">								<xs:annotation>									<xs:documentation xml:lang="de">Abrechnungdaten der Brokerprovision</xs:documentation>									<xs:documentation xml:lang="en">Invoicing data of broker provisioning</xs:documentation>									<xs:documentation xml:lang="fr">Données du décompte de commission du broker</xs:documentation>									<xs:documentation xml:lang="it">Dati di conteggio della provvigione broker</xs:documentation>								</xs:annotation>								<xs:complexType>									<xs:choice>										<xs:element name="commStatementTotalOnly" type="commStatementTotalType">											<xs:annotation>												<xs:documentation xml:lang="de">Abrechnung der Brokerprovision nur als Gesamttotal</xs:documentation>												<xs:documentation xml:lang="en">Invoicing of broker provisioning only as overall total</xs:documentation>												<xs:documentation xml:lang="fr">Décompte de commission du broker uniquement en tant que total général</xs:documentation>												<xs:documentation xml:lang="it">Conteggio della provvigione broker solo come totale complessivo</xs:documentation>											</xs:annotation>										</xs:element>										<xs:element name="commStatementList">											<xs:annotation>												<xs:documentation xml:lang="de">Abrechnung der Brokerprovision mit Total und Einzelpositionen (1-n)</xs:documentation>												<xs:documentation xml:lang="en">Invoicing of broker provisioning with the total and individual items (1-n)</xs:documentation>												<xs:documentation xml:lang="fr">Décompte de commission du broker avec total et positions individuelles (1-n)</xs:documentation>												<xs:documentation xml:lang="it">Conteggio della provvigione broker con totale e posizioni individuali (1-n)</xs:documentation>											</xs:annotation>											<xs:complexType>												<xs:sequence>													<xs:element name="commStatementTotal" type="commStatementTotalType">														<xs:annotation>															<xs:documentation xml:lang="de">Abrechnung der Brokerprovision als Gesamttotal</xs:documentation>															<xs:documentation xml:lang="en">Invoicing of broker provisioning as an overall total</xs:documentation>															<xs:documentation xml:lang="fr">Décompte de commission du broker en tant que total général</xs:documentation>															<xs:documentation xml:lang="it">Conteggio della provvigione broker come totale complessivo</xs:documentation>														</xs:annotation>													</xs:element>													<xs:element name="commStatementDetails" maxOccurs="unbounded">														<xs:annotation>															<xs:documentation xml:lang="de">Einzelposition der  Brokerprovision (1-n)</xs:documentation>															<xs:documentation xml:lang="en">Individual position of broker commission (1-n)</xs:documentation>															<xs:documentation xml:lang="fr">Position individuelle de la commission du broker (1-n)</xs:documentation>															<xs:documentation xml:lang="it">Posizione individuale della provvigione broker (1-n)</xs:documentation>														</xs:annotation>														<xs:complexType>															<xs:sequence>																<xs:element name="datePeriodPremFrom" type="xs:date">																	<xs:annotation>																		<xs:documentation xml:lang="de">Prämie von</xs:documentation>																		<xs:documentation xml:lang="de">JJJJ-MM-TT = Beginn der Periode, für welche Provision gutgeschrieben bzw. belastet wird. Bei Teilprämien jeweils auch nur diese Teilperiode ausweisen. Empfehlung an Versicherer: Abrechnungsperiode sollte im Provisionsrecord angegeben werden, damit Broker, die auf den elektronischen Abgleich verzichten, über die entsprechenden Angaben beim Ausdruck einer Papierkopie verfügen.</xs:documentation>																		<xs:documentation xml:lang="en">Premium from</xs:documentation>																		<xs:documentation xml:lang="en">YYYY-MM-DD = Beginning of period for which the commission is credited or debited. In case of partial premium, only show this partial period. Recommendation to insurer: The invoicing period should be indicated in the provisioning record so that brokers who do not use electronic reconciliation have the details available on the print-out of a paper copy.</xs:documentation>																		<xs:documentation xml:lang="fr">Prime du</xs:documentation>																		<xs:documentation xml:lang="fr">AAAA-MM-JJ = Début de la période pour laquelle la commission est créditée resp. débitée. Pour les primes partielles montrer également respectivement uniquement ces périodes partielles. Recommandation à l'assureur: la période de décompte doit être donnée dans l'enregistrement commission, afin que les brokers qui renoncent à un recoupement électronique disposent des indications correspondantes lors de l'impression d'une copie papier.</xs:documentation>																		<xs:documentation xml:lang="it">Premio dal</xs:documentation>																		<xs:documentation xml:lang="it">AAAA-MM-GG = Inizio del periodo per quale il provvigione viene addebitato o accreditato. Per i premi parziali indicare solo il rispettivo periodo. Raccomandazione agli assicuratori: il periodo di rendicontazione deve essere indicato nel record delle provvigioni per consentire ai broker che non effettuano l’allineamento elettronico di disporre dei relativi dati tramite la stampa di una copia cartacea.</xs:documentation>																	</xs:annotation>																</xs:element>																<xs:element name="datePeriodPremTo" type="xs:date">																	<xs:annotation>																		<xs:documentation xml:lang="de">Prämie bis</xs:documentation>																		<xs:documentation xml:lang="de">JJJJ-MM-TT = Ende der Periode, für welche Provision gutgeschrieben bzw. belastet wird. Bei Teilprämien jeweils auch nur diese Teilperiode ausweisen. Empfehlung an Versicherer: Analog Datenfeld Pos. "datePeriodPremFrom"</xs:documentation>																		<xs:documentation xml:lang="en">Premium to</xs:documentation>																		<xs:documentation xml:lang="en">YYYY-MM-DD = End of period for which the commission is credited or debited. In case of partial premium, only show this partial period. Recommendation to insurer: Analog data field pos. "datePeriodPremFrom"</xs:documentation>																		<xs:documentation xml:lang="fr">Prime jusqu'au</xs:documentation>																		<xs:documentation xml:lang="fr">AAAA-MM-JJ = Fin de la période pour laquelle la commission est créditée resp. débitée. Pour les primes partielles montrer également respectivement uniquement ces périodes partielles. Recommandation à l'assureur: analogue au champ de données pos. "datePeriodPremFrom"</xs:documentation>																		<xs:documentation xml:lang="it">Premio fino al</xs:documentation>																		<xs:documentation xml:lang="it">AAAA-MM-GG = Fine del periodo per quale il provvigione viene addebitato o accreditato. Per i premi parziali indicare solo il rispettivo periodo. Raccomandazione all'assicuratore: Analogici campo dati pos. "datePeriodPremFrom"</xs:documentation>																	</xs:annotation>																</xs:element>																<xs:element name="typeAccountSettlement" type="typeAccountSettlementLocType" minOccurs="0">																	<xs:annotation>																		<xs:documentation xml:lang="de">Abrechnungsart</xs:documentation>																		<xs:documentation xml:lang="de">Bemerkung für Versicherer: Die jährliche Folgecourtage wird unter Code 04 abgerechnet. Für das Einzellebengeschäft sind die Datenfelder ‚Prämie von‘ und ‚Prämie bis‘  nicht relevant. Der Provisionsrecord soll deshalb die Angaben gemäss Datenfelder "Abrechnungsart" und "Zahlungsart und Rate" enthalten.</xs:documentation>																		<xs:documentation xml:lang="en">Invoice form</xs:documentation>																		<xs:documentation xml:lang="en">Remark for insurer: The data fields ‚Premium from‘ and ‚Premium until‘ are not relevant for the single premium business. The provisioning record should therefore contain the information according to the data fields "invoice type" and "payment type and rate".</xs:documentation>																		<xs:documentation xml:lang="fr">Genre de décompte</xs:documentation>																		<xs:documentation xml:lang="fr">Remarque pour l'assureur: le courtage subséquent annuel sera décompté sous le code 04. Pour les affaires vie individuelle les champs de données ‚prime du‘ et ‚prime jusqu'au‘ ne sont pas importants. C'est pourquoi l'enregistrement commission doit contenir les indications selon les champs de données "genre du décompte" et "genre de versement et tranche".</xs:documentation>																		<xs:documentation xml:lang="it">Tipo di conteggio</xs:documentation>																		<xs:documentation xml:lang="it">Nota per l'assicuratore: il successivo brokeraggio annuale verrà conteggiato sotto il codice 04. Per il ramo vita individuale i campi di dati, premio di "e, premio fino a" non sono importanti. Per questo motivo, la registrazione della commissione deve contenere le indicazioni in base ai campi dati "tipo di regolamento" e "tipo di pagamento e rata".</xs:documentation>																	</xs:annotation>																</xs:element>																<xs:element name="typePayInstalment" type="periodicityType" minOccurs="0">																	<xs:annotation>																		<xs:documentation xml:lang="de">Zahlungsart und Rate</xs:documentation>																		<xs:documentation xml:lang="en">Manner of payment and rate</xs:documentation>																		<xs:documentation xml:lang="fr">Genre de versement et tranche</xs:documentation>																		<xs:documentation xml:lang="it">Tipo di pagamento e rata</xs:documentation>																	</xs:annotation>																</xs:element>																<xs:element name="lobIG" type="LOBIGType">																	<xs:annotation>																		<xs:documentation xml:lang="de">Branche</xs:documentation>																		<xs:documentation xml:lang="de">Branchencode gemäss IG-Verzeichnis. Das aktuelle Dokument – Verzeichnis der Branchencodes –  steht unter www.igb2b.ch im Mitgliederbereich zur Verfügung.</xs:documentation>																		<xs:documentation xml:lang="en">Sector</xs:documentation>																		<xs:documentation xml:lang="en">Sector code in accordance with IG directory. The current document – branch code directory – is available in the members area at www.igb2b.ch.</xs:documentation>																		<xs:documentation xml:lang="fr">Branche</xs:documentation>																		<xs:documentation xml:lang="fr">Codes des branches selon le répertoire IG. Le document actuel (répertoire des codes des branches) se trouve sur www.igb2b.ch, à la rubrique «Domaine membres».</xs:documentation>																		<xs:documentation xml:lang="it">Settore</xs:documentation>																		<xs:documentation xml:lang="it">Codice settoriale in base all’elenco IG. Il documento attuale – Elenco dei codici settoriali – è disponibile su www.igb2b.ch nella sezione "Ambito membri".</xs:documentation>																	</xs:annotation>																</xs:element>																<xs:element name="lobINS" type="LOBINSType" minOccurs="0">																	<xs:annotation>																		<xs:documentation xml:lang="de">Branche / Risikobezeichnung</xs:documentation>																		<xs:documentation xml:lang="de">Branchen- oder Risikocode gemäss Codeverzeichnis des einzelnen Versicherers. Bei kombinierten Produkten, die  unterschiedlich provisioniert werden,  muss der Detaillierungsgrad mindestens der Differenzierung bei den Provisionssätzen entsprechen. Option Versicherer: - Soweit es sinnvoll ist, kann die Provision bei gleichem Provisionssatz als Total in einer Zeile ausgewiesen werden. - Anstatt eines Branchen- oder Risikocodes kann die Bezeichnung auch als Text erfolgen.</xs:documentation>																		<xs:documentation xml:lang="en">Sector / Risk description</xs:documentation>																		<xs:documentation xml:lang="en">Sector or risk code according to the individual insurer's code directory. In the case of combined products that are differently commissioned, the degree of detail must correspond to at least the difference in the commission rates. Option insurance: - If required, the provisioning can be entered in a line with the same provisioning rate as the total. - Instead of a sector or risk code the description can be also as text.</xs:documentation>																		<xs:documentation xml:lang="fr">Branche / description du risque </xs:documentation>																		<xs:documentation xml:lang="fr">Codes des branches ou de risque selon le répertoire des codes de chaque assureur. En cas de produits combinés soumis à des commissions différentes, le degré de détail doit correspondre au moins à la différenciation des taux de commission. Options assureur: - Dans la mesure où cela fait sens, la commission peut lors de taux de commission identiques être montrée comme total sur une ligne. - Au lieu d'un code de branche ou de risque la description peut également être faite en texte.</xs:documentation>																		<xs:documentation xml:lang="it">Settore / denominazione rischio</xs:documentation>																		<xs:documentation xml:lang="it">Codice settoriale o di rischio in base all'elenco dei codici dell'assicuratore individuale. In caso di prodotti combinati per i quali le provvigioni differiscono, il grado di dettaglio deve almeno corrispondere alla differenziazione dei tassi di provvigione. Opzione di assicurazione: Invece di un codice settoriale o di rischio, la denominazione potrebbe anche essere testuale.</xs:documentation>																	</xs:annotation>																</xs:element>																<xs:element name="currency" type="currencyCodeType" minOccurs="0">																	<xs:annotation>																		<xs:documentation xml:lang="de">Währung (Prämien und Provisionen)</xs:documentation>																		<xs:documentation xml:lang="de">Code gemäss ISO-Code. Ohne Angabe der Währung gilt immer die Referenzwährung, d.h. zur Zeit der Schweizer Franken (CHF).</xs:documentation>																		<xs:documentation xml:lang="en">Currency (Premium and commission)</xs:documentation>																		<xs:documentation xml:lang="en">Code according to ISO-Code. Without specifying the currency, the reference currency always applies, at the time Swiss Francs (CHF).</xs:documentation>																		<xs:documentation xml:lang="fr">Monnaie (primes et commissions)</xs:documentation>																		<xs:documentation xml:lang="fr">Code selon le code ISO. Sans indication de la monnaie, c'est toujours la monnaie de référence qui s'applique, c'est-à-dire actuellement le franc suisse (CHF).</xs:documentation>																		<xs:documentation xml:lang="it">Moneta (Premi e provvigioni)</xs:documentation>																		<xs:documentation xml:lang="it">Codice secondo il codice ISO. Senza indicazione della moneta, si applica sempre la moneta riferimento, al momento il franco svizzero (CHF).</xs:documentation>																	</xs:annotation>																</xs:element>																<xs:element name="premiumComm" type="decimal10dot2Type">																	<xs:annotation>																		<xs:documentation xml:lang="de">Provisionsberechnungsbasis</xs:documentation>																		<xs:documentation xml:lang="de">Provisionsberechtigte Prämie, Produktionssumme (Einzellebensgeschäft) oder Risikokostenprämie, Deckungskapital (Kollektivleben) gemäss Definition des Versicherers. Der Betrag ist im Normalfall positiv.</xs:documentation>																		<xs:documentation xml:lang="en">Basis on which premium calculated</xs:documentation>																		<xs:documentation xml:lang="en">Provisioning-eligible premium, production total (single premium business) or risk-cost premium, actuarial reserve (collective level) according to the insurer’s definition. The amount is normally positive.</xs:documentation>																		<xs:documentation xml:lang="fr">Base de calcul de la commission</xs:documentation>																		<xs:documentation xml:lang="fr">Prime commissionnée, somme de la production (affaires vie individuelle) ou frais de prime de risque, réserve mathématique (vie collective) selon la définition de l'assureur. Le montant est généralement positif.</xs:documentation>																		<xs:documentation xml:lang="it">Base per il calcolo della provvigione</xs:documentation>																		<xs:documentation xml:lang="it">Premio con provvigione, somma di produzione (operazioni Vita individuale) o premio dei costi di rischio, capitale di copertura (Vita collettiva) secondo la definizione dell’assicuratore. L'importo è generalmente positivo.</xs:documentation>																	</xs:annotation>																</xs:element>																<xs:element name="typeComm" type="typeCommLocType">																	<xs:annotation>																		<xs:documentation xml:lang="de">Art der Brokerprovision</xs:documentation>																		<xs:documentation xml:lang="de">Hinweis Broker und Brokersoftware: Für die Mobiliar Versicherung (MobiLife) wurden zusätzlich auch folgende Codes zugelassen (siehe Werteliste)</xs:documentation>																		<xs:documentation xml:lang="en">Form of broker commission</xs:documentation>																		<xs:documentation xml:lang="en">Note for broker and brokersoftware: For the insurer Mobiliar (MobiLife) the following codes were additionally approved (see list of values).</xs:documentation>																		<xs:documentation xml:lang="fr">Genre de la commission du broker</xs:documentation>																		<xs:documentation xml:lang="fr">Note pour les brokers et les fabricants de logiciels broker: pour la Mobilière (MobiLife) additionnellement les codes suivants ont également été autorisés (cf. liste des valeurs)</xs:documentation>																		<xs:documentation xml:lang="it">Tipo di provvigione di broker</xs:documentation>																		<xs:documentation xml:lang="it">Nota per i broker e il software per broker: per l’assicurazione Mobiliare (MobiLife) sono stati ammessi anche i seguenti codici (v. elenco).</xs:documentation>																	</xs:annotation>																</xs:element>																<xs:element name="commissionRate" type="decimal3dot3Type">																	<xs:annotation>																		<xs:documentation xml:lang="de">Provisionssatz in %</xs:documentation>																		<xs:documentation xml:lang="de">Immer Angabe in % im Format xxx.yyy. Hinweis Versicherer: z.B. Ein Provisionssatz von 0.5‰ wird mit 0.05% angezeigt.</xs:documentation>																		<xs:documentation xml:lang="en">Commission rate in %</xs:documentation>																		<xs:documentation xml:lang="en">Always specified in % in the format xxx.yyy. Note for insurer: e.g. a commission rate of 0.5‰ is shown as 0.05%.</xs:documentation>																		<xs:documentation xml:lang="fr">Taux de commission en %</xs:documentation>																		<xs:documentation xml:lang="fr">Information toujours en % en format xxx.yyy. Note pour l'assureur: Par exemple: un taux de commission de 0.5‰ sera représenté par 0.05%.</xs:documentation>																		<xs:documentation xml:lang="it">Tasso di provvigione in %</xs:documentation>																		<xs:documentation xml:lang="it">Sempre specificato in % nel formato xxx.yyy. Nota per l'assicuratore: Per esempio:  Una tasso di provvigione di 0.5‰ è indicata come 0.05%.</xs:documentation>																	</xs:annotation>																</xs:element>																<xs:element name="commissionFactor" type="decimal3dot3Type" minOccurs="0">																	<xs:annotation>																		<xs:documentation xml:lang="de">Provisionsfaktor</xs:documentation>																		<xs:documentation xml:lang="de">Faktor für die Auf- oder Abwertung des Basisprovisionssatzes.</xs:documentation>																		<xs:documentation xml:lang="en">Commission factor</xs:documentation>																		<xs:documentation xml:lang="en">Factor for the increase or decrease in the basic provisioning rate.</xs:documentation>																		<xs:documentation xml:lang="fr">Facteur de commission</xs:documentation>																		<xs:documentation xml:lang="fr">Facteur pour la réévaluation ou la dévaluation du taux de base de la commission.</xs:documentation>																		<xs:documentation xml:lang="it">Fattore di provvigione</xs:documentation>																		<xs:documentation xml:lang="it">Fattore per l’apprezzamento o il deprezzamento del tasso base di provvigione</xs:documentation>																	</xs:annotation>																</xs:element>																<xs:element name="amountComm" type="decimal10dot2Type">																	<xs:annotation>																		<xs:documentation xml:lang="de">Brokerprovision</xs:documentation>																		<xs:documentation xml:lang="de">Der Betrag ist im Normalfall positiv.</xs:documentation>																		<xs:documentation xml:lang="en">Broker commission</xs:documentation>																		<xs:documentation xml:lang="en">The amount is normaly positive.</xs:documentation>																		<xs:documentation xml:lang="fr">Commission du broker </xs:documentation>																		<xs:documentation xml:lang="fr">Le montant est généralement positif.</xs:documentation>																		<xs:documentation xml:lang="it">Provvigione di broker</xs:documentation>																		<xs:documentation xml:lang="it">L'importo è normalmente positivo.</xs:documentation>																	</xs:annotation>																</xs:element>																<xs:element name="generalAgreement" type="generalAgreementLocType" minOccurs="0">																	<xs:annotation>																		<xs:documentation xml:lang="de">Rahmenvertrags-/Flottenvertragsnummer</xs:documentation>																		<xs:documentation xml:lang="de">Bei Rahmenverträgen oder MF-Flottenverträgen ist die Angabe der Rahmenvertragsnummer erwünscht, sofern unter Policen-/Vertragsnummer die Nummer der einzelnen unter den Rahmen- oder Flottenvertrag fallenden Versicherungen angegeben wird (die Rahmenvertrags-Nr. erleichtert dem Broker die richtige Zuordnung der Provisionen).</xs:documentation>																		<xs:documentation xml:lang="en">Framework agreement/fleet agreement number</xs:documentation>																		<xs:documentation xml:lang="en">In the case of framework agreements or motor vehicle fleet agreements, the framework agreement number is required if, under the policy/agreement number, the number of the individual insurances under the framework or fleet agreement is provided (the framework agreement number makes correct assignment easier for the broker).</xs:documentation>																		<xs:documentation xml:lang="fr">Numéro de contrat-cadre / de contrat de flotte</xs:documentation>																		<xs:documentation xml:lang="fr">En cas de contrats-cadre ou de contrats de flotte VAM, l’indication du numéro du contrat-cadre est recommandée dans la mesure où le numéro de chaque assurance relevant du contrat-cadre ou du contrat de flotte est indiqué sous le numéro de police / de contrat (le numéro du contrat-cadre simplifie l’attribution de la commission pour le broker).</xs:documentation>																		<xs:documentation xml:lang="it">Numero contratto quadro/contratto parchi veicoli</xs:documentation>																		<xs:documentation xml:lang="it">In caso di contratti quadro o parchi veicoli VM è richiesto il numero del contratto quadro se alla voce numero di polizza/contratto viene riportato il numero delle singole assicurazioni che rientrano nel contratto quadro o parchi veicoli (il n. di contratto quadro aiuta il broker nella giusta attribuzione).</xs:documentation>																	</xs:annotation>																</xs:element>															</xs:sequence>														</xs:complexType>													</xs:element>												</xs:sequence>											</xs:complexType>										</xs:element>									</xs:choice>								</xs:complexType>							</xs:element>							<xs:choice>								<xs:annotation>									<xs:documentation xml:lang="de">Wahl zwischen einem Geschäfts- und Privatkunde</xs:documentation>									<xs:documentation xml:lang="en">Choice between a business and a private client</xs:documentation>									<xs:documentation xml:lang="fr">Choix entre un client entreprise et un client privé</xs:documentation>									<xs:documentation xml:lang="it">Scelta tra un cliente aziendale e un cliente privato</xs:documentation>								</xs:annotation>								<xs:element name="ICBusiness" type="ICBusinessLocType">									<xs:annotation>										<xs:documentation xml:lang="de">Kommission für eine Firma</xs:documentation>										<xs:documentation xml:lang="en">Commission for a company</xs:documentation>										<xs:documentation xml:lang="fr">Commission pour une entreprise</xs:documentation>										<xs:documentation xml:lang="it">Commissione per un'azienda</xs:documentation>									</xs:annotation>								</xs:element>								<xs:element name="ICPrivatePerson" type="ICPrivatePersonLocType">									<xs:annotation>										<xs:documentation xml:lang="de">Kommission für eine Privatperson</xs:documentation>										<xs:documentation xml:lang="en">Commission for a private person</xs:documentation>										<xs:documentation xml:lang="fr">Commission pour une personne privée</xs:documentation>										<xs:documentation xml:lang="it">Commissione per una persona privata</xs:documentation>									</xs:annotation>								</xs:element>							</xs:choice>						</xs:sequence>						<xs:attribute name="contractNo" type="contractNoType" use="required">							<xs:annotation>								<xs:documentation xml:lang="de">Vertragsnummer des laufenden Vertrages</xs:documentation>								<xs:documentation xml:lang="de">Gültige Vertragsnummer des Versicherers. Diese Nummern sind Suchbegriffe und sind im gleichen Format darzustellen, in dem sie auf allen Kundendokumenten erscheinen. So können sie auch ohne Editierungsregel vom Versicherer gedruckt werden. Der Vertragsnummernaufbau des einzelnen Versicherers kann dem IG-Verzeichnis – Vertragsnummern-Aufbau Versicherer - entnommen werden. Das aktuelle Dokument steht unter www.igb2b.ch im Mitgliederbereich zur Verfügung. Option / Empfehlung für Broker: In Bezug auf das Mitversicherungsgeschäft sollte auch die Erfassung der internen Vertragsnummer der beteiligten Versicherer möglich sein, damit eine individuelle Abrechnung der einzelnen Anteile durch diese Versicherer erfasst werden kann.</xs:documentation>								<xs:documentation xml:lang="en">Policy number of current policy</xs:documentation>								<xs:documentation xml:lang="en">Valid insurer's policy number. These numbers are search terms and should be presented in the same format in which they appear in all client documents. This means they can be printed by the insurer without an editing rule. The individual insurer's policy number structure can be found in the IG directory – insurer policy number structure. The current document is available at www.igb2b.ch in the members area. Option/recommendation for brokers: With regard to co-insurance business, it should also be possible to record the internal contract numbers of the insurers involved so that specific invoicing of the individual shares can be recorded by these insurers.</xs:documentation>								<xs:documentation xml:lang="fr">Numéro de contrat du contrat en cours</xs:documentation>								<xs:documentation xml:lang="fr">Numéro de contrat valable de l’assureur. Ces numéros constituent des critères de recherche et doivent être représentés dans le même format que celui dans lequel ils apparaissent sur tous les documents client. Ils peuvent ainsi être imprimés par l’assureur sans règle d’édition.  La structure des numéros de contrat de l’assureur figure dans le répertoire IG – structure des numéros de contrat de l’assureur. Le document actuel se trouve sur www.igb2b.ch à la rubrique «Domaine membres». Option / Recommandation pour le broker: en ce qui concerne les affaires en coassurance, la saisie du numéro de contrat interne de l'assureur participant doit également être possible, afin qu'un décompte individuel de chaque partie puisse être saisi par ces assureurs.</xs:documentation>								<xs:documentation xml:lang="it">Numero del contratto in corso</xs:documentation>								<xs:documentation xml:lang="it">Numero di contratto valido dell'assicuratore. Questi numeri sono termini di ricerca e devono essere indicati nello stesso formato in cui compaiono su tutti i documenti del cliente. Così possono essere stampati dall’assicuratore anche senza regola di editing.  La struttura del numero di contratto dell’assicuratore individuale è riportata nell’elenco IG – struttura numero di contratto assicuratori.</xs:documentation>							</xs:annotation>						</xs:attribute>					</xs:complexType>				</xs:element>				<xs:element name="commissionTotal" type="decimal10dot2Type">					<xs:annotation>						<xs:documentation xml:lang="de">Gesamttotal der Provisionsabrechnung / Buchungsmonates</xs:documentation>						<xs:documentation xml:lang="de">Total aller Provisionsgutschriften und Belastungen, der mit diesem Datenfile für den betroffenen Buchungsmonat abgerechneten Provisionierungsfälle. Das Total  muss mit der Provisionsverbuchung durch den Broker in der Finanzbuchhaltung übereinstimmen.</xs:documentation>						<xs:documentation xml:lang="en">Commission total / Posting month</xs:documentation>						<xs:documentation xml:lang="en">Total of all provisioning credits and debits of the provisioning cases invoiced with this data file for the booking month concerned. The total must correspond to the broker’s provisioning booking in financial accounting.</xs:documentation>						<xs:documentation xml:lang="fr">Total général du décompte de commission / mois comptable</xs:documentation>						<xs:documentation xml:lang="fr">Total de toutes les commissions créditées et débitées de ce fichier de données pour le mois comptable. Le total doit correspondre à la comptabilisation de la commission par le broker dans sa comptabilité financière.</xs:documentation>						<xs:documentation xml:lang="it">Totale complessivo del conteggio della provvigione / Mese contabile</xs:documentation>						<xs:documentation xml:lang="it">Totale di tutti gli addebiti e accrediti per le provvigioni relative a questo file dati nel rispettivo mese contabile. Il totale deve coincidere con la registrazione delle provvigioni da parte del broker nella contabilità finanziaria.</xs:documentation>					</xs:annotation>				</xs:element>				<xs:element name="remarks" type="remarkType" minOccurs="0">					<xs:annotation>						<xs:documentation xml:lang="de">Bemerkung</xs:documentation>						<xs:documentation xml:lang="de">Textfeld für maschinell ausgelöste und standardisierte Hinweise. Hinweis: Bemerkungen sind nur in Ausnahmefällen anzubringen und nach Möglichkeit zu unterlassen, da damit der vollständige Workflow unterbrochen wird.</xs:documentation>						<xs:documentation xml:lang="en">Remarks</xs:documentation>						<xs:documentation xml:lang="en">Text field for machine triggered and standardised information. Note: Comments should be made only in exceptional cases and avoided where possible, as they interrupt the complete workflow.</xs:documentation>						<xs:documentation xml:lang="fr">Remarques</xs:documentation>						<xs:documentation xml:lang="fr">Zone de texte pour les notes déclanchées automatiquement et standardisées. Note: les commentaires doivent être faits que dans des cas exceptionnels et doivent être si possible évités, car cela interrompt le workflow.</xs:documentation>						<xs:documentation xml:lang="it">Osservazione</xs:documentation>						<xs:documentation xml:lang="it">Campo di testo per note generate meccanicamente e standardizzate. Nota: riportare delle osservazioni solo in casi eccezionali e possibilmente tralasciarle perché interrompono il workflow completo.</xs:documentation>					</xs:annotation>				</xs:element>				<xs:element name="attachment" type="attachmentType" minOccurs="0" maxOccurs="unbounded">					<xs:annotation>						<xs:documentation xml:lang="de">Dateianhang</xs:documentation>						<xs:documentation xml:lang="en">Attachment</xs:documentation>						<xs:documentation xml:lang="fr">Pièce jointe</xs:documentation>						<xs:documentation xml:lang="it">Allegato</xs:documentation>					</xs:annotation>				</xs:element>			</xs:sequence>		</xs:complexType>	</xs:element></xs:schema>