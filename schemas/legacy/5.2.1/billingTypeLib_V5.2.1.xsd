<?xml version="1.0" encoding="UTF-8"?><!--XSD Standard for IG B2B Data exchangeCopyright © IG B2B for Insurers + Brokers, Switzerland--><!--Changes in Version V5.2: - Added translated strings in documentationChanges in Version V5.2.1 from V5.2: - Change version values and name of referenced data schema from 5.2 to 5.2.1--><xs:schema xmlns:xs="http://www.w3.org/2001/XMLSchema" xmlns="http://www.IGB2B.ch/XMLSchema" targetNamespace="http://www.IGB2B.ch/XMLSchema" elementFormDefault="qualified" version="5.2.1">	<xs:include schemaLocation="globals_V5.2.1.xsd"/>	<xs:simpleType name="brokerCodeInsLocType">		<xs:annotation>			<xs:documentation xml:lang="de">Datentyp für Brokercode Versicherer</xs:documentation>			<xs:documentation xml:lang="en">Data type for insurer's broker code</xs:documentation>			<xs:documentation xml:lang="fr">Type de données pour le code broker de l'assureur</xs:documentation>			<xs:documentation xml:lang="it">Tipo di dati per codice broker dell'assicuratore</xs:documentation>		</xs:annotation>		<xs:restriction base="xs:NMTOKEN">			<xs:maxLength value="10"/>		</xs:restriction>	</xs:simpleType>	<xs:simpleType name="agencyLocType">		<xs:annotation>			<xs:documentation xml:lang="de">Datentyp für Agentur/Verkaufsstelle/Provisionsbezüger</xs:documentation>			<xs:documentation xml:lang="en">Data type for agency/sales point/commission agent</xs:documentation>			<xs:documentation xml:lang="fr">Type de données pour Agence/Point de vente/Bénéficiaire de la commission </xs:documentation>			<xs:documentation xml:lang="it">Tipo di dati per agenzia/punto vendita/beneficiario della provvigione</xs:documentation>		</xs:annotation>		<xs:restriction base="textLine">			<xs:maxLength value="20"/>		</xs:restriction>	</xs:simpleType>	<xs:simpleType name="noInvoiceLocType">		<xs:annotation>			<xs:documentation xml:lang="de">Datentyp für Rechnungsnummer</xs:documentation>			<xs:documentation xml:lang="en">Data type for invoice number</xs:documentation>			<xs:documentation xml:lang="fr">Type de données pour le numéro de la facture</xs:documentation>			<xs:documentation xml:lang="it">Tipo di dati per il numero della fattura</xs:documentation>		</xs:annotation>		<xs:restriction base="textLine">			<xs:maxLength value="20"/>		</xs:restriction>	</xs:simpleType>	<xs:simpleType name="mailingLocType">		<xs:annotation>			<xs:documentation xml:lang="de">Datentyp für Rechnungsversand</xs:documentation>			<xs:documentation xml:lang="en">Data type for invoice dispatch</xs:documentation>			<xs:documentation xml:lang="fr">Type de données pour l'envoi de la facture</xs:documentation>			<xs:documentation xml:lang="it">Tipo di dati per l'invio della fattura</xs:documentation>		</xs:annotation>		<xs:restriction base="xs:int">			<xs:totalDigits value="2"/>			<xs:enumeration value="01">				<xs:annotation>					<xs:documentation xml:lang="de">Direkt an Kunde</xs:documentation>					<xs:documentation xml:lang="en">Directly to client</xs:documentation>					<xs:documentation xml:lang="fr">Directement au client</xs:documentation>					<xs:documentation xml:lang="it">Direttamente al clienti</xs:documentation>				</xs:annotation>			</xs:enumeration>			<xs:enumeration value="02">				<xs:annotation>					<xs:documentation xml:lang="de">An Broker</xs:documentation>					<xs:documentation xml:lang="en">To broker</xs:documentation>					<xs:documentation xml:lang="fr">A l'attention du broker</xs:documentation>					<xs:documentation xml:lang="it">Ai broker</xs:documentation>				</xs:annotation>			</xs:enumeration>			<xs:enumeration value="03">				<xs:annotation>					<xs:documentation xml:lang="de">e-Billing (elektronische Rechnungsstellung)</xs:documentation>					<xs:documentation xml:lang="en">e-Billing (electronic invoicing)</xs:documentation>					<xs:documentation xml:lang="fr">e-Billing (établissement de la facture électronique)</xs:documentation>					<xs:documentation xml:lang="it">e-Billing (fatturazione elettronica)</xs:documentation>				</xs:annotation>			</xs:enumeration>		</xs:restriction>	</xs:simpleType>	<xs:simpleType name="kindPremiumLocType">		<xs:annotation>			<xs:documentation xml:lang="de">Datentyp für Prämienart</xs:documentation>			<xs:documentation xml:lang="en">Data type for premium form</xs:documentation>			<xs:documentation xml:lang="fr">Type de données pour le genre de prime</xs:documentation>			<xs:documentation xml:lang="it">Tipo di dati per tipo di premio</xs:documentation>		</xs:annotation>		<xs:restriction base="xs:NMTOKEN">			<xs:maxLength value="2"/>			<xs:enumeration value="11">				<xs:annotation>					<xs:documentation xml:lang="de">Folgeprämie normal</xs:documentation>					<xs:documentation xml:lang="en">Normal Renewal premium</xs:documentation>					<xs:documentation xml:lang="fr">Prime subséquente normale</xs:documentation>					<xs:documentation xml:lang="it">Premio di rinnovo normale</xs:documentation>				</xs:annotation>			</xs:enumeration>			<xs:enumeration value="12">				<xs:annotation>					<xs:documentation xml:lang="de">Folgeprämie mit Tarifanpassung</xs:documentation>					<xs:documentation xml:lang="en">Renewal premium with tariff adjustment</xs:documentation>					<xs:documentation xml:lang="fr">Prime subséquente avec changement de tarif</xs:documentation>					<xs:documentation xml:lang="it">Premio di rinnovo con adeguamento tariffario</xs:documentation>				</xs:annotation>			</xs:enumeration>			<xs:enumeration value="20">				<xs:annotation>					<xs:documentation xml:lang="de">Erst- und Teilprämie (Prämie pro rata)</xs:documentation>					<xs:documentation xml:lang="en">Initial and partial premium (pro rata premium)</xs:documentation>					<xs:documentation xml:lang="fr">Prime initiale et prime partielle (prime prorata)</xs:documentation>					<xs:documentation xml:lang="it">Primo premio e premio parziale (premio pro rata)</xs:documentation>				</xs:annotation>			</xs:enumeration>			<xs:enumeration value="30">				<xs:annotation>					<xs:documentation xml:lang="de">Abrechnungsprämie (definitive Prämienabrechnung)</xs:documentation>					<xs:documentation xml:lang="en">Invoiced premium (final premium invoice)</xs:documentation>					<xs:documentation xml:lang="fr">Décompte de prime (Décompte de prime définitif)</xs:documentation>					<xs:documentation xml:lang="it">Premio conteggio (conteggio del premio definitivo)</xs:documentation>				</xs:annotation>			</xs:enumeration>			<xs:enumeration value="40">				<xs:annotation>					<xs:documentation xml:lang="de">Gewinnanteil / Überschussbeteiliung</xs:documentation>					<xs:documentation xml:lang="en">Share in profit / surplus</xs:documentation>					<xs:documentation xml:lang="fr">Part au bénéfice / Participation au bénéfice</xs:documentation>					<xs:documentation xml:lang="it">Quota di profitto / partecipazione alle eccedenze</xs:documentation>				</xs:annotation>			</xs:enumeration>			<xs:enumeration value="99">				<xs:annotation>					<xs:documentation xml:lang="de">übrige</xs:documentation>					<xs:documentation xml:lang="en">others</xs:documentation>					<xs:documentation xml:lang="fr">autres</xs:documentation>					<xs:documentation xml:lang="it">altri</xs:documentation>				</xs:annotation>			</xs:enumeration>		</xs:restriction>	</xs:simpleType>	<xs:simpleType name="typePremiumLocType">		<xs:annotation>			<xs:documentation xml:lang="de">Datentyp für Prämientyp</xs:documentation>			<xs:documentation xml:lang="en">Data type for premium type</xs:documentation>			<xs:documentation xml:lang="fr">Type de données pour type de primes</xs:documentation>			<xs:documentation xml:lang="it">Tipo di dati per tipo di premio</xs:documentation>		</xs:annotation>		<xs:restriction base="xs:NMTOKEN">			<xs:maxLength value="2"/>			<xs:enumeration value="01">				<xs:annotation>					<xs:documentation xml:lang="de">Prämien</xs:documentation>					<xs:documentation xml:lang="en">Premiums</xs:documentation>					<xs:documentation xml:lang="fr">Primes</xs:documentation>					<xs:documentation xml:lang="it">Premi</xs:documentation>				</xs:annotation>			</xs:enumeration>			<xs:enumeration value="02">				<xs:annotation>					<xs:documentation xml:lang="de">Zuschläge / Rabatte</xs:documentation>					<xs:documentation xml:lang="en">Supplements / Discounts</xs:documentation>					<xs:documentation xml:lang="fr">Suppléments / Rabais</xs:documentation>					<xs:documentation xml:lang="it">Supplementi / Sconti</xs:documentation>				</xs:annotation>			</xs:enumeration>			<xs:enumeration value="03">				<xs:annotation>					<xs:documentation xml:lang="de">Bearbeitungsgebühren</xs:documentation>					<xs:documentation xml:lang="en">Administrative charges</xs:documentation>					<xs:documentation xml:lang="fr">Frais de traitement</xs:documentation>					<xs:documentation xml:lang="it">Tasse di elaborazione</xs:documentation>				</xs:annotation>			</xs:enumeration>			<xs:enumeration value="04">				<xs:annotation>					<xs:documentation xml:lang="de">Rundungsdifferenzen</xs:documentation>					<xs:documentation xml:lang="en">Rounding differences</xs:documentation>					<xs:documentation xml:lang="fr">Des différences d'arrondi</xs:documentation>					<xs:documentation xml:lang="it">Differenze di arrotondamento</xs:documentation>				</xs:annotation>			</xs:enumeration>			<xs:enumeration value="05">				<xs:annotation>					<xs:documentation xml:lang="de">Stempelsteuer</xs:documentation>					<xs:documentation xml:lang="en">Stamp duty</xs:documentation>					<xs:documentation xml:lang="fr">Droit de timbre</xs:documentation>					<xs:documentation xml:lang="it">Imposta di bollo</xs:documentation>				</xs:annotation>			</xs:enumeration>			<xs:enumeration value="06">				<xs:annotation>					<xs:documentation xml:lang="de">Zuschlag für SVG 74/76</xs:documentation>					<xs:documentation xml:lang="en">Road Traffic Act (SVG) 74/76 supplement</xs:documentation>					<xs:documentation xml:lang="fr">Supplément pour la LCR 74/76</xs:documentation>					<xs:documentation xml:lang="it">Supplemento per LCStr 74/76</xs:documentation>				</xs:annotation>			</xs:enumeration>			<xs:enumeration value="07">				<xs:annotation>					<xs:documentation xml:lang="de">Unfallverhütungszuschlag</xs:documentation>					<xs:documentation xml:lang="en">Accident prevention supplement</xs:documentation>					<xs:documentation xml:lang="fr">Supplément pour la prévention des accidents</xs:documentation>					<xs:documentation xml:lang="it">Supplemento prevenzione infortuni</xs:documentation>				</xs:annotation>			</xs:enumeration>			<xs:enumeration value="08">				<xs:annotation>					<xs:documentation xml:lang="de">Prämien gemäss Definition des einzelnen Versicherers</xs:documentation>					<xs:documentation xml:lang="en">Premiums as defined by individual insurer</xs:documentation>					<xs:documentation xml:lang="fr">Primes selon la définition de chaque assureur.</xs:documentation>					<xs:documentation xml:lang="it">Premi secondo la definizione dell’assicuratore individuale</xs:documentation>				</xs:annotation>			</xs:enumeration>			<xs:enumeration value="09">				<xs:annotation>					<xs:documentation xml:lang="de">Zusammenfassung von zwei oder mehreren der übrigen Prämientypen</xs:documentation>					<xs:documentation xml:lang="en">Combination of two or more of the other premium types</xs:documentation>					<xs:documentation xml:lang="fr">Le groupement de deux ou de plusieurs autres types de primes</xs:documentation>					<xs:documentation xml:lang="it">Riepilogo di due o più tipi di premio restanti</xs:documentation>				</xs:annotation>			</xs:enumeration>		</xs:restriction>	</xs:simpleType>	<xs:simpleType name="scaleBonusLocType">		<xs:annotation>			<xs:documentation xml:lang="de">Datentyp für Bonussystem</xs:documentation>			<xs:documentation xml:lang="en">Data type for bonus system</xs:documentation>			<xs:documentation xml:lang="fr">Type de données pour le système de bonus</xs:documentation>			<xs:documentation xml:lang="it">Tipo di dati per sistema di bonus</xs:documentation>		</xs:annotation>		<xs:restriction base="textLine">			<xs:maxLength value="20"/>		</xs:restriction>	</xs:simpleType>	<xs:simpleType name="degreePremiumLocType">		<xs:annotation>			<xs:documentation xml:lang="de">Datentyp für Prämienstufe</xs:documentation>			<xs:documentation xml:lang="en">Data type for bonus system</xs:documentation>			<xs:documentation xml:lang="fr">Type de données pour le degré de primes</xs:documentation>			<xs:documentation xml:lang="it">Tipo di dati per il grado di premi</xs:documentation>		</xs:annotation>		<xs:restriction base="xs:NMTOKEN">			<xs:maxLength value="3"/>		</xs:restriction>	</xs:simpleType>	<xs:simpleType name="generalAgreementLocType">		<xs:annotation>			<xs:documentation xml:lang="de">Datentyp für Rahmenvertrags-/Flottenvertragsnummer</xs:documentation>			<xs:documentation xml:lang="en">Data type for framework agreement/fleet agreement number</xs:documentation>			<xs:documentation xml:lang="fr">Type de données pour numéro de contrat-cadre / de contrat de flotte</xs:documentation>			<xs:documentation xml:lang="it">Tipo di dati per numero contratto quadro/contratto parchi veicoli</xs:documentation>		</xs:annotation>		<xs:restriction base="textLine">			<xs:maxLength value="20"/>		</xs:restriction>	</xs:simpleType>	<xs:complexType name="ICBusinessLocType">		<xs:annotation>			<xs:documentation xml:lang="de">Datentyp für einen Firmenkunden</xs:documentation>			<xs:documentation xml:lang="en">Data type for a company client</xs:documentation>			<xs:documentation xml:lang="fr">Type de données pour un client entreprise</xs:documentation>			<xs:documentation xml:lang="it">Tipo di dati per un cliente commerciale</xs:documentation>		</xs:annotation>		<xs:sequence>			<xs:element name="ICBusinessName" type="companyNameType">				<xs:annotation>					<xs:documentation xml:lang="de">Firmenname</xs:documentation>					<xs:documentation xml:lang="de">Firmenname des Versicherungsnehmers. Bei einer einfachen Gesellschaft ist Vor- und Nachname einzutragen.</xs:documentation>					<xs:documentation xml:lang="en">Company name</xs:documentation>					<xs:documentation xml:lang="en">Company name of policy holder. For a simple company, name and surname must be entered.</xs:documentation>					<xs:documentation xml:lang="fr">Raison sociale</xs:documentation>					<xs:documentation xml:lang="fr">Raison sociale du preneur d'assurance. Pour une société simple, le nom et le prénom doivent être mentionnés.</xs:documentation>					<xs:documentation xml:lang="it">Nome della ditta</xs:documentation>					<xs:documentation xml:lang="it">Nome della ditta del contraente. Per una società semplice, è necessario inserire nome e cognome.</xs:documentation>				</xs:annotation>			</xs:element>			<xs:element name="ICBusinessLocality" type="addressType" minOccurs="0">				<xs:annotation>					<xs:documentation xml:lang="de">Hauptadresse des Versicherungsnehmers</xs:documentation>					<xs:documentation xml:lang="en">Mainaddress of policy holder</xs:documentation>					<xs:documentation xml:lang="fr">Adresse principale du preneur d'assurances</xs:documentation>					<xs:documentation xml:lang="it">Indirizzo principale di contraente</xs:documentation>				</xs:annotation>			</xs:element>			<xs:element name="ICBusinessUID" type="SwissCompanyIdType" minOccurs="0">				<xs:annotation>					<xs:documentation xml:lang="de">Unternehmensidentifikationsnummer (UID)</xs:documentation>					<xs:documentation xml:lang="de">UID gemäss Verzeichnis des Bundesamtes für Statistik BFS.</xs:documentation>					<xs:documentation xml:lang="en">Company identification Number (UID)</xs:documentation>					<xs:documentation xml:lang="en">According to the Federal Statistical Office (FSO) directory.</xs:documentation>					<xs:documentation xml:lang="fr">Numéro d’identification des entreprises (IDE)</xs:documentation>					<xs:documentation xml:lang="fr">IDE selon le répertoire de l’Office fédéral de la statistique OFS.</xs:documentation>					<xs:documentation xml:lang="it">Numero d’identificazione delle imprese (IDI)</xs:documentation>					<xs:documentation xml:lang="it">In base all’elenco dell’Ufficio federale di statistica UST.</xs:documentation>				</xs:annotation>			</xs:element>		</xs:sequence>	</xs:complexType>	<xs:complexType name="ICPrivatePersonLocType">		<xs:annotation>			<xs:documentation xml:lang="de">Datentyp für einen Privatkunden</xs:documentation>			<xs:documentation xml:lang="en">Data type for a private client</xs:documentation>			<xs:documentation xml:lang="fr">Type de données pour un client privé</xs:documentation>			<xs:documentation xml:lang="it">Tipo di dati per un cliente privato</xs:documentation>		</xs:annotation>		<xs:sequence>			<xs:element name="ICPrivatePersonFirstname" type="firstnameType">				<xs:annotation>					<xs:documentation xml:lang="de">Vorname des Versicherungsnehmers</xs:documentation>					<xs:documentation xml:lang="en">First name of policy holder</xs:documentation>					<xs:documentation xml:lang="fr">Prénom de preneur d'assurance</xs:documentation>					<xs:documentation xml:lang="it">Nome del contraente</xs:documentation>				</xs:annotation>			</xs:element>			<xs:element name="ICPrivatePersonSurname" type="surnameType">				<xs:annotation>					<xs:documentation xml:lang="de">Nachname des Versicherungsnehmers</xs:documentation>					<xs:documentation xml:lang="en">Surname of policy holder</xs:documentation>					<xs:documentation xml:lang="fr">Nom du preneur d'assurance</xs:documentation>					<xs:documentation xml:lang="it">Cognome del contraente</xs:documentation>				</xs:annotation>			</xs:element>			<xs:element name="ICPrivatePersonLocality" type="addressType" minOccurs="0">				<xs:annotation>					<xs:documentation xml:lang="de">Adresse des Versicherungsnehmers</xs:documentation>					<xs:documentation xml:lang="en">Address of policy holder</xs:documentation>					<xs:documentation xml:lang="fr">Adresse du preneur d'assurance</xs:documentation>					<xs:documentation xml:lang="it">Indirizzo del contraente</xs:documentation>				</xs:annotation>			</xs:element>		</xs:sequence>	</xs:complexType>	<xs:complexType name="invoiceTotalType">		<xs:annotation>			<xs:documentation xml:lang="de">Rechnungsdetails</xs:documentation>			<xs:documentation xml:lang="en">Detail of invoice</xs:documentation>			<xs:documentation xml:lang="fr">Détails de la facture</xs:documentation>			<xs:documentation xml:lang="it">Dettagli della fattura</xs:documentation>		</xs:annotation>		<xs:sequence>			<xs:element name="datePeriodPremFrom" type="xs:date">				<xs:annotation>					<xs:documentation xml:lang="de">Prämie von (Rechnungsperiode)</xs:documentation>					<xs:documentation xml:lang="de">JJJJ-MM-TT = Beginn der Periode, für welche die Prämie belastet oder gutgeschrieben wird.</xs:documentation>					<xs:documentation xml:lang="en">Premium from (billing period)</xs:documentation>					<xs:documentation xml:lang="en">YYYY-MM-DD = Beginning of the period for which the premium is debited or credited.</xs:documentation>					<xs:documentation xml:lang="fr">Prime du (période de facturation)</xs:documentation>					<xs:documentation xml:lang="fr">AAAA-MM-JJ = Début de la période, pour laquelle la prime est créditée ou débitée.</xs:documentation>					<xs:documentation xml:lang="it">Premio dal (periodo di fatturazione)</xs:documentation>					<xs:documentation xml:lang="it">YYYY-MM-DD = Inizio del periodo per quale il premio viene addebitato o accreditato.</xs:documentation>				</xs:annotation>			</xs:element>			<xs:element name="datePeriodPremTo" type="xs:date">				<xs:annotation>					<xs:documentation xml:lang="de">Prämie bis (Rechnungsperiode)</xs:documentation>					<xs:documentation xml:lang="de">JJJJ-MM-TT = Beginn der Periode, für welche die Prämie belastet oder gutgeschrieben wird.</xs:documentation>					<xs:documentation xml:lang="en">Premium to (billing period)</xs:documentation>					<xs:documentation xml:lang="en">YYYY-MM-DD = End o period for which the premium is debited or credited.</xs:documentation>					<xs:documentation xml:lang="fr">Prime jusqu'au (période de facturation)</xs:documentation>					<xs:documentation xml:lang="fr">AAAA-MM-JJ = Fin de la période, pour laquelle la prime est créditée ou débitée.</xs:documentation>					<xs:documentation xml:lang="it">Premio fino al (periodo di fatturazione)</xs:documentation>					<xs:documentation xml:lang="it">YYYY-MM-DD = Fine del periodo per quale il premio viene addebitato o accreditato.</xs:documentation>				</xs:annotation>			</xs:element>			<xs:element name="kindPremium" type="kindPremiumLocType">				<xs:annotation>					<xs:documentation xml:lang="de">Prämienart</xs:documentation>					<xs:documentation xml:lang="en">Premium form</xs:documentation>					<xs:documentation xml:lang="fr">Genre de prime</xs:documentation>					<xs:documentation xml:lang="it">Tipo di premio</xs:documentation>				</xs:annotation>			</xs:element>			<xs:element name="typePremium" type="typePremiumLocType">				<xs:annotation>					<xs:documentation xml:lang="de">Prämientyp</xs:documentation>					<xs:documentation xml:lang="de">Option für Versicherer: Die vorerwähnten Prämientypen können aufgrund der eigenen Codierung auch nach anderen Kriterien mit den folgenden Codes ausgewiesen bzw. zusammengefasst werden, sofern die Aufteilung nach Prämientyp 1 – 7 nicht möglich ist: 08 = Prämien gemäss Definition des einzelnen Versicherers / 09 = Zusammenfassung von zwei oder mehreren der übrigen Prämientypen gemäss Definition des einzelnen Versicherers.</xs:documentation>					<xs:documentation xml:lang="en">Premium type</xs:documentation>					<xs:documentation xml:lang="en">Option for insurer: The previously mentioned premium types can be entered or summarised based on their own coding or other criteria with the following codes if allocation according to premium types 1 – 7 is not possible: 08 = Premiums according to the definition of the individual insurer / 09 = Summary of two or more of the other premium types according to the definition of the individual insurer.</xs:documentation>					<xs:documentation xml:lang="fr">Type de prime</xs:documentation>					<xs:documentation xml:lang="fr">Option pour l'assureur: en raison de leur propre codage, les types de primes susmentionnés peuvent également être mentionnés resp. réunis selon d'autres critères avec les codes suivants, dès l'instant où la répartition selon le type de prime 1 – 7 n'est pas possible: 08 = Primes selon la définition de chaque assureur / 09 = Groupement de deux ou plusieurs des autres types de primes selon la définition de chaque assureur.</xs:documentation>					<xs:documentation xml:lang="it">Tipo premio</xs:documentation>					<xs:documentation xml:lang="it">Opzione di assicurazione: In base al tipo di codifica, i suddetti tipi di premio possono essere indicati o riuniti anche secondo altri criteri con i seguenti codici, a condizione che la suddivisione secondo la tipologia 1 – 7 non sia possibile: 08 = premi secondo la definizione del singolo assicuratore / 09 = unione di due o più degli altri tipi di premio secondo la definizione del singolo assicuratore.</xs:documentation>				</xs:annotation>			</xs:element>			<xs:element name="lobIG" type="LOBIGType">				<xs:annotation>					<xs:documentation xml:lang="de">Branche</xs:documentation>					<xs:documentation xml:lang="de">Branchencode gemäss IG-Verzeichnis. Das aktuelle Dokument – Verzeichnis der Branchencodes –  steht unter www.igb2b.ch im Mitgliederbereich zur Verfügung.</xs:documentation>					<xs:documentation xml:lang="en">Sector</xs:documentation>					<xs:documentation xml:lang="en">Sector code in accordance with IG directory. The current document – branch code directory – is available in the members area at www.igb2b.ch.</xs:documentation>					<xs:documentation xml:lang="fr">Branche</xs:documentation>					<xs:documentation xml:lang="fr">Codes des branches selon le répertoire IG. Le document actuel (répertoire des codes des branches) se trouve sur www.igb2b.ch, à la rubrique «Domaine membres».</xs:documentation>					<xs:documentation xml:lang="it">Settore</xs:documentation>					<xs:documentation xml:lang="it">Codice settoriale in base all’elenco IG. Il documento attuale – Elenco dei codici settoriali – è disponibile su www.igb2b.ch nella sezione "Ambito membri".</xs:documentation>				</xs:annotation>			</xs:element>			<xs:element name="lobINS" type="LOBINSType" minOccurs="0">				<xs:annotation>					<xs:documentation xml:lang="de">Branche / Risikobezeichnung</xs:documentation>					<xs:documentation xml:lang="de">Branchen- oder Risikocode gemäss Codeverzeichnis des einzelnen Versicherers. Der Detaillierungsgrad sollte den Angaben auf der Prämienrechnung entsprechen, bei kombinierten Produkten mindestens jedoch dem Branchencode. Option Versicherer: Anstatt eines Branchen- oder Risikocodes kann die Bezeichnung auch als Text erfolgen.</xs:documentation>					<xs:documentation xml:lang="en">Sector / Risk description</xs:documentation>					<xs:documentation xml:lang="en">Sector or risk code according to the individual insurer's code directory. The degree of detail must correspond with the details on the premium invoice, combined products at least the sector code. Option Insurer: Instead of a sector or risk code the description can be also as text.</xs:documentation>					<xs:documentation xml:lang="fr">Branche / Description du risque</xs:documentation>					<xs:documentation xml:lang="fr">Codes des branches ou de risque selon le répertoire des codes de chaque assureur. Le degré de détail doit correspondre avec les données sur la facture des primes, mais au minimum aux codes des branches pour les produits combinés. Option assureur: au lieu d'un code de branches ou de risque la description peut également être faite en texte.</xs:documentation>					<xs:documentation xml:lang="it">Settore / Denominazione rischio</xs:documentation>					<xs:documentation xml:lang="it">Codice settoriale o di rischio in base all'elenco dei codici dell'assicuratore individuale. Il grado de dettaglio deve corrispondere ai dettagli della fattura premio, ma almeno al codice settoriale per i prodotti combinati. Opzione di assicurazione: Invece di un codice settoriale o di rischio, la denominazione potrebbe anche essere testuale.</xs:documentation>				</xs:annotation>			</xs:element>			<xs:element name="currency" type="currencyCodeType" minOccurs="0">				<xs:annotation>					<xs:documentation xml:lang="de">Währung</xs:documentation>					<xs:documentation xml:lang="de">Code gemäss ISO-Code. Ohne Angabe der Währung gilt immer die Referenzwährung, d.h. zur Zeit der Schweizer Franken (CHF).</xs:documentation>					<xs:documentation xml:lang="en">Currency</xs:documentation>					<xs:documentation xml:lang="en">Code according to ISO code. Without specifying the currency, the reference currency always applies, at the time Swiss francs (CHF).</xs:documentation>					<xs:documentation xml:lang="fr">Monnaie</xs:documentation>					<xs:documentation xml:lang="fr">Code selon le code ISO. Sans indication de la monnaie, c'est toujours la monnaie de référence qui s'applique, c'est-à-dire actuellement le franc suisse (CHF).</xs:documentation>					<xs:documentation xml:lang="it">Moneta</xs:documentation>					<xs:documentation xml:lang="it">Codice secondo il codice ISO. Senza indicazione della moneta, si applica sempre la moneta riferimento, al momento il franco svizzero (CHF).</xs:documentation>				</xs:annotation>			</xs:element>			<xs:element name="annPremTariff" type="decimal10dot2Type" minOccurs="0">				<xs:annotation>					<xs:documentation xml:lang="de">Tarifprämie / Grundprämie / Bruttoprämie</xs:documentation>					<xs:documentation xml:lang="de">Jahresprämie ohne Stempelsteuer gemäss Tarif des Versicherers.</xs:documentation>					<xs:documentation xml:lang="en">Tariff premium / Basic premium / Gross premium</xs:documentation>					<xs:documentation xml:lang="en">Annual premium without stamp duty according to insurer's tariff</xs:documentation>					<xs:documentation xml:lang="fr">Prime tarifaire / prime de base / prime brute</xs:documentation>					<xs:documentation xml:lang="fr">Prime annuelle sans droit de timbre selon tarif de l'assureur.</xs:documentation>					<xs:documentation xml:lang="it">Premio tariffario / premio di base / premio lordo</xs:documentation>					<xs:documentation xml:lang="it">Premio annuale senza imposta di bollo secondo la tariffa dell'assicuratore.</xs:documentation>				</xs:annotation>			</xs:element>			<xs:element name="amountInvoice" type="decimal10dot2Type">				<xs:annotation>					<xs:documentation xml:lang="de">Fakturierte Prämie</xs:documentation>					<xs:documentation xml:lang="de">Dem Versicherungsnehmer die in Rechnung gestellte Prämie inkl. Stempel. Der Betrag ist im Normalfall positiv.</xs:documentation>					<xs:documentation xml:lang="en">Invoiced premium</xs:documentation>					<xs:documentation xml:lang="en">The policyholder the billed premium including stamp. The amount is normaly positive.</xs:documentation>					<xs:documentation xml:lang="fr">Prime facturée</xs:documentation>					<xs:documentation xml:lang="fr">La prime facturée au preneur d'assurance y compris le timbre. Le montant est généralement positif.</xs:documentation>					<xs:documentation xml:lang="it">Premio fatturato</xs:documentation>					<xs:documentation xml:lang="it">Il contraente il premio fatturato incluso timbro. L'importo è generalmente positivo.</xs:documentation>				</xs:annotation>			</xs:element>			<xs:element name="scaleBonus" type="scaleBonusLocType" minOccurs="0">				<xs:annotation>					<xs:documentation xml:lang="de">Bonussystem</xs:documentation>					<xs:documentation xml:lang="de">Bonussystem gemäss Tarif des einzelnen Versicherers.</xs:documentation>					<xs:documentation xml:lang="en">Bonus system</xs:documentation>					<xs:documentation xml:lang="en">Bonus system according to the individual insurer's tariff.</xs:documentation>					<xs:documentation xml:lang="fr">Système de bonus</xs:documentation>					<xs:documentation xml:lang="fr">Système de bonus selon le tarif des différents assureurs.</xs:documentation>					<xs:documentation xml:lang="it">Sistema di bonus</xs:documentation>					<xs:documentation xml:lang="it">Sistema di bonus in base alla tariffa dell'assicuratore individuale.</xs:documentation>				</xs:annotation>			</xs:element>			<xs:element name="degreePremium" type="degreePremiumLocType" minOccurs="0">				<xs:annotation>					<xs:documentation xml:lang="de">Prämienstufe</xs:documentation>					<xs:documentation xml:lang="de">Prämienstufe, die gemäss Schadenverlauf zur Anwendung kommt.</xs:documentation>					<xs:documentation xml:lang="en">Premium level</xs:documentation>					<xs:documentation xml:lang="en">Premium level that is applied according to the claims loss procedure.</xs:documentation>					<xs:documentation xml:lang="fr">Degré de prime</xs:documentation>					<xs:documentation xml:lang="fr">Degré de prime qui est appliqué en fonction de l'évolution des sinistres.</xs:documentation>					<xs:documentation xml:lang="it">Grado di premio</xs:documentation>					<xs:documentation xml:lang="it">Grado di premio, che viene utilizzato in base all'andamento dei sinistri.</xs:documentation>				</xs:annotation>			</xs:element>			<xs:element name="premiumComm" type="decimal10dot2Type" minOccurs="0">				<xs:annotation>					<xs:documentation xml:lang="de">Provisionsberechnungsbasis</xs:documentation>					<xs:documentation xml:lang="de">Provisionsberechtigte Prämie oder Produktion im Einzellebensgeschäft gemäss Definition des Versicherers. Der Betrag ist im Normalfall positiv.</xs:documentation>					<xs:documentation xml:lang="en">Basis on which premium calculated</xs:documentation>					<xs:documentation xml:lang="en">Entitled premium or production in individual life according to insurer's definition. The amount is normally positive.</xs:documentation>					<xs:documentation xml:lang="fr">Base de calcul de la commission</xs:documentation>					<xs:documentation xml:lang="fr">Prime commissionnée ou production en affaires vie individuelle suivant la définition de l'assureur. Le montant est généralement positif.</xs:documentation>					<xs:documentation xml:lang="it">Base per il calcolo delle provvigioni</xs:documentation>					<xs:documentation xml:lang="it">Provvigione intitolata o produzione in vita individuale in base alla definizione dell'assicuratore. L'importo è generalmente positivo.</xs:documentation>				</xs:annotation>			</xs:element>			<xs:element name="generalAgreement" type="generalAgreementLocType" minOccurs="0">				<xs:annotation>					<xs:documentation xml:lang="de">Rahmenvertrags-/Flottenvertragsnummer</xs:documentation>					<xs:documentation xml:lang="de">Bei Rahmenverträgen oder Flottenverträgen ist hier die Angabe der Rahmenvertrags-Nummer erwünscht, sofern unter Vertragsnummer die Nummer der einzelnen unter den Rahmen- oder Flottenvertrag fallenden Versicherungen angegeben wird (die Rahmenvertrags-Nr. erleichtert dem Broker die richtige Zuordnung der Prämien).</xs:documentation>					<xs:documentation xml:lang="en">Framework agreement/fleet agreement number</xs:documentation>					<xs:documentation xml:lang="en">In the case of framework agreements or fleet agreements, the framework agreement number is required if, under the agreement number, the number of the individual insurances under the framework or fleet agreement is provided (the framework agreement number makes correct assignment of the premiums easier for the broker)</xs:documentation>					<xs:documentation xml:lang="fr">Numéro de contrat-cadre / de contrat de flotte</xs:documentation>					<xs:documentation xml:lang="fr">En cas de contrats-cadre ou de contrats de flotte, l’indication du numéro du contrat-cadre est recommandée dans la mesure où le numéro des différentes assurances relevant du contrat-cadre ou du contrat de flotte est indiqué sous le numéro de contrat (le numéro du contrat-cadre simplifie l’attribution des primes pour le broker).</xs:documentation>					<xs:documentation xml:lang="it">Numero contratto quadro/contratto parchi veicoli</xs:documentation>					<xs:documentation xml:lang="it">In caso di contratti quadro o parchi veicoli è richiesto il numero del contratto quadro se alla voce numero di contratto viene riportato il numero delle singole assicurazioni che rientrano nel contratto quadro o parchi veicoli (il n. di contratto quadro aiuta il broker nella giusta attribuzione i premi).</xs:documentation>				</xs:annotation>			</xs:element>		</xs:sequence>	</xs:complexType></xs:schema>