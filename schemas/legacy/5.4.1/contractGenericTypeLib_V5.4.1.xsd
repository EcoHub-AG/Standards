<?xml version="1.0" encoding="UTF-8"?><!--XSD Standard for IG B2B Data exchangeCopyright © IG B2B for Insurers + Brokers, Switzerland--><!--Changes in Version V5.4rc1 from V5.2: - Change version values and name of referenced data schema from 5.2 to 5.4rc1- Insert simpleType name="frameworkContractNoLocType"- Insert new values=05,06,07 in simpleType name="mutationReasonLocType"- Insert new simpleType name="originalDocumentLocType"- Delete element name="ICBusinessLocType"- Delete element name="ICPrivatePersonLocType"- Delete french and italian textsChanges in Version V5.4.1 from V5.4rc1: - Change version values and name of referenced data schema from 5.4rc1 to 5.4.1--><xs:schema xmlns:xs="http://www.w3.org/2001/XMLSchema" xmlns="http://www.IGB2B.ch/XMLSchema" targetNamespace="http://www.IGB2B.ch/XMLSchema" elementFormDefault="qualified" version="5.4.1">	<xs:include schemaLocation="globals_V5.4.1.xsd"/>	<xs:simpleType name="frameworkContractNoLocType">		<xs:annotation>			<xs:documentation xml:lang="de">Datentyp für Rahmenvertrags-/Flottenvertragsnummer</xs:documentation>			<xs:documentation xml:lang="en">Data type for framework contract/fleet contract number</xs:documentation>		</xs:annotation>		<xs:restriction base="textLine">			<xs:maxLength value="20"/>		</xs:restriction>	</xs:simpleType>	<xs:simpleType name="brokerCodeInsLocType">		<xs:annotation>			<xs:documentation xml:lang="de">Datentyp für Brokercode Versicherer</xs:documentation>			<xs:documentation xml:lang="en">Data type for insurer's broker code</xs:documentation>		</xs:annotation>		<xs:restriction base="xs:NMTOKEN">			<xs:maxLength value="10"/>		</xs:restriction>	</xs:simpleType>	<xs:simpleType name="agencyLocType">		<xs:annotation>			<xs:documentation xml:lang="de">Datentyp für Agentur/Verkaufsstelle/Provisionsbezüger</xs:documentation>			<xs:documentation xml:lang="en">Data type for Agency/sales point/commission agent</xs:documentation>		</xs:annotation>		<xs:restriction base="textLine">			<xs:maxLength value="20"/>		</xs:restriction>	</xs:simpleType>	<xs:simpleType name="mutationReasonLocType">		<xs:annotation>			<xs:documentation xml:lang="de">Datentyp für den Mutationsgrund</xs:documentation>			<xs:documentation xml:lang="en">Data type for amendment reason</xs:documentation>		</xs:annotation>		<xs:restriction base="xs:NMTOKEN">			<xs:maxLength value="2"/>			<xs:enumeration value="01">				<xs:annotation>					<xs:documentation xml:lang="de">Neuabschluss</xs:documentation>					<xs:documentation xml:lang="en">New business</xs:documentation>				</xs:annotation>			</xs:enumeration>			<xs:enumeration value="02">				<xs:annotation>					<xs:documentation xml:lang="de">Vertragsänderung</xs:documentation>					<xs:documentation xml:lang="en">Contract modification</xs:documentation>				</xs:annotation>			</xs:enumeration>			<xs:enumeration value="03">				<xs:annotation>					<xs:documentation xml:lang="de">Ersatzgeschäft mit anderer Vertragsnummer</xs:documentation>					<xs:documentation xml:lang="en">Substitute transaction with another contract number</xs:documentation>				</xs:annotation>			</xs:enumeration>			<xs:enumeration value="04">				<xs:annotation>					<xs:documentation xml:lang="de">Vertragsaufhebung</xs:documentation>					<xs:documentation xml:lang="en">Contract cancellation</xs:documentation>				</xs:annotation>			</xs:enumeration>			<xs:enumeration value="05">				<xs:annotation>					<xs:documentation xml:lang="de">Mandatswechsel</xs:documentation>					<xs:documentation xml:lang="en">Change of mandate</xs:documentation>				</xs:annotation>			</xs:enumeration>			<xs:enumeration value="06">				<xs:annotation>					<xs:documentation xml:lang="de">Sistierung bzw. Stilllegen eines Vertrages</xs:documentation>					<xs:documentation xml:lang="en">Suspension of a contract</xs:documentation>				</xs:annotation>			</xs:enumeration>			<xs:enumeration value="07">				<xs:annotation>					<xs:documentation xml:lang="de">Reaktivierung bzw. Aktivierung nach einer Stilllegung</xs:documentation>					<xs:documentation xml:lang="en">Reactivation or activation after suspension</xs:documentation>				</xs:annotation>			</xs:enumeration>		</xs:restriction>	</xs:simpleType>	<xs:simpleType name="originalDocumentLocType">		<xs:annotation>			<xs:documentation xml:lang="de">Datentyp für Originaldokument</xs:documentation>			<xs:documentation xml:lang="en">Data type for original document</xs:documentation>		</xs:annotation>		<xs:restriction base="xs:NMTOKEN">			<xs:maxLength value="2"/>			<xs:enumeration value="01">				<xs:annotation>					<xs:documentation xml:lang="de">Ja</xs:documentation>					<xs:documentation xml:lang="en">Yes</xs:documentation>				</xs:annotation>			</xs:enumeration>			<xs:enumeration value="02">				<xs:annotation>					<xs:documentation xml:lang="de">Nein</xs:documentation>					<xs:documentation xml:lang="en">No</xs:documentation>				</xs:annotation>			</xs:enumeration>		</xs:restriction>	</xs:simpleType></xs:schema>