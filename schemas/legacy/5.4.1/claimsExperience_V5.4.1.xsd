<?xml version="1.0" encoding="UTF-8"?><!--XSD Standard for IG B2B Data exchangeCopyright © IG B2B for Insurers + Brokers, Switzerland--><!--Changes in Version V5.4rc1 from V5.2: - Change version values and names of referenced data schemas from 5.2 to 5.4rc1- Change description of element name="claimsExperience"- Delete element name="contentRec"- Change value description of element name="typeRec" and change value from 004 to 002- Change value description of element name="subTypeRec" and change value from 000 to 002- Change type of element name="ICBusiness" to "ICBusinessType"- Change type of element name="ICPrivatePerson" to "ICPrivatePersonType"- Change description of element name="contractNo"- Change name of element name="generalAgreement" to "frameworkContractNo" and its description- Change description of element name="lobIG"- Change description of element name="lobINS"- Change name of element name="periodicity" to "periodicityYear"and its description- Change name of element name="insuranceYearFrom" to "insuranceYearFromDate"- Change name of element name="insuranceYearto" to "insuranceYearToDate"- Change name of element name="periodFrom" to "claimsExperiencePeriodFromDate" and its description- Change name of element name="periodTo" to "claimsExperiencePeriodToDate" and its description- Change name of element name="updates" to "updateInformation" and its description- Delete element name="codePremium"- Delete element name="codeClaimsPayment"- Change name of element name="experienceDetails" to "claimsExperienceDetails" and add descripton- Change name of element name="experienceDataUpdate" to "experienceDataUpdateDate" and its description- Change name of element name="claimsReservesUpdate" to "claimsReservesUpdateDate" and its description- Change name of element name="currentYear" to "currentYearClaimsExperience" and its descrioption- Change name of element name="pastYear" to "pastYearClaimsExperience" and its description- Change description of element name="totalsPerLOB"- Change description of element name="totalsAllLOBs"- Change description of elements name="premium", name="payments",name="claimsReserves", name="claimsExpenditure", name="claimsRatio", name="claimsReported", name="claimsPending", name="claimsSettled", name="claimsNoCons"- Insert new element name="currency"- Delete element name="remarks"- Delete french and italian textsChanges in Version V5.4.1 from V5.4rc1: - Change version values and names of referenced data schemas from 5.4rc1 to 5.4.1--><xs:schema xmlns:xs="http://www.w3.org/2001/XMLSchema" xmlns="http://www.IGB2B.ch/XMLSchema" targetNamespace="http://www.IGB2B.ch/XMLSchema" elementFormDefault="qualified" version="5.4.1">	<xs:include schemaLocation="globals_V5.4.1.xsd"/>	<xs:include schemaLocation="claimsExperienceTypeLib_V5.4.1.xsd"/>	<xs:element name="claimsExperience">		<xs:annotation>			<xs:documentation xml:lang="de">Standard-Schema für Schadenrendement</xs:documentation>			<xs:documentation xml:lang="de">Dieser Standard definiert die Struktur eines Schadenrendements.</xs:documentation>			<xs:documentation xml:lang="en">Standard schema for claims experience</xs:documentation>			<xs:documentation xml:lang="en">This standard defines the structure of a claims experience</xs:documentation>		</xs:annotation>		<xs:complexType>			<xs:sequence>				<xs:element name="header">					<xs:annotation>						<xs:documentation xml:lang="de">Header</xs:documentation>						<xs:documentation xml:lang="en">Header</xs:documentation>					</xs:annotation>					<xs:complexType>						<xs:complexContent>							<xs:extension base="headerType">								<xs:sequence>									<xs:element name="typeRec">										<xs:annotation>											<xs:documentation xml:lang="de">Recordart</xs:documentation>											<xs:documentation xml:lang="en">Record type</xs:documentation>										</xs:annotation>										<xs:simpleType>											<xs:restriction base="typeRecType">												<xs:enumeration value="002">													<xs:annotation>														<xs:documentation xml:lang="de">Schadenrendement</xs:documentation>														<xs:documentation xml:lang="en">Claims experience</xs:documentation>													</xs:annotation>												</xs:enumeration>											</xs:restriction>										</xs:simpleType>									</xs:element>									<xs:element name="subTypeRec">										<xs:annotation>											<xs:documentation xml:lang="de">Subrecordart</xs:documentation>											<xs:documentation xml:lang="en">Sub record type</xs:documentation>										</xs:annotation>										<xs:simpleType>											<xs:restriction base="subTypeRecType">												<xs:enumeration value="002">													<xs:annotation>														<xs:documentation xml:lang="de">Schadenrendement</xs:documentation>														<xs:documentation xml:lang="en">Claims experience</xs:documentation>													</xs:annotation>												</xs:enumeration>											</xs:restriction>										</xs:simpleType>									</xs:element>									<xs:element name="versionRec">										<xs:annotation>											<xs:documentation xml:lang="de">Recordversion</xs:documentation>											<xs:documentation xml:lang="en">Record version</xs:documentation>										</xs:annotation>										<xs:simpleType>											<xs:restriction base="versionRecType">												<xs:enumeration value="5.4.1"/>											</xs:restriction>										</xs:simpleType>									</xs:element>								</xs:sequence>							</xs:extension>						</xs:complexContent>					</xs:complexType>				</xs:element>				<xs:choice>					<xs:annotation>						<xs:documentation xml:lang="de">Wahl zwischen einem Geschäfts- und Privatkunden</xs:documentation>						<xs:documentation xml:lang="en">Choice between business and private customer</xs:documentation>					</xs:annotation>					<xs:element name="ICBusiness" type="ICBusinessType">						<xs:annotation>							<xs:documentation xml:lang="de">Geschäftskunde</xs:documentation>							<xs:documentation xml:lang="en">Business customer</xs:documentation>						</xs:annotation>					</xs:element>					<xs:element name="ICPrivatePerson" type="ICPrivatePersonType">						<xs:annotation>							<xs:documentation xml:lang="de">Privatkunde</xs:documentation>							<xs:documentation xml:lang="en">Private customer</xs:documentation>						</xs:annotation>					</xs:element>				</xs:choice>				<xs:element name="contractNo" type="contractNoType">					<xs:annotation>						<xs:documentation xml:lang="de">Vertragsnummer</xs:documentation>						<xs:documentation xml:lang="de">Entspricht der eindeutigen Nummer des entsprechenden Vertrages. Vertragsnummernaufbau des einzelnen Versicherers/Unternehmens siehe Spezifikation IG B2B.</xs:documentation>						<xs:documentation xml:lang="en">Contract number</xs:documentation>						<xs:documentation xml:lang="en">This is the unique number of the corresponding contract. For the contract number structure of the individual insurer/company, see IG B2B specification.</xs:documentation>					</xs:annotation>				</xs:element>				<xs:element name="frameworkContractNo" type="frameworkContractNoLocType" minOccurs="0">					<xs:annotation>						<xs:documentation xml:lang="de">Rahmenvertrags-/Flottenvertragsnummer</xs:documentation>						<xs:documentation xml:lang="de">Die Rahmenvertrags-/Flottenvertragsnummer ist im Falle eines vorhandenen, zugehörigen Rahmen- oder Flottenvertrags hier einzufügen.</xs:documentation>						<xs:documentation xml:lang="en">Framework contract/fleet contract number</xs:documentation>						<xs:documentation xml:lang="en">The framework contract/fleet contract number is to be inserted here in the case of an existing, associated framework or fleet contract.</xs:documentation>					</xs:annotation>				</xs:element>				<xs:element name="brokerCodeIns" type="brokerCodeInsLocType" minOccurs="0">					<xs:annotation>						<xs:documentation xml:lang="de">Brokercode Versicherer</xs:documentation>						<xs:documentation xml:lang="de">Broker-Nummer gemäss dem Codeverzeichnis des einzelnen Versicherers.</xs:documentation>						<xs:documentation xml:lang="en">Insurer's broker code</xs:documentation>						<xs:documentation xml:lang="en">Broker number according to the individual insurer's code directory.</xs:documentation>					</xs:annotation>				</xs:element>				<xs:element name="agency" type="agencyLocType" minOccurs="0">					<xs:annotation>						<xs:documentation xml:lang="de">Agentur/Verkaufsstelle/Provisionsbezüger</xs:documentation>						<xs:documentation xml:lang="de">Agentur-, Verkaufsstellen- oder Provisionsbezüger-Nummer gemäss Codeverzeichnis des einzelnen Versicherers</xs:documentation>						<xs:documentation xml:lang="en">Agency/sales point/commission agent</xs:documentation>						<xs:documentation xml:lang="en">Agency, sales point or commission agent number according to the individual insurer's code directory.</xs:documentation>					</xs:annotation>				</xs:element>				<xs:element name="periodicityYear" type="periodicityYearLocType">					<xs:annotation>						<xs:documentation xml:lang="de">Periodizität Jahr</xs:documentation>						<xs:documentation xml:lang="en">Periodicity year</xs:documentation>					</xs:annotation>				</xs:element>				<xs:choice>					<xs:annotation>						<xs:documentation xml:lang="de">Wahl zwischen dem Fall, dass das Versicherungsjahr gleich dem Schadenjahr ist oder Angabe einer individuellen Definition.</xs:documentation>						<xs:documentation xml:lang="en">Choice between a case where the insurance year is the same as the year of loss or provision of an individual definition.</xs:documentation>					</xs:annotation>					<xs:element name="insuranceYearDefinition">						<xs:annotation>							<xs:documentation xml:lang="de">Element für die Definition eines individuellen Versicherungsjahres.</xs:documentation>							<xs:documentation xml:lang="en">Element for the definition of an individual insurance year.</xs:documentation>						</xs:annotation>						<xs:complexType>							<xs:sequence>								<xs:element name="insuranceYearFromDate" type="xs:date">									<xs:annotation>										<xs:documentation xml:lang="de">Beginn des Versicherungsjahres</xs:documentation>										<xs:documentation xml:lang="de">JJJJ-MM-TT =  Beginn des Versicherungsjahres</xs:documentation>										<xs:documentation xml:lang="en">Start of  insurance year</xs:documentation>										<xs:documentation xml:lang="en">YYYY-MM-DD = Start of insurance year</xs:documentation>									</xs:annotation>								</xs:element>								<xs:element name="insuranceYearToDate" type="xs:date">									<xs:annotation>										<xs:documentation xml:lang="de">Ende des Versicherungsjahres</xs:documentation>										<xs:documentation xml:lang="de">JJJJ-MM-TT =  Ende des Versicherungsjahres</xs:documentation>										<xs:documentation xml:lang="en">End of  insurance year</xs:documentation>										<xs:documentation xml:lang="en">YYYY-MM-DD = End of insurance year</xs:documentation>									</xs:annotation>								</xs:element>							</xs:sequence>						</xs:complexType>					</xs:element>					<xs:element name="insuranceYearEqualsClaimsYear" type="setValueTrueType">						<xs:annotation>							<xs:documentation xml:lang="de">Explizite Angabe, dass das Versicherungsjahr und das Schadenjahr gleich sind.</xs:documentation>							<xs:documentation xml:lang="en">Explicit indication that the insurance year and the year of loss are the same.</xs:documentation>						</xs:annotation>					</xs:element>				</xs:choice>				<xs:element name="claimsExperiencePeriodFromDate" type="xs:date">					<xs:annotation>						<xs:documentation xml:lang="de">Rendementperiode von Datum</xs:documentation>						<xs:documentation xml:lang="de">JJJJ-MM-TT =  Beginn der Periode für das erstellte Rendement</xs:documentation>						<xs:documentation xml:lang="en">Loss ratio period from</xs:documentation>						<xs:documentation xml:lang="en">YYYY-MM-DD = Start of  period for the established loss ratio</xs:documentation>					</xs:annotation>				</xs:element>				<xs:element name="claimsExperiencePeriodToDate" type="xs:date">					<xs:annotation>						<xs:documentation xml:lang="de">Rendementperiode bis Datum</xs:documentation>						<xs:documentation xml:lang="de">JJJJ-MM-TT =  Ende der Periode für das erstellte Rendement</xs:documentation>						<xs:documentation xml:lang="en">Loss ratio period to</xs:documentation>						<xs:documentation xml:lang="en">YYYY-MM-DD = End of the period for the established loss ratio</xs:documentation>					</xs:annotation>				</xs:element>				<xs:element name="updateInformation">					<xs:annotation>						<xs:documentation xml:lang="de">Informationen zur Aktualisierung</xs:documentation>						<xs:documentation xml:lang="en">Update information</xs:documentation>					</xs:annotation>					<xs:complexType>						<xs:sequence>							<xs:element name="experienceDataUpdateDate" type="xs:date">								<xs:annotation>									<xs:documentation xml:lang="de">Aktualisierungsdatum der Rendementdaten</xs:documentation>									<xs:documentation xml:lang="de">JJJJ-MM-TT =  Datum der letzten Aktualisierung der Rendementdaten, jedoch ohne Schadenreserven.</xs:documentation>									<xs:documentation xml:lang="en">Date loss ratio details last updated</xs:documentation>									<xs:documentation xml:lang="en">YYYY-MM-DD = Date of the last update oft he loss ratio details bus without claims reserves</xs:documentation>								</xs:annotation>							</xs:element>							<xs:element name="claimsReservesUpdateDate" type="xs:date">								<xs:annotation>									<xs:documentation xml:lang="de">Aktualisierungsdatum der Schadenreserven</xs:documentation>									<xs:documentation xml:lang="de">JJJJ-MM-TT =  Datum der letzten Aktualisierung der Schadenreservedaten, sofern diese im Rendement angegeben werden.</xs:documentation>									<xs:documentation xml:lang="en">Date claims reserves last updated</xs:documentation>									<xs:documentation xml:lang="en">YYYY-MM-DD = Date of the last update of the claims reserves data, if these are specified in the loss ratio.</xs:documentation>								</xs:annotation>							</xs:element>						</xs:sequence>					</xs:complexType>				</xs:element>				<xs:element name="contractStatus" minOccurs="0">					<xs:annotation>						<xs:documentation xml:lang="de">Vertragsstatus</xs:documentation>						<xs:documentation xml:lang="en">Agreement status</xs:documentation>					</xs:annotation>					<xs:simpleType>						<xs:restriction base="xs:NMTOKEN">							<xs:maxLength value="2"/>							<xs:enumeration value="01">								<xs:annotation>									<xs:documentation xml:lang="de">laufender Vertrag</xs:documentation>									<xs:documentation xml:lang="en">current agreement</xs:documentation>								</xs:annotation>							</xs:enumeration>							<xs:enumeration value="02">								<xs:annotation>									<xs:documentation xml:lang="de">sistierter Vertrag</xs:documentation>									<xs:documentation xml:lang="en">suspended agreement</xs:documentation>								</xs:annotation>							</xs:enumeration>							<xs:enumeration value="03">								<xs:annotation>									<xs:documentation xml:lang="de">ersetzter Vertrag</xs:documentation>									<xs:documentation xml:lang="en">superseded agreement</xs:documentation>								</xs:annotation>							</xs:enumeration>							<xs:enumeration value="04">								<xs:annotation>									<xs:documentation xml:lang="de">aufgehobener Vertrag</xs:documentation>									<xs:documentation xml:lang="en">cancelled agreement</xs:documentation>								</xs:annotation>							</xs:enumeration>						</xs:restriction>					</xs:simpleType>				</xs:element>				<xs:choice>					<xs:annotation>						<xs:documentation xml:lang="de">Mitbeteiligungscode</xs:documentation>						<xs:documentation xml:lang="en">Copayment code</xs:documentation>					</xs:annotation>					<xs:element name="codeCoInsurance100" type="setValueTrueType">						<xs:annotation>							<xs:documentation xml:lang="de">Mitbeteiligungscode 100% (Defaultwert)</xs:documentation>							<xs:documentation xml:lang="en">Copayment code 100% (default value)</xs:documentation>						</xs:annotation>					</xs:element>					<xs:element name="codeCoInsurancePartOnly">						<xs:annotation>							<xs:documentation xml:lang="de">Mitbeteiligungscode "nur Anteil des Versicherers"</xs:documentation>							<xs:documentation xml:lang="en">Copayment code "insurer's share only"</xs:documentation>						</xs:annotation>						<xs:complexType>							<xs:sequence>								<xs:element name="ratioCoInsurance" type="decimal3dot2Type">									<xs:annotation>										<xs:documentation xml:lang="de">Höhe des aufgeführten Anteiles</xs:documentation>										<xs:documentation xml:lang="de">Anzeige des Anteiles in %.</xs:documentation>										<xs:documentation xml:lang="en">Amount of listed share</xs:documentation>										<xs:documentation xml:lang="en">Display of the share as %.</xs:documentation>									</xs:annotation>								</xs:element>							</xs:sequence>						</xs:complexType>					</xs:element>					<xs:element name="codeCoInsuranceOtherRiskTakers" type="setValueTrueType">						<xs:annotation>							<xs:documentation xml:lang="de">Mitbeteiligungscode "weitere Risikoträger (z.B. für Rentenfälle)"</xs:documentation>							<xs:documentation xml:lang="en">Copayment code "other risk carriers (e.g. in annuity cases)"</xs:documentation>						</xs:annotation>					</xs:element>				</xs:choice>				<xs:element name="codeReserves" type="codeReservesLocType">					<xs:annotation>						<xs:documentation xml:lang="de">Code betreffend Schadenreserven</xs:documentation>						<xs:documentation xml:lang="en">Claims reserve code</xs:documentation>					</xs:annotation>				</xs:element>				<xs:element name="codeRatio" type="codeRatioLocType">					<xs:annotation>						<xs:documentation xml:lang="de">Code betreffend Schadenaufwand/-quote</xs:documentation>						<xs:documentation xml:lang="en">Claims expenditure/loss ratio code</xs:documentation>					</xs:annotation>				</xs:element>				<xs:element name="codeAssignment" type="codeAssignmentLocType">					<xs:annotation>						<xs:documentation xml:lang="de">Code betreffend Schadenzuordnung (Personenversicherungen)</xs:documentation>						<xs:documentation xml:lang="en">Claims allocation code (personal insurance)</xs:documentation>					</xs:annotation>				</xs:element>				<xs:element name="currency" type="currencyCodeType">					<xs:annotation>						<xs:documentation xml:lang="de">Währung</xs:documentation>						<xs:documentation xml:lang="de">Code gemäss ISO-Code. Ohne Angabe der Währung gilt immer die Referenzwährung, d.h. zur Zeit der Schweizer Franken (CHF).</xs:documentation>						<xs:documentation xml:lang="en">Currency</xs:documentation>						<xs:documentation xml:lang="en">Code according to ISO-Code. Without specifying the currency, the reference currency always applies, at the time Swiss Francs (CHF).</xs:documentation>					</xs:annotation>				</xs:element>				<xs:sequence>					<xs:element name="claimsExperienceDetails">						<xs:annotation>							<xs:documentation xml:lang="de">Details zum Schadenrendement</xs:documentation>							<xs:documentation xml:lang="en">Details of claims experience</xs:documentation>						</xs:annotation>						<xs:complexType>							<xs:sequence>								<xs:element name="experience" maxOccurs="unbounded">									<xs:complexType>										<xs:sequence>											<xs:element name="lobIG" type="LOBIGType">												<xs:annotation>													<xs:documentation xml:lang="de">Branchencode IG B2B</xs:documentation>													<xs:documentation xml:lang="de">Branchencode gemäss IG B2B</xs:documentation>													<xs:documentation xml:lang="en">IG B2B sector code</xs:documentation>													<xs:documentation xml:lang="en">Sector code according to IG B2B</xs:documentation>												</xs:annotation>											</xs:element>											<xs:element name="lobINS" type="LOBINSType" minOccurs="0">												<xs:annotation>													<xs:documentation xml:lang="de">Branchen-/Risikocode oder -bezeichnung des Versicherers/Vorsorgeunternehmens</xs:documentation>													<xs:documentation xml:lang="de">Branchen-/Risikocode oder -bezeichnung gemäss Verzeichnis des einzelnen Versicherers/Vorsorgeunternehmens. Der Detaillierungsgrad sollte so gewählt sein, dass für vor- und nachgelagerte Prozesse ein Bezug zu den jeweiligen Positionen (z.B. Branchencode in Prämienrechnung = Branchencode in Provisionsabrechnung) hergestellt werden kann. Bei kombinierten Produkten ist der übergeordnete Code/Bezeichnung zu wählen.</xs:documentation>													<xs:documentation xml:lang="en">Sector/risk code or designation of the insurer/pension company</xs:documentation>													<xs:documentation xml:lang="en">Sector/risk code or designation according to the directory of the individual insurer/pension company. The level of detail should be sufficient to enable upstream and downstream processes to establish a reference to the respective items (e.g. sector code in premium invoice = sector code in commission calculation). For combined products, the higher-level code/designation is to be selected.</xs:documentation>												</xs:annotation>											</xs:element>											<xs:element name="pastYearClaimsExperience" minOccurs="0" maxOccurs="unbounded">												<xs:annotation>													<xs:documentation xml:lang="de">Angaben zu den Versicherungsvorjahren. Das Rendement muss die Angaben von mindestens 6 vollen Versicherungsvorjahren enthalten. Branchen-/produktabhängig können auch mehr Jahre gefordert sein. Besteht das Vertragsverhältnis weniger als 6 volle Jahre, dann sind die Angaben der verfügbaren, vollen Versicherungsvorjahre aufzuführen. Pro Versicherungsvorjahr sind jeweils die dazugehörigen Angaben wie Prämien, Schadenzahlungen etc. aufzuführen.</xs:documentation>													<xs:documentation xml:lang="en">Details of previous insurance years. The claims experience must contain the details of at least six full insurance years. Depending on the sector/product, more years may be required. If the contractual relationship has existed for less than six full years, the details of the full insurance years available must be provided. The relevant details such as regarding premiums, claims payments, etc. must be provided for each previous insurance year.</xs:documentation>												</xs:annotation>												<xs:complexType>													<xs:sequence>														<xs:element name="premium" type="decimal10dot2Type">															<xs:annotation>																<xs:documentation xml:lang="de">Prämien</xs:documentation>																<xs:documentation xml:lang="de">Der Betrag entweder 0.00 oder positiv</xs:documentation>																<xs:documentation xml:lang="en">Premiums</xs:documentation>																<xs:documentation xml:lang="en">The amount is either 0.00 or positive</xs:documentation>															</xs:annotation>														</xs:element>														<xs:element name="payments" type="decimal10dot2Type">															<xs:annotation>																<xs:documentation xml:lang="de">Schadenzahlungen</xs:documentation>																<xs:documentation xml:lang="de">Der Betrag ist entweder 0.00 oder positiv</xs:documentation>																<xs:documentation xml:lang="en">Claims payment</xs:documentation>																<xs:documentation xml:lang="en">The amount is either 0.00 or positive</xs:documentation>															</xs:annotation>														</xs:element>														<xs:element name="claimsReserves" type="decimal10dot2Type">															<xs:annotation>																<xs:documentation xml:lang="de">Schadenreserven</xs:documentation>																<xs:documentation xml:lang="de">Der Betrag ist entweder 0.00 oder positiv</xs:documentation>																<xs:documentation xml:lang="en">Claims reserves</xs:documentation>																<xs:documentation xml:lang="en">The amount is either 0.00 or positive</xs:documentation>															</xs:annotation>														</xs:element>														<xs:element name="claimsExpenditure" type="decimal10dot2Type">															<xs:annotation>																<xs:documentation xml:lang="de">Schadenaufwand</xs:documentation>																<xs:documentation xml:lang="de">Der Betrag ist entweder 0.00 oder positiv</xs:documentation>																<xs:documentation xml:lang="en">Claims exepnses></xs:documentation>																<xs:documentation xml:lang="en">The amount is either 0.00 or positive</xs:documentation>															</xs:annotation>														</xs:element>														<xs:element name="claimsRatio" type="decimal3dot2Type">															<xs:annotation>																<xs:documentation xml:lang="de">Schadenbelastungsquote in Prozenten</xs:documentation>																<xs:documentation xml:lang="en">Claims ratio as %</xs:documentation>															</xs:annotation>														</xs:element>														<xs:element name="claimsReported" type="decimal3dot0Type">															<xs:annotation>																<xs:documentation xml:lang="de">Anzahl angemeldeter Schadenfälle</xs:documentation>																<xs:documentation xml:lang="en">Number of claims reported</xs:documentation>															</xs:annotation>														</xs:element>														<xs:element name="claimsPending" type="decimal3dot0Type">															<xs:annotation>																<xs:documentation xml:lang="de">Anzahl offener Schadenfälle</xs:documentation>																<xs:documentation xml:lang="en">Number of open claims</xs:documentation>															</xs:annotation>														</xs:element>														<xs:element name="