<?xml version="1.0" encoding="UTF-8"?><!--XSD Standard for IG B2B Data exchangeCopyright © IG B2B for Insurers + Brokers, Switzerland--><!--Changes in Version V5.4rc1 from V5.2: - Change version values and names of referenced data schemas from 5.2 to 5.4rc1- Change description of element name="mandateConfirmation" to name="mandateFeedback"- Change value description of element name="typeRec" and change value from 016 to 006- Change description of value 002 in element name="subTypeRecType"- Delete element name="contentRec"- Insert new element name="ICBusiness"- Insert new element name="ICPrivatePerson"- Change description of element name="mandateKind"- Change name of element name="mandateEntrance" to name="mandateReceiptDate" and its description- Replace element name="mandateFilingType" with element name="mandateFeedbackType"- Insert element name="mandateDesicion" - Insert element name="mandateConfirmation" and suborder elements- Replace element name="mandateValid" with element name="mandateValidFromDate"- Change name of element name="contractLOB" to "contract"- Replace element name="contractLOBIG" with element name="lobIG"- Replace element name="contractLOBINS" with element name="lobINS"- Replace attribute name="contractLOBNo" with element name="contractNo"- Insert element name="mandateRejection"- Insert element name="mandateRejectionReason"- Delete element name="remarks"- Delete french and italian textsChanges in Version V5.4.1 from V5.4rc1: - Change version values and names of referenced data schemas from 5.4rc1 to 5.4.1--><xs:schema xmlns:xs="http://www.w3.org/2001/XMLSchema" xmlns="http://www.IGB2B.ch/XMLSchema" targetNamespace="http://www.IGB2B.ch/XMLSchema" elementFormDefault="qualified" version="5.4.1">	<xs:include schemaLocation="globals_V5.4.1.xsd"/>	<xs:include schemaLocation="mandateTypeLib_V5.4.1.xsd"/>	<xs:element name="mandateFeedback">		<xs:annotation>			<xs:documentation xml:lang="de">Standard-Schema für die Rückmeldung zur Mandatseinreichung</xs:documentation>			<xs:documentation xml:lang="de">Dieser Standard definiert die Struktur der Rückmeldung zur Mandatseinreichung.</xs:documentation>			<xs:documentation xml:lang="en">Standard schema for feedback concerning the mandate submission</xs:documentation>			<xs:documentation xml:lang="en">This standard defines the structure of feedback concerning the mandate submission.</xs:documentation>		</xs:annotation>		<xs:complexType>			<xs:sequence>				<xs:element name="header">					<xs:annotation>						<xs:documentation xml:lang="de">Header</xs:documentation>						<xs:documentation xml:lang="en">Header</xs:documentation>					</xs:annotation>					<xs:complexType>						<xs:complexContent>							<xs:extension base="headerType">								<xs:sequence>									<xs:element name="typeRec">										<xs:annotation>											<xs:documentation xml:lang="de">Recordart</xs:documentation>											<xs:documentation xml:lang="en">Record type</xs:documentation>										</xs:annotation>										<xs:simpleType>											<xs:restriction base="typeRecType">												<xs:enumeration value="006">													<xs:annotation>														<xs:documentation xml:lang="de">Mandat</xs:documentation>														<xs:documentation xml:lang="en">Mandate</xs:documentation>													</xs:annotation>												</xs:enumeration>											</xs:restriction>										</xs:simpleType>									</xs:element>									<xs:element name="subTypeRec">										<xs:annotation>											<xs:documentation xml:lang="de">Subrecordart</xs:documentation>											<xs:documentation xml:lang="en">Sub record type</xs:documentation>										</xs:annotation>										<xs:simpleType>											<xs:restriction base="subTypeRecType">												<xs:enumeration value="002">													<xs:annotation>														<xs:documentation xml:lang="de">Rückmeldung zur Mandatseinreichung</xs:documentation>														<xs:documentation xml:lang="en">Feedback on the mandate submission</xs:documentation>													</xs:annotation>												</xs:enumeration>											</xs:restriction>										</xs:simpleType>									</xs:element>									<xs:element name="versionRec">										<xs:annotation>											<xs:documentation xml:lang="de">Recordversion</xs:documentation>											<xs:documentation xml:lang="en">Record version</xs:documentation>										</xs:annotation>										<xs:simpleType>											<xs:restriction base="versionRecType">												<xs:enumeration value="5.4.1"/>											</xs:restriction>										</xs:simpleType>									</xs:element>								</xs:sequence>							</xs:extension>						</xs:complexContent>					</xs:complexType>				</xs:element>				<xs:choice>					<xs:annotation>						<xs:documentation xml:lang="de">Wahl zwischen einem Geschäfts- und Privatkunden</xs:documentation>						<xs:documentation xml:lang="en">Choice between business and private customer</xs:documentation>					</xs:annotation>					<xs:element name="ICBusiness" type="ICBusinessType">						<xs:annotation>							<xs:documentation xml:lang="de">Geschäftskunde</xs:documentation>							<xs:documentation xml:lang="en">Business customer</xs:documentation>						</xs:annotation>					</xs:element>					<xs:element name="ICPrivatePerson" type="ICPrivatePersonType">						<xs:annotation>							<xs:documentation xml:lang="de">Privatkunde</xs:documentation>							<xs:documentation xml:lang="en">Private customer</xs:documentation>						</xs:annotation>					</xs:element>				</xs:choice>				<xs:element name="mandateFeedback" type="mandateFeedbackLocType">					<xs:annotation>						<xs:documentation xml:lang="de">Rückmeldung zur Mandatseinreichung</xs:documentation>						<xs:documentation xml:lang="de">Die Rückmeldung beinhaltet den Entscheid, ob dem Broker das Mandat übergeben wird.</xs:documentation>						<xs:documentation xml:lang="en">Feedback on the mandate submission</xs:documentation>						<xs:documentation xml:lang="en">The feedback includes the decision on whether the mandate will be transferred to the broker.</xs:documentation>					</xs:annotation>				</xs:element>				<xs:element name="mandateKind" type="mandateKindLocType">					<xs:annotation>						<xs:documentation xml:lang="de">Art des Brokermandats</xs:documentation>						<xs:documentation xml:lang="en">Type of broker mandate</xs:documentation>					</xs:annotation>				</xs:element>				<xs:element name="mandateReceiptDate" type="xs:date">					<xs:annotation>						<xs:documentation xml:lang="de">Eingangsdatum der Mandatseinreichung</xs:documentation>						<xs:documentation xml:lang="de">JJJJ-MM-TT = Eingangsdatum der Mandatseinreichung </xs:documentation>						<xs:documentation xml:lang="en">Date of receipt of mandate submission</xs:documentation>						<xs:documentation xml:lang="en">YYYY-MM-DD = Date of receipt of mandate submission</xs:documentation>					</xs:annotation>				</xs:element>				<xs:element name="mandateFeedbackDetails">					<xs:annotation>						<xs:documentation xml:lang="de">Details zur Rückmeldung bezüglich der Mandatseinreichung</xs:documentation>						<xs:documentation xml:lang="en">Details of feedback concerning the mandate submission</xs:documentation>					</xs:annotation>					<xs:complexType>						<xs:choice>							<xs:element name="mandateConfirmation">								<xs:annotation>									<xs:documentation xml:lang="de">Bestätigung des Mandats</xs:documentation>									<xs:documentation xml:lang="en">Confirmation of the mandate</xs:documentation>								</xs:annotation>								<xs:complexType>									<xs:sequence>										<xs:element name="mandateValidFromDate" type="xs:date">											<xs:annotation>												<xs:documentation xml:lang="de">Gültigkeitsbeginndatum des Mandats</xs:documentation>												<xs:documentation xml:lang="de">JJJJ-MM-TT = Beginndatum des Mandats bzw. der Mandatsanpassung.</xs:documentation>												<xs:documentation xml:lang="en">Validity of mandate</xs:documentation>												<xs:documentation xml:lang="en">YYYY-MM-DD = Start date of mandate or mandate adjustment.</xs:documentation>											</xs:annotation>										</xs:element>										<xs:element name="contract" maxOccurs="unbounded">											<xs:annotation>												<xs:documentation xml:lang="de">Vertrag</xs:documentation>												<xs:documentation xml:lang="en">Contract</xs:documentation>											</xs:annotation>											<xs:complexType>												<xs:sequence>													<xs:element name="contractNo" type="contractNoType">														<xs:annotation>															<xs:documentation xml:lang="de">Vertragsnummer</xs:documentation>															<xs:documentation xml:lang="de">Entspricht der eindeutigen Nummer des entsprechenden Vertrages. Vertragsnummernaufbau des einzelnen Versicherers / Unternehmens siehe Spezifikation IG B2B.</xs:documentation>															<xs:documentation xml:lang="en">Contract number</xs:documentation>															<xs:documentation xml:lang="en">This is the unique number of the corresponding contract. For the contract number structure of the individual insurer/company, see IG B2B specification.</xs:documentation>														</xs:annotation>													</xs:element>													<xs:element name="lobIG" type="LOBIGType">														<xs:annotation>															<xs:documentation xml:lang="de">Branchencode IG B2B</xs:documentation>															<xs:documentation xml:lang="de">Branchencode gemäss IG B2B</xs:documentation>															<xs:documentation xml:lang="en">IG B2B sector code</xs:documentation>															<xs:documentation xml:lang="en">Sector code according to IG B2B</xs:documentation>														</xs:annotation>													</xs:element>													<xs:element name="lobINS" type="LOBINSType" minOccurs="0">														<xs:annotation>															<xs:documentation xml:lang="de">Branchen-/Risikocode oder -bezeichnung des Versicherers/Vorsorgeunternehmens</xs:documentation>															<xs:documentation xml:lang="de">Branchen-/Risikocode oder -bezeichnung gemäss Verzeichnis des einzelnen Versicherers/Vorsorgeunternehmens. Der Detaillierungsgrad sollte so gewählt sein, dass für vor- und nachgelagerte Prozesse ein Bezug zu den jeweiligen Positionen (z.B. Branchencode in Prämienrechnung = Branchencode in Provisionsabrechnung) hergestellt werden kann. Bei kombinierten Produkten ist der übergeordnete Code/Bezeichnung zu wählen.</xs:documentation>															<xs:documentation xml:lang="en">Sector/risk code or designation of the insurer/pension company</xs:documentation>															<xs:documentation xml:lang="en">Sector/risk code or designation according to the directory of the individual insurer/pension company. The level of detail should be sufficient to enable upstream and downstream processes to establish a reference to the respective items (e.g. sector code in premium invoice = sector code in commission calculation). For combined products, the higher-level code/designation is to be selected.</xs:documentation>														</xs:annotation>													</xs:element>												</xs:sequence>											</xs:complexType>										</xs:element>									</xs:sequence>								</xs:complexType>							</xs:element>							<xs:element name="mandateRejection">								<xs:annotation>									<xs:documentation xml:lang="de">Ablehnung des Mandats</xs:documentation>									<xs:documentation xml:lang="en">Rejection of the mandate</xs:documentation>								</xs:annotation>								<xs:complexType>									<xs:sequence>										<xs:element name="mandateRejectionReason" type="remarkType" minOccurs="0">											<xs:annotation>												<xs:documentation xml:lang="de">Ablehnungsgrund des Mandats</xs:documentation>												<xs:documentation xml:lang="en">Reason for rejection of the mandate</xs:documentation>											</xs:annotation>										</xs:element>									</xs:sequence>								</xs:complexType>							</xs:element>						</xs:choice>					</xs:complexType>				</xs:element>				<xs:element name="attachment" type="attachmentType" minOccurs="0" maxOccurs="unbounded">					<xs:annotation>						<xs:documentation xml:lang="de">Dateianhang</xs:documentation>						<xs:documentation xml:lang="en">Attachment</xs:documentation>					</xs:annotation>				</xs:element>			</xs:sequence>		</xs:complexType>	</xs:element></xs:schema>