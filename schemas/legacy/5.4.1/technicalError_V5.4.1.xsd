<?xml version="1.0" encoding="UTF-8"?><!--XSD Standard for IG B2B Data exchangeCopyright © IG B2B for Insurers + Brokers, Switzerland--><!--Changes in Version V5.4 from V5.2: - Change version values and names of referenced data schemas from 5.2 to 5.4- Change description of element name="technicalError"- Delete element name="contentRec"- Change value of element name="typeRec" from 999 to 902- Change value of element name="subTypeRec" from 009 to 001- Delete french and italian textsChanges in Version V5.4.1 from V5.4: - Change version values and names of referenced data schemas from 5.4 to 5.4.1--><xs:schema xmlns:xs="http://www.w3.org/2001/XMLSchema" xmlns="http://www.IGB2B.ch/XMLSchema" targetNamespace="http://www.IGB2B.ch/XMLSchema" elementFormDefault="qualified" version="5.4.1">	<xs:include schemaLocation="globals_V5.4.1.xsd"/>	<xs:include schemaLocation="technicalErrorTypeLib_V5.4.1.xsd"/>	<xs:element name="technicalError">		<xs:annotation>			<xs:documentation xml:lang="de">Standard-Schema für technischer Fehler</xs:documentation>			<xs:documentation xml:lang="de">Dieser Standard definiert die Struktur einer technischen Fehlermeldung im Datenaustausch. Z.B. für Fehlern aus der Schemavalidierung.</xs:documentation>			<xs:documentation xml:lang="en">Standard schema for technical errors</xs:documentation>			<xs:documentation xml:lang="en">This standard defines the structure of technical error reporting in the data exchange (e.g. for errors from schema validation)</xs:documentation>		</xs:annotation>		<xs:complexType>			<xs:sequence>				<xs:element name="header">					<xs:annotation>						<xs:documentation xml:lang="de">Header</xs:documentation>						<xs:documentation xml:lang="en">Header</xs:documentation>					</xs:annotation>					<xs:complexType>						<xs:complexContent>							<xs:extension base="headerType">								<xs:sequence>									<xs:element name="typeRec">										<xs:annotation>											<xs:documentation xml:lang="de">Recordart</xs:documentation>											<xs:documentation xml:lang="en">Record type</xs:documentation>										</xs:annotation>										<xs:simpleType>											<xs:restriction base="typeRecType">												<xs:enumeration value="902">													<xs:annotation>														<xs:documentation xml:lang="de">Technischer Fehler</xs:documentation>														<xs:documentation xml:lang="en">Technical Error</xs:documentation>													</xs:annotation>												</xs:enumeration>											</xs:restriction>										</xs:simpleType>									</xs:element>									<xs:element name="subTypeRec">										<xs:annotation>											<xs:documentation xml:lang="de">Subrecordart</xs:documentation>											<xs:documentation xml:lang="en">Sub record type</xs:documentation>										</xs:annotation>										<xs:simpleType>											<xs:restriction base="subTypeRecType">												<xs:enumeration value="001">													<xs:annotation>														<xs:documentation xml:lang="de">Fehler</xs:documentation>														<xs:documentation xml:lang="en">Error</xs:documentation>													</xs:annotation>												</xs:enumeration>											</xs:restriction>										</xs:simpleType>									</xs:element>									<xs:element name="versionRec">										<xs:annotation>											<xs:documentation xml:lang="de">Recordversion</xs:documentation>											<xs:documentation xml:lang="en">Record version</xs:documentation>										</xs:annotation>										<xs:simpleType>											<xs:restriction base="versionRecType">												<xs:enumeration value="5.4.1"/>											</xs:restriction>										</xs:simpleType>									</xs:element>								</xs:sequence>							</xs:extension>						</xs:complexContent>					</xs:complexType>				</xs:element>				<xs:choice>					<xs:annotation>						<xs:documentation xml:lang="de">Wahl zwischen den verschiedenen Typen der technischen Fehlerrückmeldung</xs:documentation>						<xs:documentation xml:lang="en">Choose between on of the error type feedbacks</xs:documentation>					</xs:annotation>					<xs:element name="errorSchema">						<xs:annotation>							<xs:documentation xml:lang="de">Fehlertyp entspricht Schema</xs:documentation>							<xs:documentation xml:lang="en">Type of error schema</xs:documentation>						</xs:annotation>						<xs:complexType>							<xs:sequence>								<xs:element name="errorSubType" type="errorSchemaSubTypeLocType">									<xs:annotation>										<xs:documentation xml:lang="de">Feedbacksubtyp, Werte für Feedbacktyp Schema</xs:documentation>										<xs:documentation xml:lang="en">Feedback subtype, values for feedback type schema</xs:documentation>									</xs:annotation>								</xs:element>							</xs:sequence>						</xs:complexType>					</xs:element>					<xs:element name="errorAuthorization">						<xs:annotation>							<xs:documentation xml:lang="de">Fehlertyp entspricht Berechtigung</xs:documentation>							<xs:documentation xml:lang="en">Type of error authorization</xs:documentation>						</xs:annotation>						<xs:complexType>							<xs:sequence>								<xs:element name="errorSubType" type="errorAuthorizationSubTypeLocType">									<xs:annotation>										<xs:documentation xml:lang="de">Feedbacksubtyp, Werte für Feedbacktyp Berechtigung</xs:documentation>										<xs:documentation xml:lang="en">Feedback subtype, values ​​for feedback type authorization</xs:documentation>									</xs:annotation>								</xs:element>							</xs:sequence>						</xs:complexType>					</xs:element>					<xs:element name="errorProcessHandling">						<xs:annotation>							<xs:documentation xml:lang="de">Fehlertyp entspricht Prozessabwicklung</xs:documentation>							<xs:documentation xml:lang="en">Type of error for process handling</xs:documentation>						</xs:annotation>						<xs:complexType>							<xs:sequence>								<xs:element name="errorSubType" type="errorProcessHandlingSubTypeLocType">									<xs:annotation>										<xs:documentation xml:lang="de">Feedbacksubtyp, Werte für Feedbacktyp Prozessabwicklung</xs:documentation>										<xs:documentation xml:lang="en">Feedback subtype, values ​​for feedback type process handling</xs:documentation>									</xs:annotation>								</xs:element>							</xs:sequence>						</xs:complexType>					</xs:element>					<xs:element name="errorEmbeddedFileContent">						<xs:annotation>							<xs:documentation xml:lang="de">Fehlertyp entspricht eingebetteter Dateiinhalt</xs:documentation>							<xs:documentation xml:lang="en">Type of error embedded file content</xs:documentation>						</xs:annotation>						<xs:complexType>							<xs:sequence>								<xs:element name="errorSubType" type="errorEmbeddedFileContentSubTypeLocType">									<xs:annotation>										<xs:documentation xml:lang="de">Feedbacksubtyp, Werte für Feedbacktyp eingebetteter Dateiinhalt</xs:documentation>										<xs:documentation xml:lang="en">Feedback subtype, values ​​for feedback type embedded file content</xs:documentation>									</xs:annotation>								</xs:element>							</xs:sequence>						</xs:complexType>					</xs:element>				</xs:choice>				<xs:element name="errorMessage" type="remarkType">					<xs:annotation>						<xs:documentation xml:lang="de">Fehlermeldung</xs:documentation>						<xs:documentation xml:lang="de">Ergänzende bzw. präzisierende Fehlermeldung, welche Versicherer- oder Brokerspezifisch sein kann/darf</xs:documentation>						<xs:documentation xml:lang="en">Error message</xs:documentation>						<xs:documentation xml:lang="en">Complementary or more precise error message, which can be insurers or broker specific</xs:documentation>					</xs:annotation>				</xs:element>			</xs:sequence>		</xs:complexType>	</xs:element></xs:schema>