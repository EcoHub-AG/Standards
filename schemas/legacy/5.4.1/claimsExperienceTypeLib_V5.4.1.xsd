<?xml version="1.0" encoding="UTF-8"?><!--XSD Standard for IG B2B Data exchangeCopyright © IG B2B for Insurers + Brokers, Switzerland--><!--Changes in Version V5.4rc1 from V5.2: - Change version values and name of referenced data schema from 5.2 to 5.4rc1- Change name of simpleType name="generalAgreementLocType" to "frameworkContractNoLocType"- Delete simpleType name="codePremiumLocType"- Delete simpleType name="codeClaimsPaymentLocType"- Insert new complexType name="contractCancelledStatusLocType"- Insert new simpleType name="cancelledByLocType"- Change description of complexType name="ICBusinessLocalityLocType"- Delete complexType name="ICBusinessLocType"- Delete complexType name="ICBusinessLocalityLocType"- Delete complexType name="ICPrivatePersonLocType"- Delete complexType name="ICPrivatePersonLocalityLocType"- Change name of simpleType name="periodicityLocType" to "periodicityYearLocType" and its description- Delete french and italian textsChanges in Version V5.4.1 from V5.4rc1: - Change version values and name of referenced data schema from 5.4rc1 to 5.4.1--><xs:schema xmlns:xs="http://www.w3.org/2001/XMLSchema" xmlns="http://www.IGB2B.ch/XMLSchema" targetNamespace="http://www.IGB2B.ch/XMLSchema" elementFormDefault="qualified" version="5.4.1">	<xs:include schemaLocation="globals_V5.4.1.xsd"/>	<xs:simpleType name="frameworkContractNoLocType">		<xs:annotation>			<xs:documentation xml:lang="de">Datentyp für Rahmenvertrags-/Flottenvertragsnummer</xs:documentation>			<xs:documentation xml:lang="en">Data type for Framework contract/fleet contract number</xs:documentation>		</xs:annotation>		<xs:restriction base="textLine">			<xs:maxLength value="20"/>		</xs:restriction>	</xs:simpleType>	<xs:simpleType name="brokerCodeInsLocType">		<xs:annotation>			<xs:documentation xml:lang="de">Datentyp für Brokercode Versicherer</xs:documentation>			<xs:documentation xml:lang="en">Data type for insurer's broker code</xs:documentation>		</xs:annotation>		<xs:restriction base="xs:NMTOKEN">			<xs:maxLength value="10"/>		</xs:restriction>	</xs:simpleType>	<xs:simpleType name="agencyLocType">		<xs:annotation>			<xs:documentation xml:lang="de">Datentyp für Agentur/Verkaufsstelle/Provisionsbezüger</xs:documentation>			<xs:documentation xml:lang="en">Data type for Agency/sales point/commission agent</xs:documentation>		</xs:annotation>		<xs:restriction base="textLine">			<xs:maxLength value="20"/>		</xs:restriction>	</xs:simpleType>	<xs:simpleType name="periodicityYearLocType">		<xs:annotation>			<xs:documentation xml:lang="de">Datentyp für Periodizität Jahr</xs:documentation>			<xs:documentation xml:lang="en">Data type for periodicity year</xs:documentation>		</xs:annotation>		<xs:restriction base="xs:NMTOKEN">			<xs:enumeration value="01">				<xs:annotation>					<xs:documentation xml:lang="de">Kalenderjahr = Versicherungsjahr (Defaultwert)</xs:documentation>					<xs:documentation xml:lang="en">Calendar year = Insurance year (default value)</xs:documentation>				</xs:annotation>			</xs:enumeration>			<xs:enumeration value="02">				<xs:annotation>					<xs:documentation xml:lang="de">Kalenderjahr ≠ Versicherungsjahr</xs:documentation>					<xs:documentation xml:lang="en">Calendar year ≠ Insurance year</xs:documentation>				</xs:annotation>			</xs:enumeration>			<xs:enumeration value="03">				<xs:annotation>					<xs:documentation xml:lang="de">Versicherungsjahr, sofern dieses nicht mit dem Kalenderjahr übereinstimmt</xs:documentation>					<xs:documentation xml:lang="en">Insurance year, if this is not the same as the calendar year</xs:documentation>				</xs:annotation>			</xs:enumeration>		</xs:restriction>	</xs:simpleType>	<xs:simpleType name="codeReservesLocType">		<xs:annotation>			<xs:documentation xml:lang="de">Datentyp für Code betreffend Schadenreserven</xs:documentation>			<xs:documentation xml:lang="en">Data type for claims reserve code</xs:documentation>		</xs:annotation>		<xs:restriction base="xs:NMTOKEN">			<xs:maxLength value="2"/>			<xs:enumeration value="00">				<xs:annotation>					<xs:documentation xml:lang="de">Verzicht auf Angabe der Schadenreserven</xs:documentation>					<xs:documentation xml:lang="en">Does not state claims reserves</xs:documentation>				</xs:annotation>			</xs:enumeration>			<xs:enumeration value="01">				<xs:annotation>					<xs:documentation xml:lang="de">Einzelschadenreserven/aktuarielle Reserven</xs:documentation>					<xs:documentation xml:lang="en">Individual claims reserves/actuarial reserves </xs:documentation>				</xs:annotation>			</xs:enumeration>			<xs:enumeration value="02">				<xs:annotation>					<xs:documentation xml:lang="de">Pauschalreserven</xs:documentation>					<xs:documentation xml:lang="en">Overall reserves </xs:documentation>				</xs:annotation>			</xs:enumeration>			<xs:enumeration value="03">				<xs:annotation>					<xs:documentation xml:lang="de">Einzelschaden- und Pauschalreserven</xs:documentation>					<xs:documentation xml:lang="en">Individual claim and overall reserves </xs:documentation>				</xs:annotation>			</xs:enumeration>			<xs:enumeration value="04">				<xs:annotation>					<xs:documentation xml:lang="de">Andere Reservenbildung</xs:documentation>					<xs:documentation xml:lang="en">Other reserve formation </xs:documentation>				</xs:annotation>			</xs:enumeration>		</xs:restriction>	</xs:simpleType>	<xs:simpleType name="codeRatioLocType">		<xs:annotation>			<xs:documentation xml:lang="de">Datentyp für Code betreffend Schadenaufwand/-quote</xs:documentation>			<xs:documentation xml:lang="en">Data type for claims expenditure/loss ratio code</xs:documentation>		</xs:annotation>		<xs:restriction base="xs:NMTOKEN">			<xs:maxLength value="2"/>			<xs:enumeration value="01">				<xs:annotation>					<xs:documentation xml:lang="de">Ohne Reserven</xs:documentation>					<xs:documentation xml:lang="en">Without reserves</xs:documentation>				</xs:annotation>			</xs:enumeration>			<xs:enumeration value="02">				<xs:annotation>					<xs:documentation xml:lang="de">Mit Reserven</xs:documentation>					<xs:documentation xml:lang="en">With reserves</xs:documentation>				</xs:annotation>			</xs:enumeration>		</xs:restriction>	</xs:simpleType>	<xs:simpleType name="codeAssignmentLocType">		<xs:annotation>			<xs:documentation xml:lang="de">Datentyp für Code betreffend Schadenzuordnung (Personenversicherungen)</xs:documentation>			<xs:documentation xml:lang="en">Data type for claims allocation code (personal insurance)</xs:documentation>		</xs:annotation>		<xs:restriction base="xs:NMTOKEN">			<xs:maxLength value="2"/>			<xs:enumeration value="01">				<xs:annotation>					<xs:documentation xml:lang="de">Fälle ab 1. Tag</xs:documentation>					<xs:documentation xml:lang="en">Cases from Day 1</xs:documentation>				</xs:annotation>			</xs:enumeration>			<xs:enumeration value="02">				<xs:annotation>					<xs:documentation xml:lang="de">Fälle nach Wartefrist</xs:documentation>					<xs:documentation xml:lang="en">Cases after waiting period</xs:documentation>				</xs:annotation>			</xs:enumeration>		</xs:restriction>	</xs:simpleType>	<xs:complexType name="contractCancelledStatusLocType">		<xs:annotation>			<xs:documentation xml:lang="de">Datentyp für den Kündigungsstatus eines Vertrages</xs:documentation>			<xs:documentation xml:lang="en">Data type for the cancellation status of a contract</xs:documentation>		</xs:annotation>		<xs:choice>			<xs:annotation>				<xs:documentation xml:lang="de">Wahl zwischen bisherigen Vertrag gekündigt oder nicht</xs:documentation>				<xs:documentation xml:lang="en">Choice between preceding contract cancelled or not</xs:documentation>			</xs:annotation>			<xs:element name="contractCancelled">				<xs:annotation>					<xs:documentation xml:lang="de">Bisheriger Vertrag gekündigt</xs:documentation>					<xs:documentation xml:lang="en">Preceding contract cancelled</xs:documentation>				</xs:annotation>				<xs:complexType>					<xs:sequence>						<xs:element name="contractCancelledDate" type="xs:date">							<xs:annotation>								<xs:documentation xml:lang="de">Kündigung per</xs:documentation>								<xs:documentation xml:lang="de">Kündigungsdatum</xs:documentation>								<xs:documentation xml:lang="en">Cancelled as at</xs:documentation>								<xs:documentation xml:lang="en">Date of cancellation</xs:documentation>							</xs:annotation>						</xs:element>						<xs:element name="contractCancelledBy" type="cancelledByLocType">							<xs:annotation>								<xs:documentation xml:lang="de">Kündigung durch</xs:documentation>								<xs:documentation xml:lang="en">Cancelled by</xs:documentation>							</xs:annotation>						</xs:element>					</xs:sequence>				</xs:complexType>			</xs:element>			<xs:element name="contractNotCancelled" type="setValueTrueType">				<xs:annotation>					<xs:documentation xml:lang="de">Bisheriger Vertrag NICHT gekündigt</xs:documentation>					<xs:documentation xml:lang="en">Preceding policy NOT cancelled</xs:documentation>				</xs:annotation>			</xs:element>		</xs:choice>	</xs:complexType>	<xs:simpleType name="cancelledByLocType">		<xs:annotation>			<xs:documentation xml:lang="de">Datentyp für die Angabe einer Person, die die Kündigung durchgeführt hat</xs:documentation>			<xs:documentation xml:lang="en">Data type for the information about a person who did the termination</xs:documentation>		</xs:annotation>		<xs:restriction base="xs:NMTOKEN">			<xs:maxLength value="2"/>			<xs:enumeration value="01">				<xs:annotation>					<xs:documentation xml:lang="de">Versicherungsnehmer</xs:documentation>					<xs:documentation xml:lang="en">Policyholder</xs:documentation>				</xs:annotation>			</xs:enumeration>			<xs:enumeration value="02">				<xs:annotation>					<xs:documentation xml:lang="de">Versicherer</xs:documentation>					<xs:documentation xml:lang="en">Insurer</xs:documentation>				</xs:annotation>			</xs:enumeration>		</xs:restriction>	</xs:simpleType>	<xs:complexType name="insUVGLocType">		<xs:annotation>			<xs:documentation xml:lang="de">Deckungsart obligatorische Versicherung</xs:documentation>			<xs:documentation xml:lang="en">Type of cover for compulsory insurance</xs:documentation>		</xs:annotation>		<xs:sequence>			<xs:element name="UVGPayroll" type="decimal10dot2Type">				<xs:annotation>					<xs:documentation xml:lang="de">aktuelle UVG-Lohnsumme Männer + Frauen</xs:documentation>					<xs:documentation xml:lang="de">Gesamte UVG BU-Lohnsumme der Männer + Frauen dieser Personengruppe.</xs:documentation>					<xs:documentation xml:lang="en">Current UVG total salary amount men + women</xs:documentation>					<xs:documentation xml:lang="en">Total UVG OA total salary amount for the men + women of this group of persons.</xs:documentation>				</xs:annotation>			</xs:element>			<xs:element name="UVGSettledPayroll" type="settledPayrollLocType" maxOccurs="7">				<xs:annotation>					<xs:documentation xml:lang="de">Abgerechnete Lohnsumme</xs:documentation>					<xs:documentation xml:lang="en">Calculated salary amount</xs:documentation>				</xs:annotation>			</xs:element>			<xs:element name="UVGExpenditureBU" type="expenditureBU_NBULocType" maxOccurs="7">				<xs:annotation>					<xs:documentation xml:lang="de">Ausgaben BU (1-7)</xs:documentation>					<xs:documentation xml:lang="en">Expenditure OA (1-7)</xs:documentation>				</xs:annotation>			</xs:element>			<xs:element name="UVGExpenditureNBU" type="expenditureBU_NBULocType" maxOccurs="7">				<xs:annotation>					<xs:documentation xml:lang="de">Ausgaben NBU (1-7)</xs:documentation>					<xs:documentation xml:lang="en">Expenditure NOA (1-7)</xs:documentation>				</xs:annotation>			</xs:element>		</xs:sequence>	</xs:complexType>	<xs:complexType name="expenditureBU_NBULocType">		<xs:annotation>			<xs:documentation xml:lang="de">Datentyp für die Ausgaben bei BU und NBU</xs:documentation>			<xs:documentation xml:lang="en">Data type for expenditures at OA and NOA</xs:documentation>		</xs:annotation>		<xs:sequence>			<xs:element name="claimsReported" type="decimal3dot0Type">				<xs:annotation>					<xs:documentation xml:lang="de">Anzahl Schadenfälle</xs:documentation>					<xs:documentation xml:lang="en">Number of claims</xs:documentation>				</xs:annotation>			</xs:element>			<xs:element name="claimsPending" type="decimal3dot0Type">				<xs:annotation>					<xs:documentation xml:lang="de">Davon Anzahl pendente Schadenfälle</xs:documentation>					<xs:documentation xml:lang="en">Of which number of pending claims</xs:documentation>				</xs:annotation>			</xs:element>			<xs:element name="claimsPayments" type="decimal10dot2Type">				<xs:annotation>					<xs:documentation xml:lang="de">Total Zahlungen Kurzfristleistungen in CHF</xs:documentation>					<xs:documentation xml:lang="en">Total short-term benefit payments in CHF</xs:documentation>				</xs:annotation>			</xs:element>			<xs:element name="claimsReserves" type="decimal10dot2Type">				<xs:annotation>					<xs:documentation xml:lang="de">Total Bedarfsrückstellung in CHF</xs:documentation>					<xs:documentation xml:lang="en">Total claims reserves in CHF</xs:documentation>				</xs:annotation>			</xs:element>		</xs:sequence>		<xs:attribute name="year" type="xs:gYear" use="required">			<xs:annotation>				<xs:documentation xml:lang="de">Schadenaufwand-Jahr</xs:documentation>				<xs:documentation xml:lang="de">JJJJ = aufgeführtes Jahr. Hinweis: Start mit aktuellem Jahr rückwärts.</xs:documentation>				<xs:documentation xml:lang="en">Claims expense-Year</xs:documentation>				<xs:documentation xml:lang="en">YYYY = specified year. Note; Start with current year going backwards.</xs:documentation>			</xs:annotation>		</xs:attribute>	</xs:complexType>	<xs:complexType name="settledPayrollLocType">		<xs:annotation>			<xs:documentation xml:lang="de">Abgerechnete Lohnsumme</xs:documentation>			<xs:documentation xml:lang="en">Calculated salary amount</xs:documentation>		</xs:annotation>		<xs:sequence>			<xs:element name="payrollMaleBU" type="decimal10dot2Type">				<xs:annotation>					<xs:documentation xml:lang="de">UVG-Lohnsumme Männer BU</xs:documentation>					<xs:documentation xml:lang="de">Gesamte UVG BU-Lohnsumme der Männer. Hinweis Versicherer: Sofern noch nicht definitiv abgerechnet, wird provisorische Jahreslohnsumme mitgeteilt.</xs:documentation>					<xs:documentation xml:lang="en">UVG OA salary amount, men</xs:documentation>					<xs:documentation xml:lang="en">Total UVG OA salary amount for men. Note for insurers: If not yet definitively calculated, the provisional annual total salary amount is communicated</xs:documentation>				</xs:annotation>			</xs:element>			<xs:element name="payrollFemaleBU" type="decimal10dot2Type">				<xs:annotation>					<xs:documentation xml:lang="de">UVG-Lohnsumme Frauen BU</xs:documentation>					<xs:documentation xml:lang="de">Gesamte UVG BU-Lohnsumme der Frauen. Hinweis Versicherer: Sofern noch nicht definitiv abgerechnet, wird provisorische Jahreslohnsumme mitgeteilt.</xs:documentation>					<xs:documentation xml:lang="en">UVG salary amount, OA women</xs:documentation>					<xs:documentation xml:lang="en">Total UVG OA salary amount for women. Note for insurers: If not yet definitively calculated, the provisional annual total salary amount is communicatedxx</xs:documentation>				</xs:annotation>			</xs:element>			<xs:element name="payrollMaleNBU" type="decimal10dot2Type">				<xs:annotation>					<xs:documentation xml:lang="de">UVG-Lohnsumme Männer NBU</xs:documentation>					<xs:documentation xml:lang="de">Gesamte UVG NBU-Lohnsumme der Männer. Hinweis Versicherer: Sofern noch nicht definitiv abgerechnet, wird provisorische Jahreslohnsumme mitgeteilt.</xs:documentation>					<xs:documentation xml:lang="en">UVG NOA salary amount, men</xs:documentation>					<xs:documentation xml:lang="en">Total UVG NOA salary amount for men. Note for insurers: If not yet definitively calculated, the provisional annual total salary amount is communicated</xs:documentation>				</xs:annotation>			</xs:element>			<xs:element name="payrollFemaleNBU" type="decimal10dot2Type">				<xs:annotation>					<xs:documentation xml:lang="de">UVG-Lohnsumme Frauen NBU</xs:documentation>					<xs:documentation xml:lang="de">Gesamte UVG NBU-Lohnsumme der Frauen. Hinweis Versicherer: Sofern noch nicht definitiv abgerechnet, wird provisorische Jahreslohnsumme mitgeteilt.</xs:documentation>					<xs:documentation xml:lang="en">UVG salary amount, NOA women</xs:documentation>					<xs:documentation xml:lang="en">Total UVG NOA salary amount for women. Note for insurers: If not yet definitively calculated, the provisional annual total salary amount is communicatedxx</xs:documentation>				</xs:annotation>			</xs:element>		</xs:sequence>		<xs:attribute name="year" type="xs:gYear" use="required">			<xs:annotation>				<xs:documentation xml:lang="de">Abgerechnetes Jahr UVG-Lohnsumme</xs:documentation>				<xs:documentation xml:lang="de">JJJJ = aufgeführtes Jahr Hinweis: Start mit aktuellem Jahr rückwärts.</xs:documentation>				<xs:documentation xml:lang="en">Calculated annual UVG total salary amount</xs:documentation>				<xs:documentation xml:lang="en">YYYY = specified year. Note; Start with current year going backwards.</xs:documentation>			</xs:annotation>		</xs:attribute>	</xs:complexType>	<xs:complexType name="KTGPGsLongTailClaimsLocType">		<xs:annotation>			<xs:documentation xml:lang="de">Datentyp für Langzeitfälle</xs:documentation>			<xs:documentation xml:lang="en">Data type for long-term cases</xs:documentation>		</xs:annotation>		<xs:sequence>			<xs:element name="longTailClaimBeginDisability" type="xs:date">				<xs:annotation>					<xs:documentation xml:lang="de">Beginn Arbeitsunfähigkeit</xs:documentation>					<xs:documentation xml:lang="en">Start of disability</xs:documentation>				</xs:annotation>			</xs:element>			<xs:element name="longTailClaimStatus">				<xs:annotation>					<xs:documentation xml:lang="de">Aktueller Stand des Langzeitfalles</xs:documentation>					<xs:documentation xml:lang="en">Current situation of long-term case</xs:documentation>				</xs:annotation>				<xs:simpleType>					<xs:restriction base="xs:NMTOKEN">						<xs:maxLength value="2"/>						<xs:enumeration value="01">							<xs:annotation>								<xs:documentation xml:lang="de">erschöpft</xs:documentation>								<xs:documentation xml:lang="en">exhausted</xs:documentation>							</xs:annotation>						</xs:enumeration>						<xs:enumeration value="02">							<xs:annotation>								<xs:documentation xml:lang="de">erledigt</xs:documentation>								<xs:documentation xml:lang="en">processed</xs:documentation>							</xs:annotation>						</xs:enumeration>						<xs:enumeration value="03">							<xs:annotation>								<xs:documentation xml:lang="de">pendent</xs:documentation>								<xs:documentation xml:lang="en">pending</xs:documentation>							</xs:annotation>						</xs:enumeration>					</xs:restriction>				</xs:simpleType>			</xs:element>			<xs:element name="longTailClaimPayments" type="decimal10dot2Type">				<xs:annotation>					<xs:documentation xml:lang="de">Total Zahlungen in CHF</xs:documentation>					<xs:documentation xml:lang="en">Total payments in CHF</xs:documentation>				</xs:annotation>			</xs:element>			<xs:element name="longTailClaimReserves" type="decimal7dot0Type">				<xs:annotation>					<xs:documentation xml:lang="de">Total Bedarfsrückstellung in CHF</xs:documentation>					<xs:documentation xml:lang="en">Total claim reserves in CHF</xs:documentation>				</xs:annotation>			</xs:element>		</xs:sequence>	</xs:complexType></xs:schema>