<?xml version="1.0" encoding="UTF-8"?><!--XSD Standard for IG B2B Data exchangeCopyright © IG B2B for Insurers + Brokers, Switzerland--><!--Changes in Version V5.4rc1 from V5.2: - Change version values and names of referenced data schemas from 5.2 to 5.4rc1- Change description of element name="claimsExperienceRequest"- Delete element name="contentRec"- Change value description of element name="typeRec" and change value from 005 to 002- Change value description of element name="subTypeRec" and change value from 000 to 001- Change type of element name="ICBusiness" to "ICBusinessType"- Change type of element name="ICPrivatePerson" to "ICPrivatePersonType"- Change description of element name="contractNo"- Delete element name="remarks"- Delete french and italian textChanges in Version V5.4.1 from V5.4rc1: - Change version values and names of referenced data schemas from 5.4rc1 to 5.4.1--><xs:schema xmlns:xs="http://www.w3.org/2001/XMLSchema" xmlns="http://www.IGB2B.ch/XMLSchema" targetNamespace="http://www.IGB2B.ch/XMLSchema" elementFormDefault="qualified" version="5.4.1">	<xs:include schemaLocation="globals_V5.4.1.xsd"/>	<xs:include schemaLocation="claimsExperienceTypeLib_V5.4.1.xsd"/>	<xs:element name="claimsExperienceRequest">		<xs:annotation>			<xs:documentation xml:lang="de">Standard-Schema für Anfrage Schadenrendement</xs:documentation>			<xs:documentation xml:lang="de">Dieser Standard definiert die Struktur für die Anfrage eines Schadenrendements.</xs:documentation>			<xs:documentation xml:lang="en">Standard schema for a claims experience request</xs:documentation>			<xs:documentation xml:lang="en">This standard defines the structure for the request of a claims experience.</xs:documentation>		</xs:annotation>		<xs:complexType>			<xs:sequence>				<xs:element name="header">					<xs:annotation>						<xs:documentation xml:lang="de">Header</xs:documentation>						<xs:documentation xml:lang="en">Header</xs:documentation>					</xs:annotation>					<xs:complexType>						<xs:complexContent>							<xs:extension base="headerType">								<xs:sequence>									<xs:element name="typeRec">										<xs:annotation>											<xs:documentation xml:lang="de">Recordart</xs:documentation>											<xs:documentation xml:lang="en">Record type</xs:documentation>										</xs:annotation>										<xs:simpleType>											<xs:restriction base="typeRecType">												<xs:enumeration value="002">													<xs:annotation>														<xs:documentation xml:lang="de">Schadenrendement</xs:documentation>														<xs:documentation xml:lang="en">Claims experience</xs:documentation>													</xs:annotation>												</xs:enumeration>											</xs:restriction>										</xs:simpleType>									</xs:element>									<xs:element name="subTypeRec">										<xs:annotation>											<xs:documentation xml:lang="de">Subrecordart</xs:documentation>											<xs:documentation xml:lang="en">Sub record type</xs:documentation>										</xs:annotation>										<xs:simpleType>											<xs:restriction base="subTypeRecType">												<xs:enumeration value="001">													<xs:annotation>														<xs:documentation xml:lang="de">Schadenrendement Anfrage</xs:documentation>														<xs:documentation xml:lang="en">Claims experience request</xs:documentation>													</xs:annotation>												</xs:enumeration>											</xs:restriction>										</xs:simpleType>									</xs:element>									<xs:element name="versionRec">										<xs:annotation>											<xs:documentation xml:lang="de">Recordversion</xs:documentation>											<xs:documentation xml:lang="en">Record version</xs:documentation>										</xs:annotation>										<xs:simpleType>											<xs:restriction base="versionRecType">												<xs:enumeration value="5.4.1"/>											</xs:restriction>										</xs:simpleType>									</xs:element>								</xs:sequence>							</xs:extension>						</xs:complexContent>					</xs:complexType>				</xs:element>				<xs:choice>					<xs:annotation>						<xs:documentation xml:lang="de">Wahl zwischen einem Geschäfts- und Privatkunden</xs:documentation>						<xs:documentation xml:lang="en">Choice between business and private customer</xs:documentation>					</xs:annotation>					<xs:element name="ICBusiness" type="ICBusinessType">						<xs:annotation>							<xs:documentation xml:lang="de">Geschäftskunde</xs:documentation>							<xs:documentation xml:lang="en">Business customer</xs:documentation>						</xs:annotation>					</xs:element>					<xs:element name="ICPrivatePerson" type="ICPrivatePersonType">						<xs:annotation>							<xs:documentation xml:lang="de">Privatkunde</xs:documentation>							<xs:documentation xml:lang="en">Private customer</xs:documentation>						</xs:annotation>					</xs:element>				</xs:choice>				<xs:element name="claimsExperienceSectors" minOccurs="0">					<xs:annotation>						<xs:documentation xml:lang="de">Branchen des Schadenrendements</xs:documentation>						<xs:documentation xml:lang="en">Sectors of the claims experience</xs:documentation>					</xs:annotation>					<xs:complexType>						<xs:sequence>							<xs:element name="lobIG" type="LOBIGType" maxOccurs="unbounded">								<xs:annotation>									<xs:documentation xml:lang="de">Branchencode IG B2B</xs:documentation>									<xs:documentation xml:lang="de">Branchencode gemäss IG B2B</xs:documentation>									<xs:documentation xml:lang="en">IG B2B sector code</xs:documentation>									<xs:documentation xml:lang="en">Sector code according to IG B2B</xs:documentation>								</xs:annotation>							</xs:element>						</xs:sequence>					</xs:complexType>				</xs:element>				<xs:element name="contractNo" type="contractNoType">					<xs:annotation>						<xs:documentation xml:lang="de">Vertragsnummer</xs:documentation>						<xs:documentation xml:lang="de">Entspricht der eindeutigen Nummer des entsprechenden Vertrages. Vertragsnummernaufbau des einzelnen Versicherers / Unternehmens siehe Spezifikation IG B2B.</xs:documentation>						<xs:documentation xml:lang="en">Contract number</xs:documentation>						<xs:documentation xml:lang="en">This is the unique number of the corresponding contract. For the contract number structure of the individual insurer/company, see IG B2B specification.</xs:documentation>					</xs:annotation>				</xs:element>			</xs:sequence>		</xs:complexType>	</xs:element></xs:schema>