<?xml version="1.0" encoding="UTF-8"?><!--XSD Standard for IG B2B Data exchangeCopyright © IG B2B for Insurers + Brokers, Switzerland--><!--Changes in Version V5.4 from V5.2: - Change version values and names of referenced data schemas from 5.2 to 5.4- Change description of element name="businessFeedback"- Delete element name="contentRec"- Change value of element name="typeRec" from 900 to 901- Change value of element name="subTypeRec" from 009 to 001- Insert new element name="feedbackTypeClaimsExperience"- Insert new element name="feedbackTypeBilling"- Insert new element name="feedbackTypeCommission"- Insert new element name="feedbackTypeContract"- Insert new element name="feedbackTypeCommunication"- Insert new element name="feedbackTypeAttachment"- Delete french and italian textsChanges in Version V5.4.1 from V5.4: - Change version values and names of referenced data schemas from 5.4 to 5.4.1--><xs:schema xmlns:xs="http://www.w3.org/2001/XMLSchema" xmlns="http://www.IGB2B.ch/XMLSchema" targetNamespace="http://www.IGB2B.ch/XMLSchema" elementFormDefault="qualified" version="5.4.1">	<xs:include schemaLocation="globals_V5.4.1.xsd"/>	<xs:include schemaLocation="businessFeedbackTypeLib_V5.4.1.xsd"/>	<xs:element name="businessFeedback">		<xs:annotation>			<xs:documentation xml:lang="de">Standard-Schema für fachliches Feedback</xs:documentation>			<xs:documentation xml:lang="de">Dieser Standard definiert die Struktur einer fachlichen Rückmeldung im Datenaustausch. Es wird zur Meldung von fachlichen Fehlern verwendet.</xs:documentation>			<xs:documentation xml:lang="en">Standard schema for business feedback</xs:documentation>			<xs:documentation xml:lang="en">This standard defines the structure of business feedback in the data exchange. It is used for reporting business errors.</xs:documentation>		</xs:annotation>		<xs:complexType>			<xs:sequence>				<xs:element name="header">					<xs:annotation>						<xs:documentation xml:lang="de">Header</xs:documentation>						<xs:documentation xml:lang="en">Header</xs:documentation>					</xs:annotation>					<xs:complexType>						<xs:complexContent>							<xs:extension base="headerType">								<xs:sequence>									<xs:element name="typeRec">										<xs:annotation>											<xs:documentation xml:lang="de">Recordart</xs:documentation>											<xs:documentation xml:lang="en">Record type</xs:documentation>										</xs:annotation>										<xs:simpleType>											<xs:restriction base="typeRecType">												<xs:enumeration value="901">													<xs:annotation>														<xs:documentation xml:lang="de">Fachliches Feedback</xs:documentation>														<xs:documentation xml:lang="en">Business Feedback</xs:documentation>													</xs:annotation>												</xs:enumeration>											</xs:restriction>										</xs:simpleType>									</xs:element>									<xs:element name="subTypeRec">										<xs:annotation>											<xs:documentation xml:lang="de">Subrecordart</xs:documentation>											<xs:documentation xml:lang="en">Sub-record type</xs:documentation>										</xs:annotation>										<xs:simpleType>											<xs:restriction base="subTypeRecType">												<xs:enumeration value="001">													<xs:annotation>														<xs:documentation xml:lang="de">Feedback</xs:documentation>														<xs:documentation xml:lang="en">Feedback</xs:documentation>													</xs:annotation>												</xs:enumeration>											</xs:restriction>										</xs:simpleType>									</xs:element>									<xs:element name="versionRec">										<xs:annotation>											<xs:documentation xml:lang="de">Recordversion</xs:documentation>											<xs:documentation xml:lang="en">Record version</xs:documentation>										</xs:annotation>										<xs:simpleType>											<xs:restriction base="versionRecType">												<xs:enumeration value="5.4.1"/>											</xs:restriction>										</xs:simpleType>									</xs:element>								</xs:sequence>							</xs:extension>						</xs:complexContent>					</xs:complexType>				</xs:element>				<xs:choice>					<xs:annotation>						<xs:documentation xml:lang="de">Wählen Sie zwischen den verschiedenen Feedback-Typen</xs:documentation>						<xs:documentation xml:lang="en">Choose between the different feedback types</xs:documentation>					</xs:annotation>					<xs:element name="feedbackTypeReferenceNo">						<xs:annotation>							<xs:documentation xml:lang="de">Art der Rückmeldung für Referenznummer</xs:documentation>							<xs:documentation xml:lang="en">Type of feedback for reference number</xs:documentation>						</xs:annotation>						<xs:complexType>							<xs:sequence>								<xs:element name="feedbackSubType" type="feedbackSubTypeReferenceNoLocType">									<xs:annotation>										<xs:documentation xml:lang="de">Feedbacksubtyp, Werte für Feedbacktyp Referenznummer</xs:documentation>										<xs:documentation xml:lang="en">Feedback subtype, values ​​for feedback type reference number</xs:documentation>									</xs:annotation>								</xs:element>							</xs:sequence>						</xs:complexType>					</xs:element>					<xs:element name="feedbackTypeMandateNo">						<xs:annotation>							<xs:documentation xml:lang="de">Art der Rückmeldung für Mandat</xs:documentation>							<xs:documentation xml:lang="en">Type of feedback for Mandate</xs:documentation>						</xs:annotation>						<xs:complexType>							<xs:sequence>								<xs:element name="feedbackSubType" type="feedbackSubTypeMandateNoLocType">									<xs:annotation>										<xs:documentation xml:lang="de">Feedbacksubtyp, Werte für Feedbacktyp Mandat</xs:documentation>										<xs:documentation xml:lang="en">Feedback subtype, values ​​​​for feedback type mandate</xs:documentation>									</xs:annotation>								</xs:element>							</xs:sequence>						</xs:complexType>					</xs:element>					<xs:element name="feedbackTypeCustomerData">						<xs:annotation>							<xs:documentation xml:lang="de">Art der Rückmeldung für Kundendaten</xs:documentation>							<xs:documentation xml:lang="en">Type of feedback for customer data</xs:documentation>						</xs:annotation>						<xs:complexType>							<xs:sequence>								<xs:element name="feedbackSubType" type="feedbackSubTypeCustomerDataLocType">									<xs:annotation>										<xs:documentation xml:lang="de">Feedbacksubtyp, Werte für Feedbacktyp Kundendaten</xs:documentation>										<xs:documentation xml:lang="en">Feedback subtype, values ​​​​for feedback type customer data</xs:documentation>									</xs:annotation>								</xs:element>							</xs:sequence>						</xs:complexType>					</xs:element>					<xs:element name="feedbackTypeClaim">						<xs:annotation>							<xs:documentation xml:lang="de">Art der Rückmeldung für Schaden</xs:documentation>							<xs:documentation xml:lang="en">Type of feedback for claim</xs:documentation>						</xs:annotation>						<xs:complexType>							<xs:sequence>								<xs:element name="feedbackSubType" type="feedbackSubTypeClaimLocType">									<xs:annotation>										<xs:documentation xml:lang="de">Feedbacksubtyp, Werte für Feedbacktyp Schaden</xs:documentation>										<xs:documentation xml:lang="en">Feedback subtype, values ​​​​for feedback type claim</xs:documentation>									</xs:annotation>								</xs:element>							</xs:sequence>						</xs:complexType>					</xs:element>					<xs:element name="feedbackTypeClaimsExperience">						<xs:annotation>							<xs:documentation xml:lang="de">Art der Rückmeldung für Schadenrendement</xs:documentation>							<xs:documentation xml:lang="en">Type of feedback for claimsexperience</xs:documentation>						</xs:annotation>						<xs:complexType>							<xs:sequence>								<xs:element name="feedbackSubType" type="feedbackSubTypeClaimsExperienceLocType">									<xs:annotation>										<xs:documentation xml:lang="de">Feedbacksubtyp, Werte für Feedbacktyp Schadenrendement</xs:documentation>										<xs:documentation xml:lang="en">Feedback subtype, values ​​​​for feedback type claimsexperience</xs:documentation>									</xs:annotation>								</xs:element>							</xs:sequence>						</xs:complexType>					</xs:element>					<xs:element name="feedbackTypeBilling">						<xs:annotation>							<xs:documentation xml:lang="de">Art der Rückmeldung für Rechnung/Mahnung</xs:documentation>							<xs:documentation xml:lang="en">Type of feedback for billing/reminder</xs:documentation>						</xs:annotation>						<xs:complexType>							<xs:sequence>								<xs:element name="feedbackSubType" type="feedbackSubTypeBillingLocType">									<xs:annotation>										<xs:documentation xml:lang="de">Feedbacksubtyp, Werte für Feedbacktyp Rechnung/Mahnung</xs:documentation>										<xs:documentation xml:lang="en">Feedback subtype, values ​​​​for feedback type billing/reminder</xs:documentation>									</xs:annotation>								</xs:element>							</xs:sequence>						</xs:complexType>					</xs:element>					<xs:element name="feedbackTypeCommission">						<xs:annotation>							<xs:documentation xml:lang="de">Art der Rückmeldung für Provisionsabrechnung</xs:documentation>							<xs:documentation xml:lang="en">Type of feedback for commission</xs:documentation>						</xs:annotation>						<xs:complexType>							<xs:sequence>								<xs:element name="feedbackSubType" type="feedbackSubTypeCommissionLocType">									<xs:annotation>										<xs:documentation xml:lang="de">Feedbacksubtyp, Werte für Feedbacktyp Provisionsabrechnung</xs:documentation>										<xs:documentation xml:lang="en">Feedback subtype, values ​​​​for feedback type commission</xs:documentation>									</xs:annotation>								</xs:element>							</xs:sequence>						</xs:complexType>					</xs:element>					<xs:element name="feedbackTypeOffer">						<xs:annotation>							<xs:documentation xml:lang="de">Art der Rückmeldung für Offertanfrage/Offerte</xs:documentation>							<xs:documentation xml:lang="en">Type of feedback for offer</xs:documentation>						</xs:annotation>						<xs:complexType>							<xs:sequence>								<xs:element name="feedbackSubType" type="feedbackSubTypeOfferLocType">									<xs:annotation>										<xs:documentation xml:lang="de">Feedbacksubtyp, Werte für Feedbacktyp Offertanfrage/Offerte</xs:documentation>										<xs:documentation xml:lang="en">Feedback subtype, values ​​​​for feedback type offer</xs:documentation>									</xs:annotation>								</xs:element>							</xs:sequence>						</xs:complexType>					</xs:element>					<xs:element name="feedbackTypeContract">						<xs:annotation>							<xs:documentation xml:lang="de">Art der Rückmeldung für Vertrag</xs:documentation>							<xs:documentation xml:lang="en">Type of feedback for contract</xs:documentation>						</xs:annotation>						<xs:complexType>							<xs:sequence>								<xs:element name="feedbackSubType" type="feedbackSubTypeContractLocType">									<xs:annotation>										<xs:documentation xml:lang="de">Feedbacksubtyp, Werte für Feedbacktyp Vertrag</xs:documentation>										<xs:documentation xml:lang="en">Feedback subtype, values ​​​​for feedback type contract</xs:documentation>									</xs:annotation>								</xs:element>							</xs:sequence>						</xs:complexType>					</xs:element>					<xs:element name="feedbackTypeCommunication">						<xs:annotation>							<xs:documentation xml:lang="de">Art der Rückmeldung für Kommunikation</xs:documentation>							<xs:documentation xml:lang="en">Type of feedback for communication</xs:documentation>						</xs:annotation>						<xs:complexType>							<xs:sequence>								<xs:element name="feedbackSubType" type="feedbackSubTypeCommunicationLocType">									<xs:annotation>										<xs:documentation xml:lang="de">Feedbacksubtyp, Werte für Feedbacktyp Kommunikation</xs:documentation>										<xs:documentation xml:lang="en">Feedback subtype, values ​​​​for feedback type communication</xs:documentation>									</xs:annotation>								</xs:element>							</xs:sequence>						</xs:complexType>					</xs:element>					<xs:element name="feedbackTypeAttachment">						<xs:annotation>							<xs:documentation xml:lang="de">Art der Rückmeldung für angehängte Dateien</xs:documentation>							<xs:documentation xml:lang="en">Type of feedback for attachments</xs:documentation>						</xs:annotation>						<xs:complexType>							<xs:sequence>								<xs:element name="feedbackSubType" type="feedbackSubTypeAttachmentLocType">									<xs:annotation>										<xs:documentation xml:lang="de">Feedbacksubtyp, Werte für Feedbacktyp Attachment</xs:documentation>										<xs:documentation xml:lang="en">Feedback subtype, values ​​​​for feedback type attachment</xs:documentation>									</xs:annotation>								</xs:element>							</xs:sequence>						</xs:complexType>					</xs:element>				</xs:choice>				<xs:element name="feedbackMessage" type="remarkType">					<xs:annotation>						<xs:documentation xml:lang="de">Feedback Nachricht</xs:documentation>						<xs:documentation xml:lang="de">Ergänzende bzw. präzisierende Fehlermeldung, welche Versicherer- oder Broker spezifisch sein kann/darf</xs:documentation>						<xs:documentation xml:lang="en">Feedback message</xs:documentation>						<xs:documentation xml:lang="en">Complementary or more precise error message, which can be insurers or broker specific</xs:documentation>					</xs:annotation>				</xs:element>			</xs:sequence>		</xs:complexType>	</xs:element></xs:schema>