	<xs:simpleType name="SwissLicensePlateType">
		<xs:annotation>
			<xs:documentation xml:lang="de">Datentyp für Schweizer Fahrzeug-Nummernschilder</xs:documentation>
			<xs:documentation xml:lang="de">Gültige Beispiele: ZH 12345 | ZH 345 | ZH 123045 | ZH 003045; Ungültige Beispiele: QW 12345 | ZH 3 345 | ZH 1A;</xs:documentation>
			<xs:documentation xml:lang="en">Data type for Swiss license plates</xs:documentation>
			<xs:documentation xml:lang="en">Examples accepted: ZH 12345 | ZH 345 | ZH 123045 | ZH 003045; Examples invalid: QW 12345 | ZH 3 345 | ZH 1A</xs:documentation>
		</xs:annotation>
		<xs:restriction base="xs:string">
			<xs:maxLength value="9"/>
			<xs:pattern value="(AG|AR|AI|BL|BS|BE|FR|GE|GL|GR|JU|LU|NE|NW|OW|SH|SZ|SO|SG|TI|TG|UR|VD|VS|ZG|ZH) (\d{1,6})"/>
		</xs:restriction>
	</xs:simpleType>
	<xs:simpleType name="contractNoType">
		<xs:annotation>
			<xs:documentation xml:lang="de">Datentyp für Vertragsnummer</xs:documentation>
			<xs:documentation xml:lang="de">Beispiele: 3.555.444.3.11 | D33 3334/3 | A333'333'33</xs:documentation>
			<xs:documentation xml:lang="en">Data type for contract numbers</xs:documentation>
			<xs:documentation xml:lang="en">Examples: 3.555.444.3.11 | D33 3334/3 | A333'333'33</xs:documentation>
		</xs:annotation>
		<xs:restriction base="xs:string">
			<xs:maxLength value="30"/>
			<xs:pattern value=".*[\S].*"/>
		</xs:restriction>
	</xs:simpleType>
	<xs:simpleType name="riskUVGNoType">
		<xs:annotation>
			<xs:documentation xml:lang="de">Datentyp für Risikonummer gemäss Risikoklassifikation UVG der Schweizerischen Bundesbehörde</xs:documentation>
			<xs:documentation xml:lang="de">Beispiele: 2110.02 | 2823.03 | 6350.44</xs:documentation>
			<xs:documentation xml:lang="en">Data type for the risk number according to the Swiss Accident Insurance Act (AIA) risk classification of the Swiss federal authorities</xs:documentation>
			<xs:documentation xml:lang="en">Examples: 2110.02 | 2823.03 | 6350.44</xs:documentation>
		</xs:annotation>
		<xs:restriction base="xs:string">
			<xs:length value="7"/>
			<xs:pattern value="[0-9]{4}.[0-9]{2}"/>
		</xs:restriction>
	</xs:simpleType>
