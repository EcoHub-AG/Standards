<!--
Changes in Version V5.4 from V5.2: 
- Change version value from 5.2 to 5.4
- Change Headertype:
	- Insert new element name="sender" and suborder different sender types
	- Insert new element name="recipient" and suborder the different recipient types
	- Insert new element name="otherExchangePartner" as a new sender type and recipient type
	- Insert new element name="digitalPartnerId" in otherExchangePartner
	- Insert new element name="companyId" in all sender and recipient types
	- Insert new element name="companyName" in all sender and recipient types, it replaces element name="insurerName" and element name="brokerName"
	- Delete element name="insurerName", element name="insurerAddress", element name="insurerEmail" and element name="insurerURL" in element name="insurer"
	- Delete element name="brokerName", element name="brokerAddress", element name="brokerEmail" and element name="brokerURL" in element name="broker" 
- Change description of simpleType name="SwissCompanyIdType"
- Insert new simpleType name="companyIdType"
- Insert restriction values in simpleType name="contentRecType"
- Insert new complexType name="ICBusinessType" and complexType name="ICBusinessLegalType"
- Insert new complexType name="ICPrivatePersonType" and complexType name="ICPrivatePersonLocalityType"
- Insert additional values in simpleType name="genderType"
- Change description of simpleType name="LOBIGType" and insert and change codes
- Change description of simpleType name="LOBINSType"
- Change description of simpleType name="SwissLicensePlateType"
- Change name of element name="riskUVGType" to "riskUVGNoType" and its description
- Insert new complexType name="contactType"
- Insert new element name="clerkEmail"
- Change description of simpleType name="companyNameType"
- Change description of simpleType name="firstnameType"
- Change description of simpleType name="surnameType"
- Insert new simpleType name="nameType"
- Change restriction of simpleType name="zipType"
- Insert new complexType name="digitalDocumentType" 
- Insert new complexType name="digitalSignedDocumentType" with subordered simpleType name="digitalSignatureTrustLevelType"
- Delete french and italian texts
Changes in Version V5.4.1 from V5.4: 
- Change version value from 5.4 to 5.4.1
- Change maxLength and pattern of simpleType name="regFINMANoType"
-->
<xs:schema xmlns:xs="http://www.w3.org/2001/XMLSchema" xmlns="http://www.IGB2B.ch/XMLSchema" targetNamespace="http://www.IGB2B.ch/XMLSchema" elementFormDefault="qualified" version="5.4.1">
	<xs:complexType name="headerType">
		<xs:sequence>
			<xs:element name="identificationNo" type="UUIDType">
				<xs:annotation>
					<xs:documentation xml:lang="de">Identifikations-Nummer</xs:documentation>
					<xs:documentation xml:lang="de">bei der Prozessinitiierung erstellte UUID (Version 4)
						Format: XXXXXXXX-XXXX-XXXX-XXXX-XXXXXXXXXXXX
						Zahl im Hexadezimalformat in fünf Gruppen unterteilt, Angabe des Wertes inkl. Gruppen-Separator</xs:documentation>
					<xs:documentation xml:lang="en">Identification Number</xs:documentation>
					<xs:documentation xml:lang="en">In the process initiation created UUID (version 4) 
						Format: XXXXXXXX-XXXX-XXXX-XXXX-XXXXXXXXXXXX
						Number in hexadecimal format divided into five groups, details of the value including group separator</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="dateTimeCreation" type="xs:dateTime">
				<xs:annotation>
					<xs:documentation xml:lang="de">Erstellt am</xs:documentation>
					<xs:documentation xml:lang="de">yyyy-mm-ddThh:mm:ss = Erstellungsdatum des Records</xs:documentation>
					<xs:documentation xml:lang="en">Created on</xs:documentation>
					<xs:documentation xml:lang="en">yyyy-mm-ddThh:mm:ss = Record creation date</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="sender">
				<xs:annotation>
					<xs:documentation xml:lang="de">Sender</xs:documentation>
					<xs:documentation xml:lang="en">Sender</xs:documentation>
				</xs:annotation>
				<xs:complexType>
					<xs:choice>
						<xs:annotation>
							<xs:documentation xml:lang="de">Wahl des Sendertyps</xs:documentation>
							<xs:documentation xml:lang="en">Selection of the sender type</xs:documentation>
						</xs:annotation>
						<xs:element name="insurer">
							<xs:annotation>
								<xs:documentation xml:lang="de">Versicherer</xs:documentation>
								<xs:documentation xml:lang="en">Insurer</xs:documentation>
							</xs:annotation>
							<xs:complexType>
								<xs:annotation>
									<xs:documentation xml:lang="de">Strukturdefinition für grundlegende Informationen über den Versicherer</xs:documentation>
									<xs:documentation xml:lang="en">Data type definition for structure of basic insurer information</xs:documentation>
								</xs:annotation>
								<xs:sequence>
									<xs:element name="insurerCode" type="insurerCodeType">
										<xs:annotation>
											<xs:documentation xml:lang="de">Versicherernummer FINMA</xs:documentation>
											<xs:documentation xml:lang="en">FINMA insurer number</xs:documentation>
										</xs:annotation>
									</xs:element>
									<xs:element name="companyId" type="companyIdType" minOccurs="0">
										<xs:annotation>
											<xs:documentation xml:lang="de">Unternehmensidentifikationsnummern (UID)</xs:documentation>
											<xs:documentation xml:lang="de">Unternehmensidentifikationsnummern (UID) für Unternehmen der Schweiz oder EU-Mitgliedstaaten gemäss UID-Register der jeweiligen Behörde.</xs:documentation>
											<xs:documentation xml:lang="en">Company identification number (UID)</xs:documentation>
											<xs:documentation xml:lang="en">Unique business identification numbers (UIDs) for companies based in Switzerland or EU member states as specified in the UID register of the respective authority</xs:documentation>
										</xs:annotation>
									</xs:element>
									<xs:element name="companyName" type="companyNameType">
										<xs:annotation>
											<xs:documentation xml:lang="de">Firmenname</xs:documentation>
											<xs:documentation xml:lang="de">Firmenname. Bei einer einfachen Gesellschaft ist Vor- und Nachname einzutragen.</xs:documentation>
											<xs:documentation xml:lang="en">Company name</xs:documentation>
											<xs:documentation xml:lang="en">Company name. For a simple company, name and surname must be entered.</xs:documentation>
										</xs:annotation>
									</xs:element>
								</xs:sequence>
							</xs:complexType>
						</xs:element>
						<xs:element name="broker">
							<xs:annotation>
								<xs:documentation xml:lang="de">Broker</xs:documentation>
								<xs:documentation xml:lang="en">Broker</xs:documentation>
							</xs:annotation>
							<xs:complexType>
								<xs:annotation>
									<xs:documentation xml:lang="de">Strukturdefinition für grundlegende Informationen über den Broker</xs:documentation>
									<xs:documentation xml:lang="en">Data type definition for structure of basic broker information</xs:documentation>
								</xs:annotation>
								<xs:sequence>
									<xs:element name="brokerRegisterNo" type="regFINMANoType">
										<xs:annotation>
											<xs:documentation xml:lang="de">Register Nummer des Brokers</xs:documentation>
											<xs:documentation xml:lang="de">Registernummer für die unabhängigen Vermittler gemäss Definition der IG B2B, d.h. Prefix mit Ländercode und Registerstelle plus Registernummer der FINMA bzw. FMA (Beispiel: CHE-FINMA-10001). Hinweis für Versicherer: Für diejenigen Vermittler, die sich nicht bei der FINMA bzw. FMA registrieren müssen, jedoch am elektronischen Datenaustausch teilnehmen wollen, z.B. Inhouse-Broker, muss der von der IG B2B zugeteilte Code (Beispiel: CHE-IGB2B-50001) erfasst werden.</xs:documentation>
											<xs:documentation xml:lang="en">Register number of the broker</xs:documentation>
											<xs:documentation xml:lang="en">Registers number for the independent intermediaries as defined by the IG B2B ; prefix with country code and register plus register number FINMA or FMA (example: CHE-FINMA-10001). Note for insurers: For those intermediaries who are not at FINMA or FMA but want to participate on electronic data exchange, e.g., in-house brokers, the code assigned by IG B2B (example: CHE-IGB2B-50001) must be recorded.</xs:documentation>
										</xs:annotation>
									</xs:element>
									<xs:element name="companyId" type="companyIdType" minOccurs="0">
										<xs:annotation>
											<xs:documentation xml:lang="de">Unternehmensidentifikationsnummern (UID)</xs:documentation>
											<xs:documentation xml:lang="de">Unternehmensidentifikationsnummern (UID) für Unternehmen der Schweiz oder EU-Mitgliedstaaten gemäss UID-Register der jeweiligen Behörde.</xs:documentation>
											<xs:documentation xml:lang="en">Company identification number (UID)</xs:documentation>
											<xs:documentation xml:lang="en">Unique business identification numbers (UIDs) for companies based in Switzerland or EU member states as specified in the UID register of the respective authority.</xs:documentation>
										</xs:annotation>
									</xs:element>
									<xs:element name="companyName" type="companyNameType">
										<xs:annotation>
											<xs:documentation xml:lang="de">Firmenname</xs:documentation>
											<xs:documentation xml:lang="de">Firmenname. Bei einer einfachen Gesellschaft ist Vor- und Nachname einzutragen.</xs:documentation>
											<xs:documentation xml:lang="en">Company name</xs:documentation>
											<xs:documentation xml:lang="en">Company name. For a simple company, name and surname must be entered.</xs:documentation>
										</xs:annotation>
									</xs:element>
								</xs:sequence>
							</xs:complexType>
						</xs:element>
						<xs:element name="otherExchangePartner">
							<xs:annotation>
								<xs:documentation xml:lang="de">Anderer Partner für Datenaustausch</xs:documentation>
								<xs:documentation xml:lang="en">Other partner for data exchange</xs:documentation>
							</xs:annotation>
							<xs:complexType>
								<xs:annotation>
									<xs:documentation xml:lang="de">Strukturdefinition für grundlegende Informationen über den Partner für den Datenaustausch</xs:documentation>
									<xs:documentation xml:lang="en">Structure definition for basic information about the data exchange partner</xs:documentation>
								</xs:annotation>
								<xs:sequence>
									<xs:element name="digitalPartnerId" type="UUIDType">
										<xs:annotation>
											<xs:documentation xml:lang="de">Identifikationsnummer des digitalen Partners</xs:documentation>
											<xs:documentation xml:lang="de">Eindeutige Identifikationsnummer des digitalen Partners</xs:documentation>
											<xs:documentation xml:lang="en">Identification number of the digital partner</xs:documentation>
											<xs:documentation xml:lang="en">Unique identification number of the digital partner</xs:documentation>
										</xs:annotation>
									</xs:element>
									<xs:element name="companyId" type="companyIdType" minOccurs="0">
										<xs:annotation>
											<xs:documentation xml:lang="de">Unternehmensidentifikationsnummern (UID)</xs:documentation>
											<xs:documentation xml:lang="de">Unternehmensidentifikationsnummern (UID) für Unternehmen der Schweiz oder EU-Mitgliedstaaten gemäss UID-Register der jeweiligen Behörde.</xs:documentation>
											<xs:documentation xml:lang="en">Company identification number (UID)</xs:documentation>
											<xs:documentation xml:lang="en">Unique business identification numbers (UIDs) for companies based in Switzerland or EU member states as specified in the UID register of the respective authority</xs:documentation>
										</xs:annotation>
									</xs:element>
									<xs:element name="companyName" type="companyNameType">
										<xs:annotation>
											<xs:documentation xml:lang="de">Firmenname</xs:documentation>
											<xs:documentation xml:lang="de">Firmenname. Bei einer einfachen Gesellschaft ist Vor- und Nachname einzutragen.</xs:documentation>
											<xs:documentation xml:lang="en">Company name</xs:documentation>
											<xs:documentation xml:lang="en">Company name. For a simple company, name and surname must be entered.</xs:documentation>
										</xs:annotation>
									</xs:element>
								</xs:sequence>
							</xs:complexType>
						</xs:element>
					</xs:choice>
				</xs:complexType>
			</xs:element>
			<xs:element name="recipient">
				<xs:annotation>
					<xs:documentation xml:lang="de">Empfänger</xs:documentation>
					<xs:documentation xml:lang="en">Recipient</xs:documentation>
				</xs:annotation>
				<xs:complexType>
					<xs:choice>
						<xs:annotation>
							<xs:documentation xml:lang="de">Wahl des Empfängertyps</xs:documentation>
							<xs:documentation xml:lang="en">Selection of the recipient type</xs:documentation>
						</xs:annotation>
						<xs:element name="insurer">
							<xs:annotation>
								<xs:documentation xml:lang="de">Versicherer</xs:documentation>
								<xs:documentation xml:lang="en">Insurer</xs:documentation>
							</xs:annotation>
							<xs:complexType>
								<xs:annotation>
									<xs:documentation xml:lang="de">Strukturdefinition für grundlegende Informationen über den Versicherer</xs:documentation>
									<xs:documentation xml:lang="en">Data type definition for structure of basic insurer information</xs:documentation>
								</xs:annotation>
								<xs:sequence>
									<xs:element name="insurerCode" type="insurerCodeType">
										<xs:annotation>
											<xs:documentation xml:lang="de">Versicherernummer FINMA</xs:documentation>
											<xs:documentation xml:lang="en">FINMA insurer number</xs:documentation>
										</xs:annotation>
									</xs:element>
									<xs:element name="companyId" type="companyIdType" minOccurs="0">
										<xs:annotation>
											<xs:documentation xml:lang="de">Unternehmensidentifikationsnummern (UID)</xs:documentation>
											<xs:documentation xml:lang="de">Unternehmensidentifikationsnummern (UID) für Unternehmen der Schweiz oder EU-Mitgliedstaaten gemäss UID-Register der jeweiligen Behörde.</xs:documentation>
											<xs:documentation xml:lang="en">Company identification number (UID)</xs:documentation>
											<xs:documentation xml:lang="en">Unique business identification numbers (UIDs) for companies based in Switzerland or EU member states as specified in the UID register of the respective authority.</xs:documentation>
										</xs:annotation>
									</xs:element>
									<xs:element name="companyName" type="companyNameType">
										<xs:annotation>
											<xs:documentation xml:lang="de">Firmenname</xs:documentation>
											<xs:documentation xml:lang="de">Firmenname. Bei einer einfachen Gesellschaft ist Vor- und Nachname einzutragen.</xs:documentation>
											<xs:documentation xml:lang="en">Company name</xs:documentation>
											<xs:documentation xml:lang="en">Company name. For a simple company, name and surname must be entered.</xs:documentation>
										</xs:annotation>
									</xs:element>
								</xs:sequence>
							</xs:complexType>
						</xs:element>
						<xs:element name="broker">
							<xs:annotation>
								<xs:documentation xml:lang="de">Broker</xs:documentation>
								<xs:documentation xml:lang="en">Broker</xs:documentation>
							</xs:annotation>
							<xs:complexType>
								<xs:annotation>
									<xs:documentation xml:lang="de">Strukturdefinition für grundlegende Informationen über den Broker</xs:documentation>
									<xs:documentation xml:lang="en">Data type definition for structure of basic broker information</xs:documentation>
								</xs:annotation>
								<xs:sequence>
									<xs:element name="brokerRegisterNo" type="regFINMANoType">
										<xs:annotation>
											<xs:documentation xml:lang="de">Register Nummer des Brokers</xs:documentation>
											<xs:documentation xml:lang="de">Registernummer für die unabhängigen Vermittler gemäss Definition der IG B2B, d.h. Prefix mit Ländercode und Registerstelle plus Registernummer der FINMA bzw. FMA (Beispiel: CHE-FINMA-10001). Hinweis für Versicherer: Für diejenigen Vermittler, die sich nicht bei der FINMA bzw. FMA registrieren müssen, jedoch am elektronischen Datenaustausch teilnehmen wollen, z.B. Inhouse-Broker, muss der von der IG B2B zugeteilte Code (Beispiel: CHE-IGB2B-50001) erfasst werden.</xs:documentation>
											<xs:documentation xml:lang="en">Register number of the broker</xs:documentation>
											<xs:documentation xml:lang="en">Registers number for the independent intermediaries as defined by the IG B2B ; prefix with country code and register plus register number FINMA or FMA (example: CHE-FINMA-10001). Note for insurers: For those intermediaries who are not at FINMA or FMA but want to participate on electronic data exchange, e.g., in-house brokers, the code assigned by IG B2B (example: CHE-IGB2B-50001) must be recorded.</xs:documentation>
										</xs:annotation>
									</xs:element>
									<xs:element name="companyId" type="companyIdType" minOccurs="0">
										<xs:annotation>
											<xs:documentation xml:lang="de">Unternehmensidentifikationsnummern (UID)</xs:documentation>
											<xs:documentation xml:lang="de">Unternehmensidentifikationsnummern (UID) für Unternehmen der Schweiz oder EU-Mitgliedstaaten gemäss UID-Register der jeweiligen Behörde.</xs:documentation>
											<xs:documentation xml:lang="en">Company identification number (UID)</xs:documentation>
											<xs:documentation xml:lang="en">Unique business identification numbers (UIDs) for companies based in Switzerland or EU member states as specified in the UID register of the respective authority.</xs:documentation>
										</xs:annotation>
									</xs:element>
									<xs:element name="companyName" type="companyNameType">
										<xs:annotation>
											<xs:documentation xml:lang="de">Firmenname</xs:documentation>
											<xs:documentation xml:lang="de">Firmenname. Bei einer einfachen Gesellschaft ist Vor- und Nachname einzutragen.</xs:documentation>
											<xs:documentation xml:lang="en">Company name</xs:documentation>
											<xs:documentation xml:lang="en">Company name. For a simple company, name and surname must be entered.</xs:documentation>
										</xs:annotation>
									</xs:element>
								</xs:sequence>
							</xs:complexType>
						</xs:element>
						<xs:element name="otherExchangePartner">
							<xs:annotation>
								<xs:documentation xml:lang="de">Anderer Partner für Datenaustausch</xs:documentation>
								<xs:documentation xml:lang="en">Other partner for data exchange</xs:documentation>
							</xs:annotation>
							<xs:complexType>
								<xs:annotation>
									<xs:documentation xml:lang="de">Strukturdefinition für grundlegende Informationen über den Partner für den Datenaustausch</xs:documentation>
									<xs:documentation xml:lang="en">Structure definition for basic information about the data exchange partner</xs:documentation>
								</xs:annotation>
								<xs:sequence>
									<xs:element name="digitalPartnerId" type="UUIDType">
										<xs:annotation>
											<xs:documentation xml:lang="de">Identifikationsnummer des digitalen Partners</xs:documentation>
											<xs:documentation xml:lang="de">Eindeutige Identifikationsnummer des digitalen Partners</xs:documentation>
											<xs:documentation xml:lang="en">Identification number of the digital partner</xs:documentation>
											<xs:documentation xml:lang="en">Unique identification number of the digital partner</xs:documentation>
										</xs:annotation>
									</xs:element>
									<xs:element name="companyId" type="companyIdType" minOccurs="0">
										<xs:annotation>
											<xs:documentation xml:lang="de">Unternehmensidentifikationsnummern (UID)</xs:documentation>
											<xs:documentation xml:lang="de">Unternehmensidentifikationsnummern (UID) für Unternehmen der Schweiz oder EU-Mitgliedstaaten gemäss UID-Register der jeweiligen Behörde.</xs:documentation>
											<xs:documentation xml:lang="en">Company identification number (UID)</xs:documentation>
											<xs:documentation xml:lang="en">Unique business identification numbers (UIDs) for companies based in Switzerland or EU member states as specified in the UID register of the respective authority.</xs:documentation>
										</xs:annotation>
									</xs:element>
									<xs:element name="companyName" type="companyNameType">
										<xs:annotation>
											<xs:documentation xml:lang="de">Firmenname</xs:documentation>
											<xs:documentation xml:lang="de">Firmenname. Bei einer einfachen Gesellschaft ist Vor- und Nachname einzutragen.</xs:documentation>
											<xs:documentation xml:lang="en">Company name</xs:documentation>
											<xs:documentation xml:lang="en">Company name. For a simple company, name and surname must be entered.</xs:documentation>
										</xs:annotation>
									</xs:element>
								</xs:sequence>
							</xs:complexType>
						</xs:element>
					</xs:choice>
				</xs:complexType>
			</xs:element>
		</xs:sequence>
	</xs:complexType>
	<xs:simpleType name="typeRecType">
		<xs:annotation>
			<xs:documentation xml:lang="de">Datentyp für den Record Type gemäss IG B2B Spezifikation</xs:documentation>
			<xs:documentation xml:lang="en">Data type for main records according to IG B2B specification</xs:documentation>
		</xs:annotation>
		<xs:restriction base="xs:NMTOKEN">
			<xs:maxLength value="3"/>
		</xs:restriction>
	</xs:simpleType>
	<xs:simpleType name="subTypeRecType">
		<xs:annotation>
			<xs:documentation xml:lang="de">Datentyp für den Sub Record Type gemäss IG B2B Spezifikation</xs:documentation>
			<xs:documentation xml:lang="en">Data type for sub records according to IG B2B specification</xs:documentation>
		</xs:annotation>
		<xs:restriction base="xs:NMTOKEN">
			<xs:maxLength value="3"/>
		</xs:restriction>
	</xs:simpleType>
	<xs:simpleType name="versionRecType">
		<xs:annotation>
			<xs:documentation xml:lang="de">Datentyp für die Version gemäss IG B2B Spezifikation</xs:documentation>
			<xs:documentation xml:lang="en">Data type for record versions according IG B2B specification</xs:documentation>
		</xs:annotation>
		<xs:restriction base="xs:string">
			<xs:maxLength value="12"/>
		</xs:restriction>
	</xs:simpleType>
	<xs:simpleType name="contentRecType">
		<xs:annotation>
			<xs:documentation xml:lang="de">Datentyp für den Content Record</xs:documentation>
			<xs:documentation xml:lang="en">Data type for record content</xs:documentation>
		</xs:annotation>
		<xs:restriction base="xs:NMTOKEN">
			<xs:maxLength value="2"/>
			<xs:enumeration value="00">
				<xs:annotation>
					<xs:documentation xml:lang="de">Kein Recordinhalt</xs:documentation>
					<xs:documentation xml:lang="en">No record content</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="01">
				<xs:annotation>
					<xs:documentation xml:lang="de">Nichtleben</xs:documentation>
					<xs:documentation xml:lang="en">Non-life</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="02">
				<xs:annotation>
					<xs:documentation xml:lang="de">Einzelleben</xs:documentation>
					<xs:documentation xml:lang="en">Individual life</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="03">
				<xs:annotation>
					<xs:documentation xml:lang="de">Kollektivleben</xs:documentation>
					<xs:documentation xml:lang="en">Group life</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="04">
				<xs:annotation>
					<xs:documentation xml:lang="de">Kombination einzelner oder aller Geschäftsbereiche</xs:documentation>
					<xs:documentation xml:lang="en">Combination of individual or all business units</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="90">
				<xs:annotation>
					<xs:documentation xml:lang="de">Business</xs:documentation>
					<xs:documentation xml:lang="en">Business</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="99">
				<xs:annotation>
					<xs:documentation xml:lang="de">Technisch</xs:documentation>
					<xs:documentation xml:lang="en">Technical</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
		</xs:restriction>
	</xs:simpleType>
	<!--General Type Definitions-->
	<xs:simpleType name="textLine">
		<xs:annotation>
			<xs:documentation xml:lang="de">Datentyp für einzeilige Textfelder</xs:documentation>
			<xs:documentation xml:lang="en">Data type for single line text fields</xs:documentation>
		</xs:annotation>
		<xs:restriction base="xs:string">
			<xs:pattern value="[\S ]*[\S][\S ]*"/>
		</xs:restriction>
	</xs:simpleType>
	<xs:simpleType name="textMultiLine">
		<xs:annotation>
			<xs:documentation xml:lang="de">Datentyp für mehrzeilige Textfelder</xs:documentation>
			<xs:documentation xml:lang="en">Data type for multi line text fields</xs:documentation>
		</xs:annotation>
		<xs:restriction base="xs:string">
			<xs:pattern value="(.|\r|\n)*[\S](.|\r|\n)*"/>
		</xs:restriction>
	</xs:simpleType>
	<xs:simpleType name="yesNoType">
		<xs:annotation>
			<xs:documentation xml:lang="de">Datentyp für ja / nein Felder</xs:documentation>
