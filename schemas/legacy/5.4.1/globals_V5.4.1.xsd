	<xs:simpleType name="zipType">
		<xs:annotation>
			<xs:documentation xml:lang="de">Datentyp für Postleitzahlen (PLZ)</xs:documentation>
			<xs:documentation xml:lang="de">Beispiele: 4154 | 53474 | WC2N 4HS | 10003 | M5P 2N7</xs:documentation>
			<xs:documentation xml:lang="en">Data type for ZIP codes</xs:documentation>
			<xs:documentation xml:lang="en">Examples: 4154 | 53474 | WC2N 4HS | 10003 | M5P 2N7</xs:documentation>
			<!-- 4154 (Swiss ZIP code) | 53474 (German ZIP code) | WC2N 4HS (UK ZIP code) | 10003 (US ZIP code) | M5P 2N7 (CA ZIP code) -->
		</xs:annotation>
		<xs:restriction base="xs:string">
			<xs:maxLength value="10"/>
			<xs:pattern value="[A-Z0-9]{1}[A-Z0-9 ]{0,7}[A-Z0-9]{2}"/>
		</xs:restriction>
