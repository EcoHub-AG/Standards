<?xml version="1.0" encoding="UTF-8"?><!--XSD Standard for IG B2B Data exchangeCopyright © IG B2B for Insurers + Brokers, Switzerland--><!--Changes in Version V5.4rc1 from V5.2: - Change version values and name of referenced data schema from 5.2 to 5.4rc1- Delete simpleType name="insContractModeLocType"- Change name of simpleType name="mandateFilingTypeLocType" to "mandateSubmissionTypeLocType"- Change name of simpleType name="mandateFilingAllTypeLocType" to "mandateFeedbackTypeLocType" and its description- Delete simpleType name="docViaBrokerLocType"- Delete simpleType name="invoiceViaBrokerLocType"- Delete simpleType name="docAdvertisingLocType"- Insert simpleType name="shippingMethodLocType"- Delete complexType name="ICBusinessMainSubLocType"- Change name of elment name="managementContract" to name="ICBusinessManagementContract"- Insert element name="birthday" under element name="ICPrivatePersonMandateSingle" and delete type definition- Change name of element name="ICPrivatePersonMandateSingle" to "singlePersonMandate" and insert new element name="birthday"- Change name of element name="ICPrivatePersonMandateFamily" to "familyMandate" and insert new element name="additionalPerson"- Delete complexType name="ICBusinessLocType"- Delete complexType name="ICPrivatePersonLocType"- Insert new complexType name="digitalSignatureType"- Insert new simpleType name="digitalSignatureTrustLevelType"- Delete french and italian textsChanges in Version V5.4.1 from V5.4rc1: - Change version values and name of referenced data schema from 5.4rc1 to 5.4.1--><xs:schema xmlns:xs="http://www.w3.org/2001/XMLSchema" xmlns="http://www.IGB2B.ch/XMLSchema" targetNamespace="http://www.IGB2B.ch/XMLSchema" elementFormDefault="qualified" version="5.4.1">	<xs:include schemaLocation="globals_V5.4.1.xsd"/>	<xs:simpleType name="mandateKindLocType">		<xs:annotation>			<xs:documentation xml:lang="de">Datentyp für Art des Brokermandats</xs:documentation>			<xs:documentation xml:lang="en">Data type for type of broker mandate</xs:documentation>		</xs:annotation>		<xs:restriction base="xs:NMTOKEN">			<xs:maxLength value="2"/>			<xs:enumeration value="01">				<xs:annotation>					<xs:documentation xml:lang="de">Betreuungsmandat</xs:documentation>					<xs:documentation xml:lang="en">Caretaking mandate</xs:documentation>				</xs:annotation>			</xs:enumeration>			<xs:enumeration value="02">				<xs:annotation>					<xs:documentation xml:lang="de">Beratungsmandat / Auditmandat / Informationsmandat</xs:documentation>					<xs:documentation xml:lang="en">Advisory mandate / Audit mandate / Information mandate</xs:documentation>				</xs:annotation>			</xs:enumeration>		</xs:restriction>	</xs:simpleType>	<xs:simpleType name="mandateSubmissionTypeLocType">		<xs:annotation>			<xs:documentation xml:lang="de">Datentyp für Art der Mandatseinreichung</xs:documentation>			<xs:documentation xml:lang="en">Data type for type of mandate submission</xs:documentation>		</xs:annotation>		<xs:restriction base="xs:NMTOKEN">			<xs:maxLength value="2"/>			<xs:enumeration value="01">				<xs:annotation>					<xs:documentation xml:lang="de">Neues Mandat</xs:documentation>					<xs:documentation xml:lang="en">New mandate</xs:documentation>				</xs:annotation>			</xs:enumeration>			<xs:enumeration value="02">				<xs:annotation>					<xs:documentation xml:lang="de">Mandatsanpassung</xs:documentation>					<xs:documentation xml:lang="en">Amendment of mandate</xs:documentation>				</xs:annotation>			</xs:enumeration>		</xs:restriction>	</xs:simpleType>	<xs:simpleType name="mandateFeedbackLocType">		<xs:annotation>			<xs:documentation xml:lang="en">Datentyp für die Rückmeldung zur Mandatseinreichung</xs:documentation>			<xs:documentation xml:lang="en">Data type for the feedback on the mandate submission</xs:documentation>		</xs:annotation>		<xs:restriction base="xs:NMTOKEN">			<xs:maxLength value="2"/>			<xs:enumeration value="01">				<xs:annotation>					<xs:documentation xml:lang="de">Bestätigung des neuen Mandats</xs:documentation>					<xs:documentation xml:lang="en">Confirmation of the new mandate</xs:documentation>				</xs:annotation>			</xs:enumeration>			<xs:enumeration value="02">				<xs:annotation>					<xs:documentation xml:lang="de">Ablehnung der Mandats</xs:documentation>					<xs:documentation xml:lang="en">Rejection of the mandate</xs:documentation>				</xs:annotation>			</xs:enumeration>		</xs:restriction>	</xs:simpleType>	<xs:simpleType name="mandateCancellationReasonLocType">		<xs:annotation>			<xs:documentation xml:lang="en">Datentyp für den Grund der Mandatsauflösung</xs:documentation>			<xs:documentation xml:lang="en">Data type for type of mandate cancellation</xs:documentation>		</xs:annotation>		<xs:restriction base="xs:NMTOKEN">			<xs:maxLength value="2"/>			<xs:enumeration value="01">				<xs:annotation>					<xs:documentation xml:lang="de">Auflösung des Kundenverhältnisses zwischen Kunde und Broker</xs:documentation>					<xs:documentation xml:lang="en">Cancellation of the customer relationship between the customer and the broker</xs:documentation>				</xs:annotation>			</xs:enumeration>			<xs:enumeration value="02">				<xs:annotation>					<xs:documentation xml:lang="de">Auflösung des Kundenverhältnisses zwischen Kunde und Versicherer</xs:documentation>					<xs:documentation xml:lang="en">Cancellation of the customer relationship between the customer and the insurer</xs:documentation>				</xs:annotation>			</xs:enumeration>			<xs:enumeration value="03">				<xs:annotation>					<xs:documentation xml:lang="de">Wechsel des Mandats auf einen anderen Broker</xs:documentation>					<xs:documentation xml:lang="en">Changing the mandate to another broker</xs:documentation>				</xs:annotation>			</xs:enumeration>		</xs:restriction>	</xs:simpleType>	<xs:simpleType name="shippingMethodLocType">		<xs:annotation>			<xs:documentation xml:lang="de">Datentyp für Versandart</xs:documentation>			<xs:documentation xml:lang="en">Data type for shipping</xs:documentation>		</xs:annotation>		<xs:restriction base="xs:NMTOKEN">			<xs:maxLength value="2"/>			<xs:enumeration value="01">				<xs:annotation>					<xs:documentation xml:lang="de">Versand an Broker</xs:documentation>					<xs:documentation xml:lang="en">Shipping to broker</xs:documentation>				</xs:annotation>			</xs:enumeration>			<xs:enumeration value="02">				<xs:annotation>					<xs:documentation xml:lang="de">Versand direkt an Kunde</xs:documentation>					<xs:documentation xml:lang="en">Shipping direct to customer</xs:documentation>				</xs:annotation>			</xs:enumeration>			<xs:enumeration value="03">				<xs:annotation>					<xs:documentation xml:lang="de">Versand an Broker und Kunde</xs:documentation>					<xs:documentation xml:lang="en">Shipping to broker and customer</xs:documentation>				</xs:annotation>			</xs:enumeration>			<xs:enumeration value="04">				<xs:annotation>					<xs:documentation xml:lang="de">Kein Versand</xs:documentation>					<xs:documentation xml:lang="en">No shipping</xs:documentation>				</xs:annotation>			</xs:enumeration>		</xs:restriction>	</xs:simpleType>	<xs:simpleType name="coBrokerNameLocType">		<xs:annotation>			<xs:documentation xml:lang="de">Datentyp für Name / Vorname des Co-Brokers</xs:documentation>			<xs:documentation xml:lang="en">Data type for name /first name of co-broker</xs:documentation>		</xs:annotation>		<xs:restriction base="textLine">			<xs:maxLength value="100"/>		</xs:restriction>	</xs:simpleType>	<xs:complexType name="mandateCoverageType">		<xs:annotation>			<xs:documentation xml:lang="de">Datentyp für die Abdeckung des Mandats</xs:documentation>			<xs:documentation xml:lang="en">Data type for the coverage of the mandate</xs:documentation>		</xs:annotation>		<xs:choice>			<xs:annotation>				<xs:documentation xml:lang="de">Wahl zwischen Mandat für Geschäfts- oder Privatkunden</xs:documentation>				<xs:documentation xml:lang="en">Choice between mandate for a business customer or private customer</xs:documentation>			</xs:annotation>			<xs:element name="mandateBusiness">				<xs:annotation>					<xs:documentation xml:lang="de">Mandat Geschäftskunde</xs:documentation>					<xs:documentation xml:lang="en">Business customer</xs:documentation>				</xs:annotation>				<xs:complexType>					<xs:sequence>						<xs:choice>							<xs:annotation>								<xs:documentation xml:lang="de">Wahl zwischen einem Einzel- oder Gruppenmandat</xs:documentation>								<xs:documentation xml:lang="en">Choice between a single or a group mandate</xs:documentation>							</xs:annotation>							<xs:element name="ICBusinessMandateSingle" type="setValueTrueType">								<xs:annotation>									<xs:documentation xml:lang="de">Explizite Angabe, dass es sich um ein Einzelmandat handelt</xs:documentation>									<xs:documentation xml:lang="en">Explicit indication that this is an individual mandate</xs:documentation>								</xs:annotation>							</xs:element>							<xs:element name="ICBusinessMandateGroup" maxOccurs="unbounded">								<xs:annotation>									<xs:documentation xml:lang="de">Definition der Organisationseinheit (1-n)</xs:documentation>									<xs:documentation xml:lang="en">Definition of organisation unit (1-n)</xs:documentation>								</xs:annotation>								<xs:complexType>									<xs:sequence>										<xs:element name="orgUnitName" type="companyNameType">											<xs:annotation>												<xs:documentation xml:lang="de">Name der Tochter-/Schwesterfirma</xs:documentation>												<xs:documentation xml:lang="en">Name of subsidiary / affiliate</xs:documentation>											</xs:annotation>										</xs:element>										<xs:element name="orgUnitUID" type="SwissCompanyIdType" minOccurs="0">											<xs:annotation>												<xs:documentation xml:lang="de">UID-Nr. der Tochter-/Schwesterfirma</xs:documentation>												<xs:documentation xml:lang="de">Gemäss Verzeichnis des Bundesamtes für Statistik BFS</xs:documentation>												<xs:documentation xml:lang="en">UID number of subsidiary / affiliate</xs:documentation>												<xs:documentation xml:lang="en">According to the Federal Statistical Office (FSO) directory</xs:documentation>											</xs:annotation>										</xs:element>									</xs:sequence>								</xs:complexType>							</xs:element>						</xs:choice>						<xs:choice>							<xs:annotation>								<xs:documentation xml:lang="de">Wahl zwischen einem Mandat für einen Haupt- oder Untervertrag</xs:documentation>								<xs:documentation xml:lang="en">Choice between a mandate for a main contract or subcontract</xs:documentation>							</xs:annotation>							<xs:element name="ICBusinessMainContract" type="setValueTrueType">								<xs:annotation>									<xs:documentation xml:lang="de">Explizite Angabe, dass es sich um einen Hauptvertag handelt</xs:documentation>									<xs:documentation xml:lang="en">Explicit indication that this is a principle contract</xs:documentation>								</xs:annotation>							</xs:element>							<xs:element name="ICBusinessSubContract">								<xs:annotation>									<xs:documentation xml:lang="de">Explizite Angabe, dass es sich um ein Untervertrag handelt</xs:documentation>									<xs:documentation xml:lang="en">Explicit indication that this is a sub-contract</xs:documentation>								</xs:annotation>								<xs:complexType>									<xs:sequence>										<xs:element name="ICBusinessManagementContract" type="yesNoType">											<xs:annotation>												<xs:documentation xml:lang="de">Verweis auf den Verwaltungsvertrag</xs:documentation>												<xs:documentation xml:lang="en">Reference to the administrative agreement</xs:documentation>											</xs:annotation>										</xs:element>									</xs:sequence>								</xs:complexType>							</xs:element>						</xs:choice>					</xs:sequence>				</xs:complexType>			</xs:element>			<xs:element name="mandatePrivatePerson">				<xs:annotation>					<xs:documentation xml:lang="de">Mandat Privatkunde</xs:documentation>					<xs:documentation xml:lang="en">Mandate private customer</xs:documentation>				</xs:annotation>				<xs:complexType>					<xs:choice>						<xs:annotation>							<xs:documentation xml:lang="de">Wahl zwischen einem Mandat für eine Einzelperson oder Familie</xs:documentation>							<xs:documentation xml:lang="en">Choice between individual or family insurance</xs:documentation>						</xs:annotation>						<xs:element name="singlePersonMandate">							<xs:annotation>								<xs:documentation xml:lang="de">Mandat für eine Einzelperson</xs:documentation>								<xs:documentation xml:lang="en">Mandate for an individual person</xs:documentation>							</xs:annotation>							<xs:complexType>								<xs:sequence>									<xs:element name="birthday" type="xs:date">										<xs:annotation>											<xs:documentation xml:lang="de">Geburtsdatum</xs:documentation>											<xs:documentation xml:lang="en">Date of birth</xs:documentation>										</xs:annotation>									</xs:element>								</xs:sequence>							</xs:complexType>						</xs:element>						<xs:element name="familyMandate">							<xs:annotation>								<xs:documentation xml:lang="de">Mandat für eine Familie</xs:documentation>								<xs:documentation xml:lang="en">Mandate for a family</xs:documentation>							</xs:annotation>							<xs:complexType>								<xs:sequence>									<xs:element name="additionalPerson" maxOccurs="unbounded">										<xs:annotation>											<xs:documentation xml:lang="de">Zusätzliche Person</xs:documentation>											<xs:documentation xml:lang="en">Additional person</xs:documentation>										</xs:annotation>										<xs:complexType>											<xs:sequence>												<xs:element name="firstname" type="firstnameType">													<xs:annotation>														<xs:documentation xml:lang="de">Vorname der zusätzlichen Person</xs:documentation>														<xs:documentation xml:lang="en">First name of additional person</xs:documentation>													</xs:annotation>												</xs:element>												<xs:element name="surname" type="surnameType">													<xs:annotation>														<xs:documentation xml:lang="de">Nachname der zusätzlichen Person</xs:documentation>														<xs:documentation xml:lang="en">Surname of additional person</xs:documentation>													</xs:annotation>												</xs:element>												<xs:element name="birthday" type="xs:date">													<xs:annotation>														<xs:documentation xml:lang="de">Geburtsdatum der zusätzlichen Person</xs:documentation>														<xs:documentation xml:lang="en">Date of birth of additional person</xs:documentation>													</xs:annotation>												</xs:element>											</xs:sequence>										</xs:complexType>									</xs:element>								</xs:sequence>							</xs:complexType>						</xs:element>					</xs:choice>				</xs:complexType>			</xs:element>		</xs:choice>	</xs:complexType></xs:schema>