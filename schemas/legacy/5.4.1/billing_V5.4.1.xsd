<?xml version="1.0" encoding="UTF-8"?><!--XSD Standard for IG B2B Data exchangeCopyright © IG B2B for Insurers + Brokers, Switzerland--><!--Changes in Version V5.4rc1 from V5.2: - Change version values and names of referenced data schemas from 5.2 to 5.4rc1- Change description of element name="billing"- Delete element name="contentRec"- Change description of value 001 of element name="typeRec"- Change description of value 001 of element name="subTypeRec" and delete value 002- Change type of element name="ICBusiness" to "ICBusinessType"- Change type of element name="ICPrivatePerson" to "ICPrivatePersonType"- Change description of element name="contractNo"- Change name of element name="generalAgreement" to "frameworkContractNo" and its description- Change description of element name="invoiceNo"- Change name of element name="mailing" to "shippingMethod" and its description- Change description and type of element name="invoiceTotal"- Change name of element name="invoiceDetails" to "invoiceDetailItem" and its type and description- Insert new element name="billingDocument"- Change element name="attachment" to minOccurs="0" maxOccurs="unbounded"- Delete element name="remarks" - Delete french and italian textsChanges in Version V5.4 from V5.4rc1: - Change version values and names of referenced data schemas from 5.4rc1 to 5.4Changes in Version V5.4.1 from V5.4: - Change version values and names of referenced data schemas from 5.4 to 5.4.1--><xs:schema xmlns:xs="http://www.w3.org/2001/XMLSchema" xmlns="http://www.IGB2B.ch/XMLSchema" targetNamespace="http://www.IGB2B.ch/XMLSchema" elementFormDefault="qualified" version="5.4.1">	<xs:include schemaLocation="globals_V5.4.1.xsd"/>	<xs:include schemaLocation="billingTypeLib_V5.4.1.xsd"/>	<xs:element name="billing">		<xs:annotation>			<xs:documentation xml:lang="de">Standard-Schema für Rechnung</xs:documentation>			<xs:documentation xml:lang="de">Dieser Standard definiert die Struktur einer einzelnen Rechnung. Rechnungen können z.B. Prämienrechnungen z.H Kunde sein.</xs:documentation>			<xs:documentation xml:lang="en">Standard schema for an invoice</xs:documentation>			<xs:documentation xml:lang="en">This standard defines the structure of an individual invoice. Invoices may be, for example, premium invoices for a customer.</xs:documentation>		</xs:annotation>		<xs:complexType>			<xs:sequence>				<xs:element name="header">					<xs:annotation>						<xs:documentation xml:lang="de">Header</xs:documentation>						<xs:documentation xml:lang="en">Header</xs:documentation>					</xs:annotation>					<xs:complexType>						<xs:complexContent>							<xs:extension base="headerType">								<xs:sequence>									<xs:element name="typeRec">										<xs:annotation>											<xs:documentation xml:lang="de">Recordart</xs:documentation>											<xs:documentation xml:lang="en">Record type</xs:documentation>										</xs:annotation>										<xs:simpleType>											<xs:restriction base="typeRecType">												<xs:enumeration value="001">													<xs:annotation>														<xs:documentation xml:lang="de">Rechnungen</xs:documentation>														<xs:documentation xml:lang="en">Invoices</xs:documentation>													</xs:annotation>												</xs:enumeration>											</xs:restriction>										</xs:simpleType>									</xs:element>									<xs:element name="subTypeRec">										<xs:annotation>											<xs:documentation xml:lang="de">Subrecordart</xs:documentation>											<xs:documentation xml:lang="en">Sub-record type</xs:documentation>										</xs:annotation>										<xs:simpleType>											<xs:restriction base="subTypeRecType">												<xs:enumeration value="001">													<xs:annotation>														<xs:documentation xml:lang="de">Prämienrechnung</xs:documentation>														<xs:documentation xml:lang="en">Premium invoice</xs:documentation>													</xs:annotation>												</xs:enumeration>											</xs:restriction>										</xs:simpleType>									</xs:element>									<xs:element name="versionRec">										<xs:annotation>											<xs:documentation xml:lang="de">Recordversion</xs:documentation>											<xs:documentation xml:lang="en">Record version</xs:documentation>										</xs:annotation>										<xs:simpleType>											<xs:restriction base="versionRecType">												<xs:enumeration value="5.4.1"/>											</xs:restriction>										</xs:simpleType>									</xs:element>								</xs:sequence>							</xs:extension>						</xs:complexContent>					</xs:complexType>				</xs:element>				<xs:choice>					<xs:annotation>						<xs:documentation xml:lang="de">Wahl zwischen einem Geschäfts- und Privatkunde</xs:documentation>						<xs:documentation xml:lang="en">Choice between business and private customer</xs:documentation>					</xs:annotation>					<xs:element name="ICBusiness" type="ICBusinessType">						<xs:annotation>							<xs:documentation xml:lang="de">Geschäftskunde</xs:documentation>							<xs:documentation xml:lang="en">Business customer</xs:documentation>						</xs:annotation>					</xs:element>					<xs:element name="ICPrivatePerson" type="ICPrivatePersonType">						<xs:annotation>							<xs:documentation xml:lang="de">Privatkunde</xs:documentation>							<xs:documentation xml:lang="en">Private customer</xs:documentation>						</xs:annotation>					</xs:element>				</xs:choice>				<xs:element name="contractNo" type="contractNoType">					<xs:annotation>						<xs:documentation xml:lang="de">Vertragsnummer</xs:documentation>						<xs:documentation xml:lang="de">Entspricht der eindeutigen Nummer des entsprechenden Vertrages. Vertragsnummernaufbau des einzelnen Versicherers / Unternehmens siehe Spezifikation IG B2B.</xs:documentation>						<xs:documentation xml:lang="en">Contract number</xs:documentation>						<xs:documentation xml:lang="en">This is the unique number of the corresponding contract. For the contract number structure of the individual insurer/company, see IG B2B specification.</xs:documentation>					</xs:annotation>				</xs:element>				<xs:element name="contractNoReplaced" type="contractNoType" minOccurs="0">					<xs:annotation>						<xs:documentation xml:lang="de">Vertragsnummer des ersetzen Vertrages</xs:documentation>						<xs:documentation xml:lang="de">Wenn dem Vertrag eine neue Vertragsnummer zugeteilt wurde, so wird hier die Vertragsnummer des ersetzten Vertrages mitgeliefert werden.</xs:documentation>						<xs:documentation xml:lang="en">Contract number of the replaced contract</xs:documentation>						<xs:documentation xml:lang="en">If a new contract number has been assigned to the contract, the contract number of the replaced contract will be provided here.</xs:documentation>					</xs:annotation>				</xs:element>				<xs:element name="frameworkContractNo" type="frameworkContractNoLocType" minOccurs="0">					<xs:annotation>						<xs:documentation xml:lang="de">Rahmenvertrags-/Flottenvertragsnummer</xs:documentation>						<xs:documentation xml:lang="de">Die Rahmenvertrags-/Flottenvertragsnummer ist im Falle eines vorhandenen, zugehörigen Rahmen- oder Flottenvertrags hier einzufügen.</xs:documentation>						<xs:documentation xml:lang="en">Framework contract/fleet contract number</xs:documentation>						<xs:documentation xml:lang="en">The framework contract/fleet contract number is to be inserted here in the case of an existing, associated framework or fleet contract.</xs:documentation>					</xs:annotation>				</xs:element>				<xs:element name="brokerCodeIns" type="brokerCodeInsLocType" minOccurs="0">					<xs:annotation>						<xs:documentation xml:lang="de">Brokercode Versicherer</xs:documentation>						<xs:documentation xml:lang="de">Broker-Nummer gemäss dem Codeverzeichnis des einzelnen Versicherers.</xs:documentation>						<xs:documentation xml:lang="en">Brokercode Insurer</xs:documentation>						<xs:documentation xml:lang="en">Broker number according to the individual insurer's code directory.</xs:documentation>					</xs:annotation>				</xs:element>				<xs:element name="agency" type="agencyLocType" minOccurs="0">					<xs:annotation>						<xs:documentation xml:lang="de">Agentur/Verkaufsstelle/Provisionsbezüger</xs:documentation>						<xs:documentation xml:lang="de">Agentur-, Verkaufsstellen- oder Provisionsbezüger-Nummer gemäss Codeverzeichnis des einzelnen Versicherers.</xs:documentation>						<xs:documentation xml:lang="en">Agency/sales point/commission agent</xs:documentation>						<xs:documentation xml:lang="en">Agency, sales point or commission agent number according to the individual insurer's code directory.</xs:documentation>					</xs:annotation>				</xs:element>				<xs:element name="invoiceNo" type="noInvoiceLocType" minOccurs="0">					<xs:annotation>						<xs:documentation xml:lang="de">Rechnungsnummer</xs:documentation>						<xs:documentation xml:lang="de">Die Rechnungsnummer ist das eindeutige Identifikationsmerkmal einer Rechnung (z.B. Prämienrechnung).</xs:documentation>						<xs:documentation xml:lang="en">Invoice number</xs:documentation>						<xs:documentation xml:lang="en">The invoice number is the unique identifier of an invoice (e.g. premium invoice).</xs:documentation>					</xs:annotation>				</xs:element>				<xs:element name="invoiceDate" type="xs:date" minOccurs="0">					<xs:annotation>						<xs:documentation xml:lang="de">Rechnungserstellungsdatum</xs:documentation>						<xs:documentation xml:lang="de">JJJJ-MM-TT = Datum der Rechnungserstellung.</xs:documentation>						<xs:documentation xml:lang="en">Date invoice generated</xs:documentation>						<xs:documentation xml:lang="en">YYYY-MM-DD = Date of invoice generated.</xs:documentation>					</xs:annotation>				</xs:element>				<xs:element name="shippingMethod" type="shippingMethodLocType" minOccurs="0">					<xs:annotation>						<xs:documentation xml:lang="de">Versandart</xs:documentation>						<xs:documentation xml:lang="en">Delivery method</xs:documentation>					</xs:annotation>				</xs:element>				<xs:element name="invoice">					<xs:annotation>						<xs:documentation xml:lang="de">Rechnungsdaten</xs:documentation>						<xs:documentation xml:lang="en">Data of invoice</xs:documentation>					</xs:annotation>					<xs:complexType>						<xs:sequence>							<xs:element name="invoiceTotal" type="invoiceDetailType">								<xs:annotation>									<xs:documentation xml:lang="de">Rechnungstotal</xs:documentation>									<xs:documentation xml:lang="de">Das Rechnungstotal weist das Total aller Rechnungsdetailpositionen aus. Es deckt somit das ganze Spektrum der Detailwerte ab und bildet die Totalwerte entsprechend ihrer Attributdefinition in Form von Summen, Maximumwerten oder auch Kombinationswerten aus Wertelisten etc. ab.</xs:documentation>									<xs:documentation xml:lang="en">Invoice total</xs:documentation>									<xs:documentation xml:lang="en">The invoice total displays the total of all invoice detail items. It therefore encompasses the whole spectrum of detailed values and displays the total values according to their attribute definition in the form of totals, maximum values or combined values derived from lists of values, etc.</xs:documentation>								</xs:annotation>							</xs:element>							<xs:element name="invoiceDetailItem" type="invoiceDetailType" minOccurs="0" maxOccurs="unbounded">								<xs:annotation>									<xs:documentation xml:lang="de">Rechnungsdetailposition</xs:documentation>									<xs:documentation xml:lang="en">Invoice detail item</xs:documentation>								</xs:annotation>							</xs:element>						</xs:sequence>					</xs:complexType>				</xs:element>				<xs:element name="billingDocument" type="attachmentType" maxOccurs="unbounded">					<xs:annotation>						<xs:documentation xml:lang="de">Rechnungsdokument</xs:documentation>						<xs:documentation xml:lang="de">Dies beinhaltet eine digitale Kopie des originalen Rechnungsdokuments.</xs:documentation>						<xs:documentation xml:lang="en">Invoice document</xs:documentation>						<xs:documentation xml:lang="en">This includes a digital copy of the original invoice document.</xs:documentation>					</xs:annotation>				</xs:element>				<xs:element name="attachment" type="attachmentType" minOccurs="0" maxOccurs="unbounded">					<xs:annotation>						<xs:documentation xml:lang="de">Dateianhang</xs:documentation>						<xs:documentation xml:lang="en">Attachment</xs:documentation>					</xs:annotation>				</xs:element>			</xs:sequence>		</xs:complexType>	</xs:element></xs:schema>