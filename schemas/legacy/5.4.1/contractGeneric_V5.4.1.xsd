<?xml version="1.0" encoding="UTF-8"?><!--XSD Standard for IG B2B Data exchangeCopyright © IG B2B for Insurers + Brokers, Switzerland--><!--Changes in Version V5.4rc1 from V5.2: - Change version values and names of referenced data schemas from 5.2 to 5.4rc1- Change description of element name="contractGeneric"- Delete element name="contentRec"- Change value description of element name="typeRec" and change value from 010 to 005- Change value description of element name="subTypeRec" and change value from 004 to 002- Change type of element name="ICBusiness" to "ICBusinessType"- Change type of element name="ICPrivatePerson" to "ICPrivatePersonType"- Insert new element name="contractMutation" and subordinate element name="mutationDate" and element name="mutationReason"- Change description of element name="contractNo"- Insert new element name="frameworkContractNo"- Change description of element name="lobIG"- Change description of element name="lobINS"- Change name of element name="insuranceBegin" to "insuranceStartDate" and change description- Change name of element name="insuranceEnd" to "insuranceEndDate" and change description- Change element "customerNo" to a optional field- Insert new element name="brokerCodeIns" - Insert new element name="agency"- Change description of element name="obligatoryBillingContract"- Insert new element name="automaticRenewal"- Insert new element name="noticePeriod"- Insert new element name="tariffPremium" - Delete element name="premNetContract"- Delete element name="premGrossTotalContract"- Change name of element name="extraAmountPremTot" to "instalmentSurcharge" and its description- Change name of element name="taxPremTotal" to "taxesStatutoryCharges" and its description- Insert new element name="contractDocument" incl. extension "originalDocument"- Change element name="attachment" to minOccurs="0" maxOccurs="unbounded"- Delete element name="remarks" - Delete french and italian textsChanges in Version V5.4.1 from V5.4rc1: - Change version values and names of referenced data schemas from 5.4rc1 to 5.4.1--><xs:schema xmlns:xs="http://www.w3.org/2001/XMLSchema" xmlns="http://www.IGB2B.ch/XMLSchema" targetNamespace="http://www.IGB2B.ch/XMLSchema" elementFormDefault="qualified" version="5.4.1">	<xs:include schemaLocation="globals_V5.4.1.xsd"/>	<xs:include schemaLocation="contractGenericTypeLib_V5.4.1.xsd"/>	<xs:element name="contractGeneric">		<xs:annotation>			<xs:documentation xml:lang="de">Standard-Schema für Vertrag</xs:documentation>			<xs:documentation xml:lang="de">Dieser Standard definiert die Struktur eines einzelnen Vertrages / Police.</xs:documentation>			<xs:documentation xml:lang="en">Standard schema for contract</xs:documentation>			<xs:documentation xml:lang="en">This standard defines the structure of an individual contract.</xs:documentation>		</xs:annotation>		<xs:complexType>			<xs:sequence>				<xs:element name="header">					<xs:annotation>						<xs:documentation xml:lang="de">Header</xs:documentation>						<xs:documentation xml:lang="en">Header</xs:documentation>					</xs:annotation>					<xs:complexType>						<xs:complexContent>							<xs:extension base="headerType">								<xs:sequence>									<xs:element name="typeRec">										<xs:annotation>											<xs:documentation xml:lang="de">Recordart</xs:documentation>											<xs:documentation xml:lang="en">Record type</xs:documentation>										</xs:annotation>										<xs:simpleType>											<xs:restriction base="typeRecType">												<xs:enumeration value="005">													<xs:annotation>														<xs:documentation xml:lang="de">Vertrag</xs:documentation>														<xs:documentation xml:lang="en">Contract</xs:documentation>													</xs:annotation>												</xs:enumeration>											</xs:restriction>										</xs:simpleType>									</xs:element>									<xs:element name="subTypeRec">										<xs:annotation>											<xs:documentation xml:lang="de">Subrecordart</xs:documentation>											<xs:documentation xml:lang="en">Sub-record type</xs:documentation>										</xs:annotation>										<xs:simpleType>											<xs:restriction base="subTypeRecType">												<xs:enumeration value="002">													<xs:annotation>														<xs:documentation xml:lang="de">Vertrag generisch</xs:documentation>														<xs:documentation xml:lang="en">Contract generic</xs:documentation>													</xs:annotation>												</xs:enumeration>											</xs:restriction>										</xs:simpleType>									</xs:element>									<xs:element name="versionRec">										<xs:annotation>											<xs:documentation xml:lang="de">Recordversion</xs:documentation>											<xs:documentation xml:lang="en">Record version</xs:documentation>										</xs:annotation>										<xs:simpleType>											<xs:restriction base="versionRecType">												<xs:enumeration value="5.4.1"/>											</xs:restriction>										</xs:simpleType>									</xs:element>								</xs:sequence>							</xs:extension>						</xs:complexContent>					</xs:complexType>				</xs:element>				<xs:choice>					<xs:annotation>						<xs:documentation xml:lang="de">Wahl zwischen einem Geschäfts- und Privatkunden</xs:documentation>						<xs:documentation xml:lang="en">Choice between business and private customer</xs:documentation>					</xs:annotation>					<xs:element name="ICBusiness" type="ICBusinessType">						<xs:annotation>							<xs:documentation xml:lang="de">Geschäftskunde</xs:documentation>							<xs:documentation xml:lang="en">Business customer</xs:documentation>						</xs:annotation>					</xs:element>					<xs:element name="ICPrivatePerson" type="ICPrivatePersonType">						<xs:annotation>							<xs:documentation xml:lang="de">Privatkunde</xs:documentation>							<xs:documentation xml:lang="en">Private customer</xs:documentation>						</xs:annotation>					</xs:element>				</xs:choice>				<xs:element name="contractMutation">					<xs:annotation>						<xs:documentation xml:lang="de">Vertragsmutation</xs:documentation>						<xs:documentation xml:lang="de">Unter einer Vertragsmutation wird eine Änderung verstanden, welche eine Deckungs- oder Prämienänderungen zur Folge hat. Also immer dann, wenn der Versicherer dem Kunden einen neuen Vertrag schicken würde.</xs:documentation>						<xs:documentation xml:lang="en">Contract mutation</xs:documentation>						<xs:documentation xml:lang="en">A contract mutation is understood to be an amendment that results in a change in the applicable cover or premium. This is the case whenever the insurer sends the customer a new contract.</xs:documentation>					</xs:annotation>					<xs:complexType>						<xs:sequence>							<xs:element name="mutationDate" type="xs:date">								<xs:annotation>									<xs:documentation xml:lang="de">Mutationsdatum</xs:documentation>									<xs:documentation xml:lang="de">Effektives Datum, ab wann die Mutation gültig ist</xs:documentation>									<xs:documentation xml:lang="en">Amendment date</xs:documentation>									<xs:documentation xml:lang="en">Effective date from when the mutation is valid</xs:documentation>								</xs:annotation>							</xs:element>							<xs:element name="mutationReason" type="mutationReasonLocType">								<xs:annotation>									<xs:documentation xml:lang="de">Mutationsgrund</xs:documentation>									<xs:documentation xml:lang="en">Reson of mutation</xs:documentation>								</xs:annotation>							</xs:element>						</xs:sequence>					</xs:complexType>				</xs:element>				<xs:element name="customerNo" type="xs:string" minOccurs="0">					<xs:annotation>						<xs:documentation xml:lang="de">Kunden-/Partnernummer</xs:documentation>						<xs:documentation xml:lang="en">Customer or partner number</xs:documentation>					</xs:annotation>				</xs:element>				<xs:element name="contractNo" type="contractNoType">					<xs:annotation>						<xs:documentation xml:lang="de">Vertragsnummer</xs:documentation>						<xs:documentation xml:lang="de">Entspricht der eindeutigen Nummer des entsprechenden Vertrages. Vertragsnummernaufbau des einzelnen Versicherers / Unternehmens siehe Spezifikation IG B2B.</xs:documentation>						<xs:documentation xml:lang="en">Contract number</xs:documentation>						<xs:documentation xml:lang="en">This is the unique number of the corresponding contract. For the contract number structure of the individual insurer/company, see IG B2B specification.</xs:documentation>					</xs:annotation>				</xs:element>				<xs:element name="frameworkContractNo" type="frameworkContractNoLocType" minOccurs="0">					<xs:annotation>						<xs:documentation xml:lang="de">Rahmenvertrags-/Flottenvertragsnummer</xs:documentation>						<xs:documentation xml:lang="de">Die Rahmenvertrags-/Flottenvertragsnummer ist im Falle eines vorhandenen, zugehörigen Rahmen- oder Flottenvertrags hier einzufügen.</xs:documentation>						<xs:documentation xml:lang="en">Framework contract/fleet contract number</xs:documentation>						<xs:documentation xml:lang="en">The framework contract/fleet contract number is to be inserted here in the case of an existing, associated framework or fleet contract.</xs:documentation>					</xs:annotation>				</xs:element>				<xs:element name="brokerCodeIns" type="brokerCodeInsLocType" minOccurs="0">					<xs:annotation>						<xs:documentation xml:lang="de">Brokercode Versicherer</xs:documentation>						<xs:documentation xml:lang="de">Broker-Nummer gemäss dem Codeverzeichnis des einzelnen Versicherers.</xs:documentation>						<xs:documentation xml:lang="en">Insurer's broker code</xs:documentation>						<xs:documentation xml:lang="en">Broker number according to the individual insurer's code directory.</xs:documentation>					</xs:annotation>				</xs:element>				<xs:element name="agency" type="agencyLocType" minOccurs="0">					<xs:annotation>						<xs:documentation xml:lang="de">Agentur/Verkaufsstelle/Provisionsbezüger</xs:documentation>						<xs:documentation xml:lang="de">Agentur-, Verkaufsstellen- oder Provisionsbezüger-Nummer gemäss Codeverzeichnis des einzelnen Versicherers</xs:documentation>						<xs:documentation xml:lang="en">Agency/sales point/commission agent</xs:documentation>						<xs:documentation xml:lang="en">Agency, sales point or commission agent number according to the individual insurer's code directory.</xs:documentation>					</xs:annotation>				</xs:element>				<xs:element name="lobIG" type="LOBIGType">					<xs:annotation>						<xs:documentation xml:lang="de">Branchecode IG B2B</xs:documentation>						<xs:documentation xml:lang="de">Branchencode gemäss IG B2B</xs:documentation>						<xs:documentation xml:lang="en">IG B2B sector code</xs:documentation>						<xs:documentation xml:lang="en">Sector code according to IG B2B</xs:documentation>					</xs:annotation>				</xs:element>				<xs:element name="lobINS" type="LOBINSType" minOccurs="0">					<xs:annotation>						<xs:documentation xml:lang="de">Branchen-/Risikocode oder -bezeichnung des Versicherers/Vorsorgeunternehmens</xs:documentation>						<xs:documentation xml:lang="de">Branchen-/Risikocode oder -bezeichnung gemäss Verzeichnis des einzelnen Versicherers/Vorsorgeunternehmens. Der Detaillierungsgrad sollte so gewählt sein, dass für vor- und nachgelagerte Prozesse ein Bezug zu den jeweiligen Positionen (z.B. Branchencode in Prämienrechnung = Branchencode in Provisionsabrechnung) hergestellt werden kann. Bei kombinierten Produkten ist der übergeordnete Code / Bezeichnung zu wählen.</xs:documentation>						<xs:documentation xml:lang="en">Sector/risk code or designation of the insurer/pension company</xs:documentation>						<xs:documentation xml:lang="en">Sector/risk code or designation according to the directory of the individual insurer/pension company. The level of detail should be sufficient to enable upstream and downstream processes to establish a reference to the respective items (e.g. sector code in premium invoice = sector code in commission calculation). For combined products, the higher-level code/designation is to be selected.</xs:documentation>					</xs:annotation>				</xs:element>				<xs:element name="insuranceStartDate" type="xs:date">					<xs:annotation>						<xs:documentation xml:lang="de">Datum Versicherungsbeginn</xs:documentation>						<xs:documentation xml:lang="de">JJJJ-MM-TT = Datum des Versicherungsbeginns</xs:documentation>						<xs:documentation xml:lang="en">Insurance start date</xs:documentation>						<xs:documentation xml:lang="en">YYYY-MM-DD = Date of insurance inception date</xs:documentation>					</xs:annotation>				</xs:element>				<xs:element name="insuranceEndDate" type="xs:date">					<xs:annotation>						<xs:documentation xml:lang="de">Datum Vertragsende</xs:documentation>						<xs:documentation xml:lang="de">JJJJ-MM-TT = Datum des Versicherungsende</xs:documentation>						<xs:documentation xml:lang="en">Insurance end date</xs:documentation>						<xs:documentation xml:lang="en">YYYY-MM-DD = Date of insurance end date</xs:documentation>					</xs:annotation>				</xs:element>				<xs:element name="typePayInstalment" type="periodicityType">					<xs:annotation>						<xs:documentation xml:lang="de">Zahlungsart und Rate</xs:documentation>						<xs:documentation xml:lang="de">Gibt an, mit welcher Zahlungsart die Prämie bezahlt wird.</xs:documentation>						<xs:documentation xml:lang="en">Manner of payment and rate</xs:documentation>						<xs:documentation xml:lang="en">Details of how the premium is paid.</xs:documentation>					</xs:annotation>				</xs:element>				<xs:element name="annualCancellation" type="yesNoType">					<xs:annotation>						<xs:documentation xml:lang="de">Jährliches Kündigungsrecht</xs:documentation>						<xs:documentation xml:lang="en">Annual cancellation entitlement </xs:documentation>					</xs:annotation>				</xs:element>				<xs:element name="automaticRenewal" type="yesNoType">					<xs:annotation>						<xs:documentation xml:lang="de">Automatische Verlängerung</xs:documentation>						<xs:documentation xml:lang="en">Automatic renewal</xs:documentation>					</xs:annotation>				</xs:element>				<xs:element name="noticePeriod" type="decimal2dot0Type">					<xs:annotation>						<xs:documentation xml:lang="de">Kündigungsfrist</xs:documentation>						<xs:documentation xml:lang="de">Angabe der Kündigungsfrist in Anzahl Monaten.</xs:documentation>						<xs:documentation xml:lang="en">Notice period</xs:documentation>						<xs:documentation xml:lang="en">Indication of the notice period in number of months.</xs:documentation>					</xs:annotation>				</xs:element>				<xs:element name="premiumDueDate" type="xs:gMonthDay" minOccurs="0">					<xs:annotation>						<xs:documentation xml:lang="de">Prämienfälligkeit</xs:documentation>						<xs:documentation xml:lang="de">--MM-TT = Monat und Tag der Hauptfälligkeit der Prämie, Format gemäss ISO 8601 Spezifikation (z.B. --11-15 für den 15. November).</xs:documentation>						<xs:documentation xml:lang="en">Premium due date </xs:documentation>						<xs:documentation xml:lang="en">--MM-DD = Month and day when main premium due, format acording to ISO 8601 specification (e.g. --11-15 for November 15).</xs:documentation>					</xs:annotation>				</xs:element>				<xs:element name="currency" type="currencyCodeType">					<xs:annotation>						<xs:documentation xml:lang="de">Währung</xs:documentation>						<xs:documentation xml:lang="de">Code gemäss ISO-Code. Ohne Angabe der Währung gilt immer die Referenzwährung, d.h. zur Zeit der Schweizer Franken (CHF).</xs:documentation>						<xs:documentation xml:lang="en">Currency</xs:documentation>						<xs:documentation xml:lang="en">Code according to ISO-Code. Without specifying the currency, the reference currency always applies, at the time Swiss Francs (CHF).</xs:documentation>					</xs:annotation>				</xs:element>				<xs:element name="tariffPremium" type="decimal10dot2Type">					<xs:annotation>						<xs:documentation xml:lang="de">Tarifprämie</xs:documentation>						<xs:documentation xml:lang="de">Betrag für Tarifprämie des Vertrages: Entspricht der zu fakturierenden Prämie ohne Steuern, gesetzliche Abgaben und Ratenzuschläge oder eingerechnete Überschussbeteiligungen. Kann bereits Rabatte, Skonti, Gewinnanteile, Reduktionen/Zuschläge aufgrund von Bonus-/Malussysteme oder sonstige Zu- und Abschläge enthalten. Kann Anpassungen aufgrund von Indexveränderungen erfahren. Überschussbeteiligungen sind in Form separater Rechnungen zu fakturieren.</xs:documentation>						<xs:documentation xml:lang="en">Tariff premium</xs:documentation>						<xs:documentation xml:lang="en">Amount for tariff premium of the contract: This is the premium to be invoiced excluding taxes, statutory charges and instalment surcharges or any surplus sharing included. It may already include rebates, discounts, profit sharing, reductions/surcharges due to bonus/malus systems or other increases or reductions. It may be subject to adjustments as a result of index changes. Surplus participations are to be invoiced in the form of separate invoices.</xs:documentation>					</xs:annotation>				</xs:element>				<xs:element name="instalmentSurcharge" type="decimal10dot2Type">					<xs:annotation>						<xs:documentation xml:lang="de">Ratenzuschlag</xs:documentation>						<xs:documentation xml:lang="de">Betrag Ratenzuschlag: Zuschlag auf die Tarifprämie aufgrund des Zahlens in Raten.</xs:documentation>						<xs:documentation xml:lang="en">Instalment surcharge</xs:documentation>						<xs:documentation xml:lang="en">Instalment surcharge amount: Surcharge on the tariff premium due to payment in instalments.</xs:documentation>					</xs:annotation>				</xs:element>				<xs:element name="taxesStatutoryCharges" type="decimal10dot2Type">					<xs:annotation>						<xs:documentation xml:lang="de">Steuern und gesetzliche Abgaben</xs:documentation>						<xs:documentation xml:lang="de">Betrag für Steuern und gesetzliche Abgaben: Zuschläge auf Tarifprämie aufgrund der eidg. Stempelabgabe, Unfallverhütungsbeiträgen, Elementarschadenbeitrag, Feuerlöschsteuer und SVG-Beiträgen.</xs:documentation>						<xs:documentation xml:lang="en">Taxes and statutory charges</xs:documentation>						<xs:documentation xml:lang="en">Amount for taxes and statutory charges: Surcharges on the tariff premium due to federal stamp duty, accident prevention contributions, natural hazard contributions, firefighting taxes and RTA contributions.</xs:documentation>					</xs:annotation>				</xs:element>				<xs:element name="obligatoryBillingContract" type="yesNoType">					<xs:annotation>						<xs:documentation xml:lang="de">Abrechnungs- bzw. deklarationspflichtiger Vertrag</xs:documentation>						<xs:documentation xml:lang="de">Handelt es sich um einen Vertrag, der periodisch abgerechnet wird bzw. einer Deklarationspflicht (z.B. Deklaration von Lohn- oder Umsatzsummen) unterliegt?</xs:documentation>						<xs:documentation xml:lang="en">Contract subject to accounting and declaration requirements</xs:documentation>						<xs:documentation xml:lang="en">Is it a contract that is billed periodically or is subject to a declaration obligation (e.g. declaration of wage or sales totals)?</xs:documentation>					</xs:annotation>				</xs:element>				<xs:element name="indexedContract" type="yesNoType">					<xs:annotation>						<xs:documentation xml:lang="de">Indexierter Vertrag</xs:documentation>						<xs:documentation xml:lang="de">Handelt es sich um einen indexierten Vertrag, der sich periodisch an einem aktuellen Index anpasst (z.B. Index der Konsumentenpreise)?</xs:documentation>						<xs:documentation xml:lang="en">Indexed contract</xs:documentation>						<xs:documentation xml:lang="en">Is it a contract that is billed periodically or is subject to a declaration obligation (e.g. declaration of wage or sales totals)?</xs:documentation>					</xs:annotation>				</xs:element>				<xs:choice>					<xs:annotation>						<xs:documentation xml:lang="de">Wahl, ob eine Überschussbeteiligung vorhanden ist oder nicht</xs:documentation>						<xs:documentation xml:lang="en">Choice of whether there is a profit participation or not</xs:documentation>					</xs:annotation>					<xs:element name="profitParticipation">						<xs:annotation>							<xs:documentation xml:lang="de">Überschussbeteiligung vorhanden</xs:documentation>							<xs:documentation xml:lang="en">Surplus available</xs:documentation>						</xs:annotation>						<xs:complexType>							<xs:sequence>								<xs:element name="ppBeginnDate" type="xs:date">									<xs:annotation>										<xs:documentation xml:lang="de">Beginndatum Überschussperiode</xs:documentation>										<xs:documentation xml:lang="en">Starting date surplus period</xs:documentation>									</xs:annotation>								</xs:element>								<xs:element name="ppEndDate" type="xs:date">									<xs:annotation>										<xs:documentation xml:lang="de">Enddatum Überschussperiode</xs:documentation>										<xs:documentation xml:lang="en">End date of surplus period</xs:documentation>									</xs:annotation>								</xs:element>							</xs:sequence>						</xs:complexType>					</xs:element>					<xs:element name="profitParticipationNA" type="setValueTrueType">						<xs:annotation>							<xs:documentation xml:lang="de">keine Überschussbeteiligung vorhanden</xs:documentation>							<xs:documentation xml:lang="en">No surplus available</xs:documentation>						</xs:annotation>					</xs:element>				</xs:choice>				<xs:element name="contractDocument" maxOccurs="unbounded">					<xs:annotation>						<xs:documentation xml:lang="de">Vertrags-/Policendokument</xs:documentation>						<xs:documentation xml:lang="de">Dies beinhaltet eine digitale Kopie des Vertrags-/Policendokuments.</xs:documentation>						<xs:documentation xml:lang="en">Contract/policy document</xs:documentation>						<xs:documentation xml:lang="en">This includes a digital copy of the contract/policy document.</xs:documentation>					</xs:annotation>					<xs:complexType>						<xs:complexContent>							<xs:extension base="attachmentType">								<xs:sequence>									<xs:element name="originalDocument" type="originalDocumentLocType">										<xs:annotation>											<xs:documentation xml:lang="de">Originaldokument</xs:documentation>											<xs:documentation xml:lang="de">Legt fest, ob es sich beim Attachment um die digitale Version des Originaldokuments oder um ein Ersatzdokument handelt. Ersatzdokumente sind nur für nicht verfügbare digitale Originaldokumente, welche vor dem 01.01.2012 erstellt wurden, erlaubt.</xs:documentation>											<xs:documentation xml:lang="en">Original document</xs:documentation>											<xs:documentation xml:lang="en">Specifies whether the attachment is the digital version of the original document or a replacement document. Replacement documents are only permitted in the case of unavailable original digital documents which were created before 1 January 2012.</xs:documentation>										</xs:annotation>									</xs:element>								</xs:sequence>							</xs:extension>						</xs:complexContent>					</xs:complexType>				</xs:element>				<xs:element name="attachment" type="attachmentType" minOccurs="0" maxOccurs="unbounded">					<xs:annotation>						<xs:documentation xml:lang="de">Dateianhang</xs:documentation>						<xs:documentation xml:lang="en">Attachment</xs:documentation>					</xs:annotation>				</xs:element>			</xs:sequence>		</xs:complexType>	</xs:element></xs:schema>