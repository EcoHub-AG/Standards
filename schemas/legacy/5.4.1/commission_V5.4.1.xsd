<?xml version="1.0" encoding="UTF-8"?><!--XSD Standard for IG B2B Data exchangeCopyright © IG B2B for Insurers + Brokers, Switzerland--><!--Changes in Version V5.4 from V5.2: - Change version values and names of referenced data schemas from 5.2 to 5.4- Change description of element name="commission"- Delete element name="contentRec"- Change value description of element name="subTypeRec" and change value from 000 to 001- Change name of element name="contractC" to "contract" and its description- Change description of element name="contractNo"- Change description of element name="exchangeDateFrom"- Change description of element name="exchangeDateTo"- Change name of element name="generalAgreement" to "frameworkContractNo" and its description and insert in element name="contract" - Delete element name="generalAgreement" under elements name="commStatementTotalOnly", name="commStatementTotal" and name="commStatementDetails"- Change description of element name="invoiceNo"- Change description of element name="commStatementTotalOnly"- Change description of element name="commStatementList"- Change description of element name="commStatementTotal"- Change name of element name="commStatementDetails" to "commStatementDetail" and its description- Change name of element name="datePeriodPremFrom" to "premiumPeriodFromDate" and its description- Change name of element name="datePeriodPremTo" to "premiumPeriodToDate" and its description- Change description of element name="lobIG"- Change description of element name="lobINS"- Change element name="currency" to a mandatory field- Change description of element name="commissionTotal"- Change type of element name="ICBusiness" to "ICBusinessType"- Change type of element name="ICPrivatePerson" to "ICPrivatePersonType"- Change name of element name="amountComm" to "commissionAmount"- Delete element name="remarks" - Delete french and italian textsChanges in Version V5.4.1 from V5.4: - Change version values and names of referenced data schemas from 5.4 to 5.4.1--><xs:schema xmlns:xs="http://www.w3.org/2001/XMLSchema" xmlns="http://www.IGB2B.ch/XMLSchema" targetNamespace="http://www.IGB2B.ch/XMLSchema" elementFormDefault="qualified" version="5.4.1">	<xs:include schemaLocation="globals_V5.4.1.xsd"/>	<xs:include schemaLocation="commissionTypeLib_V5.4.1.xsd"/>	<xs:element name="commission">		<xs:annotation>			<xs:documentation xml:lang="de">Standard-Schema für die Provisionsierung</xs:documentation>			<xs:documentation xml:lang="de">Dieser Standard definiert die Struktur der Auflistung von Provisionen für einen Broker.</xs:documentation>			<xs:documentation xml:lang="en">Standard schema for commissioning</xs:documentation>			<xs:documentation xml:lang="en">This standard defines the structure of the listing of commissions for a broker.</xs:documentation>		</xs:annotation>		<xs:complexType>			<xs:sequence>				<xs:element name="header">					<xs:annotation>						<xs:documentation xml:lang="de">Header</xs:documentation>						<xs:documentation xml:lang="en">Header</xs:documentation>					</xs:annotation>					<xs:complexType>						<xs:complexContent>							<xs:extension base="headerType">								<xs:sequence>									<xs:element name="typeRec">										<xs:annotation>											<xs:documentation xml:lang="de">Recordart</xs:documentation>											<xs:documentation xml:lang="en">Record type</xs:documentation>										</xs:annotation>										<xs:simpleType>											<xs:restriction base="typeRecType">												<xs:enumeration value="003">													<xs:annotation>														<xs:documentation xml:lang="de">Provisionen</xs:documentation>														<xs:documentation xml:lang="en">Commission</xs:documentation>													</xs:annotation>												</xs:enumeration>											</xs:restriction>										</xs:simpleType>									</xs:element>									<xs:element name="subTypeRec">										<xs:annotation>											<xs:documentation xml:lang="de">Subrecordart</xs:documentation>											<xs:documentation xml:lang="en">Sub-record type</xs:documentation>										</xs:annotation>										<xs:simpleType>											<xs:restriction base="subTypeRecType">												<xs:enumeration value="001">													<xs:annotation>														<xs:documentation xml:lang="de">Provisionen</xs:documentation>														<xs:documentation xml:lang="en">Commission</xs:documentation>													</xs:annotation>												</xs:enumeration>											</xs:restriction>										</xs:simpleType>									</xs:element>									<xs:element name="versionRec">										<xs:annotation>											<xs:documentation xml:lang="de">Recordversion</xs:documentation>											<xs:documentation xml:lang="en">Record version</xs:documentation>										</xs:annotation>										<xs:simpleType>											<xs:restriction base="versionRecType">												<xs:enumeration value="5.4.1"/>											</xs:restriction>										</xs:simpleType>									</xs:element>								</xs:sequence>							</xs:extension>						</xs:complexContent>					</xs:complexType>				</xs:element>				<xs:element name="exchangeDateFrom" type="xs:date">					<xs:annotation>						<xs:documentation xml:lang="de">Abrechnungsperiode von Datum</xs:documentation>						<xs:documentation xml:lang="de">Die Abrechnungsperiode von (JJJJ-MM-TT) ist das Beginndatum der Abrechnungsperiode. Die Abrechnungsperiode ist der Zeitraum, in welchem die Auszahlungen der Provisionen dieses Datenfiles stattfindet. Sie bezieht sich dementsprechend immer auf das gesamte Datenfile und muss nicht zwingend einen Bezug zu den Prämienperioden der einzelnen Provisionspositionen haben.</xs:documentation>						<xs:documentation xml:lang="en">Billing period from date</xs:documentation>						<xs:documentation xml:lang="en">The billing period from (YYYY-MM-DD) is the start date of the billing period. The billing period is the period during which the commission payments for this data file are paid out. It therefore always relates to the entire data file and does not necessarily have to refer to the premium periods of the individual commission items.</xs:documentation>					</xs:annotation>				</xs:element>				<xs:element name="exchangeDateTo" type="xs:date">					<xs:annotation>						<xs:documentation xml:lang="de">Abrechnungsperiode bis Datum</xs:documentation>						<xs:documentation xml:lang="de">Die Abrechnungsperiode bis (JJJJ-MM-TT) ist das Enddatum der Abrechnungsperiode. Die Abrechnungsperiode ist der Zeitraum, in welchem die Auszahlungen der Provisionen dieses Datenfiles stattfindet. Sie bezieht sich dementsprechend immer auf das gesamte Datenfile und muss nicht zwingend einen Bezug zu den Prämienperioden der einzelnen Provisionspositionen haben.</xs:documentation>						<xs:documentation xml:lang="en">Billing period to date</xs:documentation>						<xs:documentation xml:lang="en">The billing period to (YYYY-MM-DD) is the end date of the billing period. The billing period is the period during which the commission payments for this data file are paid out. It therefore always relates to the entire data file and does not necessarily have to refer to the premium periods of the individual commission items.</xs:documentation>					</xs:annotation>				</xs:element>				<xs:element name="contract" maxOccurs="unbounded">					<xs:annotation>						<xs:documentation xml:lang="de">Vertrag</xs:documentation>						<xs:documentation xml:lang="en">Contract</xs:documentation>					</xs:annotation>					<xs:complexType>						<xs:sequence>							<xs:element name="contractNoReplaced" type="contractNoType" minOccurs="0">								<xs:annotation>									<xs:documentation xml:lang="de">Vertragsnummer des ersetzten Vertrages</xs:documentation>									<xs:documentation xml:lang="de">Wenn dem Vertrag eine neue Vertragsnummer zugeteilt wurde, so wird hier die Vertragsnummer des ersetzten Vertrages mitgeliefert werden.</xs:documentation>									<xs:documentation xml:lang="en">Contract number of the replaced contract</xs:documentation>									<xs:documentation xml:lang="en">If a new contract number has been assigned to the contract, the contract number of the replaced contract will be provided here.</xs:documentation>								</xs:annotation>							</xs:element>							<xs:element name="frameworkContractNo" type="frameworkContractNoLocType" minOccurs="0">								<xs:annotation>									<xs:documentation xml:lang="de">Rahmenvertrags-/Flottenvertragsnummer</xs:documentation>									<xs:documentation xml:lang="de">Die Rahmenvertrags-/Flottenvertragsnummer ist im Falle eines vorhandenen, zugehörigen Rahmen- oder Flottenvertrags hier einzufügen.</xs:documentation>									<xs:documentation xml:lang="en">The framework contract/fleet contract number</xs:documentation>									<xs:documentation xml:lang="en">The framework contract/fleet contract number is to be inserted here in the case of an existing, associated framework or fleet contract.</xs:documentation>								</xs:annotation>							</xs:element>							<xs:element name="brokerCodeIns" type="brokerCodeInsLocType" minOccurs="0">								<xs:annotation>									<xs:documentation xml:lang="de">Brokercode Versicherer</xs:documentation>									<xs:documentation xml:lang="de">Broker-Nummer gemäss dem Codeverzeichnis des einzelnen Versicherers.</xs:documentation>									<xs:documentation xml:lang="en">Insurer's broker code</xs:documentation>									<xs:documentation xml:lang="en">Broker number according to the individual insurer's code directory</xs:documentation>								</xs:annotation>							</xs:element>							<xs:element name="agency" type="agencyLocType" minOccurs="0">								<xs:annotation>									<xs:documentation xml:lang="de">Agentur/Verkaufsstelle/Provisionsbezüger</xs:documentation>									<xs:documentation xml:lang="de">Agentur-, Verkaufsstellen- oder Provisionsbezüger-Nummer gemäss Codeverzeichnis des einzelnen Versicherers.</xs:documentation>									<xs:documentation xml:lang="en">Agency/sales point/commission agent</xs:documentation>									<xs:documentation xml:lang="en">Agency, sales point or commission agent number according to the individual insurer's code directory.</xs:documentation>								</xs:annotation>							</xs:element>							<xs:element name="invoiceNo" type="noInvoiceLocType" minOccurs="0">								<xs:annotation>									<xs:documentation xml:lang="de">Rechnungsnummer</xs:documentation>									<xs:documentation xml:lang="de">Die Rechnungsnummer ist das eindeutige Identifikationsmerkmal einer Rechnung (z.B. Prämienrechnung).</xs:documentation>									<xs:documentation xml:lang="en">Invoice number</xs:documentation>									<xs:documentation xml:lang="en">The invoice number is the unique identifier of an invoice (e.g. premium invoice).</xs:documentation>								</xs:annotation>							</xs:element>							<xs:element name="invoiceDate" type="xs:date" minOccurs="0">								<xs:annotation>									<xs:documentation xml:lang="de">Rechnungserstellungsdatum</xs:documentation>									<xs:documentation xml:lang="de">JJJJ-MM-TT = Datum der Rechnungserstellung.</xs:documentation>									<xs:documentation xml:lang="en">Invoice creation date</xs:documentation>									<xs:documentation xml:lang="en">YYYY-MM-DD = Date of invoice creation.</xs:documentation>								</xs:annotation>							</xs:element>							<xs:element name="commStatement">								<xs:annotation>									<xs:documentation xml:lang="de">Abrechnungdaten der Brokerprovision</xs:documentation>									<xs:documentation xml:lang="en">Invoicing data of broker provisioning</xs:documentation>								</xs:annotation>								<xs:complexType>									<xs:choice>										<xs:element name="commStatementTotalOnly" type="commStatementTotalType">											<xs:annotation>												<xs:documentation xml:lang="de">Abrechnung der Brokerprovision nur als Total ohne Detailpositionen</xs:documentation>												<xs:documentation xml:lang="en">Calculation of the broker commission only as a total without detailed items</xs:documentation>											</xs:annotation>										</xs:element>										<xs:element name="commStatementList">											<xs:annotation>												<xs:documentation xml:lang="de">Abrechnung der Brokerprovision mit Total und Detailpositionen (1-n)</xs:documentation>												<xs:documentation xml:lang="en">Calculation of the broker commission with total and detailed items (1–n)</xs:documentation>											</xs:annotation>											<xs:complexType>												<xs:sequence>													<xs:element name="commStatementTotal" type="commStatementTotalType">														<xs:annotation>															<xs:documentation xml:lang="de">Abrechnung der Brokerprovision als Total</xs:documentation>															<xs:documentation xml:lang="de">Das Total der Brokerprovision weist das Total aller Detailpositionen aus. Es deckt somit das ganze Spektrum der Detailwerte ab und bildet die Totalwerte entsprechend ihrer Attributdefinition in Form von Summen, Maximumwerten oder auch Kombinationswerten aus Wertelisten etc. ab.</xs:documentation>															<xs:documentation xml:lang="en">Calculation of the broker commission as a total</xs:documentation>															<xs:documentation xml:lang="en">The total broker commission displays the total of all detailed items. It therefore encompasses the whole spectrum of detailed values and displays the total values according to their attribute definition in the form of totals, maximum values or combined values derived from lists of values, etc.</xs:documentation>														</xs:annotation>													</xs:element>													<xs:element name="commStatementDetail" maxOccurs="unbounded">														<xs:annotation>															<xs:documentation xml:lang="de">Detailposition der Brokerprovision (1-n)</xs:documentation>															<xs:documentation xml:lang="en">Individual position of broker commission (1-n)</xs:documentation>														</xs:annotation>														<xs:complexType>															<xs:sequence>																<xs:element name="premiumPeriodFromDate" type="xs:date">																	<xs:annotation>																		<xs:documentation xml:lang="de">Prämienperiode von Datum</xs:documentation>																		<xs:documentation xml:lang="de">Das Prämienperiode von Datum (JJJJ-MM-TT) entspricht dem Beginndatum der Prämienperiode, für welche eine Rechnung gestellt oder eine Provision gutgeschrieben bzw. belastet wird. Bei Teilprämien ist jeweils auch nur diese Teilperiode auszuweisen. Die Prämienperiode kann auch Zeiträume aus Vor- oder Folgemonaten beinhalten. Im Falle einer Provisionsabrechnung ist die Bedingung, dass das Prämienperiode von Datum vor oder in der Abrechnungsperiode dieses Datenfiles liegt, ansonsten deutet dies auf eine Inkonsistenz hin (Provisionen werden i.d.R. nicht vor Vertragsdauerbeginn bezahlt). Bei nicht prämienperiodebezogenen Provisionen (z.B. einmalige Geschenkprämien) ist das Versicherungsbeginn Datum des Vertrages hier einzufügen.</xs:documentation>																		<xs:documentation xml:lang="en">Premium period from date</xs:documentation>																		<xs:documentation xml:lang="en">The premium period from date (YYYY-MM-DD) corresponds to the start date of the premium period for which an invoice is issued or a commission is credited or debited. In the case of partial premiums, only this partial period is to be shown. The premium period may also include periods from previous or subsequent months. In the case of a commission calculation, the premium period from date must fall before or during the billing period for this data file, or else an inconsistency will be indicated (commissions are generally not paid before the start of the contract period). For commissions not related to premium periods (e.g. one-off gift premiums), the insurance start date of the contract must be inserted here.</xs:documentation>																	</xs:annotation>																</xs:element>																<xs:element name="premiumPeriodToDate" type="xs:date">																	<xs:annotation>																		<xs:documentation xml:lang="de">Prämienperiode bis Datum</xs:documentation>																		<xs:documentation xml:lang="de">Das Prämienperiode bis Datum (JJJJ-MM-TT) entspricht dem Enddatum der Prämienperiode, für welche eine Rechnung gestellt oder Provision gutgeschrieben bzw. belastet wird. Bei Teilprämien ist jeweils auch nur diese Teilperiode auszuweisen. Die Prämienperiode kann auch Zeiträume aus Vor- oder Folgemonaten beinhalten. Bei nicht prämienperiodebezogenen Provisionen (z.B. einmalige Geschenkprämien) ist das Versicherungsende Datum des Vertrages hier einzufügen.</xs:documentation>																		<xs:documentation xml:lang="en">Premium period to date</xs:documentation>																		<xs:documentation xml:lang="en">The premium period to date (YYYY-MM-DD) corresponds to the end date of the premium period for which an invoice is issued or a commission is credited or debited. In the case of partial premiums, only this partial period is to be shown. The premium period may also include periods from previous or subsequent months. For commissions not related to premium periods (e.g. one-off gift premiums), the insurance end date of the contract must be inserted here.</xs:documentation>																	</xs:annotation>																</xs:element>																<xs:element name="typeAccountSettlement" type="typeAccountSettlementLocType" minOccurs="0">																	<xs:annotation>																		<xs:documentation xml:lang="de">Abrechnungsart</xs:documentation>																		<xs:documentation xml:lang="de">Bemerkung für Versicherer: Die jährliche Folgecourtage wird unter Code 04 abgerechnet. Für das Einzellebengeschäft sind die Datenfelder ‚Prämie von‘ und ‚Prämie bis‘  nicht relevant. Der Provisionsrecord soll deshalb die Angaben gemäss Datenfelder "Abrechnungsart" und "Zahlungsart und Rate" enthalten.</xs:documentation>																		<xs:documentation xml:lang="en">Invoice form</xs:documentation>																		<xs:documentation xml:lang="en">Remark for insurer: The data fields ‚Premium from‘ and ‚Premium until‘ are not relevant for the single premium business. The provisioning record should therefore contain the information according to the data fields "invoice type" and "payment type and rate".</xs:documentation>																	</xs:annotation>																</xs:element>																<xs:element name="typePayInstalment" type="periodicityType" minOccurs="0">																	<xs:annotation>																		<xs:documentation xml:lang="de">Zahlungsart und Rate</xs:documentation>																		<xs:documentation xml:lang="en">Manner of payment and rate</xs:documentation>																	</xs:annotation>																</xs:element>																<xs:element name="lobIG" type="LOBIGType">																	<xs:annotation>																		<xs:documentation xml:lang="de">Branchencode IG B2B</xs:documentation>																		<xs:documentation xml:lang="de">Branchencode gemäss IG B2B</xs:documentation>																		<xs:documentation xml:lang="en">IG B2B sector code</xs:documentation>																		<xs:documentation xml:lang="en">Sector code according to IG B2B</xs:documentation>																	</xs:annotation>																</xs:element>																<xs:element name="lobINS" type="LOBINSType" minOccurs="0">																	<xs:annotation>																		<xs:documentation xml:lang="de">Branchen-/Risikocode oder -bezeichnung des Versicherers/Vorsorgeunternehmens</xs:documentation>																		<xs:documentation xml:lang="de">Branchen-/Risikocode oder -bezeichnung gemäss Verzeichnis des einzelnen Versicherers/Vorsorgeunternehmens. Der Detaillierungsgrad sollte so gewählt sein, dass für vor- und nachgelagerte Prozesse ein Bezug zu den jeweiligen Positionen (z.B. Branchencode in Prämienrechnung = Branchencode in Provisionsabrechnung) hergestellt werden kann. Bei kombinierten Produkten ist der übergeordnete Code/Bezeichnung zu wählen.</xs:documentation>																		<xs:documentation xml:lang="en">Sector/risk code or designation of the insurer/pension company</xs:documentation>																		<xs:documentation xml:lang="en">Sector/risk code or designation according to the directory of the individual insurer/pension company. The level of detail should be sufficient to enable upstream and downstream processes to establish a reference to the respective items (e.g. sector code in premium invoice = sector code in commission calculation). For combined products, the higher-level code/designation is to be selected.</xs:documentation>																	</xs:annotation>																</xs:element>																<xs:element name="currency" type="currencyCodeType">																	<xs:annotation>																		<xs:documentation xml:lang="de">Währung</xs:documentation>																		<xs:documentation xml:lang="de">Code gemäss ISO-Code. Ohne Angabe der Währung gilt immer die Referenzwährung, d.h. zur Zeit der Schweizer Franken (CHF).</xs:documentation>																		<xs:documentation xml:lang="en">Currency</xs:documentation>																		<xs:documentation xml:lang="en">Code according to ISO-Code. Without specifying the currency, the reference currency always applies, at the time Swiss Francs (CHF).</xs:documentation>																	</xs:annotation>																</xs:element>																<xs:element name="premiumComm" type="decimal10dot2Type">																	<xs:annotation>																		<xs:documentation xml:lang="de">Provisionsberechnungsbasis</xs:documentation>																		<xs:documentation xml:lang="de">Provisionsberechtigte Prämie, Produktionssumme (Einzellebensgeschäft) oder Risikokostenprämie, Deckungskapital (Kollektivleben) gemäss Definition des Versicherers. Der Betrag ist im Normalfall positiv.</xs:documentation>																		<xs:documentation xml:lang="en">Basis on which premium calculated</xs:documentation>																		<xs:documentation xml:lang="en">Provisioning-eligible premium, production total (single premium business) or risk-cost premium, actuarial reserve (collective level) according to the insurer’s definition. The amount is normally positive.</xs:documentation>																	</xs:annotation>																</xs:element>																<xs:element name="typeComm" type="typeCommLocType">																	<xs:annotation>																		<xs:documentation xml:lang="de">Art der Brokerprovision</xs:documentation>																		<xs:documentation xml:lang="de">Hinweis Broker und Brokersoftware: Für die Mobiliar Versicherung (MobiLife) wurden zusätzlich auch folgende Codes zugelassen (siehe Werteliste)</xs:documentation>																		<xs:documentation xml:lang="en">Form of broker commission</xs:documentation>																		<xs:documentation xml:lang="en">Note for broker and brokersoftware: For the insurer Mobiliar (MobiLife) the following codes were additionally approved (see list of values).</xs:documentation>																	</xs:annotation>																</xs:element>																<xs:element name="commissionRate" type="decimal3dot3Type">																	<xs:annotation>																		<xs:documentation xml:lang="de">Provisionssatz in %</xs:documentation>																		<xs:documentation xml:lang="de">Immer Angabe in % im Format xxx.yyy. Hinweis Versicherer: z.B. Ein Provisionssatz von 0.5‰ wird mit 0.05% angezeigt.</xs:documentation>																		<xs:documentation xml:lang="en">Commission rate in %</xs:documentation>																		<xs:documentation xml:lang="en">Always specified in % in the format xxx.yyy. Note for insurer: e.g. a commission rate of 0.5‰ is shown as 0.05%.</xs:documentation>																	</xs:annotation>																</xs:element>																<xs:element name="commissionFactor" type="decimal3dot3Type" minOccurs="0">																	<xs:annotation>																		<xs:documentation xml:lang="de">Provisionsfaktor</xs:documentation>																		<xs:documentation xml:lang="de">Faktor für die Auf- oder Abwertung des Basisprovisionssatzes.</xs:documentation>																		<xs:documentation xml:lang="en">Commission factor</xs:documentation>																		<xs:documentation xml:lang="en">Factor for the increase or decrease in the basic provisioning rate.</xs:documentation>																	</xs:annotation>																</xs:element>																<xs:element name="commissionAmount" type="decimal10dot2Type">																	<xs:annotation>																		<xs:documentation xml:lang="de">Brokerprovision</xs:documentation>																		<xs:documentation xml:lang="de">Der Betrag ist im Normalfall positiv.</xs:documentation>																		<xs:documentation xml:lang="en">Broker commission</xs:documentation>																		<xs:documentation xml:lang="en">The amount is normaly positive.</xs:documentation>																	</xs:annotation>																</xs:element>															</xs:sequence>														</xs:complexType>													</xs:element>												</xs:sequence>											</xs:complexType>										</xs:element>									</xs:choice>								</xs:complexType>							</xs:element>							<xs:choice>								<xs:annotation>									<xs:documentation xml:lang="de">Wahl zwischen einem Geschäfts- und Privatkunden</xs:documentation>									<xs:documentation xml:lang="en">Choice between business and private customer</xs:documentation>								</xs:annotation>								<xs:element name="ICBusiness" type="ICBusinessType">									<xs:annotation>										<xs:documentation xml:lang="de">Geschäftskunde</xs:documentation>										<xs:documentation xml:lang="en">Business customer</xs:documentation>									</xs:annotation>								</xs:element>								<xs:element name="ICPrivatePerson" type="ICPrivatePersonType">									<xs:annotation>										<xs:documentation xml:lang="de">Privatkunde</xs:documentation>										<xs:documentation xml:lang="en">Private customer</xs:documentation>									</xs:annotation>								</xs:element>							</xs:choice>						</xs:sequence>						<xs:attribute name="contractNo" type="contractNoType" use="required">							<xs:annotation>								<xs:documentation xml:lang="de">Vertragsnummer</xs:documentation>								<xs:documentation xml:lang="de">Entspricht der eindeutigen Nummer des entsprechenden Vertrages. Vertragsnummernaufbau des einzelnen Versicherers/Unternehmens siehe Spezifikation IG B2B.</xs:documentation>								<xs:documentation xml:lang="en">Contract number</xs:documentation>								<xs:documentation xml:lang="en">This is the unique number of the corresponding contract. For the contract number structure of the individual insurer/company, see IG B2B specification.</xs:documentation>							</xs:annotation>						</xs:attribute>					</xs:complexType>				</xs:element>				<xs:element name="commissionTotal" type="decimal10dot2Type">					<xs:annotation>						<xs:documentation xml:lang="de">Gesamttotal der Provisionsabrechnung/Buchungsperiode</xs:documentation>						<xs:documentation xml:lang="de">Total aller Provisionsgutschriften und Belastungen, der mit diesem Datenfile für die betroffene Buchungsperiode abgerechneten Provisionierungsfälle. Die Abrechnung kann aufgrund von Stornobuchungen/Verrechnungen etc. different gegenüber der effektiven Auszahlung von Provisionen sein.</xs:documentation>						<xs:documentation xml:lang="en">Total commission calculation/accounting period</xs:documentation>						<xs:documentation xml:lang="en">Total of all commission credits and debits of the commission cases settled with this data file for the accounting period concerned. The calculation may differ from the effective payment of commissions due to reverse postings/offsetting, etc.</xs:documentation>		