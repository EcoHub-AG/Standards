{
  "$schema": "https://json-schema.org/draft/2019-09/schema",
  "description": "Offer acceptance",
  "type": "object",
  "properties": {
    "offerDocument": {
      "type": "object",
      "properties": {
        "signedOffer": {
          "description": "Is offer signed",
          "type": "boolean"
        },
        "signedDocument": {
          "description": "Signed document",
          "type": "array",
          "minItems": 1,
          "items": {
            "$ref": "../../../Generics/v0.6.0/FileType.json"
          }
        }
      },
      "required": [
        "signedOffer"
      ],
      "additionalProperties": false
    },
    "otherAttachmentDocument": {
      "description": "Additional signed or non-signed documents e.g. flyers, product fact sheets, health questionnaire",
      "$ref": "../../../Generics/v0.6.0/MultiFileType.json"
    }
  },
  "required": [
    "offerDocument"
  ],
  "additionalProperties": false
}