{
	"$schema": "https://json-schema.org/draft/2019-09/schema",
	"description": "Fixed insured payroll",
	"type": "array",
	"minItems": 1,
	"items": {
		"type": "object",
		"properties": {
			"personId": {
				"description": "ID of the insured Person",
				"type": "string",
				"format": "uuid",
				"examples": ["345aa049-7693-4710-89d7-e13b87ab988a"]
			},
			"role": {
				"$ref": "BusinessRelationPersonType.json"
			},
			"firstname": {
				"$ref": "../../../Generics/v1.0.0/FirstNameType.json"
			},
			"surname": {
				"$ref": "../../../Generics/v1.0.0/SurNameType.json"
			},
			"birthDate": {
				"$ref": "../../../Generics/v1.0.0/BirthDateType.json"
			},
			"gender": {
				"$ref": "../../../Generics/v1.0.0/GenderType.json"
			},
			"fixedPayroll": {
				"description": "Fixed payroll amount",
				"$ref": "../../../Generics/v1.0.0/MonetaryAmountType.json"
			},
			"benefitCoverage": {
				"$ref": "BenefitCoverageType.json"
			},
			"insuranceType": {
				"$ref": "InsuranceTypeType.json"
			},
			"coverage": {
				"description": "Daily sickness benefits insurance coverage",
				"$ref": "CoverageType.json"
			}
		},
		"additionalProperties": false,
		"required": [
			"surname",
			"firstname",
			"birthDate",
			"gender",
			"benefitCoverage",
			"insuranceType",
			"coverage"
		]
	}
}