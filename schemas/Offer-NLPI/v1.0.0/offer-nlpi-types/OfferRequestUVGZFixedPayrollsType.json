{
	"$schema": "https://json-schema.org/draft/2019-09/schema",
	"type": "array",
	"description": "Fixed insured payroll",
	"minItems": 1,
	"items": {
		"type": "object",
		"properties": {
			"personId": {
				"description": "ID of the insured Person",
				"type": "string",
				"format": "uuid",
				"examples": ["345aa049-7693-4710-89d7-e13b87ab988a"]
			},
			"role": {
				"type": "string",
				"description": "Role of the insured person",
				"$ref": "BusinessRelationPersonType.json"
			},
			"firstname": {
				"$ref": "../../../Generics/v1.0.0/FirstNameType.json"
			},
			"surname": {
				"$ref": "../../../Generics/v1.0.0/SurNameType.json"
			},
			"birthDate": {
				"$ref": "../../../Generics/v1.0.0/BirthDateType.json"
			},
			"gender": {
				"$ref": "../../../Generics/v1.0.0/GenderType.json"
			},
			"fixedPayroll": {
				"description": "Fixed payroll amount in CHF",
				"$ref": "FixedPayrollType.json"
			},
			"surplusPayroll": {
				"description": "Surplus payroll wages in CHF",
				"$ref": "FixedPayrollType.json"
			},
			"coverage": {
				"$ref": "UVGZCoverageType.json"
			}
		},
		"required": [
			"personId",
			"role",
			"gender",
			"surname",
			"firstname",
			"birthDate",
			"fixedPayroll",
			"coverage"
		]
	}
}