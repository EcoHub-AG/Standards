{
	"$schema": "https://json-schema.org/draft/2019-09/schema",
	"type": "object",
	"description": "Schema for a request for an offer in the Offer NLPI process",
	"properties": {
		"processIdentificationNo": {
			"$ref": "../../../Generics/v1.0.0/ProcessIdentificationNoType.json"
		},
		"timestamp": {
			"$ref": "../../../Generics/v1.0.0/TimestampType.json"
		},
		"header": {
			"$ref": "../../../Generics/v1.0.0/HeaderType.json"
		},
		"customer": {
			"$ref": "../offer-nlpi-types/OfferCustomerType.json"
		},
		"coInsuredCustomer": {
			"type": "array",
			"minItems": 1,
			"items": {
				"$ref": "../offer-nlpi-types/OfferCustomerType.json"
			},
			"unevaluatedItems": false
		},
		"agency": {
			"$ref": "../../../Generics/v1.0.0/AgencyType.json"
		},
		"brokerContact": {
			"$ref": "../../../Generics/v1.0.0/ContactType.json"
		},
		"offerRequestType": {
			"$ref": "../offer-nlpi-types/OfferTypeType.json"
		},
		"offerToDate": {
			"type": "string",
			"format": "date",
			"description": "Date of offer by which the insurer must submit the offer"
		},
		"offerLanguage": {
			"$ref": "../../../Generics/v1.0.0/LanguageType.json",
			"description": "Language in which the offer must be made out"
		},
		"currency": {
			"$ref": "../../../Generics/v1.0.0/CurrencyISOCodeType.json"
		},
		"previousInsurer": {
			"description": "Previous insurer",
			"$ref": "../offer-nlpi-types/PreviousInsurerType.json"
		},
		"insuranceStartDate": {
			"$ref": "../offer-nlpi-types/InsuranceStartDateType.json"
		},
		"insuranceDurationInYears": {
			"$ref": "../offer-nlpi-types/InsuranceDurationInYearsType.json"
		},
		"DSB": {
			"$ref": "../offer-nlpi-types/OfferRequestDSBType.json"
		},
		"comment": {
			"$ref": "../../../Generics/v1.0.0/RestrictedStringType.json"
		}
	},
	"additionalProperties": false,
	"required": [
		"processIdentificationNo",
		"insuranceStartDate",
		"previousInsurer",
		"offerRequestType",
		"currency",
		"DSB"
	],
	"unevaluatedProperties": false
}