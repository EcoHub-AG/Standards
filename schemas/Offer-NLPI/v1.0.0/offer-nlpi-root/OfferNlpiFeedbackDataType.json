{
	"$schema": "https://json-schema.org/draft/2019-09/schema",
	"description": "Offer NLPI feedback version 0.3.0",
	"type": "object",
	"properties": {
		"processIdentificationNo": {
			"$ref": "../../../Generics/v1.0.0/ProcessIdentificationNoType.json"
		},
		"timestamp": {
			"$ref": "../../../Generics/v1.0.0/TimestampType.json"
		},
		"header": {
			"$ref": "../../../Generics/v1.0.0/HeaderType.json"
		},
		"customer": {
			"$ref": "../../../Generics/v1.0.0/CustomerType.json"
		},
		"quotationNo": {
			"$ref": "../../../Generics/v1.0.0/QuotationNoType.json"
		},
		"offerDecision": {
			"description": "Decision whether an offer is accepted or rejected",
			"type": "object",
			"properties": {
				"offerAcceptance": {
					"$ref": "../offer-nlpi-types/OfferAcceptanceType.json"
				},
				"offerRejection": {
					"$ref": "../offer-nlpi-types/OfferRejectionType.json"
				}
			},
			"dependencies": {
				"offerAcceptance": {
					"not": {
						"required": [
							"offerRejection"
						]
					}
				},
				"offerRejection": {
					"not": {
						"required": [
							"offerAcceptance"
						]
					}
				}
			},
			"additionalProperties": false
		},
		"offerFeedbackMessage": {
			"$ref": "../../../Generics/v1.0.0/RestrictedStringType.json",
			"description": "Feedback text for the offer acceptance or rejection decision"
		},
		"brokerContact": {
			"$ref": "../../../Generics/v1.0.0/ContactType.json"
		}
	},
	"additionalProperties": false,
	"required": [
		"processIdentificationNo",
		"timestamp",
		"header",
		"customer",
		"quotationNo",
		"offerDecision",
		"brokerContact"
	],
	"unevaluatedProperties": false
}